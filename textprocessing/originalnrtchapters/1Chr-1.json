[
    {
        "chapter": 1,
        "verse": 1,
        "text": "Адам, Сиф, Енос,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 2,
        "text": "Каинан, Малелеил, Иаред,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 3,
        "text": "Енох, Мафусал, Ламех,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 4,
        "text": "Ной и его сыновья Сим, Хам, Иафет.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 5,
        "text": "Сыновья Иафета: Гомер, Магог, Мадай, Иаван, Тувал, Мешех, Фирас.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 6,
        "text": "Сыновья Гомера: Аскеназ, Рифат, Фогарма.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 7,
        "text": "Сыновья Иавана: Елиса, Фарсис, Киттим, Доданим.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 8,
        "text": "Сыновья Хама: Куш, Мицраим, Пут, Ханаан.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 9,
        "text": "Сыновья Куша: Сева, Хавила, Савта, Раама, Савтеха. Сыновья Раамы: Шева, Дедан.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 10,
        "text": "Куш был отцом Нимрода, который стал первым на земле могучим воином.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 11,
        "text": "Мицраим был отцом лудеев, анамеев, легавеев, нафтухеев,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 12,
        "text": "патрусеев, каслухеев (от которых произошли филистимляне) и кафтореев.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 13,
        "text": "Ханаан был отцом Сидона, его первенца, Хетта,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 14,
        "text": "Иевусея, Аморрея, Гергесея,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 15,
        "text": "Евея, Аркея, Синея,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 16,
        "text": "Арвадея, Цемарея, Хамафея.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 17,
        "text": "Сыновья Сима: Елам, Ассур, Арфаксад, Луд, Арам. Сыновья Арама: Уц, Хул, Гефер, Мешех.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 18,
        "text": "Арфаксад был отцом Салы, а Сала - отцом Евера.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 19,
        "text": "У Евера родилось двое сыновей: одного звали Фалек (потому что в его дни земля была разделена), а его брата - Иоктан.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 20,
        "text": "Иоктан был отцом Алмодада, Шалефа, Хацармавета, Иераха,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 21,
        "text": "Гадорама, Узала, Диклы,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 22,
        "text": "Евала, Авимаила, Шевы,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 23,
        "text": "Офира, Хавилы и Иовава. Все они были сыновьями Иоктана.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 24,
        "text": "Сим, Арфаксад, Сала;",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 25,
        "text": "Евер, Фалек, Рагав;",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 26,
        "text": "Серух, Нахор, Фарра;",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 27,
        "text": "Аврам (позже он был переименован в Авраама).",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 28,
        "text": "Сыновья Авраама: Исаак и Измаил.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 29,
        "text": "Вот их родословия: первенец Измаила Наваиоф, Кедар, Адбеел, Мивсам,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 30,
        "text": "Мишма, Дума, Масса, Хадад, Фема,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 31,
        "text": "Иетур, Нафиш и Кедма. Это сыновья Измаила.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 32,
        "text": "Сыновья, которых родила Хеттура, наложница Авраама: Зимран, Иокшан, Медан, Мадиан, Ишбак и Шуах. Сыновья Иокшана: Шева и Дедан.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 33,
        "text": "Сыновья Мадиана: Ефа, Ефер, Ханох, Авида, Елдага - все они потомки Хеттуры.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 34,
        "text": "Авраам был отцом Исаака. Сыновья Исаака: Исав и Израиль.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 35,
        "text": "Сыновья Исава: Елифаз, Рагуил, Иеуш, Иеглом, и Корей.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 36,
        "text": "Сыновья Елифаза: Феман, Омар, Цефо, Гафам, Кеназ, от Тимны - Амалик.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 37,
        "text": "Сыновья Рагуила: Нахаф, Зерах, Шамма и Миза.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 38,
        "text": "Сыновья Сеира: Лотан, Шовал, Цивеон, Ана, Дишон, Ецер и Дишан.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 39,
        "text": "Сыновья Лотана: Хори, Гомам. Сестрой Лотана была Тимна.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 40,
        "text": "Сыновья Шовала: Алеан, Манахаф, Евал, Шефо и Онам. Сыновья Цивеона: Аиа и Ана.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 41,
        "text": "Сын Аны: Дишон. Сыновья Дишона: Хемдан, Эшбан, Ифран и Херан.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 42,
        "text": "Сыновья Ецера: Билган, Зааван и Акан. Сыновья Дишана: Уц и Аран.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 43,
        "text": "Вот цари, которые правили в Едоме еще до того, как в Израиле появились цари: Бела, сын Веора, чей город назывался Дингава.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 44,
        "text": "После смерти Белы царем вместо него стал Иовав, сын Зераха, из Восоры.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 45,
        "text": "После смерти Иовава царем вместо него стал Хушам из страны феманитян.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 46,
        "text": "После смерти Хушама царем вместо него стал Гадад, сын Бедада, который разбил мадианитян в земле Моава. Его город назывался Авив.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 47,
        "text": "После смерти Гадада царем вместо него стал Самла из Масреки.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 48,
        "text": "После смерти Самлы царем вместо него стал Саул из Реховофа-на-Реке.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 49,
        "text": "После смерти Саула царем вместо него стал Ваал-Ханан, сын Ахбора.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 50,
        "text": "После смерти Ваал-Ханана царем вместо него стал Гадад. Его город назывался Пау, а имя его жены было Мегетавеель; она была дочерью Матреда, внучкой Мезагава.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 51,
        "text": "Потом умер и Гадад. Вождями Едома были: Тимна, Алва, Иетеф,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 52,
        "text": "Оливема, Эла, Пинон,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 53,
        "text": "Кеназ, Феман, Мивцар,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 54,
        "text": "Магдиил, Ирам. Это вожди Едома.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    }
]