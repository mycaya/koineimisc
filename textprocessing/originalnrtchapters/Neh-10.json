        "verse": 15,
        "text": "Вунний, Азгад, Бевай,",
        "translation_id": "NRT",
        "book_id": "Neh",
        "book_name": "Nehemiah"
    },
    {
        "chapter": 10,
        "verse": 16,
        "text": "Адония, Бигвай, Адин,",
        "translation_id": "NRT",
