  {
    "chapter": 6,
    "verse": 19,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 3,
    "text": "Однако она не"
  },
  {
    "chapter": 6,
    "verse": 19,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 4,
