[
  {
    "chapter": 6,
    "verse": 1,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "Иисус вместе с учениками"
  },
  {
    "chapter": 6,
    "verse": 1,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "ушел оттуда"
  },
  {
    "chapter": 6,
    "verse": 1,
    "book_name": "Mark",
    "book_id": "Mark",
