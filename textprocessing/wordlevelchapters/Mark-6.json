  },
  {
    "chapter": 6,
    "verse": 14,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 8,
    "text": "и поэтому в Нем"
  },
  {
    "chapter": 6,
    "verse": 14,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 9,
    "text": "действуют такие силы."
  },
  {
    "chapter": 6,
    "verse": 15,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "Другие говорили,"
  },
  {
    "chapter": 6,
    "verse": 15,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "что это Илия."
  },
  {
    "chapter": 6,
    "verse": 15,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 3,
    "text": "А третьи утверждали,"
  },
  {
    "chapter": 6,
    "verse": 15,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 4,
    "text": "что Он пророк,"
  },
  {
    "chapter": 6,
    "verse": 15,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 5,
    "text": "подобный"
  },
  {
    "chapter": 6,
    "verse": 15,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 6,
