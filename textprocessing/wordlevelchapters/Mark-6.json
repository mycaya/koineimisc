  },
  {
    "chapter": 6,
    "verse": 16,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "Услышав эти разговоры,"
  },
  {
    "chapter": 6,
    "verse": 16,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "Ирод решил:"
  },
  {
    "chapter": 6,
    "verse": 16,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 3,
    "text": "Это Иоанн,"
  },
  {
    "chapter": 6,
    "verse": 16,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 4,
    "text": "которого я обезглавил,"
  },
  {
    "chapter": 6,
    "verse": 16,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 5,
    "text": "он воскрес из мертвых."
  },
  {
    "chapter": 6,
    "verse": 17,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "В свое время Ирод приказал"
