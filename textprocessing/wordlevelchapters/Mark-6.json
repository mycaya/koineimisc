    "chapter": 6,
    "verse": 1,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 4,
    "text": "город."
  },
  {
    "chapter": 6,
    "verse": 2,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "Когда наступила суббота,"
  },
  {
    "chapter": 6,
    "verse": 2,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "Он начал учить в синагоге."
  },
  {
    "chapter": 6,
    "verse": 2,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 3,
    "text": "Многие,"
  },
  {
    "chapter": 6,
    "verse": 2,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 4,
    "text": "слушая Его,"
  },
  {
    "chapter": 6,
    "verse": 2,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 5,
    "text": "удивлялись:"
  },
  {
    "chapter": 6,
    "verse": 2,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 6,
    "text": "Откуда это у Него? Кто дал"
  },
  {
    "chapter": 6,
    "verse": 2,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 7,
    "text": "Ему такую мудрость? Как Он"
  },
  {
    "chapter": 6,
    "verse": 2,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 8,
    "text": "совершает такие чудеса?"
  },
  {
    "chapter": 6,
    "verse": 3,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "Разве Он не плотник,"
  },
  {
    "chapter": 6,
    "verse": 3,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "не сын Марии,"
  },
  {
    "chapter": 6,
    "verse": 3,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 3,
    "text": "не брат Иакова,"
  },
  {
    "chapter": 6,
    "verse": 3,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 4,
    "text": "Иосии,"
  },
  {
    "chapter": 6,
    "verse": 3,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 5,
    "text": "Иуды и Симона? И"
  },
  {
    "chapter": 6,
    "verse": 3,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 6,
    "text": "разве не здесь,"
  },
  {
    "chapter": 6,
    "verse": 3,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 7,
    "text": "у нас,"
  },
  {
    "chapter": 6,
    "verse": 3,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 8,
    "text": "живут Его сестры? И"
  },
  {
    "chapter": 6,
    "verse": 3,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 9,
    "text": "они отвергали Его."
  },
  {
    "chapter": 6,
    "verse": 4,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "Иисус же сказал им:"
  },
  {
    "chapter": 6,
    "verse": 4,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "Не бывает пророк без чести,"
  },
  {
    "chapter": 6,
    "verse": 4,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 3,
    "text": "разве только в городе"
  },
  {
    "chapter": 6,
    "verse": 4,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 4,
    "text": "своем и в своем доме."
  },
  {
    "chapter": 6,
    "verse": 5,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "И Он не мог"
  },
  {
    "chapter": 6,
    "verse": 5,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "совершить там ни одного чуда,"
  },
  {
    "chapter": 6,
    "verse": 5,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 3,
    "text": "лишь исцелил"
  },
  {
    "chapter": 6,
    "verse": 5,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 4,
    "text": "нескольких больных,"
  },
  {
    "chapter": 6,
    "verse": 5,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 5,
    "text": "возложив на них руки."
  },
  {
    "chapter": 6,
    "verse": 6,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "Он удивлялся"
  },
  {
    "chapter": 6,
    "verse": 6,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "неверию этих людей."
  },
  {
    "chapter": 6,
    "verse": 6,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 3,
    "text": "Иисус ходил по"
  },
  {
    "chapter": 6,
    "verse": 6,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 4,
    "text": "окрестным селениям и учил."
  },
  {
    "chapter": 6,
    "verse": 7,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "Он созвал двенадцать"
  },
  {
    "chapter": 6,
    "verse": 7,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "учеников и начал посылать"
  },
  {
    "chapter": 6,
    "verse": 7,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 3,
    "text": "их по двое,"
  },
  {
    "chapter": 6,
    "verse": 7,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 4,
    "text": "дав им власть над"
  },
  {
    "chapter": 6,
    "verse": 7,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 5,
    "text": "нечистыми духами."
  },
  {
    "chapter": 6,
    "verse": 8,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "Он дал им"
  },
  {
    "chapter": 6,
    "verse": 8,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "такое наставление:"
  },
  {
    "chapter": 6,
    "verse": 8,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 3,
    "text": "Не берите с собой ничего,"
  },
  {
    "chapter": 6,
    "verse": 8,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 4,
    "text": "кроме посоха:"
  },
  {
    "chapter": 6,
    "verse": 8,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 5,
    "text": "ни хлеба,"
  },
  {
    "chapter": 6,
    "verse": 8,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 6,
    "text": "ни сумки,"
  },
  {
    "chapter": 6,
    "verse": 8,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 7,
    "text": "ни денег в поясе."
  },
  {
    "chapter": 6,
    "verse": 9,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "Обувайтесь в сандалии,"
  },
  {
    "chapter": 6,
    "verse": 9,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "не берите запасной одежды."
  },
  {
    "chapter": 6,
    "verse": 10,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "И если где-нибудь вы"
  },
  {
    "chapter": 6,
    "verse": 10,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "остановитесь в чьем-либо доме,"
  },
  {
    "chapter": 6,
    "verse": 10,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 3,
    "text": "то оставайтесь"
  },
  {
    "chapter": 6,
    "verse": 10,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 4,
    "text": "там до тех пор,"
  },
  {
    "chapter": 6,
    "verse": 10,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 5,
    "text": "пока не покинете это место."
  },
  {
    "chapter": 6,
    "verse": 11,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "А если где-либо вас не"
  },
  {
    "chapter": 6,
    "verse": 11,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "примут"
  },
  {
    "chapter": 6,
    "verse": 11,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 3,
    "text": "и не захотят слушать вас,"
  },
  {
    "chapter": 6,
    "verse": 11,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 4,
    "text": "то,"
  },
  {
    "chapter": 6,
    "verse": 11,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 5,
    "text": "уходя,"
  },
  {
    "chapter": 6,
    "verse": 11,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 6,
    "text": "стряхните пыль с ваших ног,"
  },
  {
    "chapter": 6,
    "verse": 11,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 7,
    "text": "пусть это будет"
  },
  {
    "chapter": 6,
    "verse": 11,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 8,
    "text": "свидетельством против них."
  },
  {
    "chapter": 6,
    "verse": 12,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "Ученики отправились в"
  },
  {
    "chapter": 6,
    "verse": 12,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "путь и проповедовали людям,"
  },
  {
    "chapter": 6,
    "verse": 12,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 3,
    "text": "призывая их к покаянию."
  },
  {
    "chapter": 6,
    "verse": 13,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "Они изгнали много демонов,"
  },
  {
    "chapter": 6,
    "verse": 13,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "и многих больных помазали"
  },
  {
    "chapter": 6,
    "verse": 13,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 3,
