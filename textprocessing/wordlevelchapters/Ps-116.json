[
  {
    "chapter": 116,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Песнь восхождения Давида."
  },
  {
    "chapter": 116,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Обрадовался я,"
  },
  {
    "chapter": 116,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "когда мне сказали:"
  },
  {
    "chapter": 116,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Пойдем в дом Господень."
  },
  {
    "chapter": 116,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ноги наши стоят"
  },
  {
    "chapter": 116,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "у твоих ворот,"
  },
  {
    "chapter": 116,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Иерусалим."
  },
  {
    "chapter": 116,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Иерусалим"
  },
  {
    "chapter": 116,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "построен как город,"
  },
  {
    "chapter": 116,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "связанный воедино."
  },
  {
    "chapter": 116,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Туда поднимаются роды,"
  },
  {
    "chapter": 116,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "роды Господни,"
  },
  {
    "chapter": 116,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "по закону Израилю,"
  },
  {
    "chapter": 116,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "воздать хвалу"
  },
  {
    "chapter": 116,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Имени Господа."
  },
  {
    "chapter": 116,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Там стоят престолы суда,"
  },
  {
    "chapter": 116,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "престолы дома Давидова."
  },
  {
    "chapter": 116,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Молитесь о мире"
  },
  {
    "chapter": 116,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "для Иерусалима:"
  },
  {
    "chapter": 116,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Пусть будет"
  },
  {
    "chapter": 116,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "благополучие у любящих тебя."
  },
  {
    "chapter": 116,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть будет мир в твоих"
  },
  {
    "chapter": 116,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "стенах"
  },
  {
    "chapter": 116,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и благополучие в твоих"
  },
  {
    "chapter": 116,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "дворцах."
  },
  {
    "chapter": 116,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ради братьев и"
  },
  {
    "chapter": 116,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "моих друзей скажу:"
  },
  {
    "chapter": 116,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Мир тебе!"
  },
  {
    "chapter": 116,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ради дома Господа,"
  },
  {
    "chapter": 116,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "нашего Бога,"
  },
  {
    "chapter": 116,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "желаю блага тебе,"
  },
  {
    "chapter": 116,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Иерусалим."
  }
]