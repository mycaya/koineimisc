[
  {
    "chapter": 1,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Зачем возмущаются народы,"
  },
  {
    "chapter": 1,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и язычники"
  },
  {
    "chapter": 1,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "замышляют пустое?"
  },
  {
    "chapter": 1,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Восстают земные цари,"
  },
  {
    "chapter": 1,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и правители собираются"
  },
  {
    "chapter": 1,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "вместе против Господа"
  },
  {
    "chapter": 1,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и против Его Помазанника."
  },
  {
    "chapter": 1,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Цепи Их разорвем,"
  },
  {
    "chapter": 1,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "говорят."
  },
  {
    "chapter": 1,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Оковы Их сбросим!"
  },
  {
    "chapter": 1,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Восседающий на"
  },
  {
    "chapter": 1,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "небесах смеется,"
  },
  {
    "chapter": 1,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Владыка"
  },
  {
    "chapter": 1,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "насмехается над ними."
  },
  {
    "chapter": 1,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Тогда скажет им"
  },
  {
    "chapter": 1,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "в гневе Своем,"
  },
  {
    "chapter": 1,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и яростью Своею"
  },
  {
    "chapter": 1,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "приведет их в смятение:"
  },
  {
    "chapter": 1,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я поставил Царя"
  },
  {
    "chapter": 1,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Моего над Сионом,"
  },
  {
    "chapter": 1,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Моей святой горой."
  },
  {
    "chapter": 1,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Возвещу волю Господа:"
  },
  {
    "chapter": 1,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Он сказал Мне:"
  },
  {
    "chapter": 1,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Ты Сын Мой,"
  },
  {
    "chapter": 1,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "сегодня Я родил Тебя."
  },
  {
    "chapter": 1,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Проси у Меня,"
  },
  {
    "chapter": 1,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и отдам народы"
  },
  {
    "chapter": 1,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Тебе в наследие,"
  },
  {
    "chapter": 1,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "края земли в Твое владение."
  },
  {
    "chapter": 1,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Сокрушишь их"
  },
  {
    "chapter": 1,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "железным скипетром,"
  },
  {
    "chapter": 1,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "как сосуд"
  },
  {
    "chapter": 1,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "глиняный расколешь."
  },
  {
    "chapter": 1,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Поэтому образумьтесь,"
  },
  {
    "chapter": 1,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "цари,"
  },
  {
    "chapter": 1,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "научитесь,"
  },
  {
    "chapter": 1,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "земные судьи!"
  },
  {
    "chapter": 1,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Служите Господу со"
  },
  {
    "chapter": 1,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "страхом и радуйтесь с трепетом."
  },
  {
    "chapter": 1,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Целуйте Сына,"
  },
  {
    "chapter": 1,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "чтобы Он не разгневался,"
  },
  {
    "chapter": 1,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и вы не погибли в пути,"
  },
  {
    "chapter": 1,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "потому что гнев Его"
  },
  {
    "chapter": 1,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "быстро вспыхивает."
  },
  {
    "chapter": 1,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Блаженны все,"
  },
  {
    "chapter": 1,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "кто у Него ищет прибежища."
  }
]