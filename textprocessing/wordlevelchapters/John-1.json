  },
  {
    "chapter": 1,
    "verse": 36,
    "book_name": "John",
    "book_id": "John",
    "part": 2,
    "text": "он сказал:"
  },
  {
    "chapter": 1,
    "verse": 36,
    "book_name": "John",
    "book_id": "John",
    "part": 3,
    "text": "Вот Агнец Божий!"
  },
  {
    "chapter": 1,
    "verse": 37,
    "book_name": "John",
    "book_id": "John",
    "part": 1,
    "text": "Оба ученика,"
  },
  {
    "chapter": 1,
    "verse": 37,
    "book_name": "John",
    "book_id": "John",
