    "verse": 28,
    "book_name": "John",
    "book_id": "John",
    "part": 3,
    "text": "берегу реки Иордан,"
  },
  {
    "chapter": 1,
    "verse": 28,
    "book_name": "John",
    "book_id": "John",
    "part": 4,
    "text": "там,"
  },
  {
    "chapter": 1,
    "verse": 28,
    "book_name": "John",
    "book_id": "John",
    "part": 5,
    "text": "где крестил Иоанн."
  },
  {
    "chapter": 1,
    "verse": 29,
    "book_name": "John",
    "book_id": "John",
    "part": 1,
    "text": "На следующий день Иоанн"
  },
  {
    "chapter": 1,
    "verse": 29,
    "book_name": "John",
    "book_id": "John",
    "part": 2,
    "text": "увидел"
  },
  {
