  },
  {
    "chapter": 1,
    "verse": 39,
    "book_name": "John",
    "book_id": "John",
    "part": 5,
    "text": "Они пошли,"
  },
  {
    "chapter": 1,
    "verse": 39,
    "book_name": "John",
    "book_id": "John",
    "part": 6,
    "text": "увидели,"
  },
  {
    "chapter": 1,
    "verse": 39,
    "book_name": "John",
    "book_id": "John",
