[
  {
    "chapter": 47,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Псалом Асафа."
  },
  {
    "chapter": 47,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господь Бог богов;"
  },
  {
    "chapter": 47,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Он призывает землю"
  },
  {
    "chapter": 47,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "от востока до запада."
  },
  {
    "chapter": 47,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "С Сиона,"
  },
  {
    "chapter": 47,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "совершенного красотой,"
  },
  {
    "chapter": 47,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Бог являет Свой свет."
  },
  {
    "chapter": 47,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Бог наш идет и в"
  },
  {
    "chapter": 47,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "безмолвии не останется."
  },
  {
    "chapter": 47,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Огонь пожирает перед Ним,"
  },
  {
    "chapter": 47,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и вокруг Него буря ярится."
  },
  {
    "chapter": 47,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он призывает небеса сверху"
  },
  {
    "chapter": 47,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и землю быть свидетелями"
  },
  {
    "chapter": 47,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Его суда над Своим народом:"
  },
  {
    "chapter": 47,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Соберите ко"
  },
  {
    "chapter": 47,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Мне Моих верных,"
  },
  {
    "chapter": 47,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "кто заключил со"
  },
  {
    "chapter": 47,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Мной завет при жертве."
  },
  {
    "chapter": 47,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "И небеса возвещают"
  },
  {
    "chapter": 47,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "о праведности Его,"
  },
  {
    "chapter": 47,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "потому что Сам Бог судья."
  },
  {
    "chapter": 47,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Пауза"
  },
  {
    "chapter": 47,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Слушай,"
  },
  {
    "chapter": 47,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "народ Мой,"
  },
  {
    "chapter": 47,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и Я буду говорить;"
  },
  {
    "chapter": 47,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Израиль,"
  },
  {
    "chapter": 47,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Я буду против тебя"
  },
  {
    "chapter": 47,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "свидетельствовать."
  },
  {
    "chapter": 47,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "Я Бог,"
  },
  {
    "chapter": 47,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 8,
    "text": "твой Бог."
  },
  {
    "chapter": 47,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Не за жертвы твои Я тебя"
  },
  {
    "chapter": 47,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "корю твои всесожжения"
  },
  {
    "chapter": 47,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "всегда предо Мною."
  },
  {
    "chapter": 47,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Мне не нужен ни"
  },
  {
    "chapter": 47,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "бык из твоих загонов,"
  },
  {
    "chapter": 47,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "ни козлы из твоих дворов,"
  },
  {
    "chapter": 47,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "ведь все звери в лесу Мои,"
  },
  {
    "chapter": 47,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и скот на тысяче гор."
  },
  {
    "chapter": 47,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я знаю всех"
  },
  {
    "chapter": 47,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "птиц в горах и все,"
  },
  {
    "chapter": 47,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "что живет в полях."
  },
  {
    "chapter": 47,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Если бы Я был голоден,"
  },
  {
    "chapter": 47,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Я бы тебе не сказал,"
  },
  {
    "chapter": 47,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "ведь вселенная Моя и все,"
  },
  {
    "chapter": 47,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "что наполняет ее."
  },
  {
    "chapter": 47,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Разве Я ем мясо быков"
  },
  {
    "chapter": 47,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "или пью козлиную кровь?"
  },
  {
    "chapter": 47,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Принеси хвалу"
  },
  {
    "chapter": 47,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "в жертву Богу,"
  },
  {
    "chapter": 47,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "исполни свои"
  },
  {
    "chapter": 47,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "обеты пред Всевышним,"
  },
  {
    "chapter": 47,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "и тогда призови"
  },
  {
    "chapter": 47,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Меня в день бедствия,"
  },
  {
    "chapter": 47,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и Я спасу тебя,"
  },
  {
    "chapter": 47,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и ты прославишь Меня."
  },
  {
    "chapter": 47,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но нечестивому Бог говорит:"
  },
  {
    "chapter": 47,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Как ты смеешь возвещать Мои"
  },
  {
    "chapter": 47,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "уставы"
  },
  {
    "chapter": 47,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и говорить о Моем завете?"
  },
  {
    "chapter": 47,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты ненавидишь Мое"
  },
  {
    "chapter": 47,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "наставление"
  },
  {
    "chapter": 47,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и бросаешь за спину Мои"
  },
  {
    "chapter": 47,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "слова."
  },
  {
    "chapter": 47,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Увидев вора,"
  },
  {
    "chapter": 47,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "ты с ним сближаешься,"
  },
  {
    "chapter": 47,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и водишь дружбу"
  },
  {
    "chapter": 47,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "с прелюбодеями."
  },
  {
    "chapter": 47,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты даешь"
  },
  {
    "chapter": 47,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "злословить своим устам,"
  },
  {
    "chapter": 47,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и язык твой сплетает ложь."
  },
  {
    "chapter": 47,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты всегда обвиняешь брата,"
  },
  {
    "chapter": 47,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "клевещешь на"
  },
  {
    "chapter": 47,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "сына матери твоей."
  },
  {
    "chapter": 47,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты творил это,"
  },
  {
    "chapter": 47,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "а Я молчал;"
  },
  {
    "chapter": 47,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "ты решил,"
  },
  {
    "chapter": 47,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "что и Я как ты."
  },
  {
    "chapter": 47,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Но Я обличу тебя и"
  },
  {
    "chapter": 47,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "в глаза тебя обвиню."
  },
  {
    "chapter": 47,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Запомните это,"
  },
  {
    "chapter": 47,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "забывающие о Боге,"
  },
  {
    "chapter": 47,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "или Я растерзаю вас,"
  },
  {
    "chapter": 47,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и не будет для"
  },
  {
    "chapter": 47,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "вас спасителя."
  },
  {
    "chapter": 47,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Приносящий Мне в"
  },
  {
    "chapter": 47,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "жертву хвалу чтит Меня;"
  },
  {
    "chapter": 47,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "тому,"
  },
  {
    "chapter": 47,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "кто идет по"
  },
  {
    "chapter": 47,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "праведному пути,"
  },
  {
    "chapter": 47,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Я явлю спасение Божье."
  }
]