  {
    "chapter": 13,
    "verse": 3,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 6,
    "text": "Иаков,"
  },
  {
    "chapter": 13,
    "verse": 3,
    "book_name": "Mark",
    "book_id": "Mark",
