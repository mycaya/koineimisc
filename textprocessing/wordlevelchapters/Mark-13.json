  },
  {
    "chapter": 13,
    "verse": 11,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 4,
    "text": "заранее о том,"
  },
  {
    "chapter": 13,
    "verse": 11,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 5,
    "text": "что вам сказать."
  },
  {
    "chapter": 13,
    "verse": 11,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 6,
    "text": "Говорите то,"
  },
  {
    "chapter": 13,
    "verse": 11,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 7,
