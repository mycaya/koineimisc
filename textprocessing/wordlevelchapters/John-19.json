  },
  {
    "chapter": 19,
    "verse": 19,
    "book_name": "John",
    "book_id": "John",
    "part": 1,
    "text": "Пилат приказал прикрепить"
  },
  {
    "chapter": 19,
    "verse": 19,
    "book_name": "John",
    "book_id": "John",
    "part": 2,
    "text": "на кресте табличку с"
  },
  {
    "chapter": 19,
    "verse": 19,
    "book_name": "John",
    "book_id": "John",
    "part": 3,
    "text": "надписью:"
  },
  {
    "chapter": 19,
    "verse": 19,
    "book_name": "John",
    "book_id": "John",
    "part": 4,
    "text": "ИИСУС из"
  },
  {
    "chapter": 19,
    "verse": 19,
    "book_name": "John",
    "book_id": "John",
    "part": 5,
    "text": "Назарета Царь Иудеев."
  },
  {
    "chapter": 19,
    "verse": 20,
    "book_name": "John",
    "book_id": "John",
    "part": 1,
    "text": "Надпись эту"
  },
  {
    "chapter": 19,
    "verse": 20,
    "book_name": "John",
    "book_id": "John",
    "part": 2,
    "text": "читало много иудеев,"
  },
  {
    "chapter": 19,
    "verse": 20,
    "book_name": "John",
    "book_id": "John",
    "part": 3,
    "text": "так как место распятия"
  },
  {
    "chapter": 19,
    "verse": 20,
    "book_name": "John",
    "book_id": "John",
    "part": 4,
    "text": "Иисуса было близко к городу,"
  },
  {
    "chapter": 19,
    "verse": 20,
    "book_name": "John",
    "book_id": "John",
    "part": 5,
    "text": "а написано"
  },
  {
    "chapter": 19,
    "verse": 20,
    "book_name": "John",
    "book_id": "John",
    "part": 6,
    "text": "было по-еврейски,"
  },
  {
    "chapter": 19,
    "verse": 20,
    "book_name": "John",
    "book_id": "John",
    "part": 7,
    "text": "по-латыни и по-гречески."
  },
  {
    "chapter": 19,
    "verse": 21,
    "book_name": "John",
    "book_id": "John",
    "part": 1,
    "text": "Начальники иудеев"
  },
  {
    "chapter": 19,
    "verse": 21,
    "book_name": "John",
    "book_id": "John",
    "part": 2,
    "text": "возразили Пилату:"
  },
  {
    "chapter": 19,
    "verse": 21,
