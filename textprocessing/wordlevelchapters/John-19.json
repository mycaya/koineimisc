[
  {
    "chapter": 19,
    "verse": 1,
    "book_name": "John",
    "book_id": "John",
    "part": 1,
    "text": "Тогда Пилат"
  },
  {
    "chapter": 19,
    "verse": 1,
    "book_name": "John",
    "book_id": "John",
    "part": 2,
    "text": "приказал бить Иисуса."
  },
  {
    "chapter": 19,
    "verse": 2,
    "book_name": "John",
    "book_id": "John",
    "part": 1,
    "text": "Солдаты сплели венок"
  },
  {
    "chapter": 19,
    "verse": 2,
    "book_name": "John",
    "book_id": "John",
    "part": 2,
    "text": "из терновника"
  },
  {
    "chapter": 19,
    "verse": 2,
    "book_name": "John",
    "book_id": "John",
    "part": 3,
    "text": "и надели Ему на голову."
  },
  {
    "chapter": 19,
    "verse": 2,
    "book_name": "John",
    "book_id": "John",
    "part": 4,
    "text": "Они одели Его в"
  },
