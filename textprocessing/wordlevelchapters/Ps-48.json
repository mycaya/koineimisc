[
  {
    "chapter": 48,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Дирижеру хора."
  },
  {
    "chapter": 48,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Псалом Давида,"
  },
  {
    "chapter": 48,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "когда пророк"
  },
  {
    "chapter": 48,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Нафан пришел к нему,"
  },
  {
    "chapter": 48,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "после того,"
  },
  {
    "chapter": 48,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "как Давид"
  },
  {
    "chapter": 48,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "согрешил с Вирсавией."
  },
  {
    "chapter": 48,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Боже,"
  },
  {
    "chapter": 48,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "помилуй меня по"
  },
  {
    "chapter": 48,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Твоей милости,"
  },
  {
    "chapter": 48,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "по великой Твоей любви"
  },
  {
    "chapter": 48,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "изгладь мои беззакония."
  },
  {
    "chapter": 48,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Омой меня от"
  },
  {
    "chapter": 48,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "неправды и от греха очисти,"
  },
  {
    "chapter": 48,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "потому что я"
  },
  {
    "chapter": 48,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "сознаю мои беззакония,"
  },
  {
    "chapter": 48,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и грех мой"
  },
  {
    "chapter": 48,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "всегда предо мной."
  },
  {
    "chapter": 48,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Против Тебя Одного я"
  },
  {
    "chapter": 48,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "согрешил"
  },
  {
    "chapter": 48,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и в Твоих глазах сделал"
  },
  {
    "chapter": 48,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "зло."
  },
  {
    "chapter": 48,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Ты справедлив в Своем"
  },
  {
    "chapter": 48,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "приговоре"
  },
  {
    "chapter": 48,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "и безупречен в суде Своем."
  },
  {
    "chapter": 48,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Поистине,"
  },
  {
    "chapter": 48,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "грешником я родился,"
  },
  {
    "chapter": 48,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "грешным зачала"
  },
  {
    "chapter": 48,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "меня моя мать."
  },
  {
    "chapter": 48,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но Ты желаешь истины,"
  },
  {
    "chapter": 48,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "сокрытой в сердце,"
  },
  {
    "chapter": 48,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "так наполни сердце"
  },
  {
    "chapter": 48,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "мое Твоей мудростью."
  },
  {
    "chapter": 48,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Очисти меня иссопом,"
  },
  {
    "chapter": 48,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и буду чист;"
  },
  {
    "chapter": 48,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "омой меня,"
  },
  {
    "chapter": 48,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и стану белее снега."
  },
  {
    "chapter": 48,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Дай мне услышать"
  },
  {
    "chapter": 48,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "веселье и радость;"
  },
  {
    "chapter": 48,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "пусть порадуются кости,"
  },
  {
    "chapter": 48,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Тобой сокрушенные."
  },
  {
    "chapter": 48,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Отврати лицо от моих"
  },
  {
    "chapter": 48,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "грехов и неправду мою изгладь."
  },
  {
    "chapter": 48,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Сотвори во мне"
  },
  {
    "chapter": 48,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "чистое сердце,"
  },
  {
    "chapter": 48,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Боже,"
  },
  {
    "chapter": 48,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и обнови во мне правый дух."
  },
  {
    "chapter": 48,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Не отвергни меня от Себя"
  },
  {
    "chapter": 48,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и Твоего Святого Духа не"
  },
  {
    "chapter": 48,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "отними у меня."
  },
  {
    "chapter": 48,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Верни мне радость Твоего"
  },
  {
    "chapter": 48,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "спасения"
  },
  {
    "chapter": 48,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и Духом владычественным"
  },
  {
    "chapter": 48,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "поддержи меня"
  },
  {
    "chapter": 48,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Тогда я научу"
  },
  {
    "chapter": 48,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "нечестивых Твоим путям,"
  },
  {
    "chapter": 48,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и грешники к"
  },
  {
    "chapter": 48,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Тебе обратятся."
  },
  {
    "chapter": 48,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Боже,"
  },
  {
    "chapter": 48,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "избавь меня от"
  },
  {
    "chapter": 48,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "кровопролития,"
  },
  {
    "chapter": 48,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Боже моего спасения,"
  },
  {
    "chapter": 48,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "и язык мой"
  },
  {
    "chapter": 48,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "восславит праведность Твою."
  },
  {
    "chapter": 48,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Открой мне уста,"
  },
  {
    "chapter": 48,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Владыка,"
  },
  {
    "chapter": 48,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и они Тебя возвеличат."
  },
  {
    "chapter": 48,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Жертвы Тебе"
  },
  {
    "chapter": 48,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "неугодны я дал бы их,"
  },
  {
    "chapter": 48,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "всесожжений Ты не желаешь."
  },
  {
    "chapter": 48,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Жертва Богу"
  },
  {
    "chapter": 48,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "дух сокрушенный;"
  },
  {
    "chapter": 48,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "сокрушенное и"
  },
  {
    "chapter": 48,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "скорбящее сердце,"
  },
  {
    "chapter": 48,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Боже,"
  },
  {
    "chapter": 48,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Ты не пренебрежешь."
  },
  {
    "chapter": 48,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Сотвори Сиону добро"
  },
  {
    "chapter": 48,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "по благоволению Твоему,"
  },
  {
    "chapter": 48,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "заново возведи"
  },
  {
    "chapter": 48,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "стены Иерусалима."
  },
  {
    "chapter": 48,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Тогда должные жертвы,"
  },
  {
    "chapter": 48,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "возношения и"
  },
  {
    "chapter": 48,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "всесожжения будут угодны Тебе;"
  },
  {
    "chapter": 48,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "тогда принесут быков"
  },
  {
    "chapter": 48,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "на Твой жертвенник."
  }
]