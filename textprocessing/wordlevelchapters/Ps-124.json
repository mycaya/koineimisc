[
  {
    "chapter": 124,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Песнь восхождения."
  },
  {
    "chapter": 124,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Из глубин воззвал я к Тебе,"
  },
  {
    "chapter": 124,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Господи."
  },
  {
    "chapter": 124,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Владыка,"
  },
  {
    "chapter": 124,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "услышь мой голос,"
  },
  {
    "chapter": 124,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "будь внимателен"
  },
  {
    "chapter": 124,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "к моим молениям."
  },
  {
    "chapter": 124,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Если бы Ты,"
  },
  {
    "chapter": 124,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 124,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "вел счет беззакониям,"
  },
  {
    "chapter": 124,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "о Владыка,"
  },
  {
    "chapter": 124,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "кто бы устоял?"
  },
  {
    "chapter": 124,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но у Тебя есть прощение;"
  },
  {
    "chapter": 124,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "пусть боятся Тебя."
  },
  {
    "chapter": 124,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "На Господа надеюсь,"
  },
  {
    "chapter": 124,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "надеется душа моя,"
  },
  {
    "chapter": 124,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и на слово Его уповаю."
  },
  {
    "chapter": 124,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Душа моя ждет"
  },
  {
    "chapter": 124,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Владыку более,"
  },
  {
    "chapter": 124,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "чем охранники утра,"
  },
  {
    "chapter": 124,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "да,"
  },
  {
    "chapter": 124,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "более,"
  },
  {
    "chapter": 124,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "чем охранники утра."
  },
  {
    "chapter": 124,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Да уповает"
  },
  {
    "chapter": 124,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Израиль на Господа,"
  },
  {
    "chapter": 124,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "потому что у Господа"
  },
  {
    "chapter": 124,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "милость и великое"
  },
  {
    "chapter": 124,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "избавление у Него."
  },
  {
    "chapter": 124,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он избавит Израиль от"
  },
  {
    "chapter": 124,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "всех его беззаконий."
  }
]