[
  {
    "chapter": 79,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Песнь."
  },
  {
    "chapter": 79,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Псалом Асафа."
  },
  {
    "chapter": 79,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Боже,"
  },
  {
    "chapter": 79,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "не молчи и не"
  },
  {
    "chapter": 79,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "безмолвствуй! Не будь в покое,"
  },
  {
    "chapter": 79,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "о Бог!"
  },
  {
    "chapter": 79,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Посмотри,"
  },
  {
    "chapter": 79,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "враги Твои шумят,"
  },
  {
    "chapter": 79,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и ненавидящие"
  },
  {
    "chapter": 79,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Тебя подняли голову."
  },
  {
    "chapter": 79,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Против Твоего народа"
  },
  {
    "chapter": 79,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "составили коварный замысел"
  },
  {
    "chapter": 79,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и сговариваются против"
  },
  {
    "chapter": 79,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "оберегаемых Тобою."
  },
  {
    "chapter": 79,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Сказали:"
  },
  {
    "chapter": 79,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Пойдем и истребим"
  },
  {
    "chapter": 79,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "их из числа народов,"
  },
  {
    "chapter": 79,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "чтобы не"
  },
  {
    "chapter": 79,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "вспоминалось больше имя Израиля."
  },
  {
    "chapter": 79,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Сговорились единодушно,"
  },
  {
    "chapter": 79,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "заключили против Тебя союз."
  },
  {
    "chapter": 79,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Селения Едома"
  },
  {
    "chapter": 79,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и измаильтяне,"
  },
  {
    "chapter": 79,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Моав и агаряне,"
  },
  {
    "chapter": 79,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Гевал,"
  },
  {
    "chapter": 79,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Аммон и Амалик,"
  },
  {
    "chapter": 79,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "филистимляне с"
  },
  {
    "chapter": 79,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "жителями Тира."
  },
  {
    "chapter": 79,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Даже Ассирия"
  },
  {
    "chapter": 79,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "присоединилась к ним,"
  },
  {
    "chapter": 79,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "добавив силы потомкам Лота."
  },
  {
    "chapter": 79,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Пауза"
  },
  {
    "chapter": 79,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Сделай им то же,"
  },
  {
    "chapter": 79,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "что и Мадиану,"
  },
  {
    "chapter": 79,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "что и Сисаре и"
  },
  {
    "chapter": 79,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Иавину у реки Киссона,"
  },
  {
    "chapter": 79,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "которые были"
  },
  {
    "chapter": 79,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "истреблены в Ен-Доре"
  },
  {
    "chapter": 79,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и стали удобрением для"
  },
  {
    "chapter": 79,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "земли."
  },
  {
    "chapter": 79,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Поступи с их вождями,"
  },
  {
    "chapter": 79,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "как с Оривом и Зивом,"
  },
  {
    "chapter": 79,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и с их правителями,"
  },
  {
    "chapter": 79,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "как с Зевеем и Салманом,"
  },
  {
    "chapter": 79,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "которые говорили:"
  },
  {
    "chapter": 79,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Возьмем себе во"
  },
  {
    "chapter": 79,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "владение землю Божью."
  },
  {
    "chapter": 79,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Бог мой,"
  },
  {
    "chapter": 79,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "пусть будут они"
  },
  {
    "chapter": 79,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "как перекати-поле,"
  },
  {
    "chapter": 79,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "как солома,"
  },
  {
    "chapter": 79,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "гонимая ветром."
  },
  {
    "chapter": 79,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Как огонь пожирает лес и"
  },
  {
    "chapter": 79,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "как пламя опаляет горы,"
  },
  {
    "chapter": 79,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "так погони их Своею бурею"
  },
  {
    "chapter": 79,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и вихрем Своим приведи"
  },
  {
    "chapter": 79,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "их в смятение."
  },
  {
    "chapter": 79,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Покрой бесчестием их лица,"
  },
  {
    "chapter": 79,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "чтобы воззвали Имя Твое,"
  },
  {
    "chapter": 79,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Господи!"
  },
  {
    "chapter": 79,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть постыдятся и"
  },
  {
    "chapter": 79,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "смутятся навсегда,"
  },
  {
    "chapter": 79,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "пусть погибнут в бесчестии."
  },
  {
    "chapter": 79,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть познают,"
  },
  {
    "chapter": 79,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "что Ты,"
  },
  {
    "chapter": 79,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Чье Имя Господь,"
  },
  {
    "chapter": 79,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "один лишь Ты"
  },
  {
    "chapter": 79,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Всевышний над землею."
  }
]