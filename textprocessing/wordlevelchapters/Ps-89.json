[
  {
    "chapter": 89,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "О Господи,"
  },
  {
    "chapter": 89,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Бог отмщения,"
  },
  {
    "chapter": 89,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Бог отмщения,"
  },
  {
    "chapter": 89,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "яви Себя!"
  },
  {
    "chapter": 89,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Поднимись,"
  },
  {
    "chapter": 89,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Судья вселенной,"
  },
  {
    "chapter": 89,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "воздай гордым по их делам."
  },
  {
    "chapter": 89,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господи,"
  },
  {
    "chapter": 89,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "как долго еще? Как долго"
  },
  {
    "chapter": 89,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "еще нечестивые будут"
  },
  {
    "chapter": 89,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "торжествовать?"
  },
  {
    "chapter": 89,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Они произносят"
  },
  {
    "chapter": 89,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "свои дерзкие речи;"
  },
  {
    "chapter": 89,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "превозносятся все злодеи."
  },
  {
    "chapter": 89,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Они сокрушают Твой народ"
  },
  {
    "chapter": 89,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и угнетают Твое наследие."
  },
  {
    "chapter": 89,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Они убивают"
  },
  {
    "chapter": 89,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "вдов и чужеземцев,"
  },
  {
    "chapter": 89,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и умерщвляют сирот,"
  },
  {
    "chapter": 89,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "и говорят:"
  },
  {
    "chapter": 89,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господь не увидит,"
  },
  {
    "chapter": 89,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и не обратит"
  },
  {
    "chapter": 89,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "внимание Бог Иакова."
  },
  {
    "chapter": 89,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Образумьтесь,"
  },
  {
    "chapter": 89,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "глупые люди!"
  },
  {
    "chapter": 89,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Когда вы поумнеете,"
  },
  {
    "chapter": 89,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "невежды?"
  },
  {
    "chapter": 89,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Давший ухо разве не"
  },
  {
    "chapter": 89,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "услышит? Сотворивший глаз"
  },
  {
    "chapter": 89,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "разве не увидит?"
  },
  {
    "chapter": 89,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Наказывающий народы разве"
  },
  {
    "chapter": 89,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "не обличит? Он учит"
  },
  {
    "chapter": 89,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "человека знанию,"
  },
  {
    "chapter": 89,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "поэтому знает"
  },
  {
    "chapter": 89,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "человеческие помыслы,"
  },
  {
    "chapter": 89,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Он знает,"
  },
  {
    "chapter": 89,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "что они всего лишь суета."
  },
  {
    "chapter": 89,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Благословен человек,"
  },
  {
    "chapter": 89,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "которого Ты,"
  },
  {
    "chapter": 89,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Господи,"
  },
  {
    "chapter": 89,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "наставляешь и"
  },
  {
    "chapter": 89,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "учишь Твоему Закону,"
  },
  {
    "chapter": 89,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "чтобы дать ему"
  },
  {
    "chapter": 89,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "покой в дни бедствия,"
  },
  {
    "chapter": 89,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "пока не выкопана"
  },
  {
    "chapter": 89,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "будет яма нечестивому."
  },
  {
    "chapter": 89,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ведь не покинет Господь"
  },
  {
    "chapter": 89,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "народа Своего"
  },
  {
    "chapter": 89,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и наследия Своего не"
  },
  {
    "chapter": 89,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "бросит."
  },
  {
    "chapter": 89,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но суд"
  },
  {
    "chapter": 89,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "возвратится к правде,"
  },
  {
    "chapter": 89,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и все чистые"
  },
  {
    "chapter": 89,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "сердцем последуют ему."
  },
  {
    "chapter": 89,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Кто постоит за меня против"
  },
  {
    "chapter": 89,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "нечестивых? И кто встанет"
  },
  {
    "chapter": 89,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "за меня против злодеев?"
  },
  {
    "chapter": 89,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Если бы не Господь"
  },
  {
    "chapter": 89,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "был мне помощником,"
  },
  {
    "chapter": 89,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "поселилась бы моя"
  },
  {
    "chapter": 89,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "душа в земле молчания."
  },
  {
    "chapter": 89,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Когда я сказал:"
  },
  {
    "chapter": 89,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Колеблется моя нога,"
  },
  {
    "chapter": 89,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Твоя милость,"
  },
  {
    "chapter": 89,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Господи,"
  },
  {
    "chapter": 89,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "поддержала меня."
  },
  {
    "chapter": 89,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Среди многих моих скорбей"
  },
  {
    "chapter": 89,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Твое утешение радует душу"
  },
  {
    "chapter": 89,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "мою."
  },
  {
    "chapter": 89,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Станет ли близ Тебя"
  },
  {
    "chapter": 89,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "престол губителей,"
  },
  {
    "chapter": 89,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "издающий"
  },
  {
    "chapter": 89,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "несправедливые законы?"
  },
  {
    "chapter": 89,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Толпятся они вокруг души"
  },
  {
    "chapter": 89,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "праведника"
  },
  {
    "chapter": 89,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и осуждают невинную кровь."
  },
  {
    "chapter": 89,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но Господь моя защита,"
  },
  {
    "chapter": 89,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и мой Бог скала"
  },
  {
    "chapter": 89,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "моего убежища."
  },
  {
    "chapter": 89,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Обратит на них"
  },
  {
    "chapter": 89,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "их же злодеяние и в"
  },
  {
    "chapter": 89,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "их же беззаконии истребит"
  },
  {
    "chapter": 89,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "их;"
  },
  {
    "chapter": 89,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "истребит их Господь,"
  },
  {
    "chapter": 89,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "наш Бог."
  }
]