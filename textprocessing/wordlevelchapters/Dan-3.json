    "verse": 23,
    "book_name": "Daniel",
    "book_id": "Dan",
    "part": 3,
    "text": "крепко связанные,"
  },
  {
    "chapter": 3,
    "verse": 23,
    "book_name": "Daniel",
    "book_id": "Dan",
    "part": 4,
    "text": "в огненную печь."
  },
  {
    "chapter": 3,
    "verse": 24,
    "book_name": "Daniel",
    "book_id": "Dan",
    "part": 1,
    "text": "Царь Навуходоносор в"
  },
