    "part": 9,
    "text": "которую я поставил?"
  },
  {
    "chapter": 3,
    "verse": 15,
    "book_name": "Daniel",
    "book_id": "Dan",
    "part": 1,
    "text": "Итак,"
  },
  {
    "chapter": 3,
    "verse": 15,
    "book_name": "Daniel",
    "book_id": "Dan",
    "part": 2,
    "text": "если вы готовы,"
