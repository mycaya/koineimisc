    "verse": 22,
    "book_name": "Daniel",
    "book_id": "Dan",
    "part": 6,
    "text": "поднявших Седраха,"
  },
  {
    "chapter": 3,
    "verse": 22,
    "book_name": "Daniel",
    "book_id": "Dan",
    "part": 7,
    "text": "Мисаха и Авденаго,"
  },
  {
    "chapter": 3,
    "verse": 23,
    "book_name": "Daniel",
    "book_id": "Dan",
    "part": 1,
