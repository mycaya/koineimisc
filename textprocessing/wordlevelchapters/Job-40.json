[
  {
    "chapter": 40,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "- Препояшь себя,"
  },
  {
    "chapter": 40,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "как мужчина;"
  },
  {
    "chapter": 40,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Я буду спрашивать,"
  },
  {
    "chapter": 40,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "а ты отвечай."
  },
  {
    "chapter": 40,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Опровергнешь ли ты Мой"
  },
  {
    "chapter": 40,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "суд? Обвинишь ли Меня,"
  },
  {
    "chapter": 40,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "чтобы себя оправдать?"
  },
  {
    "chapter": 40,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Такова ли мощь твоя,"
  },
  {
    "chapter": 40,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "как у Бога,"
  },
  {
    "chapter": 40,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и гремит ли голос твой,"
  },
  {
    "chapter": 40,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "как Его?"
  },
  {
    "chapter": 40,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Что ж,"
  },
  {
    "chapter": 40,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "укрась себя"
  },
  {
    "chapter": 40,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "славой и блеском,"
  },
  {
    "chapter": 40,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "оденься в честь и величие."
  },
  {
    "chapter": 40,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Дай волю"
  },
  {
    "chapter": 40,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "ярости своего гнева,"
  },
  {
    "chapter": 40,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "посмотри на"
  },
  {
    "chapter": 40,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "гордого и смири его,"
  },
  {
    "chapter": 40,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "посмотри на"
  },
  {
    "chapter": 40,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "надменного и унизь его,"
  },
  {
    "chapter": 40,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "придави злодеев к земле."
  },
  {
    "chapter": 40,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Зарой их всех вместе в"
  },
  {
    "chapter": 40,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "землю и окутай их лица тьмой."
  },
  {
    "chapter": 40,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Тогда и Сам Я тебе скажу,"
  },
  {
    "chapter": 40,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "что твоя рука тебя спасла."
  },
  {
    "chapter": 40,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Присмотрись к чудовищу,"
  },
  {
    "chapter": 40,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Я создал его,"
  },
  {
    "chapter": 40,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "как тебя;"
  },
  {
    "chapter": 40,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "он ест траву,"
  },
  {
    "chapter": 40,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "как вол."
  },
  {
    "chapter": 40,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Что за сила в бедрах его,"
  },
  {
    "chapter": 40,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "что за крепость в"
  },
  {
    "chapter": 40,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "мускулах живота!"
  },
  {
    "chapter": 40,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Поворачивает своим хвостом,"
  },
  {
    "chapter": 40,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "как кедром;"
  },
  {
    "chapter": 40,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "жилы бедер его сплетены."
  },
  {
    "chapter": 40,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Ноги его,"
  },
  {
    "chapter": 40,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "как медные трубы,"
  },
  {
    "chapter": 40,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "кости его,"
  },
  {
    "chapter": 40,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "как железные прутья."
  },
  {
    "chapter": 40,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Он величайшее"
  },
  {
    "chapter": 40,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "среди Божьих дел;"
  },
  {
    "chapter": 40,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "лишь его Творец может"
  },
  {
    "chapter": 40,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "меч к нему поднести."
  },
  {
    "chapter": 40,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Горы пищу ему дают,"
  },
  {
    "chapter": 40,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "где резвятся"
  },
  {
    "chapter": 40,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "все дикие звери."
  },
  {
    "chapter": 40,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "В зарослях лотоса он лежит,"
  },
  {
    "chapter": 40,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "скрыт среди"
  },
  {
    "chapter": 40,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "тростников болотных."
  },
  {
    "chapter": 40,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Лотосы прячут его в тени,"
  },
  {
    "chapter": 40,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "ивы у ручья его окружают."
  },
  {
    "chapter": 40,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Бушует река не страшно ему;"
  },
  {
    "chapter": 40,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "невозмутим он,"
  },
  {
    "chapter": 40,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "хоть хлынь ему"
  },
  {
    "chapter": 40,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "в пасть Иордан."
  },
  {
    "chapter": 40,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Кто схватит его у него же"
  },
  {
    "chapter": 40,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "на глазах? Кто багор в его"
  },
  {
    "chapter": 40,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "нос проденет?"
  },
  {
    "chapter": 40,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Можешь ли вытащить удочкой"
  },
  {
    "chapter": 40,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Левиафана"
  },
  {
    "chapter": 40,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и связать веревкой его"
  },
  {
    "chapter": 40,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "язык?"
  },
  {
    "chapter": 40,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Проденешь ли канат ему в"
  },
  {
    "chapter": 40,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "нос"
  },
  {
    "chapter": 40,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и пронзишь ли челюсть его"
  },
  {
    "chapter": 40,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "крюком?"
  },
  {
    "chapter": 40,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Станет ли он тебя умолять"
  },
  {
    "chapter": 40,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и кротко с тобой говорить?"
  },
  {
    "chapter": 40,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Заключит ли он"
  },
  {
    "chapter": 40,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "с тобой договор,"
  },
  {
    "chapter": 40,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "что пойдет в"
  },
  {
    "chapter": 40,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "услуженье к тебе навек?"
  },
  {
    "chapter": 40,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Станешь ли с ним,"
  },
  {
    "chapter": 40,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "как с птичкой,"
  },
  {
    "chapter": 40,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "играть,"
  },
  {
    "chapter": 40,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "привяжешь ли на"
  },
  {
    "chapter": 40,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "забаву служанкам?"
  },
  {
    "chapter": 40,
    "verse": 25,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Станут ли рыбаки"
  },
  {
    "chapter": 40,
    "verse": 25,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "о нем торговаться,"
  },
  {
    "chapter": 40,
    "verse": 25,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "между купцами тушу делить?"
  },
  {
    "chapter": 40,
    "verse": 26,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Пронзишь ли ты"
  },
  {
    "chapter": 40,
    "verse": 26,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "кожу его копьем,"
  },
  {
    "chapter": 40,
    "verse": 26,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и рыбацкой острогой голову?"
  },
  {
    "chapter": 40,
    "verse": 27,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Тронь его раз и"
  },
  {
    "chapter": 40,
    "verse": 27,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "больше не встанешь;"
  },
  {
    "chapter": 40,
    "verse": 27,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "никогда не забудешь битву!"
  }
]