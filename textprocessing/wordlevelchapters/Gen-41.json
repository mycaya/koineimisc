  },
  {
    "chapter": 41,
    "verse": 41,
    "book_name": "Genesis",
    "book_id": "Gen",
    "part": 2,
    "text": "Смотри,"
  },
  {
    "chapter": 41,
    "verse": 41,
    "book_name": "Genesis",
    "book_id": "Gen",
    "part": 3,
    "text": "я ставлю тебя над"
  },
  {
    "chapter": 41,
    "verse": 41,
    "book_name": "Genesis",
    "book_id": "Gen",
    "part": 4,
    "text": "всей землей Египта."
  },
  {
    "chapter": 41,
    "verse": 42,
    "book_name": "Genesis",
    "book_id": "Gen",
    "part": 1,
    "text": "Он снял с пальца свой"
  },
  {
    "chapter": 41,
    "verse": 42,
    "book_name": "Genesis",
    "book_id": "Gen",
    "part": 2,
    "text": "перстень"
  },
  {
    "chapter": 41,
    "verse": 42,
    "book_name": "Genesis",
    "book_id": "Gen",
