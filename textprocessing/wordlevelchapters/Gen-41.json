    "chapter": 41,
    "verse": 45,
    "book_name": "Genesis",
    "book_id": "Gen",
    "part": 2,
    "text": "имя Цафнаф-Панеах"
  },
  {
    "chapter": 41,
    "verse": 45,
    "book_name": "Genesis",
    "book_id": "Gen",
    "part": 3,
    "text": "и отдал ему в жены Асенефу,"
  },
