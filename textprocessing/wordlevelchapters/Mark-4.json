  {
    "chapter": 4,
    "verse": 26,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 3,
    "text": "как если бы"
  },
  {
    "chapter": 4,
    "verse": 26,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 4,
