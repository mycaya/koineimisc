[
  {
    "chapter": 17,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Дирижеру хора."
  },
  {
    "chapter": 17,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Псалом Давида."
  },
  {
    "chapter": 17,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Небеса"
  },
  {
    "chapter": 17,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "возвещают Божью славу,"
  },
  {
    "chapter": 17,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "о делах Его рук"
  },
  {
    "chapter": 17,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "возвещает их свод."
  },
  {
    "chapter": 17,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "День"
  },
  {
    "chapter": 17,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "повествует об этом дню,"
  },
  {
    "chapter": 17,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "ночь открывает знание ночи."
  },
  {
    "chapter": 17,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Хотя они не"
  },
  {
    "chapter": 17,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "используют ни речи,"
  },
  {
    "chapter": 17,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "ни слов,"
  },
  {
    "chapter": 17,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и от них не слышно и звука,"
  },
  {
    "chapter": 17,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "их голос"
  },
  {
    "chapter": 17,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "проходит по всей земле,"
  },
  {
    "chapter": 17,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "но их слова до краев света."
  },
  {
    "chapter": 17,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "В небесах Он"
  },
  {
    "chapter": 17,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "поставил шатер для солнца,"
  },
  {
    "chapter": 17,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "и оно выходит,"
  },
  {
    "chapter": 17,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "словно жених из"
  },
  {
    "chapter": 17,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "спальни своей,"
  },
  {
    "chapter": 17,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и пробегает свой путь,"
  },
  {
    "chapter": 17,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "как бегун,"
  },
  {
    "chapter": 17,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "радующийся"
  },
  {
    "chapter": 17,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "своей выносливости."
  },
  {
    "chapter": 17,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Встает оно на одном краю"
  },
  {
    "chapter": 17,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "небес"
  },
  {
    "chapter": 17,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и совершает свой путь к"
  },
  {
    "chapter": 17,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "другому краю,"
  },
  {
    "chapter": 17,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "и ничто от жара"
  },
  {
    "chapter": 17,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "его не скрыто."
  },
  {
    "chapter": 17,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Закон Господа совершенен,"
  },
  {
    "chapter": 17,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "обновляет душу."
  },
  {
    "chapter": 17,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Откровение"
  },
  {
    "chapter": 17,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Господа непреложно,"
  },
  {
    "chapter": 17,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "умудряет простых."
  },
  {
    "chapter": 17,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Наставления"
  },
  {
    "chapter": 17,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господа праведны,"
  },
  {
    "chapter": 17,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "радуют сердце."
  },
  {
    "chapter": 17,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Повеления"
  },
  {
    "chapter": 17,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Господа лучезарны,"
  },
  {
    "chapter": 17,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "просветляют глаза."
  },
  {
    "chapter": 17,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Страх Господень чист,"
  },
  {
    "chapter": 17,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "пребывает вовеки."
  },
  {
    "chapter": 17,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Определения Господа"
  },
  {
    "chapter": 17,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "истинны и все праведны."
  },
  {
    "chapter": 17,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Они желаннее золота,"
  },
  {
    "chapter": 17,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "даже множества"
  },
  {
    "chapter": 17,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "золота чистого;"
  },
  {
    "chapter": 17,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "слаще,"
  },
  {
    "chapter": 17,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "нежели мед,"
  },
  {
    "chapter": 17,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "нежели капли из сот."
  },
  {
    "chapter": 17,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Слуга Твой ими храним,"
  },
  {
    "chapter": 17,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "в соблюдении их"
  },
  {
    "chapter": 17,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "большая награда."
  },
  {
    "chapter": 17,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Кто к ошибкам своим не"
  },
  {
    "chapter": 17,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "слеп? От невольных"
  },
  {
    "chapter": 17,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "проступков меня очисти,"
  },
  {
    "chapter": 17,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "и от сознательных"
  },
  {
    "chapter": 17,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "грехов удержи Твоего слугу,"
  },
  {
    "chapter": 17,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "не дай им власти надо мной."
  },
  {
    "chapter": 17,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Тогда я буду непорочен и"
  },
  {
    "chapter": 17,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "чист от большого греха,"
  },
  {
    "chapter": 17,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть слова моих уст"
  },
  {
    "chapter": 17,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и раздумья моего сердца"
  },
  {
    "chapter": 17,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "будут угодны Тебе,"
  },
  {
    "chapter": 17,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "о Господи,"
  },
  {
    "chapter": 17,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "моя скала и мой избавитель!"
  }
]