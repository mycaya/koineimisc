  },
  {
    "chapter": 5,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "бедного от меча их уст;"
  },
  {
    "chapter": 5,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Он спасает его"
  },
  {
    "chapter": 5,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "от руки могучих."
  },
  {
    "chapter": 5,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "И есть надежда у нищего,"
  },
  {
    "chapter": 5,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и неправда"
  },
  {
