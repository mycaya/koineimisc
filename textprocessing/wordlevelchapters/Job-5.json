    "chapter": 5,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "в безопасности,"
  },
  {
    "chapter": 5,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "осмотришь владения"
  },
  {
