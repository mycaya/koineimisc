  },
  {
    "chapter": 5,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Он поранит,"
  },
  {
    "chapter": 5,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "но Сам перевяжет;"
  },
  {
    "chapter": 5,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Он поражает,"
  },
  {
