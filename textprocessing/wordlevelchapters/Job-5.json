    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "но рука Его исцеляет."
  },
  {
    "chapter": 5,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
