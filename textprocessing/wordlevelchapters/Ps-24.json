[
  {
    "chapter": 24,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Псалом Давида."
  },
  {
    "chapter": 24,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 24,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "оправдай меня,"
  },
  {
    "chapter": 24,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "ведь я жил"
  },
  {
    "chapter": 24,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "беспорочной жизнью."
  },
  {
    "chapter": 24,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Я уповал на Господа,"
  },
  {
    "chapter": 24,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "не колеблясь."
  },
  {
    "chapter": 24,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Проверь меня,"
  },
  {
    "chapter": 24,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 24,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "испытай меня,"
  },
  {
    "chapter": 24,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "исследуй"
  },
  {
    "chapter": 24,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "сердце мое и разум."
  },
  {
    "chapter": 24,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ведь Твоя милость"
  },
  {
    "chapter": 24,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "пред очами моими,"
  },
  {
    "chapter": 24,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "я всегда хожу"
  },
  {
    "chapter": 24,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "в Твоей истине."
  },
  {
    "chapter": 24,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я не сижу с людьми"
  },
  {
    "chapter": 24,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "лживыми и с коварными не пойду."
  },
  {
    "chapter": 24,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я ненавижу сборище"
  },
  {
    "chapter": 24,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "грешников и с нечестивыми не сяду."
  },
  {
    "chapter": 24,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Буду омывать свои руки в"
  },
  {
    "chapter": 24,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "невинности"
  },
  {
    "chapter": 24,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и обходить Твой жертвенник,"
  },
  {
    "chapter": 24,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Господь,"
  },
  {
    "chapter": 24,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "воспевая Тебе хвалу и"
  },
  {
    "chapter": 24,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "говоря о всех чудесах Твоих."
  },
  {
    "chapter": 24,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господи,"
  },
  {
    "chapter": 24,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "я люблю дом,"
  },
  {
    "chapter": 24,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "в котором Ты обитаешь,"
  },
  {
    "chapter": 24,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "место,"
  },
  {
    "chapter": 24,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "где слава Твоя живет."
  },
  {
    "chapter": 24,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Не погуби души"
  },
  {
    "chapter": 24,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "моей с грешными,"
  },
  {
    "chapter": 24,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "жизни моей с кровожадными,"
  },
  {
    "chapter": 24,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "у которых в"
  },
  {
    "chapter": 24,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "руках злой умысел,"
  },
  {
    "chapter": 24,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "чьи правые"
  },
  {
    "chapter": 24,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "руки взяток полны."
  },
  {
    "chapter": 24,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "А я живу беспорочно."
  },
  {
    "chapter": 24,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Избавь меня и помилуй!"
  },
  {
    "chapter": 24,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ноги мои стоят"
  },
  {
    "chapter": 24,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "на ровной земле;"
  },
  {
    "chapter": 24,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "в большом собрании"
  },
  {
    "chapter": 24,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "восхвалю я Господа."
  }
]