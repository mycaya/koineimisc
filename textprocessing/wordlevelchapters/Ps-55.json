[
  {
    "chapter": 55,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Дирижеру хора."
  },
  {
    "chapter": 55,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "На мотив Не погуби."
  },
  {
    "chapter": 55,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Мольба Давида."
  },
  {
    "chapter": 55,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Подлинно ли,"
  },
  {
    "chapter": 55,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "правители,"
  },
  {
    "chapter": 55,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "говорите вы беспристрастно?"
  },
  {
    "chapter": 55,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Справедливо ли судите меж"
  },
  {
    "chapter": 55,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "людьми?"
  },
  {
    "chapter": 55,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Нет,"
  },
  {
    "chapter": 55,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "неправду вы замышляете,"
  },
  {
    "chapter": 55,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "ваши руки творят"
  },
  {
    "chapter": 55,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "на земле произвол."
  },
  {
    "chapter": 55,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "От утробы матери"
  },
  {
    "chapter": 55,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "нечестивые среди отступников;"
  },
  {
    "chapter": 55,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "с рождения сбились"
  },
  {
    "chapter": 55,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "с пути и обманывают."
  },
  {
    "chapter": 55,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Яд их подобен яду змеиному,"
  },
  {
    "chapter": 55,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "яду кобры,"
  },
  {
    "chapter": 55,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "что уши свои затыкает"
  },
  {
    "chapter": 55,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "и не слышит"
  },
  {
    "chapter": 55,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "голоса заклинателя,"
  },
  {
    "chapter": 55,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "как бы тот ни был искусен."
  },
  {
    "chapter": 55,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Раздроби зубы у них во рту,"
  },
  {
    "chapter": 55,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "о Боже;"
  },
  {
    "chapter": 55,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "вырви,"
  },
  {
    "chapter": 55,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Господи,"
  },
  {
    "chapter": 55,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "клыки у львов!"
  },
  {
    "chapter": 55,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Да исчезнут,"
  },
  {
    "chapter": 55,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "как высохшая вода;"
  },
  {
    "chapter": 55,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "пусть будут как"
  },
  {
    "chapter": 55,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "надломленные стрелы в натянутом луке,"
  },
  {
    "chapter": 55,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "как улитка,"
  },
  {
    "chapter": 55,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "что растает,"
  },
  {
    "chapter": 55,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "слизью изойдя,"
  },
  {
    "chapter": 55,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "как мертворожденный,"
  },
  {
    "chapter": 55,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "что не увидит света."
  },
  {
    "chapter": 55,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Прежде чем ваши котлы"
  },
  {
    "chapter": 55,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "согреет горящий терн зелен"
  },
  {
    "chapter": 55,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "он или сух,"
  },
  {
    "chapter": 55,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "нечестивые погублены будут."
  },
  {
    "chapter": 55,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Праведники возрадуются,"
  },
  {
    "chapter": 55,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "когда увидят возмездие,"
  },
  {
    "chapter": 55,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "когда омоют стопы"
  },
  {
    "chapter": 55,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "в крови нечестивых."
  },
  {
    "chapter": 55,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "И будут тогда говорить:"
  },
  {
    "chapter": 55,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Поистине,"
  },
  {
    "chapter": 55,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "праведным есть награда;"
  },
  {
    "chapter": 55,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "поистине,"
  },
  {
    "chapter": 55,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "есть Бог,"
  },
  {
    "chapter": 55,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "судящий на земле."
  }
]