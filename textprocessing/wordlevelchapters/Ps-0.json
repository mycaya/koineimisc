[
  {
    "chapter": 0,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Блажен человек,"
  },
  {
    "chapter": 0,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "который не следует"
  },
  {
    "chapter": 0,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "совету нечестивых,"
  },
  {
    "chapter": 0,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "не стоит на пути грешников,"
  },
  {
    "chapter": 0,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "и не сидит в"
  },
  {
    "chapter": 0,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "собрании насмехающихся,"
  },
  {
    "chapter": 0,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "но в Господнем Законе"
  },
  {
    "chapter": 0,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "находит удовольствие,"
  },
  {
    "chapter": 0,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и о Законе Его"
  },
  {
    "chapter": 0,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "размышляет день и ночь."
  },
  {
    "chapter": 0,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он как дерево,"
  },
  {
    "chapter": 0,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "посаженное у потоков вод,"
  },
  {
    "chapter": 0,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "которое приносит"
  },
  {
    "chapter": 0,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "плод в свое время,"
  },
  {
    "chapter": 0,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "и чей лист не вянет."
  },
  {
    "chapter": 0,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Что бы он ни сделал,"
  },
  {
    "chapter": 0,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "во всем преуспеет."
  },
  {
    "chapter": 0,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Не таковы"
  },
  {
    "chapter": 0,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "нечестивые! Они как мякина,"
  },
  {
    "chapter": 0,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "которую гонит ветер."
  },
  {
    "chapter": 0,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Поэтому не устоят на суде"
  },
  {
    "chapter": 0,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "нечестивые"
  },
  {
    "chapter": 0,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и грешники в собрании"
  },
  {
    "chapter": 0,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "праведных."
  },
  {
    "chapter": 0,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ведь Господь"
  },
  {
    "chapter": 0,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "охраняет путь праведных,"
  },
  {
    "chapter": 0,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "а путь нечестивых погибнет."
  }
]