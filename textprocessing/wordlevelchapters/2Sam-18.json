[
  {
    "chapter": 18,
    "verse": 1,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Давид осмотрел людей,"
  },
  {
    "chapter": 18,
    "verse": 1,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "которые были с ним,"
  },
  {
    "chapter": 18,
    "verse": 1,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "и назначил над ними"
  },
  {
    "chapter": 18,
    "verse": 1,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "тысячников и сотников."
  },
  {
    "chapter": 18,
    "verse": 2,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Давид отправил войска"
  },
  {
    "chapter": 18,
    "verse": 2,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "треть под началом Иоава,"
  },
  {
    "chapter": 18,
    "verse": 2,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "треть под началом"
  },
  {
    "chapter": 18,
    "verse": 2,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "сына Саруи Авессая,"
  },
  {
    "chapter": 18,
    "verse": 2,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "брата Иоава,"
  },
  {
    "chapter": 18,
    "verse": 2,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 6,
    "text": "и треть под"
  },
  {
    "chapter": 18,
    "verse": 2,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 7,
    "text": "началом гефянина Иттая."
  },
  {
    "chapter": 18,
    "verse": 2,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 8,
    "text": "Царь сказал воинам:"
  },
  {
    "chapter": 18,
    "verse": 2,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 9,
    "text": "Я сам непременно"
  },
  {
    "chapter": 18,
    "verse": 2,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 10,
    "text": "выступлю вместе с вами."
  },
  {
    "chapter": 18,
    "verse": 3,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Но люди сказали:"
  },
  {
    "chapter": 18,
    "verse": 3,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "Не ходи;"
  },
  {
    "chapter": 18,
    "verse": 3,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "если мы будем"
  },
  {
    "chapter": 18,
    "verse": 3,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "вынуждены бежать,"
  },
  {
    "chapter": 18,
    "verse": 3,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "они не обратят"
  },
  {
    "chapter": 18,
    "verse": 3,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 6,
    "text": "на это внимания."
  },
  {
    "chapter": 18,
    "verse": 3,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 7,
    "text": "Даже если"
  },
  {
    "chapter": 18,
    "verse": 3,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 8,
    "text": "половина из нас погибнет,"
  },
  {
    "chapter": 18,
    "verse": 3,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 9,
    "text": "они не обратят внимания;"
  },
  {
    "chapter": 18,
    "verse": 3,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 10,
    "text": "но ты стоишь"
  },
  {
    "chapter": 18,
    "verse": 3,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 11,
    "text": "десяти тысяч из нас."
  },
  {
    "chapter": 18,
    "verse": 3,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 12,
    "text": "Поэтому лучше тебе"
  },
  {
    "chapter": 18,
    "verse": 3,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 13,
    "text": "помогать нам из города."
  },
  {
    "chapter": 18,
    "verse": 4,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Царь ответил:"
  },
  {
    "chapter": 18,
    "verse": 4,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "Я сделаю так,"
  },
  {
    "chapter": 18,
    "verse": 4,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "как вам кажется лучше."
  },
  {
    "chapter": 18,
    "verse": 4,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "И царь стоял у ворот,"
  },
  {
    "chapter": 18,
    "verse": 4,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "пока весь народ не выступил"
  },
  {
    "chapter": 18,
    "verse": 4,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 6,
    "text": "отрядами по сотням"
  },
  {
    "chapter": 18,
    "verse": 4,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 7,
    "text": "и тысячам."
  },
  {
    "chapter": 18,
    "verse": 5,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Царь приказал Иоаву,"
  },
  {
    "chapter": 18,
    "verse": 5,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "Авессаю и Иттаю:"
  },
  {
    "chapter": 18,
    "verse": 5,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "Ради меня обращайтесь с"
  },
  {
    "chapter": 18,
    "verse": 5,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "юношей Авессаломом мягко."
  },
  {
    "chapter": 18,
    "verse": 5,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "Все воины слышали,"
  },
  {
    "chapter": 18,
    "verse": 5,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 6,
    "text": "как царь давал приказ об"
  },
  {
    "chapter": 18,
    "verse": 5,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 7,
    "text": "Авессаломе каждому"
  },
  {
    "chapter": 18,
    "verse": 5,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 8,
    "text": "из военачальников."
  },
  {
    "chapter": 18,
    "verse": 6,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Войско выступило в поле,"
  },
  {
    "chapter": 18,
    "verse": 6,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "чтобы сразиться с Израилем,"
  },
  {
    "chapter": 18,
    "verse": 6,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "и битва"
  },
  {
    "chapter": 18,
    "verse": 6,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "произошла в лесу Ефрема."
  },
  {
    "chapter": 18,
    "verse": 7,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Израильское войско было"
  },
  {
    "chapter": 18,
    "verse": 7,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "разбито там людьми Давида,"
  },
  {
    "chapter": 18,
    "verse": 7,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "и потери в тот день были"
  },
  {
    "chapter": 18,
    "verse": 7,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "очень велики двадцать тысяч"
  },
  {
    "chapter": 18,
    "verse": 7,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "человек."
  },
  {
    "chapter": 18,
    "verse": 8,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Битва распространилась"
  },
  {
    "chapter": 18,
    "verse": 8,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "по всем окрестностям,"
  },
  {
    "chapter": 18,
    "verse": 8,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "и лес погубил в тот"
  },
  {
    "chapter": 18,
    "verse": 8,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "день больше жизней,"
  },
  {
    "chapter": 18,
    "verse": 8,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "чем меч."
  },
  {
    "chapter": 18,
    "verse": 9,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Авессалому случилось"
  },
  {
    "chapter": 18,
    "verse": 9,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "встретиться с людьми Давида."
  },
  {
    "chapter": 18,
    "verse": 9,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "Он был на своем муле,"
  },
  {
    "chapter": 18,
    "verse": 9,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "и когда мул"
  },
  {
    "chapter": 18,
    "verse": 9,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "пробегал под большим дубом,"
  },
  {
    "chapter": 18,
    "verse": 9,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 6,
    "text": "голова Авессалома"
  },
  {
    "chapter": 18,
    "verse": 9,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 7,
    "text": "запуталась в густых ветвях."
  },
  {
    "chapter": 18,
    "verse": 9,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 8,
    "text": "Сам он остался"
  },
  {
    "chapter": 18,
    "verse": 9,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 9,
    "text": "висеть в воздухе,"
  },
  {
    "chapter": 18,
    "verse": 9,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 10,
    "text": "а мул,"
  },
  {
    "chapter": 18,
    "verse": 9,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 11,
    "text": "на котором он ехал,"
  },
  {
    "chapter": 18,
    "verse": 9,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 12,
    "text": "побежал дальше."
  },
  {
    "chapter": 18,
    "verse": 10,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Когда один из"
  },
  {
    "chapter": 18,
    "verse": 10,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "людей увидел это,"
  },
  {
    "chapter": 18,
    "verse": 10,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "он доложил Иоаву:"
  },
  {
    "chapter": 18,
    "verse": 10,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "Я только что видел"
  },
  {
    "chapter": 18,
    "verse": 10,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "Авессалома висящим на дубе."
  },
  {
    "chapter": 18,
    "verse": 11,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Иоав сказал человеку,"
  },
  {
    "chapter": 18,
    "verse": 11,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "который"
  },
  {
    "chapter": 18,
    "verse": 11,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "доложил ему об этом:"
  },
  {
    "chapter": 18,
    "verse": 11,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "Что! Ты видел его? Почему"
  },
  {
    "chapter": 18,
    "verse": 11,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "ты не поверг его там же на"
  },
  {
    "chapter": 18,
    "verse": 11,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 6,
    "text": "землю? Я бы с радостью"
  },
  {
    "chapter": 18,
    "verse": 11,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 7,
    "text": "наградил тебя десятью"
  },
  {
    "chapter": 18,
    "verse": 11,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 8,
    "text": "шекелями серебра"
  },
  {
    "chapter": 18,
    "verse": 11,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 9,
    "text": "и воинским поясом."
  },
  {
    "chapter": 18,
    "verse": 12,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Но человек ответил:"
  },
  {
    "chapter": 18,
    "verse": 12,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "Даже если бы мне в руки"
  },
  {
    "chapter": 18,
    "verse": 12,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "отвесили десять тысяч шекелей,"
  },
  {
    "chapter": 18,
    "verse": 12,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "я бы не поднял"
  },
  {
    "chapter": 18,
    "verse": 12,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "руки на царского сына."
  },
  {
    "chapter": 18,
    "verse": 12,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 6,
    "text": "Мы сами слышали,"
  },
  {
    "chapter": 18,
    "verse": 12,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 7,
    "text": "как царь приказал тебе,"
  },
  {
    "chapter": 18,
    "verse": 12,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 8,
    "text": "Авессаю и Иттаю:"
  },
  {
    "chapter": 18,
    "verse": 12,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 9,
    "text": "Ради меня защитите"
  },
  {
    "chapter": 18,
    "verse": 12,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 10,
    "text": "юношу Авессалома."
  },
  {
    "chapter": 18,
    "verse": 13,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "С другой стороны,"
  },
  {
    "chapter": 18,
    "verse": 13,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "если бы я рискнул своей"
  },
  {
    "chapter": 18,
    "verse": 13,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "жизнью ведь от царя ничего"
  },
  {
    "chapter": 18,
    "verse": 13,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "не скроешь,"
  },
  {
    "chapter": 18,
    "verse": 13,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "ты остался бы в стороне."
  },
  {
    "chapter": 18,
    "verse": 14,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Иоав сказал:"
  },
  {
    "chapter": 18,
    "verse": 14,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "Нечего мне"
  },
  {
    "chapter": 18,
    "verse": 14,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "терять с тобой время."
  },
  {
    "chapter": 18,
    "verse": 14,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "Он взял в руку три дротика"
  },
  {
    "chapter": 18,
    "verse": 14,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "и вонзил"
  },
  {
    "chapter": 18,
    "verse": 14,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 6,
    "text": "их в сердце Авессалому,"
  },
  {
    "chapter": 18,
    "verse": 14,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 7,
    "text": "пока тот,"
  },
  {
    "chapter": 18,
    "verse": 14,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 8,
    "text": "еще живой,"
  },
  {
    "chapter": 18,
    "verse": 14,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 9,
    "text": "висел на дубе."
  },
  {
    "chapter": 18,
    "verse": 15,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Десять оруженосцев"
  },
  {
    "chapter": 18,
    "verse": 15,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "Иоава окружили Авессалома,"
  },
  {
    "chapter": 18,
    "verse": 15,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "поразили и умертвили его."
  },
  {
    "chapter": 18,
    "verse": 16,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Затем Иоав затрубил в рог,"
  },
  {
    "chapter": 18,
    "verse": 16,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "и воины прекратили"
  },
  {
    "chapter": 18,
    "verse": 16,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "преследовать Израиль,"
  },
  {
    "chapter": 18,
    "verse": 16,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "потому что"
  },
  {
    "chapter": 18,
    "verse": 16,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "Иоав остановил их."
  },
  {
    "chapter": 18,
    "verse": 17,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Они взяли Авессалома,"
  },
  {
    "chapter": 18,
    "verse": 17,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "бросили его в большую яму в"
  },
  {
    "chapter": 18,
    "verse": 17,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "лесу"
  },
  {
    "chapter": 18,
    "verse": 17,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "и воздвигли над ним"
  },
  {
    "chapter": 18,
    "verse": 17,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "огромную груду камней."
  },
  {
    "chapter": 18,
    "verse": 17,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 6,
    "text": "Тем временем все"
  },
  {
    "chapter": 18,
    "verse": 17,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 7,
    "text": "израильтяне разбежались по"
  },
  {
    "chapter": 18,
    "verse": 17,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 8,
    "text": "своим домам."
  },
  {
    "chapter": 18,
    "verse": 18,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Еще при жизни Авессалом"
  },
  {
    "chapter": 18,
    "verse": 18,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "взял камень"
  },
  {
    "chapter": 18,
    "verse": 18,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "и поставил его в долине"
  },
  {
    "chapter": 18,
    "verse": 18,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "Царей как памятник себе,"
  },
  {
    "chapter": 18,
    "verse": 18,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "потому что думал:"
  },
  {
    "chapter": 18,
    "verse": 18,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 6,
    "text": "У меня нет сына,"
  },
  {
    "chapter": 18,
    "verse": 18,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 7,
    "text": "чтобы сохранить"
  },
  {
    "chapter": 18,
    "verse": 18,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 8,
    "text": "в памяти мое имя."
  },
  {
    "chapter": 18,
    "verse": 18,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 9,
    "text": "Он назвал"
  },
  {
    "chapter": 18,
    "verse": 18,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 10,
    "text": "камень своим именем,"
  },
  {
    "chapter": 18,
    "verse": 18,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 11,
    "text": "и называется он Памятник"
  },
  {
    "chapter": 18,
    "verse": 18,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 12,
    "text": "Авессалому по сегодняшний"
  },
  {
    "chapter": 18,
    "verse": 18,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 13,
    "text": "день."
  },
  {
    "chapter": 18,
    "verse": 19,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Ахимаац же,"
  },
  {
    "chapter": 18,
    "verse": 19,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "сын Садока,"
  },
  {
    "chapter": 18,
    "verse": 19,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "сказал:"
  },
  {
    "chapter": 18,
    "verse": 19,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "Позволь мне побежать и"
  },
  {
    "chapter": 18,
    "verse": 19,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "принести царю весть о том,"
  },
  {
    "chapter": 18,
    "verse": 19,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 6,
    "text": "что Господь избавил"
  },
  {
    "chapter": 18,
    "verse": 19,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 7,
    "text": "его от руки его врагов."
  },
  {
    "chapter": 18,
    "verse": 20,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "- Сегодня весть"
  },
  {
    "chapter": 18,
    "verse": 20,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "понесешь не ты,"
  },
  {
    "chapter": 18,
    "verse": 20,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "сказал Иоав."
  },
  {
    "chapter": 18,
    "verse": 20,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "Ты можешь принести"
  },
  {
    "chapter": 18,
    "verse": 20,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "вести в другой раз,"
  },
  {
    "chapter": 18,
    "verse": 20,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 6,
    "text": "но сегодня не делай этого,"
  },
  {
    "chapter": 18,
    "verse": 20,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 7,
    "text": "потому что сын царя умер."
  },
  {
    "chapter": 18,
    "verse": 21,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Иоав сказал одному кушиту:"
  },
  {
    "chapter": 18,
    "verse": 21,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "Ступай,"
  },
  {
    "chapter": 18,
    "verse": 21,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "расскажи царю о том,"
  },
  {
    "chapter": 18,
    "verse": 21,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "что ты видел."
  },
  {
    "chapter": 18,
    "verse": 21,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "Кушит поклонился"
  },
  {
    "chapter": 18,
    "verse": 21,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 6,
    "text": "Иоаву и побежал."
  },
  {
    "chapter": 18,
    "verse": 22,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Сын Садока Ахимаац"
  },
  {
    "chapter": 18,
    "verse": 22,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "снова сказал Иоаву:"
  },
  {
    "chapter": 18,
    "verse": 22,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "Будь что будет,"
  },
  {
    "chapter": 18,
    "verse": 22,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "позволь мне"
  },
  {
    "chapter": 18,
    "verse": 22,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "побежать вслед за кушитом."
  },
  {
    "chapter": 18,
    "verse": 22,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 6,
    "text": "Но Иоав ответил:"
  },
  {
    "chapter": 18,
    "verse": 22,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 7,
    "text": "Мой сын,"
  },
  {
    "chapter": 18,
    "verse": 22,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 8,
    "text": "зачем ты хочешь"
  },
  {
    "chapter": 18,
    "verse": 22,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 9,
    "text": "идти? За такую весть не"
  },
  {
    "chapter": 18,
    "verse": 22,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 10,
    "text": "получишь награды."
  },
  {
    "chapter": 18,
    "verse": 23,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Он сказал:"
  },
  {
    "chapter": 18,
    "verse": 23,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "Будь что будет,"
  },
  {
    "chapter": 18,
    "verse": 23,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "я хочу бежать."
  },
  {
    "chapter": 18,
    "verse": 23,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "И Иоав сказал:"
  },
  {
    "chapter": 18,
    "verse": 23,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "Беги! Ахимаац побежал по"
  },
  {
    "chapter": 18,
    "verse": 23,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 6,
    "text": "дороге долины"
  },
  {
    "chapter": 18,
    "verse": 23,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 7,
    "text": "и обогнал кушита."
  },
  {
    "chapter": 18,
    "verse": 24,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "В то время как Давид сидел"
  },
  {
    "chapter": 18,
    "verse": 24,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "между внутренними"
  },
  {
    "chapter": 18,
    "verse": 24,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "и внешними воротами,"
  },
  {
    "chapter": 18,
    "verse": 24,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "дозорный поднялся на"
  },
  {
    "chapter": 18,
    "verse": 24,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "крышу над воротами к стене."
  },
  {
    "chapter": 18,
    "verse": 24,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 6,
    "text": "Подняв глаза,"
  },
  {
    "chapter": 18,
    "verse": 24,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 7,
    "text": "он увидел человека,"
  },
  {
    "chapter": 18,
    "verse": 24,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 8,
    "text": "бежавшего в одиночку."
  },
  {
    "chapter": 18,
    "verse": 25,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Дозорный позвал царя"
  },
  {
    "chapter": 18,
    "verse": 25,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "и доложил ему об этом."
  },
  {
    "chapter": 18,
    "verse": 25,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "Царь сказал:"
  },
  {
    "chapter": 18,
    "verse": 25,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "Если он один,"
  },
  {
    "chapter": 18,
    "verse": 25,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "то,"
  },
  {
    "chapter": 18,
    "verse": 25,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 6,
    "text": "должно быть,"
  },
  {
    "chapter": 18,
    "verse": 25,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 7,
    "text": "у него хорошая весть."
  },
  {
    "chapter": 18,
    "verse": 25,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 8,
    "text": "Человек все"
  },
  {
    "chapter": 18,
    "verse": 25,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 9,
    "text": "приближался и приближался."
  },
  {
    "chapter": 18,
    "verse": 26,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Потом дозорный увидел"
  },
  {
    "chapter": 18,
    "verse": 26,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "другого бегущего человека"
  },
  {
    "chapter": 18,
    "verse": 26,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "и закричал служащему у"
  },
  {
    "chapter": 18,
    "verse": 26,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "ворот:"
  },
  {
    "chapter": 18,
    "verse": 26,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "Вот,"
  },
  {
    "chapter": 18,
    "verse": 26,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 6,
    "text": "еще один человек"
  },
  {
    "chapter": 18,
    "verse": 26,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 7,
    "text": "бежит! Царь сказал:"
  },
  {
    "chapter": 18,
    "verse": 26,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 8,
    "text": "Должно быть,"
  },
  {
    "chapter": 18,
    "verse": 26,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 9,
    "text": "и он несет хорошую весть."
  },
  {
    "chapter": 18,
    "verse": 27,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Дозорный сказал:"
  },
  {
    "chapter": 18,
    "verse": 27,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "Мне кажется,"
  },
  {
    "chapter": 18,
    "verse": 27,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "что первый бежит,"
  },
  {
    "chapter": 18,
    "verse": 27,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "как Ахимаац,"
  },
  {
    "chapter": 18,
    "verse": 27,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "сын Садока."
  },
  {
    "chapter": 18,
    "verse": 27,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 6,
    "text": "Это хороший человек,"
  },
  {
    "chapter": 18,
    "verse": 27,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 7,
    "text": "сказал царь."
  },
  {
    "chapter": 18,
    "verse": 27,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 8,
    "text": "Он идет с хорошей вестью."
  },
  {
    "chapter": 18,
    "verse": 28,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "И Ахимаац,"
  },
  {
    "chapter": 18,
    "verse": 28,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "приблизившись,"
  },
  {
    "chapter": 18,
    "verse": 28,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "закричал царю:"
  },
  {
    "chapter": 18,
    "verse": 28,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "Все хорошо! Он поклонился"
  },
  {
    "chapter": 18,
    "verse": 28,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "царю лицом до земли"
  },
  {
    "chapter": 18,
    "verse": 28,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 6,
    "text": "и сказал:"
  },
  {
    "chapter": 18,
    "verse": 28,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 7,
    "text": "Слава Господу,"
  },
  {
    "chapter": 18,
    "verse": 28,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 8,
    "text": "твоему Богу! Он"
  },
  {
    "chapter": 18,
    "verse": 28,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 9,
    "text": "отдал тебе людей,"
  },
  {
    "chapter": 18,
    "verse": 28,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 10,
    "text": "которые подняли свои"
  },
  {
    "chapter": 18,
    "verse": 28,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 11,
    "text": "руки на моего господина,"
  },
  {
    "chapter": 18,
    "verse": 28,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 12,
    "text": "царя."
  },
  {
    "chapter": 18,
    "verse": 29,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Царь спросил:"
  },
  {
    "chapter": 18,
    "verse": 29,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "В безопасности ли юноша"
  },
  {
    "chapter": 18,
    "verse": 29,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "Авессалом? Ахимаац ответил:"
  },
  {
    "chapter": 18,
    "verse": 29,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "Я видел сильное"
  },
  {
    "chapter": 18,
    "verse": 29,
    "book_name": "2 Samuel",
