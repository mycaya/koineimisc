[
  {
    "chapter": 100,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Благодарите Господа,"
  },
  {
    "chapter": 100,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "призывайте Его Имя;"
  },
  {
    "chapter": 100,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "возвещайте среди"
  },
  {
    "chapter": 100,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "народов о Его делах."
  },
  {
    "chapter": 100,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Воспойте Ему,"
  },
  {
    "chapter": 100,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "пойте песни Ему,"
  },
  {
    "chapter": 100,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "говорите обо"
  },
  {
    "chapter": 100,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "всех Его чудесах."
  },
  {
    "chapter": 100,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Хвалитесь Его"
  },
  {
    "chapter": 100,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "святым Именем;"
  },
  {
    "chapter": 100,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "пусть веселятся"
  },
  {
    "chapter": 100,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "сердца ищущих Господа."
  },
  {
    "chapter": 100,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ищите Господа и силу Его:"
  },
  {
    "chapter": 100,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "ищите лица Его всегда."
  },
  {
    "chapter": 100,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Помните чудеса,"
  },
  {
    "chapter": 100,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "которые Он сотворил,"
  },
  {
    "chapter": 100,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Его знамения и"
  },
  {
    "chapter": 100,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "суды Его уст."
  },
  {
    "chapter": 100,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "О потомки Авраама,"
  },
  {
    "chapter": 100,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Его слуги,"
  },
  {
    "chapter": 100,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "о сыновья Иакова,"
  },
  {
    "chapter": 100,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "избранные Его."
  },
  {
    "chapter": 100,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он Господь,"
  },
  {
    "chapter": 100,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "наш Бог,"
  },
  {
    "chapter": 100,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "суды Его по всей земле."
  },
  {
    "chapter": 100,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он помнит Свой завет вечно,"
  },
  {
    "chapter": 100,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "слово,"
  },
  {
    "chapter": 100,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "данное тысяче поколений;"
  },
  {
    "chapter": 100,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "завет,"
  },
  {
    "chapter": 100,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "что Он заключил с Авраамом,"
  },
  {
    "chapter": 100,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и клятву Свою Исааку."
  },
  {
    "chapter": 100,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Утвердил Он это Иакову в"
  },
  {
    "chapter": 100,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Закон"
  },
  {
    "chapter": 100,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и Израилю вечным заветом,"
  },
  {
    "chapter": 100,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "сказав:"
  },
  {
    "chapter": 100,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Я отдам вам ханаанскую"
  },
  {
    "chapter": 100,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "землю в удел вашего наследия."
  },
  {
    "chapter": 100,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Когда они были малочисленны"
  },
  {
    "chapter": 100,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и незначительны"
  },
  {
    "chapter": 100,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и были чужеземцами на этой"
  },
  {
    "chapter": 100,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "земле,"
  },
  {
    "chapter": 100,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "когда они странствовали"
  },
  {
    "chapter": 100,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "из одной страны в другую,"
  },
  {
    "chapter": 100,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "из одного царства"
  },
  {
    "chapter": 100,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "к народу другому,"
  },
  {
    "chapter": 100,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он никому не"
  },
  {
    "chapter": 100,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "позволил их притеснять,"
  },
  {
    "chapter": 100,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "ради них укорял Он царей:"
  },
  {
    "chapter": 100,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Не трогайте помазанников"
  },
  {
    "chapter": 100,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Моих"
  },
  {
    "chapter": 100,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и пророкам Моим не делайте"
  },
  {
    "chapter": 100,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "зла."
  },
  {
    "chapter": 100,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он послал голод на землю,"
  },
  {
    "chapter": 100,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "сломил хлебный стебель."
  },
  {
    "chapter": 100,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он послал перед"
  },
  {
    "chapter": 100,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "ними человека,"
  },
  {
    "chapter": 100,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "в рабство был продан Иосиф."
  },
  {
    "chapter": 100,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Стеснили оковами его ноги,"
  },
  {
    "chapter": 100,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "надели на шею"
  },
  {
    "chapter": 100,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "железное ярмо,"
  },
  {
    "chapter": 100,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "пока не"
  },
  {
    "chapter": 100,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "исполнилось слово Господне,"
  },
  {
    "chapter": 100,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "пока слово Господне не"
  },
  {
    "chapter": 100,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "доказало его правоту."
  },
  {
    "chapter": 100,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Царь приказал,"
  },
  {
    "chapter": 100,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и развязали его,"
  },
  {
    "chapter": 100,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "владыка народов"
  },
  {
    "chapter": 100,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "освободил его."
  },
  {
    "chapter": 100,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он сделал его господином"
  },
  {
    "chapter": 100,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "над своим домом"
  },
  {
    "chapter": 100,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и правителем над всем своим"
  },
  {
    "chapter": 100,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "владением,"
  },
  {
    "chapter": 100,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "чтобы он наставлял"
  },
  {
    "chapter": 100,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "его приближенных,"
  },
  {
    "chapter": 100,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "как хотел,"
  },
  {
    "chapter": 100,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и старейшин его"
  },
  {
    "chapter": 100,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "учил мудрости."
  },
  {
    "chapter": 100,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Тогда пришел"
  },
  {
    "chapter": 100,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Израиль в Египет,"
  },
  {
    "chapter": 100,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "поселился"
  },
  {
    "chapter": 100,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Иаков в земле Хама."
  },
  {
    "chapter": 100,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "И умножил Бог Свой народ"
  },
  {
    "chapter": 100,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и сделал его сильнее его"
  },
  {
    "chapter": 100,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "врагов."
  },
  {
    "chapter": 100,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Возбудил в сердцах врагов"
  },
  {
    "chapter": 100,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "ненависть к Своему народу"
  },
  {
    "chapter": 100,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и хитрость против Его"
  },
  {
    "chapter": 100,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "рабов."
  },
  {
    "chapter": 100,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он послал Моисея,"
  },
  {
    "chapter": 100,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Своего слугу,"
  },
  {
    "chapter": 100,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и Аарона,"
  },
  {
    "chapter": 100,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "которого избрал."
  },
  {
    "chapter": 100,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Они показали среди них Его"
  },
  {
    "chapter": 100,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "чудеса"
  },
  {
    "chapter": 100,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и Его знамения в земле"
  },
  {
    "chapter": 100,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Хама."
  },
  {
    "chapter": 100,
    "verse": 28,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он послал тьму,"
  },
  {
    "chapter": 100,
    "verse": 28,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и стало темно,"
  },
  {
    "chapter": 100,
    "verse": 28,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и они не"
  },
  {
    "chapter": 100,
    "verse": 28,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "воспротивились Его слову."
  },
  {
    "chapter": 100,
    "verse": 29,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Обратил воды их в"
  },
  {
    "chapter": 100,
    "verse": 29,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "кровь и погубил в них рыбу."
  },
  {
    "chapter": 100,
    "verse": 30,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Земля их закишела"
  },
  {
    "chapter": 100,
    "verse": 30,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "множеством лягушек,"
  },
  {
    "chapter": 100,
    "verse": 30,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "которые были"
  },
  {
    "chapter": 100,
    "verse": 30,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "даже в покоях царей."
  },
  {
    "chapter": 100,
    "verse": 31,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он произнес слово,"
  },
  {
    "chapter": 100,
    "verse": 31,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "налетели тучи мух,"
  },
  {
    "chapter": 100,
    "verse": 31,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "комары"
  },
  {
    "chapter": 100,
    "verse": 31,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "заполнили все их земли"
  },
  {
    "chapter": 100,
    "verse": 32,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он послал на них град"
  },
  {
    "chapter": 100,
    "verse": 32,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "вместо дождя"
  },
  {
    "chapter": 100,
    "verse": 32,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и пылающий огонь на"
  },
  {
    "chapter": 100,
    "verse": 32,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "их землю."
  },
  {
    "chapter": 100,
    "verse": 33,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Побил виноградники и инжир,"
  },
  {
    "chapter": 100,
    "verse": 33,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "поломал деревья в их земле."
  },
  {
    "chapter": 100,
    "verse": 34,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Сказал,"
  },
  {
    "chapter": 100,
    "verse": 34,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и пришла"
  },
  {
    "chapter": 100,
    "verse": 34,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "саранча целые тучи,"
  },
  {
    "chapter": 100,
    "verse": 34,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "нет им числа,"
  },
  {
    "chapter": 100,
    "verse": 35,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "и съели всю траву в их"
  },
  {
    "chapter": 100,
    "verse": 35,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "земле и плоды на их полях."
  },
  {
    "chapter": 100,
    "verse": 36,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Также Он поразил всех"
  },
  {
    "chapter": 100,
    "verse": 36,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "первенцев в"
  },
  {
    "chapter": 100,
    "verse": 36,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "их земле первые плоды"
  },
  {
    "chapter": 100,
    "verse": 36,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "их мужской силы."
  },
  {
    "chapter": 100,
    "verse": 37,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он вывел Израиль с"
  },
  {
    "chapter": 100,
    "verse": 37,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "серебром и золотом,"
  },
  {
    "chapter": 100,
    "verse": 37,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и не было ни"
  },
  {
    "chapter": 100,
    "verse": 37,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "одного среди родов,"
  },
  {
    "chapter": 100,
    "verse": 37,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "кто бы споткнулся."
  },
  {
    "chapter": 100,
    "verse": 38,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Египет"
  },
  {
    "chapter": 100,
    "verse": 38,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "обрадовался их уходу,"
  },
  {
    "chapter": 100,
    "verse": 38,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "потому что страх"
  },
  {
    "chapter": 100,
    "verse": 38,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "перед ними напал на него."
  },
  {
    "chapter": 100,
    "verse": 39,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Бог простер облако,"
  },
  {
    "chapter": 100,
    "verse": 39,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "чтобы укрыть их,"
  },
  {
    "chapter": 100,
    "verse": 39,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и огонь,"
  },
  {
    "chapter": 100,
    "verse": 39,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "чтобы светить ночью."
  },
  {
    "chapter": 100,
    "verse": 40,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Народ просил,"
  },
  {
    "chapter": 100,
    "verse": 40,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и Он послал перепелов,"
  },
  {
    "chapter": 100,
    "verse": 40,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и насыщал людей"
  },
  {
    "chapter": 100,
    "verse": 40,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "хлебом с небес."
  },
  {
    "chapter": 100,
    "verse": 41,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Рассек скалу,"
  },
  {
    "chapter": 100,
    "verse": 41,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и полились воды,"
  },
  {
    "chapter": 100,
    "verse": 41,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "рекой потекли в пустыне."
  },
  {
    "chapter": 100,
    "verse": 42,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ведь Он вспомнил"
  },
  {
    "chapter": 100,
    "verse": 42,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Свое святое слово,"
  },
  {
    "chapter": 100,
    "verse": 42,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "к Аврааму,"
  },
  {
    "chapter": 100,
    "verse": 42,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Своему слуге."
  },
  {
    "chapter": 100,
    "verse": 43,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он вывел Свой"
  },
  {
    "chapter": 100,
    "verse": 43,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "народ в радости,"
  },
  {
    "chapter": 100,
    "verse": 43,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "избранных Своих в веселии."
  },
  {
    "chapter": 100,
    "verse": 44,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Дал им земли народов,"
  },
  {
    "chapter": 100,
    "verse": 44,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и унаследовали то,"
  },
  {
    "chapter": 100,
    "verse": 44,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "над чем трудились другие,"
  },
  {
    "chapter": 100,
    "verse": 45,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "чтобы соблюдали Его"
  },
  {
    "chapter": 100,
    "verse": 45,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "установления и хранили Его законы."
  },
  {
    "chapter": 100,
    "verse": 45,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Аллилуйя!"
  }
]