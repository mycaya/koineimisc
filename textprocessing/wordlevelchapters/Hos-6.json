  {
    "chapter": 6,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "вернемся к Господу."
  },
  {
    "chapter": 6,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "Он уязвил нас,"
  },
  {
    "chapter": 6,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "но Он же и исцелит нас."
  },
  {
    "chapter": 6,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 7,
    "text": "Он поранил нас,"
  },
  {
    "chapter": 6,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 8,
    "text": "но Он перевяжет наши раны."
  },
  {
    "chapter": 6,
    "verse": 2,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Через два дня"
  },
  {
    "chapter": 6,
    "verse": 2,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "Он оживит нас,"
  },
  {
    "chapter": 6,
    "verse": 2,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "а на третий день"
  },
  {
    "chapter": 6,
    "verse": 2,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "Он восстановит нас,"
  },
  {
    "chapter": 6,
    "verse": 2,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "чтобы мы могли жить"
  },
  {
    "chapter": 6,
    "verse": 2,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "в Его присутствии."
  },
  {
    "chapter": 6,
    "verse": 3,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Узнаем же Господа."
  },
  {
    "chapter": 6,
    "verse": 3,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "Будем"
  },
  {
    "chapter": 6,
    "verse": 3,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "стремиться узнать Его."
  },
  {
    "chapter": 6,
    "verse": 3,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "Верно как и то,"
  },
  {
    "chapter": 6,
    "verse": 3,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "что восходит солнце,"
  },
  {
    "chapter": 6,
    "verse": 3,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "Он явится."
  },
  {
    "chapter": 6,
    "verse": 3,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 7,
    "text": "Он придет к нам,"
  },
  {
    "chapter": 6,
    "verse": 3,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 8,
    "text": "как дождь,"
  },
  {
    "chapter": 6,
    "verse": 3,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 9,
    "text": "подобно весенним дождям,"
  },
  {
    "chapter": 6,
    "verse": 3,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 10,
    "text": "что орошают землю."
  },
  {
    "chapter": 6,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "- Что же Мне"
  },
  {
    "chapter": 6,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "делать с тобой,"
  },
  {
    "chapter": 6,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "Ефрем? Что же"
  },
  {
    "chapter": 6,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "Мне делать с тобой,"
  },
  {
    "chapter": 6,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "Иудея? Ваша верность"
  },
  {
    "chapter": 6,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "как утренний туман,"
  },
  {
    "chapter": 6,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 7,
    "text": "словно роса,"
  },
  {
    "chapter": 6,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 8,
    "text": "что вскоре исчезает."
  },
  {
    "chapter": 6,
    "verse": 5,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Поэтому Я растерзал вас на"
  },
  {
    "chapter": 6,
    "verse": 5,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "куски через Моих пророков."
  },
  {
    "chapter": 6,
    "verse": 5,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "Я убил вас"
  },
  {
    "chapter": 6,
    "verse": 5,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "словами Моих уст."
  },
  {
    "chapter": 6,
    "verse": 5,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "Словно молния,"
  },
  {
    "chapter": 6,
    "verse": 5,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "вспыхнул Мой суд над вами."
  },
  {
    "chapter": 6,
    "verse": 6,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Ведь Я милости хочу,"
  },
  {
    "chapter": 6,
    "verse": 6,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "а не жертвы,"
  },
  {
    "chapter": 6,
    "verse": 6,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "и познания Бога,"
  },
  {
    "chapter": 6,
    "verse": 6,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "нежели всесожжения."
  },
  {
    "chapter": 6,
    "verse": 7,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Подобно Адаму,"
  },
  {
    "chapter": 6,
    "verse": 7,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "вы нарушили завет."
  },
  {
    "chapter": 6,
    "verse": 7,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "Там вы не были Мне верны."
  },
  {
    "chapter": 6,
    "verse": 8,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Галаад запятнан кровью,"
  },
  {
    "chapter": 6,
    "verse": 8,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "город нечестивых людей."
  },
  {
    "chapter": 6,
    "verse": 9,
    "book_name": "Hosea",
    "book_id": "Hos",
