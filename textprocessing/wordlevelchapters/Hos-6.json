  },
  {
    "chapter": 6,
    "verse": 10,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "осквернил себя Израиль."
  },
  {
    "chapter": 6,
    "verse": 11,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "И для тебя,"
  },
  {
    "chapter": 6,
    "verse": 11,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "Иудея,"
  },
  {
