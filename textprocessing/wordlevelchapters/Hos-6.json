[
  {
    "chapter": 6,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "- В своем страдании они"
  },
  {
    "chapter": 6,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "будут настойчиво искать Меня:"
  },
  {
    "chapter": 6,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "Пойдем и"
