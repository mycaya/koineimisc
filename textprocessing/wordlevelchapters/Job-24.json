[
  {
    "chapter": 24,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Почему Всемогущий не"
  },
  {
    "chapter": 24,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "назначит время для суда?"
  },
  {
    "chapter": 24,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Почему те,"
  },
  {
    "chapter": 24,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "кто знает Его,"
  },
  {
    "chapter": 24,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "этих дней ожидают тщетно?"
  },
  {
    "chapter": 24,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Люди передвигают межи,"
  },
  {
    "chapter": 24,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "пасут украденные стада."
  },
  {
    "chapter": 24,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "У сироты угоняют осла и"
  },
  {
    "chapter": 24,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "вола у вдовы отнимают в залог;"
  },
  {
    "chapter": 24,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "с дороги"
  },
  {
    "chapter": 24,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "сталкивают бедняка,"
  },
  {
    "chapter": 24,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и должны скрываться"
  },
  {
    "chapter": 24,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "все страдальцы земли."
  },
  {
    "chapter": 24,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Словно дикие"
  },
  {
    "chapter": 24,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "ослы в пустыне,"
  },
  {
    "chapter": 24,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "выходят на"
  },
  {
    "chapter": 24,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "свой труд бедные,"
  },
  {
    "chapter": 24,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "разыскивая в степи"
  },
  {
    "chapter": 24,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 6,
    "text": "пищу себе и детям своим."
  },
  {
    "chapter": 24,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Жнут они не на своих полях,"
  },
  {
    "chapter": 24,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и виноград"
  },
  {
    "chapter": 24,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "собирают у нечестивых."
  },
  {
    "chapter": 24,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Не имея одежды,"
  },
  {
    "chapter": 24,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "ночуют нагими"
  },
  {
    "chapter": 24,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "ничего у них нет,"
  },
  {
    "chapter": 24,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "чтоб от стужи прикрыться."
  },
  {
    "chapter": 24,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Секут их в горах дожди;"
  },
  {
    "chapter": 24,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "льнут они к скалам,"
  },
  {
    "chapter": 24,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "ища приюта."
  },
  {
    "chapter": 24,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "От груди отнимается сирота,"
  },
  {
    "chapter": 24,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и младенцев у"
  },
  {
    "chapter": 24,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "бедных берут в залог."
  },
  {
    "chapter": 24,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Не имея одежды,"
  },
  {
    "chapter": 24,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "ходят они нагими;"
  },
  {
    "chapter": 24,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и,"
  },
  {
    "chapter": 24,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "нося снопы,"
  },
  {
    "chapter": 24,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "остаются голодными."
  },
  {
    "chapter": 24,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Между стенами давят"
  },
  {
    "chapter": 24,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "масло они из оливок;"
  },
  {
    "chapter": 24,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "страдая от жажды,"
  },
  {
    "chapter": 24,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "топчут они виноград."
  },
  {
    "chapter": 24,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Стонут в городе умирающие,"
  },
  {
    "chapter": 24,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и зовут на помощь"
  },
  {
    "chapter": 24,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "уста израненных."
  },
  {
    "chapter": 24,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "Но Бог не замечает"
  },
  {
    "chapter": 24,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "несправедливости."
  },
  {
    "chapter": 24,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Есть те,"
  },
  {
    "chapter": 24,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "кто восстают против света,"
  },
  {
    "chapter": 24,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "они не знают путей его,"
  },
  {
    "chapter": 24,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "и не следуют по ним."
  },
  {
    "chapter": 24,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Меркнет день,"
  },
  {
    "chapter": 24,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и встает убийца,"
  },
  {
    "chapter": 24,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "чтобы убить"
  },
  {
    "chapter": 24,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "бедняка и нищего."
  },
  {
    "chapter": 24,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "Он крадется в ночи,"
  },
  {
    "chapter": 24,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 6,
    "text": "как вор."
  },
  {
    "chapter": 24,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Ждет сумерек"
  },
  {
    "chapter": 24,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "око распутника;"
  },
  {
    "chapter": 24,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "он думает:"
  },
  {
    "chapter": 24,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "Ничьи глаза меня не увидят,"
  },
  {
    "chapter": 24,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "и закутывает лицо."
  },
  {
    "chapter": 24,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "В темноте он"
  },
  {
    "chapter": 24,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "вламывается в дома,"
  },
  {
    "chapter": 24,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "а днем запирается у себя;"
  },
  {
    "chapter": 24,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "свет ему неведом."
  },
  {
    "chapter": 24,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Непроглядная тьма его утро;"
  },
  {
    "chapter": 24,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и отрадны ему"
  },
  {
    "chapter": 24,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "ужасы кромешной тьмы."
  },
  {
    "chapter": 24,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Но они лишь"
  },
  {
    "chapter": 24,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "пена на глади вод;"
  },
  {
    "chapter": 24,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "их земельный надел проклят,"
  },
  {
    "chapter": 24,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "и никто не пойдет к ним"
  },
  {
    "chapter": 24,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "топтать в давильне виноград."
  },
  {
    "chapter": 24,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Сушь и зной поглощают снег,"
  },
  {
    "chapter": 24,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "а грешников мир мертвых."
  },
  {
    "chapter": 24,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Позабудет их"
  },
  {
    "chapter": 24,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "материнское чрево,"
  },
  {
    "chapter": 24,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "полакомится ими червь;"
  },
  {
    "chapter": 24,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "о злодеях больше"
  },
  {
    "chapter": 24,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "никто не вспомнит,"
  },
  {
    "chapter": 24,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 6,
    "text": "они будут сломаны,"
  },
  {
    "chapter": 24,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 7,
    "text": "словно дерево."
  },
  {
    "chapter": 24,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Они притесняют неплодную"
  },
  {
    "chapter": 24,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и бездетную"
  },
  {
    "chapter": 24,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и вдове не делают добра."
  },
  {
    "chapter": 24,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Но Бог и сильных"
  },
  {
    "chapter": 24,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "одолеет Своей силой;"
  },
  {
    "chapter": 24,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "они утвердились,"
  },
  {
    "chapter": 24,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "но нет им за"
  },
  {
    "chapter": 24,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "жизнь ручательства."
  },
  {
    "chapter": 24,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Он дает им"
  },
  {
    "chapter": 24,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "покоиться безмятежно,"
  },
  {
    "chapter": 24,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "но глаза Его видят их пути."
  },
  {
    "chapter": 24,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "На миг вознесутся,"
  },
  {
    "chapter": 24,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и вот их нет;"
  },
  {
    "chapter": 24,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "они падают и умирают,"
  },
  {
    "chapter": 24,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "как все;"
  },
  {
    "chapter": 24,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "их жнут,"
  },
  {
    "chapter": 24,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 6,
    "text": "как колосья."
  },
  {
    "chapter": 24,
    "verse": 25,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Разве это не так? Кто во"
  },
  {
    "chapter": 24,
    "verse": 25,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "лжи меня обличит"
  },
  {
    "chapter": 24,
    "verse": 25,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и в ничто обратит мою речь?"
  }
]