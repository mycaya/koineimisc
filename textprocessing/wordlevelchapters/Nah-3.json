[
  {
    "chapter": 3,
    "verse": 1,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Горе кровавому городу,"
  },
  {
    "chapter": 3,
    "verse": 1,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "полному лжи,"
  },
  {
    "chapter": 3,
    "verse": 1,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "в котором не"
  },
  {
    "chapter": 3,
    "verse": 1,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "прекращаются воровство и грабеж!"
  },
  {
    "chapter": 3,
    "verse": 2,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Слышны щелканье кнута,"
  },
  {
    "chapter": 3,
    "verse": 2,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "стук колес,"
  },
  {
    "chapter": 3,
    "verse": 2,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "топот скачущих"
  },
  {
    "chapter": 3,
    "verse": 2,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "лошадей и грохот колесниц."
  },
  {
    "chapter": 3,
    "verse": 3,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Несется конница,"
  },
  {
    "chapter": 3,
    "verse": 3,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "сверкают мечи,"
  },
  {
    "chapter": 3,
    "verse": 3,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "блестят копья!"
  },
  {
    "chapter": 3,
    "verse": 3,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "Множество пораженных,"
  },
  {
    "chapter": 3,
    "verse": 3,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 5,
    "text": "целые груды трупов,"
  },
  {
    "chapter": 3,
    "verse": 3,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 6,
    "text": "которых не сосчитать."
  },
  {
    "chapter": 3,
    "verse": 3,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 7,
    "text": "Люди спотыкаются"
  },
  {
    "chapter": 3,
    "verse": 3,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 8,
    "text": "о тела пораженных."
  },
  {
    "chapter": 3,
    "verse": 4,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "И все это за великое"
  },
  {
    "chapter": 3,
    "verse": 4,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "распутство блудницы,"
  },
  {
    "chapter": 3,
    "verse": 4,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "пленительной и"
  },
  {
    "chapter": 3,
    "verse": 4,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "искусной в чарах,"
  },
  {
    "chapter": 3,
    "verse": 4,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 5,
    "text": "покоряющей народы своим"
  },
  {
    "chapter": 3,
    "verse": 4,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 6,
    "text": "распутством"
  },
  {
    "chapter": 3,
    "verse": 4,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 7,
    "text": "и племена своим"
  },
  {
    "chapter": 3,
    "verse": 4,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 8,
    "text": "колдовством."
  },
  {
    "chapter": 3,
    "verse": 5,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "- Я против тебя,"
  },
  {
    "chapter": 3,
    "verse": 5,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "возвещает Господь сил,"
  },
  {
    "chapter": 3,
    "verse": 5,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "Я задеру твой"
  },
  {
    "chapter": 3,
    "verse": 5,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "подол тебе на лицо,"
  },
  {
    "chapter": 3,
    "verse": 5,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 5,
    "text": "Я покажу народам твою"
  },
  {
    "chapter": 3,
    "verse": 5,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 6,
    "text": "наготу и царствам твой срам."
  },
  {
    "chapter": 3,
    "verse": 6,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Я забросаю тебя грязью,"
  },
  {
    "chapter": 3,
    "verse": 6,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "буду относиться к тебе с"
  },
  {
    "chapter": 3,
    "verse": 6,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "презрением"
  },
  {
    "chapter": 3,
    "verse": 6,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "и выставлю тебя на всеобщее"
  },
  {
    "chapter": 3,
    "verse": 6,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 5,
    "text": "обозрение."
  },
  {
    "chapter": 3,
    "verse": 7,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "И будет так,"
  },
  {
    "chapter": 3,
    "verse": 7,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "что каждый,"
  },
  {
    "chapter": 3,
    "verse": 7,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "кто увидит тебя,"
  },
  {
    "chapter": 3,
    "verse": 7,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "побежит от тебя и скажет:"
  },
  {
    "chapter": 3,
    "verse": 7,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 5,
    "text": "Ниневия в руинах,"
  },
  {
    "chapter": 3,
    "verse": 7,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 6,
    "text": "кто будет ее оплакивать?."
  },
  {
    "chapter": 3,
    "verse": 7,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 7,
    "text": "Где найду Я того,"
  },
  {
    "chapter": 3,
    "verse": 7,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 8,
    "text": "кто утешит ее?"
  },
  {
    "chapter": 3,
    "verse": 8,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Разве ты лучше"
  },
  {
    "chapter": 3,
    "verse": 8,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "города Но-Аммона,"
  },
  {
    "chapter": 3,
    "verse": 8,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "расположенного между реками"
  },
  {
    "chapter": 3,
    "verse": 8,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "и окруженного водой? Река"
  },
  {
    "chapter": 3,
    "verse": 8,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 5,
    "text": "была его защитою,"
  },
  {
    "chapter": 3,
    "verse": 8,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 6,
    "text": "и вода его стеною."
  },
  {
    "chapter": 3,
    "verse": 9,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Куш и Египет были"
  },
  {
    "chapter": 3,
    "verse": 9,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "его безграничной силой,"
  },
  {
    "chapter": 3,
    "verse": 9,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "Пут и Ливия были в"
  },
  {
    "chapter": 3,
    "verse": 9,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "числе его союзников."
  },
  {
    "chapter": 3,
    "verse": 10,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Но он все же был взят в"
  },
  {
    "chapter": 3,
    "verse": 10,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "плен и отправлен в изгнание."
  },
  {
    "chapter": 3,
    "verse": 10,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "Его младенцы разбиты на"
  },
  {
    "chapter": 3,
    "verse": 10,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "перекрестках всех улиц."
  },
  {
    "chapter": 3,
    "verse": 10,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 5,
    "text": "О его знатных"
  },
  {
    "chapter": 3,
    "verse": 10,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 6,
    "text": "людях бросали жребий,"
  },
  {
    "chapter": 3,
    "verse": 10,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 7,
    "text": "и всех их заковали в цепи."
  },
  {
    "chapter": 3,
    "verse": 11,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Так и ты опьянеешь,"
  },
  {
    "chapter": 3,
    "verse": 11,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "скроешься и будешь"
  },
  {
    "chapter": 3,
    "verse": 11,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "искать защиты от врага."
  },
  {
    "chapter": 3,
    "verse": 12,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Твои крепости подобны"
  },
  {
    "chapter": 3,
    "verse": 12,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "инжиру со спелыми плодами:"
  },
  {
    "chapter": 3,
    "verse": 12,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "если тряхнуть их,"
  },
  {
    "chapter": 3,
    "verse": 12,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "то они упадут"
  },
  {
    "chapter": 3,
    "verse": 12,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 5,
    "text": "прямо в рот тому,"
  },
  {
    "chapter": 3,
    "verse": 12,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 6,
    "text": "кто ест."
  },
  {
    "chapter": 3,
    "verse": 13,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Взгляни на свои войска там"
  },
  {
    "chapter": 3,
    "verse": 13,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "одни женщины! Ворота твоей"
  },
  {
    "chapter": 3,
    "verse": 13,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "земли распахнуты настежь"
  },
  {
    "chapter": 3,
    "verse": 13,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "для твоих врагов,"
  },
  {
    "chapter": 3,
    "verse": 13,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 5,
    "text": "а их засовы пожрал огонь."
  },
  {
    "chapter": 3,
    "verse": 14,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Начерпай воды и"
  },
  {
    "chapter": 3,
    "verse": 14,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "запасись ею на время осады,"
  },
  {
    "chapter": 3,
    "verse": 14,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "укрепи свою оборону!"
  },
  {
    "chapter": 3,
    "verse": 14,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "Иди и намеси глины,"
  },
  {
    "chapter": 3,
    "verse": 14,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 5,
    "text": "для кирпичей,"
  },
  {
    "chapter": 3,
    "verse": 14,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 6,
    "text": "чтобы укрепить"
  },
  {
    "chapter": 3,
    "verse": 14,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 7,
    "text": "ими свои стены"
  },
  {
    "chapter": 3,
    "verse": 15,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Огонь поглотит тебя,"
  },
  {
    "chapter": 3,
    "verse": 15,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "меч сразит тебя и,"
  },
  {
    "chapter": 3,
    "verse": 15,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "как саранча,"
  },
  {
    "chapter": 3,
    "verse": 15,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "пожрет тебя."
  },
  {
    "chapter": 3,
    "verse": 15,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 5,
    "text": "Размножайся,"
  },
  {
    "chapter": 3,
    "verse": 15,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 6,
    "text": "как кузнечик,"
  },
  {
    "chapter": 3,
    "verse": 15,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 7,
    "text": "размножайся,"
  },
  {
    "chapter": 3,
    "verse": 15,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 8,
    "text": "как саранча!"
  },
  {
    "chapter": 3,
    "verse": 16,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Торговцев у"
  },
  {
    "chapter": 3,
    "verse": 16,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "тебя стало больше,"
  },
  {
    "chapter": 3,
    "verse": 16,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "чем звезд на небе,"
  },
  {
    "chapter": 3,
    "verse": 16,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "но подобно саранче,"
  },
  {
    "chapter": 3,
    "verse": 16,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 5,
    "text": "они опустошат"
  },
  {
    "chapter": 3,
    "verse": 16,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 6,
    "text": "землю и улетят."
  },
  {
    "chapter": 3,
    "verse": 17,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Твоя охрана,"
  },
  {
    "chapter": 3,
    "verse": 17,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "как саранча,"
  },
  {
    "chapter": 3,
    "verse": 17,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "твои военачальники,"
  },
  {
    "chapter": 3,
    "verse": 17,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "как полчища саранчи,"
  },
  {
    "chapter": 3,
    "verse": 17,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 5,
    "text": "которые располагаются"
  },
  {
    "chapter": 3,
    "verse": 17,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 6,
    "text": "на стенах в холодные дни,"
  },
  {
    "chapter": 3,
    "verse": 17,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 7,
    "text": "но как пригреет солнце,"
  },
  {
    "chapter": 3,
    "verse": 17,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 8,
    "text": "разлетаются,"
  },
  {
    "chapter": 3,
    "verse": 17,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 9,
    "text": "а куда,"
  },
  {
    "chapter": 3,
    "verse": 17,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 10,
    "text": "никто не знает."
  },
  {
    "chapter": 3,
    "verse": 18,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Царь Ассирии,"
  },
  {
    "chapter": 3,
    "verse": 18,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "спят твои пастухи,"
  },
  {
    "chapter": 3,
    "verse": 18,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "отдыхают твои приближенные."
  },
  {
    "chapter": 3,
    "verse": 18,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "Твои люди"
  },
  {
    "chapter": 3,
