[
  {
    "chapter": 39,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Знаешь ли ты время"
  },
  {
    "chapter": 39,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "рождения серн? Видел ли ты,"
  },
  {
    "chapter": 39,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "как рожают детенышей лани?"
  },
  {
    "chapter": 39,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Считаешь ли сколько месяцев"
  },
  {
    "chapter": 39,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "они носят плод? Знаешь ли"
  },
  {
    "chapter": 39,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "время их родов?"
  },
  {
    "chapter": 39,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Они,"
  },
  {
    "chapter": 39,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "приседая,"
  },
  {
    "chapter": 39,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "рожают,"
  },
  {
    "chapter": 39,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "освобождаясь от бремени."
  },
  {
    "chapter": 39,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "В полях"
  },
  {
    "chapter": 39,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "набираются сил их детеныши;"
  },
  {
    "chapter": 39,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "потом уходят и не"
  },
  {
    "chapter": 39,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "возвращаются к ним."
  },
  {
    "chapter": 39,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Кто дал волю диким"
  },
  {
    "chapter": 39,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "ослам? Кто развязал их узы?"
  },
  {
    "chapter": 39,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Я назначил им домом"
  },
  {
    "chapter": 39,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "степь и жилищем солончаки."
  },
  {
    "chapter": 39,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Над шумом городским смеются"
  },
  {
    "chapter": 39,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "они"
  },
  {
    "chapter": 39,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и не слышат криков"
  },
  {
    "chapter": 39,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "погонщика."
  },
  {
    "chapter": 39,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "По горам они ищут пищу"
  },
  {
    "chapter": 39,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и всякой зелени рады."
  },
  {
    "chapter": 39,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Согласится ли дикий бык"
  },
  {
    "chapter": 39,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "тебе служить? Станет ли в"
  },
  {
    "chapter": 39,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "стойле твоем ночевать?"
  },
  {
    "chapter": 39,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Привяжешь ли его"
  },
  {
    "chapter": 39,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "веревкой к плугу,"
  },
  {
    "chapter": 39,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "чтобы он"
  },
  {
    "chapter": 39,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "боронил за тобой поле?"
  },
  {
    "chapter": 39,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Понадеешься ли на силу его"
  },
  {
    "chapter": 39,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и доверишь ли труд свой"
  },
  {
    "chapter": 39,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "ему?"
  },
  {
    "chapter": 39,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Поверишь ли ты,"
  },
  {
    "chapter": 39,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "что привезет он твое"
  },
  {
    "chapter": 39,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "зерно и сложит его на гумне?"
  },
  {
    "chapter": 39,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Весело хлопает"
  },
  {
    "chapter": 39,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "страус крыльями,"
  },
  {
    "chapter": 39,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "но они не сравнятся с"
  },
  {
    "chapter": 39,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "перьями и пухом аиста."
  },
  {
    "chapter": 39,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Он на землю"
  },
  {
    "chapter": 39,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "яйца свои кладет,"
  },
  {
    "chapter": 39,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "оставляет их"
  },
  {
    "chapter": 39,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "греться в горячем песке,"
  },
  {
    "chapter": 39,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "забыв,"
  },
  {
    "chapter": 39,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "что нога может"
  },
  {
    "chapter": 39,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "их раздавить,"
  },
  {
    "chapter": 39,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "и дикий зверь растоптать."
  },
  {
    "chapter": 39,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Жесток он к птенцам,"
  },
  {
    "chapter": 39,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "словно к чужим,"
  },
  {
    "chapter": 39,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "не боится,"
  },
  {
    "chapter": 39,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "что труд его был напрасен,"
  },
  {
    "chapter": 39,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "потому что Бог обделил его"
  },
  {
    "chapter": 39,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "мудростью"
  },
  {
    "chapter": 39,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и здравого разума не дал."
  },
  {
    "chapter": 39,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Но когда он"
  },
  {
    "chapter": 39,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "поднимется и помчится,"
  },
  {
    "chapter": 39,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "конь и всадник ему смешны."
  },
  {
    "chapter": 39,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Ты ли дал коню его силу,"
  },
  {
    "chapter": 39,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "шею гривой ему облек,"
  },
  {
    "chapter": 39,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "заставил его прыгать,"
  },
  {
    "chapter": 39,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "как саранча,"
  },
  {
    "chapter": 39,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "храп его страшен?"
  },
  {
    "chapter": 39,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Он взрывает землю в долине,"
  },
  {
    "chapter": 39,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "радуясь силе,"
  },
  {
    "chapter": 39,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и мчится навстречу битве."
  },
  {
    "chapter": 39,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Он смеется над"
  },
  {
    "chapter": 39,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "страхом и не робеет,"
  },
  {
    "chapter": 39,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "от меча он не повернет."
  },
  {
    "chapter": 39,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Звенит на боку"
  },
  {
    "chapter": 39,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "у него колчан,"
  },
  {
    "chapter": 39,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "сверкают копье и дротик."
  },
  {
    "chapter": 39,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Глотает он землю"
  },
  {
    "chapter": 39,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "в порыве ярости,"
  },
  {
    "chapter": 39,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "не сдержать его"
  },
  {
    "chapter": 39,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "при трубном зове."
  },
  {
    "chapter": 39,
    "verse": 25,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "При звуке трубы ржет конь,"
  },
  {
    "chapter": 39,
    "verse": 25,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "битву он чует издалека,"
  },
  {
    "chapter": 39,
    "verse": 25,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "крик воевод и клич боевой."
  },
  {
    "chapter": 39,
    "verse": 26,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Твоей ли"
  },
  {
    "chapter": 39,
    "verse": 26,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "мудростью реет ястреб,"
  },
  {
    "chapter": 39,
    "verse": 26,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "крылья свои"
  },
  {
    "chapter": 39,
    "verse": 26,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "простирая к югу?"
  },
  {
    "chapter": 39,
    "verse": 27,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "По твоей ли воле орел"
  },
  {
    "chapter": 39,
    "verse": 27,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "взмывает"
  },
  {
    "chapter": 39,
    "verse": 27,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и строит гнездо свое на"
  },
  {
    "chapter": 39,
    "verse": 27,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "высоте?"
  },
  {
    "chapter": 39,
    "verse": 28,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Он живет на скале и"
  },
  {
    "chapter": 39,
    "verse": 28,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "ночь там проводит,"
  },
  {
    "chapter": 39,
    "verse": 28,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "его твердыня горный утес."
  },
  {
    "chapter": 39,
    "verse": 29,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Там он высматривает добычу;"
  },
  {
    "chapter": 39,
    "verse": 29,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "глаза его"
  },
  {
    "chapter": 39,
    "verse": 29,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "видят ее издалека."
  },
  {
    "chapter": 39,
    "verse": 30,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Кровь пьют его птенцы,"
  },
  {
    "chapter": 39,
    "verse": 30,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "где труп,"
  },
  {
    "chapter": 39,
    "verse": 30,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "там и он."
  },
  {
    "chapter": 39,
    "verse": 31,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Господь сказал Иову:"
  },
  {
    "chapter": 39,
    "verse": 32,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "- Будет ли спорящий со"
  },
  {
    "chapter": 39,
    "verse": 32,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Всемогущим наставлять Его?"
  },
  {
    "chapter": 39,
    "verse": 32,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Пусть обвиняющий Бога Ему"
  },
  {
    "chapter": 39,
    "verse": 32,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "ответит!"
  },
  {
    "chapter": 39,
    "verse": 33,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Тогда Иов ответил Господу:"
  },
  {
    "chapter": 39,
    "verse": 34,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "- Вот,"
  },
  {
    "chapter": 39,
    "verse": 34,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "я ничтожен как я Тебе"
  },
  {
    "chapter": 39,
    "verse": 34,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "отвечу? Руку свою кладу на уста."
  },
  {
    "chapter": 39,
    "verse": 35,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Я сказал однажды но"
  },
  {
    "chapter": 39,
    "verse": 35,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "больше не буду продолжать,"
  },
  {
    "chapter": 39,
    "verse": 35,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "дважды,"
  },
  {
    "chapter": 39,
    "verse": 35,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "но впредь не стану."
  }
]