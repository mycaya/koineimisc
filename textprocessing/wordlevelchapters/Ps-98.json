[
  {
    "chapter": 98,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Псалом Давида."
  },
  {
    "chapter": 98,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Прославь,"
  },
  {
    "chapter": 98,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "душа моя,"
  },
  {
    "chapter": 98,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Господа;"
  },
  {
    "chapter": 98,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "все нутро мое,"
  },
  {
    "chapter": 98,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "славь Его святое Имя!"
  },
  {
    "chapter": 98,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Прославь,"
  },
  {
    "chapter": 98,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "душа моя,"
  },
  {
    "chapter": 98,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Господа,"
  },
  {
    "chapter": 98,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и не забудь"
  },
  {
    "chapter": 98,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "добрые дела Его -"
  },
  {
    "chapter": 98,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Того,"
  },
  {
    "chapter": 98,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Кто прощает всю вину твою и"
  },
  {
    "chapter": 98,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "исцеляет все твои болезни;"
  },
  {
    "chapter": 98,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Кто"
  },
  {
    "chapter": 98,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "избавляет от могилы твою"
  },
  {
    "chapter": 98,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "жизнь"
  },
  {
    "chapter": 98,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и венчает тебя милостью"
  },
  {
    "chapter": 98,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "и щедротами;"
  },
  {
    "chapter": 98,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Кто наполняет"
  },
  {
    "chapter": 98,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "твою жизнь благами,"
  },
  {
    "chapter": 98,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "чтобы,"
  },
  {
    "chapter": 98,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "подобно орлу,"
  },
  {
    "chapter": 98,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "обновлялась твоя юность."
  },
  {
    "chapter": 98,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господь творит праведность"
  },
  {
    "chapter": 98,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и правосудие для всех"
  },
  {
    "chapter": 98,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "угнетенных."
  },
  {
    "chapter": 98,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он ознакомил"
  },
  {
    "chapter": 98,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Моисея с путями Своими,"
  },
  {
    "chapter": 98,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "сыновей Израиля"
  },
  {
    "chapter": 98,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "с делами Своими."
  },
  {
    "chapter": 98,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Милостив и"
  },
  {
    "chapter": 98,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "милосерден Господь:"
  },
  {
    "chapter": 98,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "долготерпелив и"
  },
  {
    "chapter": 98,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "богат милостью."
  },
  {
    "chapter": 98,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он не беспрестанно"
  },
  {
    "chapter": 98,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "сопротивляется"
  },
  {
    "chapter": 98,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и не вечно держит в Себе"
  },
  {
    "chapter": 98,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "гнев."
  },
  {
    "chapter": 98,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Не в соответствии с нашими"
  },
  {
    "chapter": 98,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "грехами поступил Он с нами"
  },
  {
    "chapter": 98,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и не по нашим преступлениям"
  },
  {
    "chapter": 98,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "воздал нам."
  },
  {
    "chapter": 98,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Как небо высоко над землею,"
  },
  {
    "chapter": 98,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "так велика Его"
  },
  {
    "chapter": 98,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "милость к боящимся Его."
  },
  {
    "chapter": 98,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Как далек восток от запада,"
  },
  {
    "chapter": 98,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "так удалил Он от"
  },
  {
    "chapter": 98,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "нас наши грехи."
  },
  {
    "chapter": 98,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Как отец"
  },
  {
    "chapter": 98,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "жалеет своих детей,"
  },
  {
    "chapter": 98,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "так Господь"
  },
  {
    "chapter": 98,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "жалеет боящихся Его,"
  },
  {
    "chapter": 98,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "ведь Он знает из чего мы,"
  },
  {
    "chapter": 98,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "помнит,"
  },
  {
    "chapter": 98,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "что мы прах."
  },
  {
    "chapter": 98,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Человек подобен траве,"
  },
  {
    "chapter": 98,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "дни его,"
  },
  {
    "chapter": 98,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "как растение"
  },
  {
    "chapter": 98,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "полевое так он цветет."
  },
  {
    "chapter": 98,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пройдет над ним ветер,"
  },
  {
    "chapter": 98,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и нет его,"
  },
  {
    "chapter": 98,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и никто не узнает место,"
  },
  {
    "chapter": 98,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "где он был."
  },
  {
    "chapter": 98,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Милость же Господа извечна"
  },
  {
    "chapter": 98,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и навсегда над боящимися"
  },
  {
    "chapter": 98,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Его,"
  },
  {
    "chapter": 98,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "и Его праведность к"
  },
  {
    "chapter": 98,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "сыновьям сыновей их,"
  },
  {
    "chapter": 98,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "к берегущим Его завет и к"
  },
  {
    "chapter": 98,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "помнящим Его наставления,"
  },
  {
    "chapter": 98,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "чтобы исполнять их."
  },
  {
    "chapter": 98,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господь в небесах"
  },
  {
    "chapter": 98,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "поставил Свой престол,"
  },
  {
    "chapter": 98,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и царская власть Его"
  },
  {
    "chapter": 98,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "господствует над всем."
  },
  {
    "chapter": 98,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Прославьте Господа,"
  },
  {
    "chapter": 98,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "ангелы Его,"
  },
  {
    "chapter": 98,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "великие силой,"
  },
  {
    "chapter": 98,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "исполняющие Его повеления,"
  },
  {
    "chapter": 98,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "и повинующиеся Его слову."
  },
  {
    "chapter": 98,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Прославьте Господа,"
  },
  {
    "chapter": 98,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "все Его воинства,"
  },
  {
    "chapter": 98,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Его служители,"
  },
  {
    "chapter": 98,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "исполняющие Его волю."
  },
  {
    "chapter": 98,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Прославьте Господа,"
  },
  {
    "chapter": 98,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "все дела Его во всех"
  },
  {
    "chapter": 98,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "местах Его правления."
  },
  {
    "chapter": 98,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Прославь,"
  },
  {
    "chapter": 98,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "душа моя,"
  },
  {
    "chapter": 98,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Господа."
  }
]