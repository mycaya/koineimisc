[
  {
    "chapter": 32,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Псалом Давида,"
  },
  {
    "chapter": 32,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "относящийся к тому времени,"
  },
  {
    "chapter": 32,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "когда он притворялся"
  },
  {
    "chapter": 32,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "безумным перед Авимелехом,"
  },
  {
    "chapter": 32,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "был изгнан от"
  },
  {
    "chapter": 32,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "него и удалился."
  },
  {
    "chapter": 32,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Восславлю Господа"
  },
  {
    "chapter": 32,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "во всякое время,"
  },
  {
    "chapter": 32,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "хвала Ему"
  },
  {
    "chapter": 32,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "всегда на устах моих."
  },
  {
    "chapter": 32,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Душа моя будет"
  },
  {
    "chapter": 32,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "хвалиться Господом;"
  },
  {
    "chapter": 32,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "пусть услышат"
  },
  {
    "chapter": 32,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "кроткие и возвеселятся."
  },
  {
    "chapter": 32,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Славьте со мною Господа;"
  },
  {
    "chapter": 32,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "превознесем Его Имя вместе!"
  },
  {
    "chapter": 32,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я искал Господа,"
  },
  {
    "chapter": 32,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и Он мне ответил и от всех"
  },
  {
    "chapter": 32,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "моих страхов меня избавил."
  },
  {
    "chapter": 32,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Кто обращал к Нему взор,"
  },
  {
    "chapter": 32,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "сияет от радости,"
  },
  {
    "chapter": 32,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "лица их не покроет стыд."
  },
  {
    "chapter": 32,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Этот бедняк воззвал,"
  },
  {
    "chapter": 32,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и Господь услышал его,"
  },
  {
    "chapter": 32,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и от всех"
  },
  {
    "chapter": 32,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "напастей его избавил."
  },
  {
    "chapter": 32,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ангел Господень"
  },
  {
    "chapter": 32,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "ополчается вокруг тех,"
  },
  {
    "chapter": 32,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "кто боится Господа,"
  },
  {
    "chapter": 32,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и избавляет их."
  },
  {
    "chapter": 32,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Вкусите и увидите,"
  },
  {
    "chapter": 32,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "как благ Господь!"
  },
  {
    "chapter": 32,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Благословен тот,"
  },
  {
    "chapter": 32,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "кто ищет прибежища у Него."
  },
  {
    "chapter": 32,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Бойтесь Господа,"
  },
  {
    "chapter": 32,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "святые Его,"
  },
  {
    "chapter": 32,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "ведь кто боится Его,"
  },
  {
    "chapter": 32,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "ни в чем не нуждается."
  },
  {
    "chapter": 32,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Молодые львы"
  },
  {
    "chapter": 32,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "бедствуют и голодают,"
  },
  {
    "chapter": 32,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "а ищущие Господа не"
  },
  {
    "chapter": 32,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "имеют нужды ни в каком благе."
  },
  {
    "chapter": 32,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Придите,"
  },
  {
    "chapter": 32,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "дети,"
  },
  {
    "chapter": 32,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "послушайте меня,"
  },
  {
    "chapter": 32,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "я научу вас"
  },
  {
    "chapter": 32,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "страху Господню."
  },
  {
    "chapter": 32,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Кто любит жизнь и"
  },
  {
    "chapter": 32,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "желает видеть добрые дни?"
  },
  {
    "chapter": 32,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Удерживай свой язык от зла"
  },
  {
    "chapter": 32,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и свои уста от коварных"
  },
  {
    "chapter": 32,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "речей."
  },
  {
    "chapter": 32,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Удаляйся от зла"
  },
  {
    "chapter": 32,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и твори добро;"
  },
  {
    "chapter": 32,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "ищи мира и стремись к нему."
  },
  {
    "chapter": 32,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Глаза Господни"
  },
  {
    "chapter": 32,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "на праведных,"
  },
  {
    "chapter": 32,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и уши Его"
  },
  {
    "chapter": 32,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "открыты для их молитвы,"
  },
  {
    "chapter": 32,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "но лицо"
  },
  {
    "chapter": 32,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господне против тех,"
  },
  {
    "chapter": 32,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "кто делает зло,"
  },
  {
    "chapter": 32,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "чтобы память о них"
  },
  {
    "chapter": 32,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "на земле истребить."
  },
  {
    "chapter": 32,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Взывают праведные,"
  },
  {
    "chapter": 32,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и Господь их слышит,"
  },
  {
    "chapter": 32,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и от всех"
  },
  {
    "chapter": 32,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "скорбей их избавляет."
  },
  {
    "chapter": 32,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Близок Господь к"
  },
  {
    "chapter": 32,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "сокрушенным сердцем"
  },
  {
    "chapter": 32,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и спасает павшего духом."
  },
  {
    "chapter": 32,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Много скорбей у праведного,"
  },
  {
    "chapter": 32,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "но от всех их"
  },
  {
    "chapter": 32,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "избавляет его Господь."
  },
  {
    "chapter": 32,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он все кости его хранит,"
  },
  {
    "chapter": 32,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "ни одна из них не"
  },
  {
    "chapter": 32,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "будет переломлена."
  },
  {
    "chapter": 32,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Погубит грешника зло;"
  },
  {
    "chapter": 32,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "ненавидящие"
  },
  {
    "chapter": 32,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "праведного будут осуждены."
  },
  {
    "chapter": 32,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господь спасает"
  },
  {
    "chapter": 32,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "жизнь Своих слуг;"
  },
  {
    "chapter": 32,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и никто,"
  },
  {
    "chapter": 32,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "который у Него"
  },
  {
    "chapter": 32,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "ищет прибежища,"
  },
  {
    "chapter": 32,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "не будет осужден."
  }
]