[
  {
    "chapter": 50,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Дирижеру хора."
  },
  {
    "chapter": 50,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Под махалат."
  },
  {
    "chapter": 50,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Наставление Давида."
  },
  {
    "chapter": 50,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Говорит безумец"
  },
  {
    "chapter": 50,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "в сердце своем:"
  },
  {
    "chapter": 50,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Нет Бога."
  },
  {
    "chapter": 50,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Они развратились,"
  },
  {
    "chapter": 50,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "гнусны их дела;"
  },
  {
    "chapter": 50,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "нет ни одного,"
  },
  {
    "chapter": 50,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "кто делал бы добро."
  },
  {
    "chapter": 50,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Бог взирает с небес"
  },
  {
    "chapter": 50,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "на сынов человеческих,"
  },
  {
    "chapter": 50,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "чтобы увидеть,"
  },
  {
    "chapter": 50,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "есть ли разумный,"
  },
  {
    "chapter": 50,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "ищущий Бога."
  },
  {
    "chapter": 50,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Все отреклись,"
  },
  {
    "chapter": 50,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "все,"
  },
  {
    "chapter": 50,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "как один,"
  },
  {
    "chapter": 50,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "развратились,"
  },
  {
    "chapter": 50,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "нет ни одного,"
  },
  {
    "chapter": 50,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "кто делал бы добро,"
  },
  {
    "chapter": 50,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "нет ни одного."
  },
  {
    "chapter": 50,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Неужели не"
  },
  {
    "chapter": 50,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "вразумятся делающие зло те,"
  },
  {
    "chapter": 50,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "кто поедает мой народ,"
  },
  {
    "chapter": 50,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "как хлеб,"
  },
  {
    "chapter": 50,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "и Бога не призывает?"
  },
  {
    "chapter": 50,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Там их охватит страх,"
  },
  {
    "chapter": 50,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "где нечего бояться."
  },
  {
    "chapter": 50,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Бог разметает кости"
  },
  {
    "chapter": 50,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "ополчающихся на тебя;"
  },
  {
    "chapter": 50,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "их покроет позор,"
  },
  {
    "chapter": 50,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "потому что отверг их Бог."
  },
  {
    "chapter": 50,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "О,"
  },
  {
    "chapter": 50,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "кто дал бы с Сиона спасение"
  },
  {
    "chapter": 50,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Израилю! Когда Бог"
  },
  {
    "chapter": 50,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "восстановит Свой народ,"
  },
  {
    "chapter": 50,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "пусть веселится"
  },
  {
    "chapter": 50,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Иаков и радуется Израиль!"
  }
]