[
  {
    "chapter": 2,
    "verse": 1,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "Горе замышляющим"
  },
  {
    "chapter": 2,
    "verse": 1,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "преступление"
  },
  {
    "chapter": 2,
    "verse": 1,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "и придумывающим на ложах"
  },
  {
    "chapter": 2,
    "verse": 1,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "злые дела! Забрезжит"
  },
  {
    "chapter": 2,
    "verse": 1,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 5,
    "text": "рассвет они их исполнят,"
  },
  {
    "chapter": 2,
    "verse": 1,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 6,
    "text": "потому что это в их силах."
  },
  {
    "chapter": 2,
    "verse": 2,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "Захотят чужие"
  },
  {
    "chapter": 2,
    "verse": 2,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "поля захватят,"
  },
  {
    "chapter": 2,
    "verse": 2,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "захотят дома отберут;"
  },
  {
    "chapter": 2,
    "verse": 2,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "обманом выживут владельца"
  },
  {
    "chapter": 2,
    "verse": 2,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 5,
    "text": "из дома и оберут наследника."
  },
  {
    "chapter": 2,
    "verse": 3,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "Поэтому так"
  },
  {
    "chapter": 2,
    "verse": 3,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "говорит Господь:"
  },
  {
    "chapter": 2,
    "verse": 3,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "Я помышляю"
  },
  {
    "chapter": 2,
    "verse": 3,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "навести на людей беду,"
  },
  {
    "chapter": 2,
    "verse": 3,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 5,
    "text": "от которой вам не спастись."
  },
  {
    "chapter": 2,
    "verse": 3,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 6,
    "text": "Не ходить вам больше гордо,"
  },
  {
    "chapter": 2,
    "verse": 3,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 7,
    "text": "так как настает"
  },
  {
    "chapter": 2,
    "verse": 3,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 8,
    "text": "время бедствий."
  },
  {
    "chapter": 2,
    "verse": 4,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "В тот день вас"
  },
  {
    "chapter": 2,
    "verse": 4,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "будут высмеивать,"
  },
  {
    "chapter": 2,
    "verse": 4,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "передразнивать"
  },
  {
    "chapter": 2,
    "verse": 4,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "словами плачевной песни:"
  },
  {
    "chapter": 2,
    "verse": 4,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 5,
    "text": "Мы совершенно разорены;"
  },
  {
    "chapter": 2,
    "verse": 4,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 6,
    "text": "Господь отобрал"
  },
  {
    "chapter": 2,
    "verse": 4,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 7,
    "text": "надел моего народа."
  },
  {
    "chapter": 2,
    "verse": 4,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 8,
    "text": "Как Он отнял его у меня! Он"
  },
  {
    "chapter": 2,
    "verse": 4,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 9,
    "text": "поля наши делит среди"
  },
  {
    "chapter": 2,
    "verse": 4,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 10,
    "text": "завоевателей."
  },
  {
    "chapter": 2,
    "verse": 5,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "Поэтому,"
  },
  {
    "chapter": 2,
    "verse": 5,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "когда будут"
  },
  {
    "chapter": 2,
    "verse": 5,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "вновь делить землю,"
  },
  {
    "chapter": 2,
    "verse": 5,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "в собрании"
  },
  {
    "chapter": 2,
    "verse": 5,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 5,
    "text": "Господнем не будет никого,"
  },
  {
    "chapter": 2,
    "verse": 5,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 6,
    "text": "кто отмерил бы ваш надел,"
  },
  {
    "chapter": 2,
    "verse": 5,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 7,
    "text": "бросив жребий."
  },
  {
    "chapter": 2,
    "verse": 6,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "- Не пророчествуйте,"
  },
  {
    "chapter": 2,
    "verse": 6,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "так говорят лжепророки,"
  },
  {
    "chapter": 2,
    "verse": 6,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "об этом нельзя"
  },
  {
    "chapter": 2,
    "verse": 6,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "пророчествовать;"
  },
  {
    "chapter": 2,
    "verse": 6,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 5,
    "text": "нас не постигнет бесчестье."
  },
  {
    "chapter": 2,
    "verse": 7,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "Разве можно так говорить,"
  },
  {
    "chapter": 2,
    "verse": 7,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "дом Иакова? Разве терпение"
  },
  {
    "chapter": 2,
    "verse": 7,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "Господа"
  },
  {
    "chapter": 2,
    "verse": 7,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "истощилось? Разве это Его"
  },
  {
    "chapter": 2,
    "verse": 7,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 5,
    "text": "дела? Разве слова Мои не во"
  },
  {
    "chapter": 2,
    "verse": 7,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 6,
    "text": "благо тому,"
  },
  {
    "chapter": 2,
    "verse": 7,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 7,
    "text": "чей путь безупречен?"
  },
  {
    "chapter": 2,
    "verse": 8,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "Уже давно Мой"
  },
  {
    "chapter": 2,
    "verse": 8,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "народ восстал,"
  },
  {
    "chapter": 2,
    "verse": 8,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "как враг;"
  },
  {
    "chapter": 2,
    "verse": 8,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "вы срываете"
  },
  {
    "chapter": 2,
    "verse": 8,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 5,
    "text": "одежду с мирных людей,"
  },
  {
    "chapter": 2,
    "verse": 8,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 6,
    "text": "с тех,"
  },
  {
    "chapter": 2,
    "verse": 8,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 7,
    "text": "кто спокойно проходит мимо,"
  },
  {
    "chapter": 2,
    "verse": 8,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 8,
    "text": "о вражде и не помышляя."
  },
  {
    "chapter": 2,
    "verse": 9,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "Женщин Моего народа вы"
  },
  {
    "chapter": 2,
    "verse": 9,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "выгоняете из их уютных домов,"
  },
  {
    "chapter": 2,
    "verse": 9,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "а детей их вы"
  },
  {
    "chapter": 2,
    "verse": 9,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "лишаете Моей славы навеки."
  },
  {
    "chapter": 2,
    "verse": 10,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "Вставайте и уходите:"
  },
  {
    "chapter": 2,
    "verse": 10,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "это больше не место покоя."
  },
  {
    "chapter": 2,
    "verse": 10,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "Из-за нечистоты"
  },
  {
    "chapter": 2,
    "verse": 10,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "оно будет разрушено,"
  },
  {
    "chapter": 2,
    "verse": 10,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 5,
    "text": "и разрушение будет ужасным."
  },
  {
    "chapter": 2,
    "verse": 11,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "Если лжец и"
  },
  {
    "chapter": 2,
    "verse": 11,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "мошенник придет и скажет:"
  },
  {
    "chapter": 2,
    "verse": 11,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "Я буду тебе"
  },
  {
    "chapter": 2,
    "verse": 11,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "пророчествовать о вине,"
  },
  {
    "chapter": 2,
    "verse": 11,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 5,
    "text": "о хмельном питье,"
  },
  {
    "chapter": 2,
    "verse": 11,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 6,
    "text": "то он и будет достойным"
  },
  {
    "chapter": 2,
    "verse": 11,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 7,
    "text": "проповедником этому народу!"
  },
  {
    "chapter": 2,
    "verse": 12,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "- Я непременно"
  },
  {
    "chapter": 2,
    "verse": 12,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "соберу всего тебя,"
  },
  {
    "chapter": 2,
    "verse": 12,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "Иаков;"
  },
  {
    "chapter": 2,
    "verse": 12,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "соберу воедино"
  },
  {
    "chapter": 2,
    "verse": 12,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 5,
    "text": "уцелевших у Израиля;"
  },
  {
    "chapter": 2,
    "verse": 12,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 6,
    "text": "соберу их вместе,"
  },
  {
    "chapter": 2,
    "verse": 12,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 7,
    "text": "словно овец в загоне,"
  },
  {
    "chapter": 2,
    "verse": 12,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 8,
    "text": "как отару на пастбище."
  },
  {
    "chapter": 2,
    "verse": 12,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 9,
    "text": "зашумит оно от"
  },
  {
    "chapter": 2,
    "verse": 12,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 10,
    "text": "многолюдства."
  },
  {
    "chapter": 2,
    "verse": 13,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "Перед ними пойдет"
  },
  {
    "chapter": 2,
    "verse": 13,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "пробивающий путь;"
  },
  {
    "chapter": 2,
    "verse": 13,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "они прорвутся"
  },
  {
    "chapter": 2,
    "verse": 13,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "через ворота и выйдут."
  },
  {
    "chapter": 2,
    "verse": 13,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 5,
    "text": "Царь их пойдет перед ними,"
  },
  {
    "chapter": 2,
    "verse": 13,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 6,
    "text": "Господь будет во главе их."
  }
]