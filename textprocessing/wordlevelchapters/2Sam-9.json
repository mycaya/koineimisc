[
  {
    "chapter": 9,
    "verse": 1,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Давид спросил:"
  },
  {
    "chapter": 9,
    "verse": 1,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "Остался ли из дома"
  },
  {
    "chapter": 9,
    "verse": 1,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "Саула кто-нибудь,"
  },
  {
    "chapter": 9,
    "verse": 1,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "кому я мог бы оказать"
  },
  {
    "chapter": 9,
    "verse": 1,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "милость ради Ионафана?"
  },
  {
    "chapter": 9,
    "verse": 2,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "В доме Саула был"
  },
  {
    "chapter": 9,
    "verse": 2,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "слуга по имени Сива."
  },
  {
    "chapter": 9,
    "verse": 2,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "Его позвали к Давиду,"
  },
  {
    "chapter": 9,
    "verse": 2,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "и царь спросил его:"
  },
  {
    "chapter": 9,
    "verse": 2,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "Ты Сива? Рад служить тебе,"
  },
  {
    "chapter": 9,
    "verse": 2,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 6,
    "text": "ответил тот."
  },
  {
    "chapter": 9,
    "verse": 3,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Царь спросил:"
  },
  {
    "chapter": 9,
    "verse": 3,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "Есть ли кто еще"
  },
  {
    "chapter": 9,
    "verse": 3,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "живой в доме Саула,"
  },
  {
    "chapter": 9,
    "verse": 3,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "кому я мог бы оказать Божью"
  },
  {
    "chapter": 9,
    "verse": 3,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "милость? Сива ответил царю:"
  },
  {
    "chapter": 9,
    "verse": 3,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 6,
    "text": "Остался еще сын Ионафана."
  },
  {
    "chapter": 9,
    "verse": 3,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 7,
    "text": "Он хромой на обе ноги."
  },
  {
    "chapter": 9,
    "verse": 4,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "- Где же он? спросил царь."
  },
  {
    "chapter": 9,
    "verse": 4,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "Сива ответил:"
  },
  {
    "chapter": 9,
    "verse": 4,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "Он в доме Махира,"
  },
  {
    "chapter": 9,
    "verse": 4,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "сына Аммиэля,"
  },
  {
    "chapter": 9,
    "verse": 4,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "в Ло-Деваре."
  },
  {
    "chapter": 9,
    "verse": 5,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Царь Давид послал"
  },
  {
    "chapter": 9,
    "verse": 5,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "забрать его из Ло-Девара,"
  },
  {
    "chapter": 9,
    "verse": 5,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "из дома Махира,"
  },
  {
    "chapter": 9,
    "verse": 5,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "сына Аммиэля."
  },
  {
    "chapter": 9,
    "verse": 6,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Когда Мемфивосфей,"
  },
  {
    "chapter": 9,
    "verse": 6,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "сын Ионафана,"
  },
  {
    "chapter": 9,
    "verse": 6,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "внук Саула,"
  },
  {
    "chapter": 9,
    "verse": 6,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "пришел к Давиду,"
  },
  {
    "chapter": 9,
    "verse": 6,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "он поклонился ему"
  },
  {
    "chapter": 9,
    "verse": 6,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 6,
    "text": "в знак почтения."
  },
  {
    "chapter": 9,
    "verse": 6,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 7,
    "text": "Давид сказал:"
  },
  {
    "chapter": 9,
    "verse": 6,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 8,
    "text": "Мемфивосфей! Я твой слуга,"
  },
  {
    "chapter": 9,
    "verse": 6,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 9,
    "text": "ответил тот."
  },
  {
    "chapter": 9,
    "verse": 7,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "- Не бойся,"
  },
  {
    "chapter": 9,
    "verse": 7,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "сказал ему Давид,"
  },
  {
    "chapter": 9,
    "verse": 7,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "потому что я непременно"
  },
  {
    "chapter": 9,
    "verse": 7,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "окажу тебе милость ради"
  },
  {
    "chapter": 9,
    "verse": 7,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "твоего отца Ионафана."
  },
  {
    "chapter": 9,
    "verse": 7,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 6,
    "text": "Я верну тебе всю землю,"
  },
  {
    "chapter": 9,
    "verse": 7,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 7,
    "text": "которая"
  },
  {
    "chapter": 9,
    "verse": 7,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 8,
    "text": "принадлежала твоему деду Саулу,"
  },
  {
    "chapter": 9,
    "verse": 7,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 9,
    "text": "и ты всегда будешь"
  },
  {
    "chapter": 9,
    "verse": 7,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 10,
    "text": "есть за моим столом."
  },
  {
    "chapter": 9,
    "verse": 8,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Мемфивосфей"
  },
  {
    "chapter": 9,
    "verse": 8,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "поклонился и сказал:"
  },
  {
    "chapter": 9,
    "verse": 8,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "Кто такой твой слуга,"
  },
  {
    "chapter": 9,
    "verse": 8,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "чтобы тебе обращать"
  },
  {
    "chapter": 9,
    "verse": 8,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "внимание на такого мертвого пса,"
  },
  {
    "chapter": 9,
    "verse": 8,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 6,
    "text": "как я?"
  },
  {
    "chapter": 9,
    "verse": 9,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Царь призвал слугу"
  },
  {
    "chapter": 9,
    "verse": 9,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "Саула Сиву и сказал ему:"
  },
  {
    "chapter": 9,
    "verse": 9,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "Я отдал внуку"
  },
  {
    "chapter": 9,
    "verse": 9,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "твоего господина все,"
  },
  {
    "chapter": 9,
    "verse": 9,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "что принадлежало"
  },
  {
    "chapter": 9,
    "verse": 9,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 6,
    "text": "Саулу и его семье."
  },
  {
    "chapter": 9,
    "verse": 10,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Вместе со своими сыновьями"
  },
  {
    "chapter": 9,
    "verse": 10,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "и слугами ты должен"
  },
  {
    "chapter": 9,
    "verse": 10,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "обрабатывать для него землю"
  },
  {
    "chapter": 9,
    "verse": 10,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "и собирать урожай,"
  },
  {
    "chapter": 9,
    "verse": 10,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "чтобы у внука твоего"
  },
  {
    "chapter": 9,
    "verse": 10,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 6,
    "text": "господина была еда."
  },
  {
    "chapter": 9,
    "verse": 10,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 7,
    "text": "А Мемфивосфей,"
  },
  {
    "chapter": 9,
    "verse": 10,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 8,
    "text": "внук твоего господина,"
  },
  {
    "chapter": 9,
    "verse": 10,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 9,
    "text": "всегда будет"
  },
  {
    "chapter": 9,
    "verse": 10,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 10,
    "text": "есть за моим столом."
  },
  {
    "chapter": 9,
    "verse": 10,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 11,
    "text": "(У Сивы же было"
  },
  {
    "chapter": 9,
    "verse": 10,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 12,
    "text": "пятнадцать сыновей и двадцать слуг."
  },
  {
    "chapter": 9,
    "verse": 10,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 13,
    "text": ")"
  },
  {
    "chapter": 9,
    "verse": 11,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Сива сказал царю:"
  },
  {
    "chapter": 9,
    "verse": 11,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "Твой слуга исполнит все,"
  },
  {
    "chapter": 9,
    "verse": 11,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "что господин мой царь"
  },
  {
    "chapter": 9,
    "verse": 11,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "приказывает сделать своему"
  },
  {
    "chapter": 9,
    "verse": 11,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "слуге."
  },
  {
    "chapter": 9,
    "verse": 11,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 6,
    "text": "И Мемфивосфей ел"
  },
  {
    "chapter": 9,
    "verse": 11,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 7,
    "text": "за столом Давида,"
  },
  {
    "chapter": 9,
    "verse": 11,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 8,
    "text": "как один из сыновей царя."
  },
  {
    "chapter": 9,
    "verse": 12,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "У Мемфивосфея был"
  },
  {
    "chapter": 9,
    "verse": 12,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "маленький сын по имени Миха."
  },
  {
    "chapter": 9,
    "verse": 12,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "Все домашние Сивы"
  },
  {
    "chapter": 9,
    "verse": 12,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "были слугами Мемфивосфея."
  },
  {
    "chapter": 9,
    "verse": 13,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Мемфивосфей"
  },
  {
    "chapter": 9,
    "verse": 13,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "жил в Иерусалиме,"
  },
  {
    "chapter": 9,
    "verse": 13,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "потому что он всегда"
  },
  {
    "chapter": 9,
    "verse": 13,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "ел за царским столом."
  },
  {
    "chapter": 9,
    "verse": 13,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "Он был хромой на обе ноги."
  }
]