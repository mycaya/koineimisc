[
  {
    "chapter": 2,
    "verse": 2,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 1,
    "text": "Находясь в желудке рыбы,"
  },
  {
    "chapter": 2,
    "verse": 2,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 2,
    "text": "Иона помолился Господу,"
  },
  {
    "chapter": 2,
    "verse": 2,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 3,
    "text": "своему Богу."
  },
  {
    "chapter": 2,
    "verse": 3,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 1,
    "text": "Он сказал:"
  },
  {
    "chapter": 2,
    "verse": 3,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 2,
    "text": "В горе я к Господу"
  },
  {
    "chapter": 2,
    "verse": 3,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 3,
    "text": "воззвал Он ответил мне."
  },
  {
    "chapter": 2,
    "verse": 3,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 4,
    "text": "Я закричал Ты услышал мой"
  },
  {
    "chapter": 2,
    "verse": 3,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 5,
    "text": "крик из глубин мира мертвых."
  },
  {
    "chapter": 2,
    "verse": 4,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 1,
    "text": "Ты в пучину меня вверг,"
  },
  {
    "chapter": 2,
    "verse": 4,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 2,
    "text": "в самое сердце морей;"
  },
  {
    "chapter": 2,
    "verse": 4,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 3,
    "text": "окружили меня потоки."
  },
  {
    "chapter": 2,
    "verse": 4,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 4,
    "text": "Все Твои волны,"
  },
  {
    "chapter": 2,
    "verse": 4,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 5,
    "text": "все Твои валы"
  },
  {
    "chapter": 2,
    "verse": 4,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 6,
    "text": "надо мной прошли."
  },
  {
    "chapter": 2,
    "verse": 5,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 1,
    "text": "Я сказал:"
  },
  {
    "chapter": 2,
    "verse": 5,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 2,
    "text": "Изгнан я от глаз Твоих;"
  },
  {
    "chapter": 2,
    "verse": 5,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 3,
    "text": "однако я вновь"
  },
  {
    "chapter": 2,
    "verse": 5,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 4,
    "text": "увижу святой храм Твой."
  },
  {
    "chapter": 2,
    "verse": 6,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 1,
    "text": "Воды надо мной сомкнулись,"
  },
  {
    "chapter": 2,
    "verse": 6,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 2,
    "text": "бездна меня обступила,"
  },
  {
    "chapter": 2,
    "verse": 6,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 3,
    "text": "и водоросли голову оплели."
  },
  {
    "chapter": 2,
    "verse": 7,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 1,
    "text": "На дно к самым"
  },
  {
    "chapter": 2,
    "verse": 7,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 2,
    "text": "корням гор я нисшел,"
  },
  {
    "chapter": 2,
    "verse": 7,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 3,
    "text": "засовы земли"
  },
  {
    "chapter": 2,
    "verse": 7,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 4,
    "text": "закрылись за мной навек,"
  },
  {
    "chapter": 2,
    "verse": 7,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 5,
    "text": "но Ты,"
  },
  {
    "chapter": 2,
    "verse": 7,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 6,
    "text": "Господи,"
  },
  {
    "chapter": 2,
    "verse": 7,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 7,
    "text": "Бог мой,"
  },
  {
    "chapter": 2,
    "verse": 7,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 8,
    "text": "извлек меня из ямы живым!"
  },
  {
    "chapter": 2,
    "verse": 8,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 1,
    "text": "Когда жизнь угасала во мне,"
  },
  {
    "chapter": 2,
    "verse": 8,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 2,
    "text": "я Господа вспомнил,"
  },
  {
    "chapter": 2,
    "verse": 8,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 3,
    "text": "и дошла молитва моя к"
  },
  {
    "chapter": 2,
    "verse": 8,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 4,
    "text": "Тебе в святой храм Твой."
  },
  {
    "chapter": 2,
    "verse": 9,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 1,
    "text": "Те,"
  },
  {
    "chapter": 2,
    "verse": 9,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 2,
    "text": "кто ничтожных идолов чтит,"
  },
  {
    "chapter": 2,
    "verse": 9,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 3,
    "text": "лишились милости,"
  },
  {
    "chapter": 2,
    "verse": 9,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 4,
    "text": "что могли получить."
  },
  {
    "chapter": 2,
    "verse": 10,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 1,
    "text": "А я с хвалебными песнями"
  },
  {
    "chapter": 2,
    "verse": 10,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 2,
    "text": "буду жертвы Тебе приносить."
  },
  {
    "chapter": 2,
    "verse": 10,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 3,
    "text": "Что обещал исполню."
  },
  {
    "chapter": 2,
    "verse": 10,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 4,
    "text": "Спасение от Господа!"
  },
  {
    "chapter": 2,
    "verse": 11,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 1,
    "text": "Господь повелел рыбе,"
  },
  {
    "chapter": 2,
    "verse": 11,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 2,
    "text": "и она"
  },
  {
    "chapter": 2,
    "verse": 11,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 3,
    "text": "изрыгнула Иону на сушу."
  }
]