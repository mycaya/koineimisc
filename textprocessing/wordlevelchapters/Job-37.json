[
  {
    "chapter": 37,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "От этого сердце мое"
  },
  {
    "chapter": 37,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "трепещет"
  },
  {
    "chapter": 37,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и бросается с места прочь."
  },
  {
    "chapter": 37,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Слышите! Слышите"
  },
  {
    "chapter": 37,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "гром Его голоса,"
  },
  {
    "chapter": 37,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "прокатился"
  },
  {
    "chapter": 37,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "грохот из уст Его."
  },
  {
    "chapter": 37,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "По всему"
  },
  {
    "chapter": 37,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "поднебесью посылает Он его,"
  },
  {
    "chapter": 37,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "посылает молнию"
  },
  {
    "chapter": 37,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "до краев земли."
  },
  {
    "chapter": 37,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Голос Его рокочет вослед,"
  },
  {
    "chapter": 37,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "гласом величия Он гремит"
  },
  {
    "chapter": 37,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и не сдерживает зарниц"
  },
  {
    "chapter": 37,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "когда голос Его услышан."
  },
  {
    "chapter": 37,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Дивно грохочет"
  },
  {
    "chapter": 37,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Бог Своим голосом,"
  },
  {
    "chapter": 37,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Он творит"
  },
  {
    "chapter": 37,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "великое не постичь нам."
  },
  {
    "chapter": 37,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Говорит Он снегу:"
  },
  {
    "chapter": 37,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Пади на землю,"
  },
  {
    "chapter": 37,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "повелевает дождям и ливням."
  },
  {
    "chapter": 37,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Он прекращает"
  },
  {
    "chapter": 37,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "труды каждого человека,"
  },
  {
    "chapter": 37,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "чтобы все люди"
  },
  {
    "chapter": 37,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "знали дело Его."
  },
  {
    "chapter": 37,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "И уходят звери в свои"
  },
  {
    "chapter": 37,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "убежища"
  },
  {
    "chapter": 37,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и в берлогах своих лежат."
  },
  {
    "chapter": 37,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Из Его покоев"
  },
  {
    "chapter": 37,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "приходит вихрь,"
  },
  {
    "chapter": 37,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и северный ветер"
  },
  {
    "chapter": 37,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "приносит стужу."
  },
  {
    "chapter": 37,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Дыханием Божьим творится"
  },
  {
    "chapter": 37,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "лед и твердеют вольные воды."
  },
  {
    "chapter": 37,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Наполняет Он тучи влагой"
  },
  {
    "chapter": 37,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и молнии мечет из облаков."
  },
  {
    "chapter": 37,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Кружат они по воле Его"
  },
  {
    "chapter": 37,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "над лицом всей земли,"
  },
  {
    "chapter": 37,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "совершая,"
  },
  {
    "chapter": 37,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "что Он повелит."
  },
  {
    "chapter": 37,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "То приходят,"
  },
  {
    "chapter": 37,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "как бич,"
  },
  {
    "chapter": 37,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "покарать людей,"
  },
  {
    "chapter": 37,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "то,"
  },
  {
    "chapter": 37,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "даруя им милость,"
  },
  {
    "chapter": 37,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 6,
    "text": "насытить землю."
  },
  {
    "chapter": 37,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Выслушай это,"
  },
  {
    "chapter": 37,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Иов;"
  },
  {
    "chapter": 37,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "подумай,"
  },
  {
    "chapter": 37,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "как чудны Божьи дела."
  },
  {
    "chapter": 37,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Знаешь ли ты,"
  },
  {
    "chapter": 37,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "как управляет ими Бог,"
  },
  {
    "chapter": 37,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "как блестит"
  },
  {
    "chapter": 37,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "зарницей из облака Своего?"
  },
  {
    "chapter": 37,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Знаешь ли ты"
  },
  {
    "chapter": 37,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "равновесие облаков,"
  },
  {
    "chapter": 37,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "чудеса Того,"
  },
  {
    "chapter": 37,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "Кто познанием совершенен?"
  },
  {
    "chapter": 37,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "О ты,"
  },
  {
    "chapter": 37,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "кому жарко в своей одежде,"
  },
  {
    "chapter": 37,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "когда замолкнет"
  },
  {
    "chapter": 37,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "земля перед южным ветром,"
  },
  {
    "chapter": 37,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "ты ли с Ним"
  },
  {
    "chapter": 37,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "распростер небеса твердые,"
  },
  {
    "chapter": 37,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "словно литое зеркало?"
  },
  {
    "chapter": 37,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Научи нас,"
  },
  {
    "chapter": 37,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "что нам Ему сказать,"
  },
  {
    "chapter": 37,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "мы не в силах"
  },
  {
    "chapter": 37,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "найти слова из-за тьмы."
  },
  {
    "chapter": 37,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Сообщить ли Ему,"
  },
  {
    "chapter": 37,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "что хочу с Ним говорить?"
  },
  {
    "chapter": 37,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Но кто себе желает смерти?"
  },
  {
    "chapter": 37,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Никто не может"
  },
  {
    "chapter": 37,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "взглянуть на солнце,"
  },
  {
    "chapter": 37,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "когда оно"
  },
  {
    "chapter": 37,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "блестит в небесах,"
  },
  {
    "chapter": 37,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "после того,"
  },
  {
    "chapter": 37,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 6,
    "text": "как расчистит их ветер."
  },
  {
    "chapter": 37,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "С севера"
  },
  {
    "chapter": 37,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "движется блеск золотой;"
  },
  {
    "chapter": 37,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Бог приближается в"
  },
  {
    "chapter": 37,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "страшном великолепии."
  },
  {
    "chapter": 37,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Всемогущий мы не"
  },
  {
    "chapter": 37,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "можем Его постичь,"
  },
  {
    "chapter": 37,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "силой и правдой Он велик,"
  },
  {
    "chapter": 37,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "праведностью богат,"
  },
  {
    "chapter": 37,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "никого не угнетает."
  },
  {
    "chapter": 37,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Поэтому,"
  },
  {
    "chapter": 37,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "боятся Его люди,"
  },
  {
    "chapter": 37,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "разве не боятся Его"
  },
  {
    "chapter": 37,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "все мудрые сердцем?"
  }
]