  },
  {
    "chapter": 114,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "В своей скорби я"
  },
  {
    "chapter": 114,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "воззвал к Господу,"
  },
  {
    "chapter": 114,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и Он ответил мне."
  },
  {
    "chapter": 114,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я сказал:"
  },
  {
    "chapter": 114,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Избавь мою душу,"
  },
  {
    "chapter": 114,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Господи,"
  },
  {
    "chapter": 114,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "от лгущих уст и"
  },
  {
    "chapter": 114,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "обманчивого языка!"
  },
  {
    "chapter": 114,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Что Он даст тебе"
  },
  {
    "chapter": 114,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и что прибавит,"
  },
  {
    "chapter": 114,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "обманчивый язык?"
  },
  {
    "chapter": 114,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он накажет тебя острыми"
  },
  {
    "chapter": 114,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "стрелами воина"
  },
  {
    "chapter": 114,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и горящими углями дрока."
  },
  {
    "chapter": 114,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Горе мне,"
  },
  {
    "chapter": 114,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "что пребываю в Мешехе и"
  },
  {
    "chapter": 114,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "живу в кедарских шатрах!"
  },
  {
    "chapter": 114,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Душа моя долго жила"
  },
  {
    "chapter": 114,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "с ненавидящими мир."
  },
  {
    "chapter": 114,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Что до меня,"
  },
  {
    "chapter": 114,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "я за мир,"
  },
  {
    "chapter": 114,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "но только"
  },
  {
    "chapter": 114,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "заговорю они к войне."
  }
