[
  {
    "chapter": 42,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Тогда Иов ответил Господу:"
  },
  {
    "chapter": 42,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "- Я знаю:"
  },
  {
    "chapter": 42,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Ты можешь все,"
  },
  {
    "chapter": 42,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и невозможно"
  },
  {
    "chapter": 42,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "противиться Тебе."
  },
  {
    "chapter": 42,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Ты спросил:"
  },
  {
    "chapter": 42,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Кто в невежестве"
  },
  {
    "chapter": 42,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "замысел Мой омрачает? Да,"
  },
  {
    "chapter": 42,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "я говорил о том,"
  },
  {
    "chapter": 42,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "чего не понимал,"
  },
  {
    "chapter": 42,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 6,
    "text": "о делах для меня чудесных,"
  },
  {
    "chapter": 42,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 7,
    "text": "которых я не знал."
  },
  {
    "chapter": 42,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Ты сказал:"
  },
  {
    "chapter": 42,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Внимай Мне,"
  },
  {
    "chapter": 42,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и Я буду говорить;"
  },
  {
    "chapter": 42,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "Я буду спрашивать,"
  },
  {
    "chapter": 42,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "а ты отвечай."
  },
  {
    "chapter": 42,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Я только слышал о Тебе,"
  },
  {
    "chapter": 42,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "а теперь мои"
  },
  {
    "chapter": 42,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "глаза видят Тебя."
  },
  {
    "chapter": 42,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Поэтому я отступаю и"
  },
  {
    "chapter": 42,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "раскаиваюсь в прахе и пепле."
  },
  {
    "chapter": 42,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Сказав это Иову,"
  },
  {
    "chapter": 42,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Господь сказал"
  },
  {
    "chapter": 42,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Елифазу из Фемана:"
  },
  {
    "chapter": 42,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "Я разгневан на тебя и на"
  },
  {
    "chapter": 42,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "двух твоих друзей за то,"
  },
  {
    "chapter": 42,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 6,
    "text": "что вы говорили"
  },
  {
    "chapter": 42,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 7,
    "text": "обо Мне не так верно,"
  },
  {
    "chapter": 42,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 8,
    "text": "как Мой слуга Иов."
  },
  {
    "chapter": 42,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Поэтому возьмите семь"
  },
  {
    "chapter": 42,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "молодых быков и семь баранов,"
  },
  {
    "chapter": 42,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "пойдите к Моему слуге Иову"
  },
  {
    "chapter": 42,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "и принесите за себя жертву"
  },
  {
    "chapter": 42,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "всесожжения."
  },
  {
    "chapter": 42,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 6,
    "text": "Мой слуга Иов"
  },
  {
    "chapter": 42,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 7,
    "text": "помолится о вас,"
  },
  {
    "chapter": 42,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 8,
    "text": "и Я приму Его молитву"
  },
  {
    "chapter": 42,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 9,
    "text": "и не поступлю с вами по"
  },
  {
    "chapter": 42,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 10,
    "text": "вашей глупости."
  },
  {
    "chapter": 42,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 11,
    "text": "Вы говорили обо"
  },
  {
    "chapter": 42,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 12,
    "text": "Мне не так верно,"
  },
  {
    "chapter": 42,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 13,
    "text": "как Мой слуга Иов."
  },
  {
    "chapter": 42,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Елифаз из Фемана,"
  },
  {
    "chapter": 42,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Вилдад из Савхы и"
  },
  {
    "chapter": 42,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Софар из Наамы сделали,"
  },
  {
    "chapter": 42,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "как велел им Господь."
  },
  {
    "chapter": 42,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "И Господь"
  },
  {
    "chapter": 42,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 6,
    "text": "принял молитву Иова."
  },
  {
    "chapter": 42,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "После того как Иов"
  },
  {
    "chapter": 42,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "помолился за своих друзей,"
  },
  {
    "chapter": 42,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Господь вернул ему"
  },
  {
    "chapter": 42,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "благополучие"
  },
  {
    "chapter": 42,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "и дал ему вдвое больше"
  },
  {
    "chapter": 42,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 6,
    "text": "того,"
  },
  {
    "chapter": 42,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 7,
    "text": "что было у него прежде."
  },
  {
    "chapter": 42,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Все его братья"
  },
  {
    "chapter": 42,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и сестры и все,"
  },
  {
    "chapter": 42,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "кто знал его прежде,"
  },
  {
    "chapter": 42,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "пришли и ели с"
  },
  {
    "chapter": 42,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "ним у него в доме."
  },
  {
    "chapter": 42,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 6,
    "text": "Они жалели и утешали"
  },
  {
    "chapter": 42,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 7,
    "text": "его за все испытания,"
  },
  {
    "chapter": 42,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 8,
    "text": "которые послал ему Господь."
  },
  {
    "chapter": 42,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 9,
    "text": "Каждый"
  },
  {
    "chapter": 42,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 10,
    "text": "из них дал ему по"
  },
  {
    "chapter": 42,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 11,
    "text": "серебрянику"
  },
  {
    "chapter": 42,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 12,
    "text": "и по золотому кольцу."
  },
  {
    "chapter": 42,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Господь благословил"
  },
  {
    "chapter": 42,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "последующие дни Иова больше,"
  },
  {
    "chapter": 42,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "нежели прежние."
  },
  {
    "chapter": 42,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "У него теперь было"
  },
  {
    "chapter": 42,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "тринадцать тысяч овец,"
  },
  {
    "chapter": 42,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 6,
    "text": "шесть тысяч верблюдов,"
  },
  {
    "chapter": 42,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 7,
    "text": "тысяча пар волов"
  },
  {
    "chapter": 42,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 8,
    "text": "и тысяча ослиц."
  },
  {
    "chapter": 42,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Еще у него было семеро"
  },
  {
    "chapter": 42,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "сыновей и три дочери."
  },
  {
    "chapter": 42,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Первую он назвал Иемима,"
  },
  {
    "chapter": 42,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "вторую Кассия,"
  },
  {
    "chapter": 42,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "а третью Керенгаппух."
  },
  {
    "chapter": 42,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Нигде на земле не было"
  },
  {
    "chapter": 42,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "женщин столь прекрасных,"
  },
  {
    "chapter": 42,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "как дочери Иова."
  },
  {
    "chapter": 42,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "Их отец дал им"
  },
  {
    "chapter": 42,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "наследство наравне с братьями."
  },
  {
    "chapter": 42,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "После этого Иов"
  },
  {
    "chapter": 42,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "прожил еще сто сорок лет."
  },
  {
    "chapter": 42,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Он видел своих детей"
  },
  {
    "chapter": 42,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "и детей своих детей до"
  },
  {
    "chapter": 42,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "четвертого поколения."
  },
  {
    "chapter": 42,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "И умер он в старости,"
  },
  {
    "chapter": 42,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "насытившись жизнью."
  }
]