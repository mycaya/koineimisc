[
  {
    "chapter": 11,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "На рассвете того дня"
  },
  {
    "chapter": 11,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "погибнет царь Израиля."
  },
  {
    "chapter": 11,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "Когда Израиль был ребенком,"
  },
  {
    "chapter": 11,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "Я любил его,"
  },
  {
    "chapter": 11,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "и из Египта"
  },
  {
    "chapter": 11,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "призвал Я сына Моего."
  },
  {
    "chapter": 11,
    "verse": 2,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Но чем больше Я"
  },
  {
    "chapter": 11,
    "verse": 2,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "звал израильтян,"
  },
  {
    "chapter": 11,
    "verse": 2,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "тем дальше они"
  },
  {
    "chapter": 11,
    "verse": 2,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "уходили от Меня."
  },
  {
    "chapter": 11,
    "verse": 2,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "Они приносили жертвы"
  },
  {
    "chapter": 11,
    "verse": 2,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "Ваалам и благовония идолам."
  },
  {
    "chapter": 11,
    "verse": 3,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Это Я научил Ефрема ходить,"
  },
  {
    "chapter": 11,
    "verse": 3,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "держа его за руки,"
  },
  {
    "chapter": 11,
    "verse": 3,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "а он не осознавал,"
  },
  {
    "chapter": 11,
    "verse": 3,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "что Я был и Тем,"
  },
  {
    "chapter": 11,
    "verse": 3,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "Кто исцелял его."
  },
  {
    "chapter": 11,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Я вел их узами любви и"
  },
  {
    "chapter": 11,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "человеческой доброты."
  },
  {
    "chapter": 11,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "Я снял ярмо с их"
  },
  {
    "chapter": 11,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "шеи и склонился,"
  },
  {
    "chapter": 11,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "чтобы накормить их."
  },
  {
    "chapter": 11,
    "verse": 5,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Не вернутся ли они в Египет"
  },
  {
    "chapter": 11,
    "verse": 5,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "и не будет ли Ассирия"
  },
  {
    "chapter": 11,
    "verse": 5,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "править ими из-за того,"
  },
  {
    "chapter": 11,
    "verse": 5,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "что они"
  },
  {
    "chapter": 11,
    "verse": 5,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "отказались покаяться?"
  },
  {
    "chapter": 11,
    "verse": 6,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Мечи"
  },
  {
    "chapter": 11,
    "verse": 6,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "засверкают в их городах,"
  },
  {
    "chapter": 11,
    "verse": 6,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "разрушат затворы их ворот"
  },
  {
    "chapter": 11,
    "verse": 6,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "и положат конец их планам."
  },
  {
    "chapter": 11,
    "verse": 7,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Мой народ склонен к тому,"
  },
  {
    "chapter": 11,
    "verse": 7,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "чтобы"
  },
  {
    "chapter": 11,
    "verse": 7,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "отворачиваться от Меня."
  },
  {
    "chapter": 11,
    "verse": 7,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "Хотя пророки и"
  },
  {
    "chapter": 11,
    "verse": 7,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "призывают его к Всевышнему,"
  },
  {
    "chapter": 11,
    "verse": 7,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "никто не превозносит Его."
  },
  {
    "chapter": 11,
    "verse": 8,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Как Я могу оставить тебя,"
  },
  {
    "chapter": 11,
    "verse": 8,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "Ефрем? Как Я"
  },
  {
    "chapter": 11,
    "verse": 8,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "могу предать тебя,"
  },
  {
    "chapter": 11,
    "verse": 8,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "Израиль? Могу ли поступить"
  },
  {
    "chapter": 11,
    "verse": 8,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "с тобой как с Адмой? Могу"
  },
  {
    "chapter": 11,
    "verse": 8,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "ли Я сделать тебе то,"
  },
  {
    "chapter": 11,
    "verse": 8,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 7,
    "text": "что Цевоиму? Мое"
  },
  {
    "chapter": 11,
    "verse": 8,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 8,
    "text": "сердце изменилось,"
  },
  {
    "chapter": 11,
    "verse": 8,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 9,
    "text": "возросло Мое сострадание."
  },
  {
    "chapter": 11,
    "verse": 9,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Я не поступлю по"
  },
  {
    "chapter": 11,
    "verse": 9,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "ярости Своего гнева,"
  },
  {
    "chapter": 11,
    "verse": 9,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "не разрушу Ефрема снова."
  },
  {
    "chapter": 11,
    "verse": 9,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "Потому что Я Бог,"
  },
  {
    "chapter": 11,
    "verse": 9,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "а не человек,"
  },
  {
    "chapter": 11,
    "verse": 9,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "Святой среди тебя."
  },
  {
    "chapter": 11,
    "verse": 9,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 7,
    "text": "Я не приду в гневе."
  },
  {
    "chapter": 11,
    "verse": 10,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Они последуют за Мной,"
  },
  {
    "chapter": 11,
    "verse": 10,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "Я зарычу,"
  },
  {
    "chapter": 11,
    "verse": 10,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "как лев."
  },
  {
    "chapter": 11,
    "verse": 10,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "Когда Я зарычу,"
  },
  {
    "chapter": 11,
    "verse": 10,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "Мои дети в"
  },
  {
    "chapter": 11,
    "verse": 10,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "трепете придут с запада."
  },
  {
    "chapter": 11,
    "verse": 11,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Они вернутся из Египта,"
  },
  {
    "chapter": 11,
    "verse": 11,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "трепеща как птицы,"
  },
  {
    "chapter": 11,
    "verse": 11,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "из Ассирии как голуби."
  },
  {
    "chapter": 11,
    "verse": 11,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "Я поселю их в их жилищах,"
  },
  {
    "chapter": 11,
    "verse": 11,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "возвещает Господь."
  },
  {
    "chapter": 11,
    "verse": 12,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Ефрем окружил Меня ложью,"
  },
  {
    "chapter": 11,
    "verse": 12,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "а дом Израилев обманом,"
  },
  {
    "chapter": 11,
    "verse": 12,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "и Иуда сильно"
  },
  {
    "chapter": 11,
    "verse": 12,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "противится Богу,"
  },
  {
    "chapter": 11,
    "verse": 12,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "и даже верному Святому."
  }
]