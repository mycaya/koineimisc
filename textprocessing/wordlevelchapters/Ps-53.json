[
  {
    "chapter": 53,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Дирижеру хора."
  },
  {
    "chapter": 53,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "На мотив О голубке"
  },
  {
    "chapter": 53,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "в дальней дубраве."
  },
  {
    "chapter": 53,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Мольба Давида,"
  },
  {
    "chapter": 53,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "когда филистимляне"
  },
  {
    "chapter": 53,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "схватили его в Гефе."
  },
  {
    "chapter": 53,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Боже,"
  },
  {
    "chapter": 53,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "помилуй меня,"
  },
  {
    "chapter": 53,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "потому что люди"
  },
  {
    "chapter": 53,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "меня затравили;"
  },
  {
    "chapter": 53,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "всякий день,"
  },
  {
    "chapter": 53,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "нападая,"
  },
  {
    "chapter": 53,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "меня теснят."
  },
  {
    "chapter": 53,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Враг травит меня весь день;"
  },
  {
    "chapter": 53,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "много тех,"
  },
  {
    "chapter": 53,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "кто против меня воюет."
  },
  {
    "chapter": 53,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "О Всевышний."
  },
  {
    "chapter": 53,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Когда я в страхе,"
  },
  {
    "chapter": 53,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "я на Тебя полагаюсь."
  },
  {
    "chapter": 53,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "На Бога,"
  },
  {
    "chapter": 53,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Чье слово я славлю,"
  },
  {
    "chapter": 53,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "на Бога я полагаюсь,"
  },
  {
    "chapter": 53,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и не устрашусь:"
  },
  {
    "chapter": 53,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "что может мне"
  },
  {
    "chapter": 53,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "сделать смертный?"
  },
  {
    "chapter": 53,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "А они весь день"
  },
  {
    "chapter": 53,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "искажают слова мои,"
  },
  {
    "chapter": 53,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "всегда замышляют"
  },
  {
    "chapter": 53,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "сделать мне зло."
  },
  {
    "chapter": 53,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Составляют заговоры,"
  },
  {
    "chapter": 53,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "таятся,"
  },
  {
    "chapter": 53,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "наблюдают за"
  },
  {
    "chapter": 53,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "каждым моими шагами,"
  },
  {
    "chapter": 53,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "желая меня убить."
  },
  {
    "chapter": 53,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Так не дай им избежать"
  },
  {
    "chapter": 53,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "наказания за их вину;"
  },
  {
    "chapter": 53,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "в гневе Твоем,"
  },
  {
    "chapter": 53,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Боже,"
  },
  {
    "chapter": 53,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "низложи народы!"
  },
  {
    "chapter": 53,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "У Тебя записаны"
  },
  {
    "chapter": 53,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "все скитания мои;"
  },
  {
    "chapter": 53,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "собери мои"
  },
  {
    "chapter": 53,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "слезы в сосуд Твой,"
  },
  {
    "chapter": 53,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "не в Твоей ли книге они?"
  },
  {
    "chapter": 53,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "И враги мои"
  },
  {
    "chapter": 53,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "обратятся вспять,"
  },
  {
    "chapter": 53,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "когда я о помощи воззову."
  },
  {
    "chapter": 53,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Так я узнаю,"
  },
  {
    "chapter": 53,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "что Бог со мной."
  },
  {
    "chapter": 53,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "На Бога,"
  },
  {
    "chapter": 53,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Чье слово я славлю,"
  },
  {
    "chapter": 53,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "на Господа,"
  },
  {
    "chapter": 53,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Чье слово я славлю,"
  },
  {
    "chapter": 53,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "на Бога полагаюсь,"
  },
  {
    "chapter": 53,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и не устрашусь:"
  },
  {
    "chapter": 53,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "что может мне"
  },
  {
    "chapter": 53,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "сделать человек?"
  },
  {
    "chapter": 53,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Боже,"
  },
  {
    "chapter": 53,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "я дам Тебе,"
  },
  {
    "chapter": 53,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "что обещал,"
  },
  {
    "chapter": 53,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "я принесу Тебе"
  },
  {
    "chapter": 53,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "жертвы благодарения."
  },
  {
    "chapter": 53,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ведь Ты меня спас от смерти"
  },
  {
    "chapter": 53,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и не дал споткнуться моим"
  },
  {
    "chapter": 53,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "ногам,"
  },
  {
    "chapter": 53,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "чтобы мне во свете"
  },
  {
    "chapter": 53,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "живых ходить пред Тобою."
  }
]