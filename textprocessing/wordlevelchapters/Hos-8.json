[
  {
    "chapter": 8,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "- Поднеси рог к"
  },
  {
    "chapter": 8,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "своим губам! Как орел,"
  },
  {
    "chapter": 8,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "налетит враг на"
  },
  {
    "chapter": 8,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "народ Господа,"
  },
  {
    "chapter": 8,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "потому что народ нарушил"
  },
  {
    "chapter": 8,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "Мой завет"
  },
  {
    "chapter": 8,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 7,
    "text": "и преступил Мой Закон."
  },
  {
    "chapter": 8,
    "verse": 2,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Израиль взывает ко Мне:"
  },
  {
    "chapter": 8,
    "verse": 2,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "О Бог наш,"
  },
  {
    "chapter": 8,
    "verse": 2,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "мы признаем Тебя!"
  },
  {
    "chapter": 8,
    "verse": 3,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Но Израиль отверг добро,"
  },
  {
    "chapter": 8,
    "verse": 3,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "враг будет"
  },
  {
    "chapter": 8,
    "verse": 3,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "преследовать его."
  },
  {
    "chapter": 8,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Он возводил царей"
  },
  {
    "chapter": 8,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "без Моего согласия,"
  },
  {
    "chapter": 8,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "выбирал вождей"
  },
  {
    "chapter": 8,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "без Моего одобрения."
  },
  {
    "chapter": 8,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "Из своего серебра"
  },
  {
    "chapter": 8,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "и золота люди делают для"
  },
  {
    "chapter": 8,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 7,
    "text": "себя"
  },
  {
    "chapter": 8,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 8,
    "text": "идолов себе на погибель."
  },
  {
    "chapter": 8,
    "verse": 5,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Я отверг"
  },
  {
    "chapter": 8,
    "verse": 5,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "твоего идола-тельца,"
  },
  {
    "chapter": 8,
    "verse": 5,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "Самария! Мой гнев"
  },
  {
    "chapter": 8,
    "verse": 5,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "воспылал на этот народ."
  },
  {
    "chapter": 8,
    "verse": 5,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "До каких пор он не"
  },
  {
    "chapter": 8,
    "verse": 5,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "будет способен очиститься?"
  },
  {
    "chapter": 8,
    "verse": 6,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Этот идол из Израиля! Он"
  },
  {
    "chapter": 8,
    "verse": 6,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "произведение рук мастера,"
  },
  {
    "chapter": 8,
    "verse": 6,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "и поэтому не Бог."
  },
  {
    "chapter": 8,
    "verse": 6,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "Телец Самарии будет"
  },
  {
    "chapter": 8,
    "verse": 6,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "непременно разбит на кусочки."
  },
  {
    "chapter": 8,
    "verse": 7,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Люди сеют ветер,"
  },
  {
    "chapter": 8,
    "verse": 7,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "а пожнут бурю."
  },
  {
    "chapter": 8,
    "verse": 7,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "У стебля пшеницы"
  },
  {
    "chapter": 8,
    "verse": 7,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "не будет колоса,"
  },
  {
    "chapter": 8,
    "verse": 7,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "и она не даст зерна."
  },
  {
    "chapter": 8,
    "verse": 7,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "А там,"
  },
  {
    "chapter": 8,
    "verse": 7,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 7,
    "text": "где пшеница"
  },
  {
    "chapter": 8,
    "verse": 7,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 8,
    "text": "произведет зерно,"
  },
  {
    "chapter": 8,
    "verse": 7,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 9,
    "text": "чужеземцы поглотят его."
  },
  {
    "chapter": 8,
    "verse": 8,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Израиль поглощен,"
  },
  {
    "chapter": 8,
    "verse": 8,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "теперь он среди"
  },
  {
    "chapter": 8,
    "verse": 8,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "народов как негодный сосуд,"
  },
  {
    "chapter": 8,
    "verse": 9,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "потому что пошел к Ассирии,"
  },
  {
    "chapter": 8,
    "verse": 9,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "как одиноко"
  },
  {
    "chapter": 8,
    "verse": 9,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "бродящий дикий осел."
  },
  {
    "chapter": 8,
    "verse": 9,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "Подарками Ефрем"
  },
  {
    "chapter": 8,
    "verse": 9,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "приобретал себе любовников."
  },
  {
    "chapter": 8,
    "verse": 10,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Хотя они и"
  },
  {
    "chapter": 8,
    "verse": 10,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "посылали дары народам,"
  },
  {
    "chapter": 8,
    "verse": 10,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "Я теперь соберу их вместе."
  },
  {
    "chapter": 8,
    "verse": 10,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "Они начнут изнемогать под"
  },
  {
    "chapter": 8,
    "verse": 10,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "гнетом могущественного царя."
  },
  {
    "chapter": 8,
    "verse": 11,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Хотя Ефрем построил много"
  },
  {
    "chapter": 8,
    "verse": 11,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "жертвенников для приношений"
  },
  {
    "chapter": 8,
    "verse": 11,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "за грех,"
  },
  {
    "chapter": 8,
    "verse": 11,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "они стали"
  },
  {
    "chapter": 8,
    "verse": 11,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "жертвенниками греха."
  },
  {
    "chapter": 8,
    "verse": 12,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Я написал им множество"
  },
  {
    "chapter": 8,
    "verse": 12,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "предписаний Моего Закона,"
  },
  {
    "chapter": 8,
    "verse": 12,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "но они рассматривали"
  },
  {
    "chapter": 8,
    "verse": 12,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "их как нечто чужое."
  },
  {
    "chapter": 8,
    "verse": 13,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Они предлагают Мне"
  },
  {
    "chapter": 8,
    "verse": 13,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "жертвоприношения и едят мясо,"
  },
  {
    "chapter": 8,
    "verse": 13,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "но Господу они неугодны."
  },
  {
    "chapter": 8,
    "verse": 13,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "Теперь Он вспомнит"
  },
  {
    "chapter": 8,
    "verse": 13,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "их грехи и накажет их."
  },
  {
    "chapter": 8,
    "verse": 13,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "Они вернутся в Египет."
  },
  {
    "chapter": 8,
    "verse": 14,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Израиль забыл своего"
  },
  {
    "chapter": 8,
    "verse": 14,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "Создателя и построил дворцы."
  },
  {
    "chapter": 8,
    "verse": 14,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "Иудея укрепила"
  },
  {
    "chapter": 8,
    "verse": 14,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "многие города,"
  },
  {
    "chapter": 8,
    "verse": 14,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "но Я пошлю"
  },
  {
    "chapter": 8,
    "verse": 14,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "огонь на их города,"
  },
  {
    "chapter": 8,
    "verse": 14,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 7,
    "text": "который пожрет их дворцы."
  }
]