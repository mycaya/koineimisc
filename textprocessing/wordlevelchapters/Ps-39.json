[
  {
    "chapter": 39,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Дирижеру хора."
  },
  {
    "chapter": 39,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Псалом Давида."
  },
  {
    "chapter": 39,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Блажен тот,"
  },
  {
    "chapter": 39,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "кто о слабом заботится,"
  },
  {
    "chapter": 39,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "во время беды"
  },
  {
    "chapter": 39,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "избавит его Господь."
  },
  {
    "chapter": 39,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господь сохранит его"
  },
  {
    "chapter": 39,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и сбережет ему жизнь,"
  },
  {
    "chapter": 39,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "счастьем одарит его на"
  },
  {
    "chapter": 39,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "земле"
  },
  {
    "chapter": 39,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "и не отдаст его на произвол"
  },
  {
    "chapter": 39,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "врагов."
  },
  {
    "chapter": 39,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господь укрепит"
  },
  {
    "chapter": 39,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "его на одре болезни,"
  },
  {
    "chapter": 39,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и с ложа"
  },
  {
    "chapter": 39,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "недуга его поднимет."
  },
  {
    "chapter": 39,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я сказал:"
  },
  {
    "chapter": 39,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Помилуй меня,"
  },
  {
    "chapter": 39,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Господь;"
  },
  {
    "chapter": 39,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "исцели меня я"
  },
  {
    "chapter": 39,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "пред Тобой согрешил."
  },
  {
    "chapter": 39,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Враги мои зло"
  },
  {
    "chapter": 39,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "говорят обо мне:"
  },
  {
    "chapter": 39,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Когда же умрет он"
  },
  {
    "chapter": 39,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и имя его погибнет?"
  },
  {
    "chapter": 39,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Если приходит"
  },
  {
    "chapter": 39,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "кто навестить меня,"
  },
  {
    "chapter": 39,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "то ложь говорит,"
  },
  {
    "chapter": 39,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "а в сердце своем"
  },
  {
    "chapter": 39,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "слагает злые слухи;"
  },
  {
    "chapter": 39,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "потом он выходит и"
  },
  {
    "chapter": 39,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "всем их рассказывает."
  },
  {
    "chapter": 39,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Все враги мои"
  },
  {
    "chapter": 39,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "шепчутся против меня,"
  },
  {
    "chapter": 39,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "думают худшее обо мне:"
  },
  {
    "chapter": 39,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Смертельный"
  },
  {
    "chapter": 39,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "недуг его одолел;"
  },
  {
    "chapter": 39,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "он слег и"
  },
  {
    "chapter": 39,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "больше ему не встать."
  },
  {
    "chapter": 39,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Даже близкий друг,"
  },
  {
    "chapter": 39,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "на которого я полагался,"
  },
  {
    "chapter": 39,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "кто ест мой хлеб,"
  },
  {
    "chapter": 39,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "поднял свою"
  },
  {
    "chapter": 39,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "пяту против меня."
  },
  {
    "chapter": 39,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но Ты,"
  },
  {
    "chapter": 39,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 39,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "помилуй меня;"
  },
  {
    "chapter": 39,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "подними меня,"
  },
  {
    "chapter": 39,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "и я воздам им!"
  },
  {
    "chapter": 39,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Из того я узнаю,"
  },
  {
    "chapter": 39,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "что угоден Тебе,"
  },
  {
    "chapter": 39,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "если враг мой не"
  },
  {
    "chapter": 39,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "превозможет меня,"
  },
  {
    "chapter": 39,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "а меня Ты поддержишь за"
  },
  {
    "chapter": 39,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "беспорочность мою"
  },
  {
    "chapter": 39,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и поставишь пред Собою"
  },
  {
    "chapter": 39,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "навеки."
  },
  {
    "chapter": 39,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Благословен Господь,"
  },
  {
    "chapter": 39,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Бог Израиля,"
  },
  {
    "chapter": 39,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "от века и до"
  },
  {
    "chapter": 39,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "века! Аминь и аминь!"
  },
  {
    "chapter": 39,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Дирижеру хора."
  },
  {
    "chapter": 39,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Наставление потомков Корея."
  },
  {
    "chapter": 39,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Как стремится лань к воде,"
  },
  {
    "chapter": 39,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "так стремится"
  },
  {
    "chapter": 39,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "душа моя к Тебе,"
  },
  {
    "chapter": 39,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Боже."
  },
  {
    "chapter": 39,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Душа моя жаждет Бога,"
  },
  {
    "chapter": 39,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "живого Бога."
  },
  {
    "chapter": 39,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Когда я приду и"
  },
  {
    "chapter": 39,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "пред Богом предстану?"
  },
  {
    "chapter": 39,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Слезы были мне"
  },
  {
    "chapter": 39,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "пищей и днем,"
  },
  {
    "chapter": 39,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и ночью,"
  },
  {
    "chapter": 39,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "когда"
  },
  {
    "chapter": 39,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "постоянно говорили мне:"
  },
  {
    "chapter": 39,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Где твой Бог?"
  },
  {
    "chapter": 39,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Сердце мое болит,"
  },
  {
    "chapter": 39,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "когда я вспоминаю,"
  },
  {
    "chapter": 39,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "как ходил в"
  },
  {
    "chapter": 39,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "многолюдстве к дому Божьему,"
  },
  {
    "chapter": 39,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "в шествии народ предваряя,"
  },
  {
    "chapter": 39,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "ходил в праздничной толпе"
  },
  {
    "chapter": 39,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "под крики радости и хвалы."
  },
  {
    "chapter": 39,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Что унываешь,"
  },
  {
    "chapter": 39,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "моя душа? Зачем"
  },
  {
    "chapter": 39,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "тревожишься? Возложи надежду на Бога,"
  },
  {
    "chapter": 39,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "ведь я еще буду славить Его"
  },
  {
    "chapter": 39,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "моего Спасителя"
  },
  {
    "chapter": 39,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "и моего Бога."
  },
  {
    "chapter": 39,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Душа моя унывает во мне,"
  },
  {
    "chapter": 39,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "поэтому я вспоминаю Тебя с"
  },
  {
    "chapter": 39,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "земли"
  },
  {
    "chapter": 39,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "иорданской с высот Ермона,"
  },
  {
    "chapter": 39,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "с горы Мицар."
  },
  {
    "chapter": 39,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Бездна бездну зовет"
  },
  {
    "chapter": 39,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "шумом Твоих водопадов;"
  },
  {
    "chapter": 39,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "все волны Твои,"
  },
  {
    "chapter": 39,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "все Твои валы"
  },
  {
    "chapter": 39,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "прошли надо мною."
  },
  {
    "chapter": 39,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Днем Господь"
  },
  {
    "chapter": 39,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "являет Свою милость,"
  },
  {
    "chapter": 39,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "ночью я пою Ему песню"
  },
  {
    "chapter": 39,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "молитву Богу моей жизни."
  },
  {
    "chapter": 39,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Скажу я Богу,"
  },
  {
    "chapter": 39,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "моей скале:"
  },
  {
    "chapter": 39,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Почему Ты меня"
  },
  {
    "chapter": 39,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "забыл? Почему я скитаюсь,"
  },
  {
    "chapter": 39,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "плача,"
  },
  {
    "chapter": 39,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "оскорбленный моим врагом?"
  },
  {
    "chapter": 39,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Как от раны"
  },
  {
    "chapter": 39,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
