[
  {
    "chapter": 2,
    "verse": 1,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Ниневия,"
  },
  {
    "chapter": 2,
    "verse": 1,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "против тебя"
  },
  {
    "chapter": 2,
    "verse": 1,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "поднимается разрушитель."
  },
  {
    "chapter": 2,
    "verse": 1,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "Охраняй крепости,"
  },
  {
    "chapter": 2,
    "verse": 1,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 5,
    "text": "стереги дорогу,"
  },
  {
    "chapter": 2,
    "verse": 1,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 6,
    "text": "укрепляй себя,"
  },
  {
    "chapter": 2,
    "verse": 1,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 7,
    "text": "собери все свои силы."
  },
  {
    "chapter": 2,
    "verse": 2,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Господь восстановит величие"
  },
  {
    "chapter": 2,
    "verse": 2,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "Иакова подобно величию"
  },
  {
    "chapter": 2,
    "verse": 2,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "Израиля,"
  },
  {
    "chapter": 2,
    "verse": 2,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "хотя опустошители"
  },
  {
    "chapter": 2,
    "verse": 2,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 5,
    "text": "опустошили их и погубили"
  },
  {
    "chapter": 2,
    "verse": 2,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 6,
    "text": "их виноградные лозы."
  },
  {
    "chapter": 2,
    "verse": 3,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Щиты бойцов его красны,"
  },
  {
    "chapter": 2,
    "verse": 3,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "его воины в"
  },
  {
    "chapter": 2,
    "verse": 3,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "багряных одеждах."
  },
  {
    "chapter": 2,
    "verse": 3,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "Сверкает металл"
  },
  {
    "chapter": 2,
    "verse": 3,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 5,
    "text": "колесниц в день,"
  },
  {
    "chapter": 2,
    "verse": 3,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 6,
    "text": "когда они"
  },
  {
    "chapter": 2,
    "verse": 3,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 7,
    "text": "приготовлены к бою,"
  },
  {
    "chapter": 2,
    "verse": 3,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 8,
    "text": "колышется лес копий."
  },
  {
    "chapter": 2,
    "verse": 4,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Проносятся по"
  },
  {
    "chapter": 2,
    "verse": 4,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "улицам колесницы,"
  },
  {
    "chapter": 2,
    "verse": 4,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "мечутся по площадям."
  },
  {
    "chapter": 2,
    "verse": 4,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "Они подобны"
  },
  {
    "chapter": 2,
    "verse": 4,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 5,
    "text": "пылающим факелам,"
  },
  {
    "chapter": 2,
    "verse": 4,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 6,
    "text": "как сверкающие молнии."
  },
  {
    "chapter": 2,
    "verse": 5,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Царь Ассирии созывает"
  },
  {
    "chapter": 2,
    "verse": 5,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "своих лучших воинов,"
  },
  {
    "chapter": 2,
    "verse": 5,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "но они спотыкаются на ходу."
  },
  {
    "chapter": 2,
    "verse": 5,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "Они устремляются к"
  },
  {
    "chapter": 2,
    "verse": 5,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 5,
    "text": "городским стенам,"
  },
  {
    "chapter": 2,
    "verse": 5,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 6,
    "text": "но против них уже"
  },
  {
    "chapter": 2,
    "verse": 5,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 7,
    "text": "возведены осадные сооружения."
  },
  {
    "chapter": 2,
    "verse": 6,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Речные ворота распахнуты,"
  },
  {
    "chapter": 2,
    "verse": 6,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "и враг разрушает"
  },
  {
    "chapter": 2,
    "verse": 6,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "царский дворец."
  },
  {
    "chapter": 2,
    "verse": 7,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Решено:"
  },
  {
    "chapter": 2,
    "verse": 7,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "Ниневия будет"
  },
  {
    "chapter": 2,
    "verse": 7,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "обнажена и уведена в плен."
  },
  {
    "chapter": 2,
    "verse": 7,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "Рабыни ее стонут,"
  },
  {
    "chapter": 2,
    "verse": 7,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 5,
    "text": "как голубки,"
  },
  {
    "chapter": 2,
    "verse": 7,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 6,
    "text": "и бьют себя в грудь."
  },
  {
    "chapter": 2,
    "verse": 8,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Ниневия как"
  },
  {
    "chapter": 2,
    "verse": 8,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "убывающий водоем:"
  },
  {
    "chapter": 2,
    "verse": 8,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "словно прорвавшаяся вода,"
  },
  {
    "chapter": 2,
    "verse": 8,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "бегут из нее люди."
  },
  {
    "chapter": 2,
    "verse": 8,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 5,
    "text": "Стойте! Стойте! кричат им,"
  },
  {
    "chapter": 2,
    "verse": 8,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 6,
    "text": "но никто не возвращается."
  },
  {
    "chapter": 2,
    "verse": 9,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Расхищайте серебро!"
  },
  {
    "chapter": 2,
    "verse": 9,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "Расхищайте золото! Нет"
  },
  {
    "chapter": 2,
    "verse": 9,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "конца их запасам"
  },
  {
    "chapter": 2,
    "verse": 9,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "и богатствам"
  },
  {
    "chapter": 2,
    "verse": 9,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 5,
    "text": "из сокровищниц."
  },
  {
    "chapter": 2,
    "verse": 10,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Разграблен,"
  },
  {
    "chapter": 2,
    "verse": 10,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "опустошен и разорен город."
  },
  {
    "chapter": 2,
    "verse": 10,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "Сердца людей"
  },
  {
    "chapter": 2,
    "verse": 10,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "замирают от страха,"
  },
  {
    "chapter": 2,
    "verse": 10,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 5,
    "text": "колени трясутся,"
  },
  {
    "chapter": 2,
    "verse": 10,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 6,
    "text": "дрожат тела и у"
  },
  {
    "chapter": 2,
    "verse": 10,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 7,
    "text": "всех бледнеют лица."
  },
  {
    "chapter": 2,
    "verse": 11,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Где же теперь Ниневия,"
  },
  {
    "chapter": 2,
    "verse": 11,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "что была как логово львов,"
  },
  {
    "chapter": 2,
    "verse": 11,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "как место,"
  },
  {
    "chapter": 2,
    "verse": 11,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "где"
  },
  {
    "chapter": 2,
    "verse": 11,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 5,
    "text": "выкармливают своих львят,"
  },
  {
    "chapter": 2,
    "verse": 11,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 6,
    "text": "по которому бродили лев,"
  },
  {
    "chapter": 2,
    "verse": 11,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 7,
    "text": "львица и львенок,"
  },
  {
    "chapter": 2,
    "verse": 11,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 8,
    "text": "и их ничто не пугало?"
  },
  {
    "chapter": 2,
    "verse": 12,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Лев растерзал достаточно"
  },
  {
    "chapter": 2,
    "verse": 12,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "добычи для своих детенышей"
  },
  {
    "chapter": 2,
    "verse": 12,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "и удавил жертву для своей"
  },
  {
    "chapter": 2,
    "verse": 12,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "львицы,"
  },
  {
    "chapter": 2,
    "verse": 12,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 5,
    "text": "наполнил добычею свои"
  },
  {
    "chapter": 2,
    "verse": 12,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 6,
    "text": "пещеры и жертвами свое логово."
  },
  {
    "chapter": 2,
    "verse": 13,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "- Я против тебя!"
  },
  {
    "chapter": 2,
    "verse": 13,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "возвещает Господь сил."
  },
  {
    "chapter": 2,
    "verse": 13,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "Я сожгу в дыму"
  },
  {
    "chapter": 2,
    "verse": 13,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "твои колесницы,"
  },
  {
    "chapter": 2,
    "verse": 13,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 5,
    "text": "и меч истребит"
  },
  {
    "chapter": 2,
    "verse": 13,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 6,
    "text": "твоих молодых львов."
  },
  {
    "chapter": 2,
    "verse": 13,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 7,
    "text": "Я не оставлю на"
  },
  {
    "chapter": 2,
    "verse": 13,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 8,
    "text": "земле добычи для тебя."
  },
  {
    "chapter": 2,
    "verse": 13,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 9,
    "text": "И не будет больше слышен"
  },
  {
    "chapter": 2,
    "verse": 13,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 10,
    "text": "голос твоих посланников."
  }
]