[
  {
    "chapter": 59,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Дирижеру хора,"
  },
  {
    "chapter": 59,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Идуфуну."
  },
  {
    "chapter": 59,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Псалом Давида."
  },
  {
    "chapter": 59,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Только в Боге"
  },
  {
    "chapter": 59,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "покой для моей души,"
  },
  {
    "chapter": 59,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "спасение мое от Него."
  },
  {
    "chapter": 59,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Только Он мне"
  },
  {
    "chapter": 59,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "скала и спасение,"
  },
  {
    "chapter": 59,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Он крепость моя я не"
  },
  {
    "chapter": 59,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "буду сильно поколеблен."
  },
  {
    "chapter": 59,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Сколько вы будете еще"
  },
  {
    "chapter": 59,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "нападать на человека,"
  },
  {
    "chapter": 59,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "крушить его все вместе,"
  },
  {
    "chapter": 59,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "словно наклонившуюся стену,"
  },
  {
    "chapter": 59,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "шатающуюся ограду?"
  },
  {
    "chapter": 59,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Думают лишь о том,"
  },
  {
    "chapter": 59,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "как свергнуть его"
  },
  {
    "chapter": 59,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "с почетного места,"
  },
  {
    "chapter": 59,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и радуются лжи."
  },
  {
    "chapter": 59,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Устами благословляют,"
  },
  {
    "chapter": 59,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "а в сердце"
  },
  {
    "chapter": 59,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "своем проклинают."
  },
  {
    "chapter": 59,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 8,
    "text": "Пауза"
  },
  {
    "chapter": 59,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Только в Боге покойся,"
  },
  {
    "chapter": 59,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "моя душа,"
  },
  {
    "chapter": 59,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "на Него надежда моя."
  },
  {
    "chapter": 59,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Только Он мне"
  },
  {
    "chapter": 59,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "скала и спасение,"
  },
  {
    "chapter": 59,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Он крепость моя;"
  },
  {
    "chapter": 59,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "я не буду поколеблен."
  },
  {
    "chapter": 59,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "В Боге"
  },
  {
    "chapter": 59,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "спасение и слава моя;"
  },
  {
    "chapter": 59,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Он крепкая скала"
  },
  {
    "chapter": 59,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "моя и мое прибежище."
  },
  {
    "chapter": 59,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Надейтесь на Него,"
  },
  {
    "chapter": 59,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "народ,"
  },
  {
    "chapter": 59,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "во всякое время,"
  },
  {
    "chapter": 59,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "сердце свое"
  },
  {
    "chapter": 59,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "пред Ним изливайте;"
  },
  {
    "chapter": 59,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "ведь Бог наше прибежище."
  },
  {
    "chapter": 59,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "Пауза"
  },
  {
    "chapter": 59,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Смертные только лишь суета,"
  },
  {
    "chapter": 59,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "люди только лишь ложь;"
  },
  {
    "chapter": 59,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "на весы положи они ничто,"
  },
  {
    "chapter": 59,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "вместе они легче дыхания."
  },
  {
    "chapter": 59,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Не надейтесь на грабеж,"
  },
  {
    "chapter": 59,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "краденным не кичитесь."
  },
  {
    "chapter": 59,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Пусть богатство растет"
  },
  {
    "chapter": 59,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "сердца к нему не прилагайте."
  },
  {
    "chapter": 59,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Один раз сказал Бог,"
  },
  {
    "chapter": 59,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "дважды я это слышал:"
  },
  {
    "chapter": 59,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "что у Бога сила,"
  },
  {
    "chapter": 59,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "у Тебя,"
  },
  {
    "chapter": 59,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Владыка,"
  },
  {
    "chapter": 59,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "милость."
  },
  {
    "chapter": 59,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Ведь Ты каждому"
  },
  {
    "chapter": 59,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "воздаешь по делам его."
  }
]