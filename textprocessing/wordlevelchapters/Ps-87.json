[
  {
    "chapter": 87,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Псалом."
  },
  {
    "chapter": 87,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Песнь на"
  },
  {
    "chapter": 87,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "празднование субботы."
  },
  {
    "chapter": 87,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Хорошо славить Господа"
  },
  {
    "chapter": 87,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и воспевать Имя Твое,"
  },
  {
    "chapter": 87,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Всевышний,"
  },
  {
    "chapter": 87,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "возглашать милость Твою"
  },
  {
    "chapter": 87,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "утром и верность Твою вечером,"
  },
  {
    "chapter": 87,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "играть на десятиструнной"
  },
  {
    "chapter": 87,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "лире и на мелодичной арфе."
  },
  {
    "chapter": 87,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ведь Ты,"
  },
  {
    "chapter": 87,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 87,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "обрадовал меня"
  },
  {
    "chapter": 87,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Своими деяниями;"
  },
  {
    "chapter": 87,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "я радостно ликую"
  },
  {
    "chapter": 87,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "о делах Твоих рук."
  },
  {
    "chapter": 87,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господи,"
  },
  {
    "chapter": 87,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "как велики"
  },
  {
    "chapter": 87,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "творения Твоих рук,"
  },
  {
    "chapter": 87,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и как глубоки"
  },
  {
    "chapter": 87,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Твои помышления!"
  },
  {
    "chapter": 87,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Глупый человек не знает"
  },
  {
    "chapter": 87,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и невежда не понимает их."
  },
  {
    "chapter": 87,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Хотя нечестивые возникают,"
  },
  {
    "chapter": 87,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "как трава,"
  },
  {
    "chapter": 87,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и злодеи процветают,"
  },
  {
    "chapter": 87,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "они исчезнут навеки."
  },
  {
    "chapter": 87,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты же,"
  },
  {
    "chapter": 87,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 87,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "навеки превознесен!"
  },
  {
    "chapter": 87,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Подлинно враги Твои,"
  },
  {
    "chapter": 87,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 87,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "подлинно враги"
  },
  {
    "chapter": 87,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Твои погибнут;"
  },
  {
    "chapter": 87,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "все злодеи будут рассеяны."
  },
  {
    "chapter": 87,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "А мой рог Ты вознесешь,"
  },
  {
    "chapter": 87,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "подобно быку,"
  },
  {
    "chapter": 87,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и умастишь меня"
  },
  {
    "chapter": 87,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "свежим маслом."
  },
  {
    "chapter": 87,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Глаза мои видели"
  },
  {
    "chapter": 87,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "поражение врагов моих,"
  },
  {
    "chapter": 87,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и уши мои слышали"
  },
  {
    "chapter": 87,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "падение злодеев,"
  },
  {
    "chapter": 87,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "восстающих на меня."
  },
  {
    "chapter": 87,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "А праведник цветет,"
  },
  {
    "chapter": 87,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "словно пальма,"
  },
  {
    "chapter": 87,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "возвышается,"
  },
  {
    "chapter": 87,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "как кедр на Ливане."
  },
  {
    "chapter": 87,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Посаженные в"
  },
  {
    "chapter": 87,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "доме Господнем,"
  },
  {
    "chapter": 87,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "они зацветут во"
  },
  {
    "chapter": 87,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "дворах храма нашего Бога."
  },
  {
    "chapter": 87,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Они и в старости"
  },
  {
    "chapter": 87,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "будут плодовиты,"
  },
  {
    "chapter": 87,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "сочны и свежи,"
  },
  {
    "chapter": 87,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "чтобы возвещать,"
  },
  {
    "chapter": 87,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "что праведен Господь,"
  },
  {
    "chapter": 87,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "скала моя,"
  },
  {
    "chapter": 87,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и нет в Нем неправды."
  }
]