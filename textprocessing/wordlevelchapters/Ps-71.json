[
  {
    "chapter": 71,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Дирижеру хора."
  },
  {
    "chapter": 71,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "На мотив Не погуби."
  },
  {
    "chapter": 71,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Псалом Асафа."
  },
  {
    "chapter": 71,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Песнь."
  },
  {
    "chapter": 71,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Благодарим Тебя,"
  },
  {
    "chapter": 71,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Боже,"
  },
  {
    "chapter": 71,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "благодарим,"
  },
  {
    "chapter": 71,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "потому что близко Твое Имя;"
  },
  {
    "chapter": 71,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "возвещают люди чудеса Твои."
  },
  {
    "chapter": 71,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты сказал:"
  },
  {
    "chapter": 71,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "В назначенный срок Я"
  },
  {
    "chapter": 71,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "буду судить справедливо."
  },
  {
    "chapter": 71,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Когда колеблется земля"
  },
  {
    "chapter": 71,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и все живущие на ней,"
  },
  {
    "chapter": 71,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Я удерживаю ее"
  },
  {
    "chapter": 71,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "на столпах ее."
  },
  {
    "chapter": 71,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Пауза"
  },
  {
    "chapter": 71,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я сказал гордецам:"
  },
  {
    "chapter": 71,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Не превозноситесь,"
  },
  {
    "chapter": 71,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и нечестивым:"
  },
  {
    "chapter": 71,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Не поднимайте рога."
  },
  {
    "chapter": 71,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Не кичитесь своей"
  },
  {
    "chapter": 71,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "мощью перед небом,"
  },
  {
    "chapter": 71,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "не говорите надменно."
  },
  {
    "chapter": 71,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ни с востока,"
  },
  {
    "chapter": 71,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "ни с запада,"
  },
  {
    "chapter": 71,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "ни с пустыни не"
  },
  {
    "chapter": 71,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "стоит ожидать возвышения."
  },
  {
    "chapter": 71,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но Бог судья:"
  },
  {
    "chapter": 71,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Он одного унижает,"
  },
  {
    "chapter": 71,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "а другого возвышает."
  },
  {
    "chapter": 71,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "В руке Господа"
  },
  {
    "chapter": 71,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "чаша с кипящим вином,"
  },
  {
    "chapter": 71,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "полным горьких приправ,"
  },
  {
    "chapter": 71,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и Он льет из нее."
  },
  {
    "chapter": 71,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Всем беззаконным земли"
  },
  {
    "chapter": 71,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "придется"
  },
  {
    "chapter": 71,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "испить чашу эту до дна."
  },
  {
    "chapter": 71,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "А я буду"
  },
  {
    "chapter": 71,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "возвещать это вечно,"
  },
  {
    "chapter": 71,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "буду воспевать Бога Иакова,"
  },
  {
    "chapter": 71,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Все роги нечестивых сломлю,"
  },
  {
    "chapter": 71,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и вознесутся"
  },
  {
    "chapter": 71,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "роги праведных."
  }
]