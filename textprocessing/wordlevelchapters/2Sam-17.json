  },
  {
    "chapter": 17,
    "verse": 28,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "принесли"
  },
  {
    "chapter": 17,
    "verse": 28,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "постельные принадлежности,"
  },
  {
    "chapter": 17,
    "verse": 28,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "чаши и глиняные сосуды."
  },
  {
    "chapter": 17,
    "verse": 28,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "Еще они принесли"
  },
  {
    "chapter": 17,
    "verse": 28,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "пшеницы и ячменя,"
  },
  {
    "chapter": 17,
    "verse": 28,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 6,
    "text": "муки и поджаренного зерна,"
  },
  {
    "chapter": 17,
    "verse": 28,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 7,
