[
  {
    "chapter": 52,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Дирижеру хора."
  },
  {
    "chapter": 52,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "На струнных инструментах."
  },
  {
    "chapter": 52,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Наставление Давида."
  },
  {
    "chapter": 52,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Боже,"
  },
  {
    "chapter": 52,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "услышь молитву мою;"
  },
  {
    "chapter": 52,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "от мольбы моей не таись;"
  },
  {
    "chapter": 52,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "услышь меня и ответь"
  },
  {
    "chapter": 52,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "мне! Думы томят меня,"
  },
  {
    "chapter": 52,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "я в смятении"
  },
  {
    "chapter": 52,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "от голоса врага,"
  },
  {
    "chapter": 52,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "от притеснения нечестивого."
  },
  {
    "chapter": 52,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Они навели на меня"
  },
  {
    "chapter": 52,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "беду и враждуют со мной,"
  },
  {
    "chapter": 52,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "пылая гневом."
  },
  {
    "chapter": 52,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Сердце мое томится;"
  },
  {
    "chapter": 52,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "ужасы смерти"
  },
  {
    "chapter": 52,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "напали на меня."
  },
  {
    "chapter": 52,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Страх и трепет"
  },
  {
    "chapter": 52,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "меня стеснили,"
  },
  {
    "chapter": 52,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и ужас меня объял."
  },
  {
    "chapter": 52,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я сказал:"
  },
  {
    "chapter": 52,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Кто дал бы мне крылья"
  },
  {
    "chapter": 52,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "голубя! Я улетел бы"
  },
  {
    "chapter": 52,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и обрел покой,"
  },
  {
    "chapter": 52,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "далеко улетел бы"
  },
  {
    "chapter": 52,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и жил в пустыне."
  },
  {
    "chapter": 52,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Пауза"
  },
  {
    "chapter": 52,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я поспешил бы найти убежище"
  },
  {
    "chapter": 52,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "вдалеке от вихрей"
  },
  {
    "chapter": 52,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и непогоды."
  },
  {
    "chapter": 52,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Смути их,"
  },
  {
    "chapter": 52,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Владыка,"
  },
  {
    "chapter": 52,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "раздели их языки,"
  },
  {
    "chapter": 52,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "так как вижу я в"
  },
  {
    "chapter": 52,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "городе насилие и раздор."
  },
  {
    "chapter": 52,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Днем и ночью они"
  },
  {
    "chapter": 52,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "обходят город по стенам,"
  },
  {
    "chapter": 52,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "полон он беззакония и беды."
  },
  {
    "chapter": 52,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Гибель царит в нем;"
  },
  {
    "chapter": 52,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "обман и коварство с"
  },
  {
    "chapter": 52,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "улиц его не уходят."
  },
  {
    "chapter": 52,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Если бы враг"
  },
  {
    "chapter": 52,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "меня оскорблял,"
  },
  {
    "chapter": 52,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "я перенес бы."
  },
  {
    "chapter": 52,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Если бы ненавистник"
  },
  {
    "chapter": 52,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "предо мной кичился,"
  },
  {
    "chapter": 52,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "я бы мог от него укрыться."
  },
  {
    "chapter": 52,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но это же ты,"
  },
  {
    "chapter": 52,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "мой равный,"
  },
  {
    "chapter": 52,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "мой товарищ,"
  },
  {
    "chapter": 52,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "мой близкий друг,"
  },
  {
    "chapter": 52,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "с кем вели мы"
  },
  {
    "chapter": 52,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "искренний разговор"
  },
  {
    "chapter": 52,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и вместе ходили в дом Божий"
  },
  {
    "chapter": 52,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "с толпой богомольцев."
  },
  {
    "chapter": 52,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть придет к"
  },
  {
    "chapter": 52,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "врагам моим смерть;"
  },
  {
    "chapter": 52,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "пусть сойдут они"
  },
  {
    "chapter": 52,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "живыми в мир мертвых,"
  },
  {
    "chapter": 52,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "из-за зла в их"
  },
  {
    "chapter": 52,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "домах и сердцах."
  },
  {
    "chapter": 52,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но я воззову к Богу,"
  },
  {
    "chapter": 52,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и Господь спасет меня."
  },
  {
    "chapter": 52,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Вечером,"
  },
  {
    "chapter": 52,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "утром и в полдень"
  },
  {
    "chapter": 52,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "я сетую и кричу,"
  },
  {
    "chapter": 52,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и голос мой Он услышит."
  },
  {
    "chapter": 52,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он вернет меня"
  },
  {
    "chapter": 52,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "невредимым из сражений,"
  },
  {
    "chapter": 52,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "которые я веду"
  },
  {
    "chapter": 52,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "против стольких врагов,"
  },
  {
    "chapter": 52,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "ополчившихся против меня."
  },
  {
    "chapter": 52,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Бог,"
  },
  {
    "chapter": 52,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "на престоле"
  },
  {
    "chapter": 52,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "сидящий от века,"
  },
  {
    "chapter": 52,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "услышит меня и смирит их,"
  },
  {
    "chapter": 52,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Пауза потому что они не"
  },
  {
    "chapter": 52,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "хотят измениться"
  },
  {
    "chapter": 52,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "и не боятся Бога."
  },
  {
    "chapter": 52,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Мой товарищ поднял"
  },
  {
    "chapter": 52,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "на своих друзей руку;"
  },
  {
    "chapter": 52,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "он нарушил свой союз."
  },
  {
    "chapter": 52,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Речь его мягче масла,"
  },
  {
    "chapter": 52,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "но в сердце его война;"
  },
  {
    "chapter": 52,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "слова нежнее"
  },
  {
    "chapter": 52,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "масла оливкового,"
  },
  {
    "chapter": 52,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "но они обнаженные мечи."
  },
  {
    "chapter": 52,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Возложи на"
  },
  {
    "chapter": 52,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господа свою заботу,"
  },
  {
    "chapter": 52,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и Он тебя подкрепит;"
  },
  {
    "chapter": 52,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "никогда Он не даст"
  },
  {
    "chapter": 52,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "поколебаться праведнику."
  },
  {
    "chapter": 52,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но нечестивых Ты,"
  },
  {
    "chapter": 52,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Боже,"
  },
  {
    "chapter": 52,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "повергнешь в"
  },
  {
    "chapter": 52,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "пропасть погибели."
  },
  {
    "chapter": 52,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Кровожадные"
  },
  {
    "chapter": 52,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "и коварные не проживут"
  },
  {
    "chapter": 52,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "и половины своих дней."
  },
  {
    "chapter": 52,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 8,
    "text": "А я на Тебя полагаюсь."
  }
]