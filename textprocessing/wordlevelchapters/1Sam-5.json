    "chapter": 5,
    "verse": 11,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 11,
    "text": "был очень страшен."
  },
  {
    "chapter": 5,
    "verse": 12,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 1,
    "text": "Те,"
  },
  {
    "chapter": 5,
    "verse": 12,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 2,
