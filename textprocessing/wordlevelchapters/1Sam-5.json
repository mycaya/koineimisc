[
  {
    "chapter": 5,
    "verse": 1,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 1,
    "text": "Захватив ковчег Бога,"
  },
  {
    "chapter": 5,
    "verse": 1,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 2,
    "text": "филистимляне принесли"
  },
  {
    "chapter": 5,
    "verse": 1,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 3,
    "text": "его из Авен-Езера в Азот."
  },
  {
    "chapter": 5,
    "verse": 2,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 1,
    "text": "Затем они внесли ковчег в"
  },
  {
    "chapter": 5,
    "verse": 2,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 2,
    "text": "храм Дагона"
  },
  {
    "chapter": 5,
    "verse": 2,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 3,
    "text": "и поставили около Дагона."
  },
  {
    "chapter": 5,
    "verse": 3,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 1,
    "text": "Когда жители Азота встали"
  },
  {
    "chapter": 5,
    "verse": 3,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 2,
    "text": "на другой день рано утром,"
  },
  {
    "chapter": 5,
    "verse": 3,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 3,
    "text": "Дагон,"
  },
  {
    "chapter": 5,
    "verse": 3,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 4,
    "text": "упавший лицом на землю,"
  },
  {
    "chapter": 5,
    "verse": 3,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 5,
    "text": "лежал перед ковчегом"
  },
  {
    "chapter": 5,
    "verse": 3,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 6,
    "text": "Господа! Они подняли Дагона"
  },
  {
    "chapter": 5,
    "verse": 3,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 7,
    "text": "и вернули на прежнее место."
  },
  {
    "chapter": 5,
    "verse": 4,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 1,
    "text": "Но на следующее утро,"
  },
  {
    "chapter": 5,
    "verse": 4,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 2,
    "text": "когда они встали,"
  },
  {
    "chapter": 5,
    "verse": 4,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 3,
    "text": "Дагон опять"
  },
  {
    "chapter": 5,
    "verse": 4,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 4,
    "text": "лежал лицом на земле,"
  },
  {
    "chapter": 5,
    "verse": 4,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 5,
    "text": "перед ковчегом Господа! Его"
  },
  {
    "chapter": 5,
    "verse": 4,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 6,
    "text": "голова и руки были отломаны"
  },
  {
    "chapter": 5,
    "verse": 4,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 7,
    "text": "и валялись на пороге,"
  },
  {
    "chapter": 5,
    "verse": 4,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 8,
    "text": "целым осталось"
  },
  {
    "chapter": 5,
    "verse": 4,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 9,
    "text": "лишь туловище."
  },
  {
    "chapter": 5,
    "verse": 5,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 1,
    "text": "Вот почему до сегодняшнего"
  },
  {
    "chapter": 5,
    "verse": 5,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 2,
    "text": "дня ни священники Дагона,"
  },
  {
    "chapter": 5,
    "verse": 5,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 3,
    "text": "ни входящие в его храм в"
  },
  {
    "chapter": 5,
    "verse": 5,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 4,
    "text": "Азоте не наступают на порог,"
  },
  {
    "chapter": 5,
    "verse": 5,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 5,
    "text": "где он лежал."
  },
  {
    "chapter": 5,
    "verse": 6,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 1,
    "text": "Господь сурово наказал"
  },
  {
    "chapter": 5,
    "verse": 6,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 2,
    "text": "жителей Азота"
  },
  {
    "chapter": 5,
    "verse": 6,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 3,
    "text": "и его окрестностей,"
  },
  {
    "chapter": 5,
    "verse": 6,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 4,
    "text": "наслав на них"
  },
  {
    "chapter": 5,
    "verse": 6,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 5,
    "text": "опустошение и поразив их наростами."
  },
  {
    "chapter": 5,
    "verse": 7,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 1,
    "text": "Когда жители"
  },
  {
    "chapter": 5,
    "verse": 7,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 2,
    "text": "Азота увидели то,"
  },
  {
    "chapter": 5,
    "verse": 7,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 3,
    "text": "что происходит,"
  },
  {
    "chapter": 5,
    "verse": 7,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 4,
    "text": "они сказали:"
  },
  {
    "chapter": 5,
    "verse": 7,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 5,
    "text": "Ковчег"
  },
  {
    "chapter": 5,
    "verse": 7,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 6,
    "text": "израильского Бога не должен"
  },
  {
    "chapter": 5,
    "verse": 7,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 7,
    "text": "находиться здесь у нас,"
  },
  {
    "chapter": 5,
    "verse": 7,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 8,
    "text": "потому что гнев Его против"
  },
  {
    "chapter": 5,
    "verse": 7,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 9,
    "text": "нас"
  },
