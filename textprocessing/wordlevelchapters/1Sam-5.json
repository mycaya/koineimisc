    "chapter": 5,
    "verse": 8,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 7,
    "text": "Пусть ковчег израильского"
  },
  {
    "chapter": 5,
    "verse": 8,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 8,
    "text": "Бога переправится в Геф."
  },
  {
    "chapter": 5,
    "verse": 8,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 9,
    "text": "И переправили"
  },
  {
    "chapter": 5,
    "verse": 8,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 10,
    "text": "ковчег Бога Израиля."
  },
  {
    "chapter": 5,
    "verse": 9,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 1,
    "text": "Но после того,"
  },
  {
    "chapter": 5,
    "verse": 9,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 2,
