    "chapter": 5,
    "verse": 10,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 5,
    "text": "жители кричали:"
  },
  {
    "chapter": 5,
    "verse": 10,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 6,
    "text": "Ковчег израильского"
  },
