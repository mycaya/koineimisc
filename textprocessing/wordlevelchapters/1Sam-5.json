    "chapter": 5,
    "verse": 9,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 8,
    "text": "молодых и старых,"
  },
  {
    "chapter": 5,
    "verse": 9,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 9,
    "text": "наростами."
  },
