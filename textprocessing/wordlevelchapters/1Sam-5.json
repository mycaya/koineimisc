    "chapter": 5,
    "verse": 10,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 7,
    "text": "Бога принесли к нам,"
  },
  {
    "chapter": 5,
    "verse": 10,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
