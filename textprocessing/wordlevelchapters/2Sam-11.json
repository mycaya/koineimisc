    "chapter": 11,
    "verse": 8,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "царского дворца,"
  },
  {
    "chapter": 11,
    "verse": 8,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "а вслед за ним"
  },
  {
    "chapter": 11,
    "verse": 8,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 6,
    "text": "понесли царский подарок."
  },
  {
    "chapter": 11,
    "verse": 9,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
