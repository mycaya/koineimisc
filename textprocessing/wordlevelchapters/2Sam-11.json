    "chapter": 11,
    "verse": 3,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "дочь Елиама и"
  },
  {
    "chapter": 11,
    "verse": 3,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 6,
    "text": "жена хетта Урии"
