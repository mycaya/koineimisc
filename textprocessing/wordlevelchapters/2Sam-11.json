    "verse": 5,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "и послала сказать Давиду:"
  },
  {
    "chapter": 11,
    "verse": 5,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "Я беременна."
  },
  {
    "chapter": 11,
    "verse": 6,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Тогда Давид"
  },
