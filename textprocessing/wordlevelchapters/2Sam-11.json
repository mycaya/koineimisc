    "verse": 13,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 6,
    "text": "циновке со слугами своего"
  },
  {
    "chapter": 11,
    "verse": 13,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 7,
    "text": "господина;"
  },
