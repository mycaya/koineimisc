    "chapter": 11,
    "verse": 3,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "Посланный сказал:"
  },
  {
    "chapter": 11,
    "verse": 3,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "Это Вирсавия,"
  },
