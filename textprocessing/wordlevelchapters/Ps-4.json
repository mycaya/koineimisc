[
  {
    "chapter": 4,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Дирижеру хора."
  },
  {
    "chapter": 4,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Для свирелей."
  },
  {
    "chapter": 4,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Псалом Давида."
  },
  {
    "chapter": 4,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Услышь слова мои,"
  },
  {
    "chapter": 4,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 4,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "пойми стенания мои!"
  },
  {
    "chapter": 4,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Внемли крику о помощи,"
  },
  {
    "chapter": 4,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "мой Царь и мой Бог,"
  },
  {
    "chapter": 4,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "ведь я молюсь Тебе."
  },
  {
    "chapter": 4,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Утром,"
  },
  {
    "chapter": 4,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 4,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Ты услышишь мой голос,"
  },
  {
    "chapter": 4,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "утром предстану с мольбой"
  },
  {
    "chapter": 4,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "пред Тобою"
  },
  {
    "chapter": 4,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "и замру в ожидании."
  },
  {
    "chapter": 4,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты не Бог,"
  },
  {
    "chapter": 4,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "желающий беззакония,"
  },
  {
    "chapter": 4,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "зло не сможет с"
  },
  {
    "chapter": 4,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Тобой обитать."
  },
  {
    "chapter": 4,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Надменные не"
  },
  {
    "chapter": 4,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "устоят перед Тобой,"
  },
  {
    "chapter": 4,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "всех делающих"
  },
  {
    "chapter": 4,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "зло Ты ненавидишь."
  },
  {
    "chapter": 4,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты погубишь говорящих ложь;"
  },
  {
    "chapter": 4,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "кровожадных и"
  },
  {
    "chapter": 4,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "коварных гнушается Господь."
  },
  {
    "chapter": 4,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но я,"
  },
  {
    "chapter": 4,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "по великой милости Твоей,"
  },
  {
    "chapter": 4,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "войду в дом Твой,"
  },
  {
    "chapter": 4,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "поклонюсь в святом храме"
  },
  {
    "chapter": 4,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Твоем в страхе перед Тобой."
  },
  {
    "chapter": 4,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Веди меня,"
  },
  {
    "chapter": 4,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 4,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "в праведности Твоей"
  },
  {
    "chapter": 4,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "из-за моих врагов;"
  },
  {
    "chapter": 4,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "путь Твой передо"
  },
  {
    "chapter": 4,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "мной сделай прямым."
  },
  {
    "chapter": 4,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ведь ни слову из"
  },
  {
    "chapter": 4,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "уст их верить нельзя,"
  },
  {
    "chapter": 4,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "сердце их исполнено пагубы."
  },
  {
    "chapter": 4,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Гортань их открытая могила,"
  },
  {
    "chapter": 4,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "языком своим они льстят."
  },
  {
    "chapter": 4,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Осуди их,"
  },
  {
    "chapter": 4,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Боже! Пусть падут"
  },
  {
    "chapter": 4,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "из-за козней своих."
  },
  {
    "chapter": 4,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Из-за многих"
  },
  {
    "chapter": 4,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "грехов их отвергни их,"
  },
  {
    "chapter": 4,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "ведь они против"
  },
  {
    "chapter": 4,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "Тебя восстали."
  },
  {
    "chapter": 4,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но пусть возрадуются все,"
  },
  {
    "chapter": 4,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "кто в Тебе ищет прибежища;"
  },
  {
    "chapter": 4,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "пусть вечно"
  },
  {
    "chapter": 4,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "поют от радости."
  },
  {
    "chapter": 4,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Окажи им Свое"
  },
  {
    "chapter": 4,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "покровительство,"
  },
  {
    "chapter": 4,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "чтобы возрадовались в"
  },
  {
    "chapter": 4,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 8,
    "text": "Тебе любящие Твое Имя."
  },
  {
    "chapter": 4,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ведь Ты"
  },
  {
    "chapter": 4,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "благословишь праведника,"
  },
  {
    "chapter": 4,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Господи,"
  },
  {
    "chapter": 4,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "как щитом,"
  },
  {
    "chapter": 4,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "окружаешь его"
  },
  {
    "chapter": 4,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "благосклонностью."
  }
]