[
  {
    "chapter": 45,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Песня."
  },
  {
    "chapter": 45,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Псалом потомков Корея."
  },
  {
    "chapter": 45,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Велик Господь"
  },
  {
    "chapter": 45,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и достоин всякой хвалы в"
  },
  {
    "chapter": 45,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "городе нашего Бога,"
  },
  {
    "chapter": 45,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "на Его святой горе."
  },
  {
    "chapter": 45,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Высока она и"
  },
  {
    "chapter": 45,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "прекрасна радость всей земли,"
  },
  {
    "chapter": 45,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "гора Сион на"
  },
  {
    "chapter": 45,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "окраинах севера,"
  },
  {
    "chapter": 45,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "это город великого Царя."
  },
  {
    "chapter": 45,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Бог пребывает в ее дворцах,"
  },
  {
    "chapter": 45,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "известен как твердыня."
  },
  {
    "chapter": 45,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Собрались цари и"
  },
  {
    "chapter": 45,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "вместе пошли войной,"
  },
  {
    "chapter": 45,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "но увидев Сион,"
  },
  {
    "chapter": 45,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "поразились;"
  },
  {
    "chapter": 45,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "ужаснулись и"
  },
  {
    "chapter": 45,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "обратились в бегство."
  },
  {
    "chapter": 45,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Объяла их дрожь,"
  },
  {
    "chapter": 45,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "муки,"
  },
  {
    "chapter": 45,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "как женщину в родах."
  },
  {
    "chapter": 45,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты сокрушил их,"
  },
  {
    "chapter": 45,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "как ветер с востока"
  },
  {
    "chapter": 45,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "разбивает фарсисские корабли."
  },
  {
    "chapter": 45,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Что мы слышали,"
  },
  {
    "chapter": 45,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "то и увидели в"
  },
  {
    "chapter": 45,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "городе Господа сил,"
  },
  {
    "chapter": 45,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "в городе нашего Бога:"
  },
  {
    "chapter": 45,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Бог хранит его"
  },
  {
    "chapter": 45,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "нерушимым навеки."
  },
  {
    "chapter": 45,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "Пауза"
  },
  {
    "chapter": 45,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "В храме Твоем,"
  },
  {
    "chapter": 45,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Боже,"
  },
  {
    "chapter": 45,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "мы размышляли о"
  },
  {
    "chapter": 45,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Твоей милости."
  },
  {
    "chapter": 45,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Как Имя Твое,"
  },
  {
    "chapter": 45,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Боже,"
  },
  {
    "chapter": 45,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "так и хвала Тебе"
  },
  {
    "chapter": 45,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "достигает краев земли."
  },
  {
    "chapter": 45,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Правая рука Твоя"
  },
  {
    "chapter": 45,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "праведности полна."
  },
  {
    "chapter": 45,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Торжествует гора Сион,"
  },
  {
    "chapter": 45,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и ликуют поселения"
  },
  {
    "chapter": 45,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Иудеи о Твоем правосудии."
  },
  {
    "chapter": 45,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пройдите вокруг Сиона,"
  },
  {
    "chapter": 45,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "обойдите его,"
  },
  {
    "chapter": 45,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "башни его сосчитайте;"
  },
  {
    "chapter": 45,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "обратите мысли к"
  },
  {
    "chapter": 45,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "его укреплениям,"
  },
  {
    "chapter": 45,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "рассмотрите дворцы его,"
  },
  {
    "chapter": 45,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "чтобы поколению"
  },
  {
    "chapter": 45,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "грядущему рассказать."
  },
  {
    "chapter": 45,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ведь Бог навеки наш Бог,"
  },
  {
    "chapter": 45,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Он будет нашим"
  },
  {
    "chapter": 45,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "вождем на века."
  }
]