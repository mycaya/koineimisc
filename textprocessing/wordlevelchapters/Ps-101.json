[
  {
    "chapter": 101,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Аллилуйя! Воздайте хвалу"
  },
  {
    "chapter": 101,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господу потому что Он благ"
  },
  {
    "chapter": 101,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и милость Его вечна."
  },
  {
    "chapter": 101,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Кто выразит могущество"
  },
  {
    "chapter": 101,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господа"
  },
  {
    "chapter": 101,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и возвестит всю Его славу?"
  },
  {
    "chapter": 101,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Блаженны те,"
  },
  {
    "chapter": 101,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "кто хранит правосудие"
  },
  {
    "chapter": 101,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и вершит праведные дела во"
  },
  {
    "chapter": 101,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "все времена!"
  },
  {
    "chapter": 101,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Вспомни меня,"
  },
  {
    "chapter": 101,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 101,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "во время"
  },
  {
    "chapter": 101,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "благоволения к Своему народу,"
  },
  {
    "chapter": 101,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "помоги и мне,"
  },
  {
    "chapter": 101,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "когда будешь спасать их,"
  },
  {
    "chapter": 101,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "чтобы я увидел"
  },
  {
    "chapter": 101,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "благополучие Твоих избранных,"
  },
  {
    "chapter": 101,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "возвеселился вместе с Твоим"
  },
  {
    "chapter": 101,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "народом"
  },
  {
    "chapter": 101,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "и хвалился с Твоим"
  },
  {
    "chapter": 101,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "наследием."
  },
  {
    "chapter": 101,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Мы согрешили,"
  },
  {
    "chapter": 101,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "как и наши предки,"
  },
  {
    "chapter": 101,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "совершили беззаконие,"
  },
  {
    "chapter": 101,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "поступили нечестиво."
  },
  {
    "chapter": 101,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Отцы наши не поняли"
  },
  {
    "chapter": 101,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Твоих чудес в Египте,"
  },
  {
    "chapter": 101,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "забыли обилие Твоей"
  },
  {
    "chapter": 101,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "милости и возмутились у моря,"
  },
  {
    "chapter": 101,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "у Красного моря."
  },
  {
    "chapter": 101,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он все же спас их"
  },
  {
    "chapter": 101,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "ради Своего Имени,"
  },
  {
    "chapter": 101,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "чтобы показать"
  },
  {
    "chapter": 101,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Свое могущество."
  },
  {
    "chapter": 101,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он приказал Красному морю,"
  },
  {
    "chapter": 101,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и оно высохло,"
  },
  {
    "chapter": 101,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и провел Он их"
  },
  {
    "chapter": 101,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "через его глубины,"
  },
  {
    "chapter": 101,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "как по пустыне."
  },
  {
    "chapter": 101,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Спас их от рук"
  },
  {
    "chapter": 101,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "ненавидящего их,"
  },
  {
    "chapter": 101,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "избавил их от руки врага."
  },
  {
    "chapter": 101,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Воды покрыли"
  },
  {
    "chapter": 101,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "противников их,"
  },
  {
    "chapter": 101,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "не осталось ни одного."
  },
  {
    "chapter": 101,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Тогда поверили они Его"
  },
  {
    "chapter": 101,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "словам и воспели Ему хвалу."
  },
  {
    "chapter": 101,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но вскоре забыли Его дела,"
  },
  {
    "chapter": 101,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "не ждали Его совета."
  },
  {
    "chapter": 101,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Возгорелись страстным"
  },
  {
    "chapter": 101,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "желанием в пустыне и"
  },
  {
    "chapter": 101,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "испытывали Бога в"
  },
  {
    "chapter": 101,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "необитаемой местности."
  },
  {
    "chapter": 101,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он дал им желаемое,"
  },
  {
    "chapter": 101,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "но послал"
  },
  {
    "chapter": 101,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "истощение в их души."
  },
  {
    "chapter": 101,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Они позавидовали"
  },
  {
    "chapter": 101,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Моисею в лагере и Аарону,"
  },
  {
    "chapter": 101,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "святому Господню."
  },
  {
    "chapter": 101,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Земля разверзлась"
  },
  {
    "chapter": 101,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и поглотила Дафана"
  },
  {
    "chapter": 101,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и все скопище Авирона."
  },
  {
    "chapter": 101,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Возгорелся"
  },
  {
    "chapter": 101,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "огонь посреди них,"
  },
  {
    "chapter": 101,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и пламя сожгло нечестивых."
  },
  {
    "chapter": 101,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Они сделали"
  },
  {
    "chapter": 101,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "изваяние быка в Хориве"
  },
  {
    "chapter": 101,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и поклонились истукану,"
  },
  {
    "chapter": 101,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "променяли свою Славу"
  },
  {
    "chapter": 101,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "на изображение быка,"
  },
  {
    "chapter": 101,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "питающегося травой."
  },
  {
    "chapter": 101,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Забыли Бога,"
  },
  {
    "chapter": 101,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "своего Спасителя,"
  },
  {
    "chapter": 101,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "сотворившего"
  },
  {
    "chapter": 101,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "великие дела в Египте,"
  },
  {
    "chapter": 101,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "чудеса в земле Хама"
  },
  {
    "chapter": 101,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и устрашающие дела у"
  },
  {
    "chapter": 101,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Красного моря."
  },
  {
    "chapter": 101,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Поэтому Он сказал,"
  },
  {
    "chapter": 101,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "что погубил бы их,"
  },
  {
    "chapter": 101,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "если бы Моисей,"
  },
  {
    "chapter": 101,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "избранный Его,"
  },
  {
    "chapter": 101,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "не встал перед"
  },
  {
    "chapter": 101,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Ним в расселине,"
  },
  {
    "chapter": 101,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "чтобы отвратить Его ярость,"
  },
  {
    "chapter": 101,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 8,
    "text": "чтобы Он не погубил их."
  },
  {
    "chapter": 101,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "И пренебрегли они"
  },
  {
    "chapter": 101,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "землей желанной,"
  },
  {
    "chapter": 101,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "не поверили Его обещанию,"
  },
  {
    "chapter": 101,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "роптали в своих шатрах и"
  },
  {
    "chapter": 101,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "не слушались голоса Господа."
  },
  {
    "chapter": 101,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Поэтому Он"
  },
  {
    "chapter": 101,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "поклялся с поднятой рукой,"
  },
  {
    "chapter": 101,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "что поразит их в пустыне,"
  },
  {
    "chapter": 101,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "а также их"
  },
  {
    "chapter": 101,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "потомков среди народов,"
  },
  {
    "chapter": 101,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и рассеет их по землям."
  },
  {
    "chapter": 101,
    "verse": 28,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Они присоединились к"
  },
  {
    "chapter": 101,
    "verse": 28,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Ваал-Фегору и ели жертвы,"
  },
  {
    "chapter": 101,
    "verse": 28,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "принесенные бездушным."
  },
  {
    "chapter": 101,
    "verse": 29,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Раздражали Его"
  },
  {
    "chapter": 101,
    "verse": 29,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "своими делами,"
  },
  {
    "chapter": 101,
    "verse": 29,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и разразился среди них мор."
  },
  {
    "chapter": 101,
    "verse": 30,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но поднялся Финеес,"
  },
  {
    "chapter": 101,
    "verse": 30,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "произвел суд,"
  },
  {
    "chapter": 101,
    "verse": 30,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и мор прекратился."
  },
  {
    "chapter": 101,
    "verse": 31,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Это вменилось в праведность"
  },
  {
    "chapter": 101,
    "verse": 31,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "ему"
  },
  {
    "chapter": 101,
    "verse": 31,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и всем его поколениям"
  },
  {
    "chapter": 101,
    "verse": 31,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "навсегда."
  },
  {
    "chapter": 101,
    "verse": 32,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Они прогневали"
  },
  {
    "chapter": 101,
    "verse": 32,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Его у вод Меривы,"
  },
  {
    "chapter": 101,
    "verse": 32,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и Моисей был"
  },
  {
    "chapter": 101,
    "verse": 32,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "наказан из-за них,"
  },
  {
    "chapter": 101,
    "verse": 33,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "потому что они"
  },
  {
    "chapter": 101,
    "verse": 33,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "возмутили его дух,"
  },
  {
    "chapter": 101,
    "verse": 33,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и он погрешил"
  },
  {
    "chapter": 101,
    "verse": 33,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "своими устами."
  },
  {
    "chapter": 101,
    "verse": 34,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Не уничтожили они народы,"
  },
  {
    "chapter": 101,
    "verse": 34,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "о которых"
  },
  {
    "chapter": 101,
    "verse": 34,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "сказал им Господь,"
  },
  {
    "chapter": 101,
    "verse": 35,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "а смешались с"
  },
  {
    "chapter": 101,
    "verse": 35,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "язычниками и научились их делам;"
  },
  {
    "chapter": 101,
    "verse": 36,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "служили их идолам,"
  },
  {
    "chapter": 101,
    "verse": 36,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "которые стали"
  },
  {
    "chapter": 101,
    "verse": 36,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "для них сетью."
  },
  {
    "chapter": 101,
    "verse": 37,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Приносили демонам в"
  },
  {
    "chapter": 101,
    "verse": 37,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "жертву своих сыновей и дочерей;"
  },
  {
    "chapter": 101,
    "verse": 38,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "проливали невинную кровь,"
  },
  {
    "chapter": 101,
    "verse": 38,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "кровь своих"
  },
  {
    "chapter": 101,
    "verse": 38,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "сыновей и дочерей,"
  },
  {
    "chapter": 101,
    "verse": 38,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "которых жертвовали"
  },
  {
    "chapter": 101,
    "verse": 38,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "ханаанским идолам,"
  },
  {
    "chapter": 101,
    "verse": 38,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "и земля"
  },
  {
    "chapter": 101,
    "verse": 38,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "осквернилась кровью."
  },
  {
    "chapter": 101,
    "verse": 39,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Они оскверняли"
  },
  {
    "chapter": 101,
    "verse": 39,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "себя своими делами,"
  },
  {
    "chapter": 101,
    "verse": 39,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "прелюбодействовали"
  },
  {
    "chapter": 101,
    "verse": 39,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "своими поступками."
  },
  {
    "chapter": 101,
    "verse": 40,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Поэтому возгорелся гнев"
  },
  {
    "chapter": 101,
    "verse": 40,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господень на Свой народ,"
  },
  {
    "chapter": 101,
    "verse": 40,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и возненавидел"
  },
  {
    "chapter": 101,
    "verse": 40,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Он Свое наследие."
  },
  {
    "chapter": 101,
    "verse": 41,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он отдал их в"
  },
  {
    "chapter": 101,
    "verse": 41,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "руки чужеземцев,"
  },
  {
    "chapter": 101,
    "verse": 41,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и ненавидящие Израиль"
  },
  {
    "chapter": 101,
    "verse": 41,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "властвовали над ними."
  },
  {
    "chapter": 101,
    "verse": 42,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Враги притесняли их,"
  },
  {
    "chapter": 101,
    "verse": 42,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и они"
  },
  {
    "chapter": 101,
    "verse": 42,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "смирились под их рукой."
  },
  {
    "chapter": 101,
    "verse": 43,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Много раз Он избавлял их,"
  },
  {
    "chapter": 101,
    "verse": 43,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "но они гневили Его своим"
  },
  {
    "chapter": 101,
    "verse": 43,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "упрямством"
  },
  {
    "chapter": 101,
    "verse": 43,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и были унижены в своем"
  },
  {
    "chapter": 101,
    "verse": 43,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "беззаконии."
  },
  {
    "chapter": 101,
    "verse": 44,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Все же Он обращал"
  },
  {
    "chapter": 101,
    "verse": 44,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "внимание на их скорбь,"
  },
  {
    "chapter": 101,
    "verse": 44,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "когда слышал их вопль,"
  },
  {
    "chapter": 101,
    "verse": 45,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "вспоминал о Своем завете с"
  },
  {
    "chapter": 101,
    "verse": 45,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "ними"
  },
  {
    "chapter": 101,
    "verse": 45,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и смягчался по Своей"
  },
  {
    "chapter": 101,
    "verse": 45,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "великой милости."
  },
  {
    "chapter": 101,
    "verse": 46,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он вызывал к ним"
  },
  {
    "chapter": 101,
    "verse": 46,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "сострадание со стороны всех,"
  },
  {
    "chapter": 101,
    "verse": 46,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "кто пленял их."
  },
  {
    "chapter": 101,
    "verse": 47,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Спаси нас,"
  },
  {
    "chapter": 101,
    "verse": 47,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 101,
    "verse": 47,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "наш Бог,"
  },
  {
    "chapter": 101,
    "verse": 47,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и собери нас из"
  },
  {
    "chapter": 101,
    "verse": 47,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "среды народов,"
  },
  {
    "chapter": 101,
    "verse": 47,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "чтобы мы воздали"
  },
  {
    "chapter": 101,
    "verse": 47,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "благодарность Твоему"
  },
  {
    "chapter": 101,
    "verse": 47,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 8,
    "text": "святому Имени"
  },
  {
    "chapter": 101,
    "verse": 47,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 9,
    "text": "и хвалились Твоей славой."
  },
  {
    "chapter": 101,
    "verse": 48,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Прославлен будь,"
  },
  {
    "chapter": 101,
    "verse": 48,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господь,"
  },
  {
    "chapter": 101,
    "verse": 48,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Бог Израиля,"
  },
  {
    "chapter": 101,
    "verse": 48,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "от века до века! И"
  },
  {
    "chapter": 101,
    "verse": 48,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "весь народ пусть скажет:"
  },
  {
    "chapter": 101,
    "verse": 48,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Аминь! Аллилуйя!"
  },
  {
    "chapter": 101,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Благодарите Господа,"
  },
  {
    "chapter": 101,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "потому что Он благ,"
  },
  {
    "chapter": 101,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и милость Его навеки!"
  },
  {
    "chapter": 101,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть скажут это"
  },
  {
    "chapter": 101,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "избавленные Им,"
  },
  {
    "chapter": 101,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "те,"
  },
  {
    "chapter": 101,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "кого Он"
  },
  {
    "chapter": 101,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "избавил от руки врага"
  },
  {
    "chapter": 101,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "и собрал из разных"
  },
  {
    "chapter": 101,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "земель с востока,"
  },
  {
    "chapter": 101,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "с запада,"
  },
  {
    "chapter": 101,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "с севера и с юга."
  },
  {
    "chapter": 101,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Блуждали они в пустыне по"
  },
  {
    "chapter": 101,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "безлюдным дорогам"
  },
  {
    "chapter": 101,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и не нашли города,"
  },
  {
    "chapter": 101,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "в котором могли"
  },
  {
    "chapter": 101,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "бы поселиться."
  },
  {
    "chapter": 101,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Они голодали и жаждали,"
  },
  {
    "chapter": 101,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и душа их томилась в них."
  },
  {
    "chapter": 101,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но воззвали они к"
  },
  {
    "chapter": 101,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господу в своем несчастье,"
  },
  {
    "chapter": 101,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и Он освободил"
  },
  {
    "chapter": 101,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "их от бедствий."
  },
  {
    "chapter": 101,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Повел их прямым"
  },
  {
    "chapter": 101,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "путем в город,"
  },
  {
    "chapter": 101,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "где они могли поселиться."
  },
  {
    "chapter": 101,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть славят Господа за Его"
  },
  {
    "chapter": 101,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "милость"
  },
  {
    "chapter": 101,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и за Его чудесные дела для"
  },
  {
    "chapter": 101,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "людей,"
  },
  {
    "chapter": 101,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "ведь Он утолил душу"
  },
  {
    "chapter": 101,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "жаждущую"
  },
  {
    "chapter": 101,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и душу голодную насытил"
  },
  {
    "chapter": 101,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "благами."
  },
  {
    "chapter": 101,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Они сидели во"
  },
  {
    "chapter": 101,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "тьме и в тени смерти,"
  },
  {
    "chapter": 101,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "окованные"
  },
  {
    "chapter": 101,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "бедствием и железом,"
  },
  {
    "chapter": 101,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "потому что восстали против"
  },
  {
    "chapter": 101,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "слов Божьих"
  },
  {
    "chapter": 101,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и пренебрегли советом"
  },
  {
    "chapter": 101,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Всевышнего."
  },
  {
    "chapter": 101,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Поэтому Он смирил их"
  },
  {
    "chapter": 101,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "сердце тяжелым трудом;"
  },
  {
    "chapter": 101,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "они падали,"
  },
  {
    "chapter": 101,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и некому было помочь."
  },
  {
    "chapter": 101,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Тогда воззвали они к"
  },
  {
    "chapter": 101,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господу в несчастье своем,"
  },
  {
    "chapter": 101,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и Он спас их от бедствий."
  },
  {
    "chapter": 101,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Вывел их из"
  },
  {
    "chapter": 101,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "темноты и из тени смерти,"
  },
  {
    "chapter": 101,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "сломав их оковы."
  },
  {
    "chapter": 101,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть славят Господа за Его"
  },
  {
    "chapter": 101,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "милость"
  },
  {
    "chapter": 101,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и за Его чудесные дела для"
  },
  {
    "chapter": 101,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "людей,"
  },
  {
    "chapter": 101,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "ведь Он сокрушил бронзовые"
  },
  {
    "chapter": 101,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "ворота"
  },
  {
    "chapter": 101,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и сломал железные засовы."
  },
  {
    "chapter": 101,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Безрассудные страдали за"
  },
  {
    "chapter": 101,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "свои грехи"
  },
  {
    "chapter": 101,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и за свое беззаконие."
  },
  {
    "chapter": 101,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "От всякой пищи"
  },
  {
    "chapter": 101,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "отвращалась их душа,"
  },
  {
    "chapter": 101,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и они приближались"
  },
  {
    "chapter": 101,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "к воротам смерти."
  },
  {
    "chapter": 101,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Тогда воззвали они к"
  },
  {
    "chapter": 101,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господу в своем несчастье,"
  },
  {
    "chapter": 101,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и Он спас их от бедствий."
  },
  {
    "chapter": 101,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Послал Свое"
  },
  {
    "chapter": 101,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "слово и излечил их,"
  },
  {
    "chapter": 101,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "избавил их от гибели."
  },
  {
    "chapter": 101,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть славят Господа за Его"
  },
  {
    "chapter": 101,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "милость"
  },
  {
    "chapter": 101,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и за Его чудесные дела для"
  },
  {
    "chapter": 101,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "сынов человеческих,"
  },
  {
    "chapter": 101,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "пусть приносят Ему жертвы"
  },
  {
    "chapter": 101,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "благодарения"
  },
  {
    "chapter": 101,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и возвещают о Его делах с"
  },
  {
    "chapter": 101,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "радостью."
  },
  {
    "chapter": 101,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ходящие на судах в море,"
  },
  {
    "chapter": 101,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "трудящиеся в больших водах"
  },
  {
    "chapter": 101,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "видели и они дела Господа,"
  },
  {
    "chapter": 101,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Его чудеса в глубинах."
  },
  {
    "chapter": 101,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он говорил,"
  },
  {
    "chapter": 101,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и восстал штормовой ветер,"
  },
  {
    "chapter": 101,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "поднимались высокие волны."
  },
  {
    "chapter": 101,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Корабли восходили до"
  },
  {
    "chapter": 101,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "небес и низвергались в бездну;"
  },
  {
    "chapter": 101,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "душа моряков"
  },
  {
    "chapter": 101,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "таяла в бедствии."
  },
  {
    "chapter": 101,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Они кружились и шатались,"
  },
  {
    "chapter": 101,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "как пьяные,"
  },
  {
    "chapter": 101,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и вся мудрость их исчезла."
  },
  {
    "chapter": 101,
    "verse": 28,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но воззвали они к"
  },
  {
    "chapter": 101,
    "verse": 28,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господу в своем несчастье,"
  },
  {
    "chapter": 101,
    "verse": 28,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и Он вывел их из бедствий."
  },
  {
    "chapter": 101,
    "verse": 29,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он превратил бурю в штиль,"
  },
  {
    "chapter": 101,
    "verse": 29,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и умолкали морские волны."
  },
  {
    "chapter": 101,
    "verse": 30,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Обрадовались люди,"
  },
  {
    "chapter": 101,
    "verse": 30,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "что волны утихли,"
  },
  {
    "chapter": 101,
    "verse": 30,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и привел Он их к"
  },
  {
    "chapter": 101,
    "verse": 30,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "желаемой гавани."
  },
  {
    "chapter": 101,
    "verse": 31,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть славят Господа за Его"
  },
  {
    "chapter": 101,
    "verse": 31,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "милость"
  },
  {
    "chapter": 101,
    "verse": 31,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и за Его чудесные дела для"
  },
  {
    "chapter": 101,
    "verse": 31,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "людей;"
  },
  {
    "chapter": 101,
    "verse": 32,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "пусть превозносят Его в"
  },
  {
    "chapter": 101,
    "verse": 32,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "народном собрании"
  },
  {
    "chapter": 101,
    "verse": 32,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и хвалят Его в кругу"
  },
  {
    "chapter": 101,
    "verse": 32,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "старейшин."
  },
  {
    "chapter": 101,
    "verse": 33,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он превращает реки в"
  },
  {
    "chapter": 101,
    "verse": 33,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "пустыню и источники вод в сушу,"
  },
  {
    "chapter": 101,
    "verse": 34,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "плодородную землю в"
  },
  {
    "chapter": 101,
    "verse": 34,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "солончак за нечестие"
  },
  {
    "chapter": 101,
    "verse": 34,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "живущих на ней."
  },
  {
    "chapter": 101,
    "verse": 35,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "И Он превращает пустыню в"
  },
  {
    "chapter": 101,
    "verse": 35,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "озеро и иссохшую землю в"
  },
  {
    "chapter": 101,
    "verse": 35,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "источники вод."
  },
  {
    "chapter": 101,
    "verse": 36,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он поселяет в ней голодных,"
  },
  {
    "chapter": 101,
    "verse": 36,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "чтобы они построили"
  },
  {
    "chapter": 101,
    "verse": 36,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "город для обитания,"
  },
  {
    "chapter": 101,
    "verse": 37,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "засевают поля и"
  },
  {
    "chapter": 101,
    "verse": 37,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "насаждают виноградники,"
  },
  {
    "chapter": 101,
    "verse": 37,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "которые приносят"
  },
  {
    "chapter": 101,
    "verse": 37,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "обильные плоды."
  },
  {
    "chapter": 101,
    "verse": 38,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он благословляет их,"
  },
  {
    "chapter": 101,
    "verse": 38,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и они сильно размножаются;"
  },
  {
    "chapter": 101,
    "verse": 38,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "не позволяет Он их"
  },
  {
    "chapter": 101,
    "verse": 38,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "стадам уменьшаться."
  },
  {
    "chapter": 101,
    "verse": 39,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "И опять их стало меньше,"
  },
  {
    "chapter": 101,
    "verse": 39,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и они пали,"
  },
  {
    "chapter": 101,
    "verse": 39,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "страдая от угнетения,"
  },
  {
    "chapter": 101,
    "verse": 39,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "бедствия и скорби."
  },
  {
    "chapter": 101,
    "verse": 40,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он"
  },
  {
    "chapter": 101,
    "verse": 40,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "изливает презрение на"
  },
  {
    "chapter": 101,
    "verse": 40,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "вождей и оставляет"
  },
  {
    "chapter": 101,
    "verse": 40,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "их блуждающими в пустыне,"
  },
  {
    "chapter": 101,
    "verse": 40,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "где нет путей."
  },
  {
    "chapter": 101,
    "verse": 41,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Бедного же Он возвышает из"
  },
  {
    "chapter": 101,
    "verse": 41,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "нищеты и умножает его род,"
  },
  {
    "chapter": 101,
    "verse": 41,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "как стадо овец."
  },
  {
    "chapter": 101,
    "verse": 42,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Праведники видят"
  },
  {
    "chapter": 101,
    "verse": 42,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "это и радуются,"
  },
  {
    "chapter": 101,
    "verse": 42,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "а нечестивые"
  },
  {
    "chapter": 101,
    "verse": 42,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "закрывают свои уста."
  },
  {
    "chapter": 101,
    "verse": 43,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Кто мудр,"
  },
  {
    "chapter": 101,
    "verse": 43,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "да уразумеет все это и"
  },
  {
    "chapter": 101,
    "verse": 43,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "поймет милость Господа."
  }
]