[
  {
    "chapter": 140,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Аллилуйя! Восхваляй,"
  },
  {
    "chapter": 140,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "душа моя,"
  },
  {
    "chapter": 140,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Господа!"
  },
  {
    "chapter": 140,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Всю свою жизнь буду"
  },
  {
    "chapter": 140,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "восхвалять Господа;"
  },
  {
    "chapter": 140,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "буду воспевать"
  },
  {
    "chapter": 140,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "хвалу моему Богу,"
  },
  {
    "chapter": 140,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "пока я жив."
  },
  {
    "chapter": 140,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Не надейтесь на правителей,"
  },
  {
    "chapter": 140,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "на человека,"
  },
  {
    "chapter": 140,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "в котором нет спасения."
  },
  {
    "chapter": 140,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Когда его дух выходит,"
  },
  {
    "chapter": 140,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и он возвращается в землю,"
  },
  {
    "chapter": 140,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "в тот самый день"
  },
  {
    "chapter": 140,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "исчезают и все его помышления."
  },
  {
    "chapter": 140,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Блажен тот,"
  },
  {
    "chapter": 140,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "кому помощник Бог Иакова,"
  },
  {
    "chapter": 140,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "кто надеется на Господа,"
  },
  {
    "chapter": 140,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "своего Бога,"
  },
  {
    "chapter": 140,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "сотворившего небо и землю,"
  },
  {
    "chapter": 140,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "море и все,"
  },
  {
    "chapter": 140,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "что его наполняет,"
  },
  {
    "chapter": 140,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Он вечно хранит"
  },
  {
    "chapter": 140,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Свою верность."
  },
  {
    "chapter": 140,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он защищает"
  },
  {
    "chapter": 140,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "дело угнетенных,"
  },
  {
    "chapter": 140,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "дает пищу голодным."
  },
  {
    "chapter": 140,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Господь отпускает"
  },
  {
    "chapter": 140,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "заключенных из темниц."
  },
  {
    "chapter": 140,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господь"
  },
  {
    "chapter": 140,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "открывает глаза слепым,"
  },
  {
    "chapter": 140,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Господь поднимает"
  },
  {
    "chapter": 140,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "всех низверженных,"
  },
  {
    "chapter": 140,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Господь любит праведных."
  },
  {
    "chapter": 140,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господь хранит чужеземцев,"
  },
  {
    "chapter": 140,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "поддерживает сирот и вдов,"
  },
  {
    "chapter": 140,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "а путь"
  },
  {
    "chapter": 140,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "нечестивых искривляет."
  },
  {
    "chapter": 140,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господь царствует вовек,"
  },
  {
    "chapter": 140,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Твой Бог,"
  },
  {
    "chapter": 140,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Сион,"
  },
  {
    "chapter": 140,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "во все поколения."
  },
  {
    "chapter": 140,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Аллилуйя!"
  }
]