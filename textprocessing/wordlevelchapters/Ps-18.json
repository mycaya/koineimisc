[
  {
    "chapter": 18,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Дирижеру хора."
  },
  {
    "chapter": 18,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Псалом Давида."
  },
  {
    "chapter": 18,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть ответит"
  },
  {
    "chapter": 18,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господь тебе в день печали."
  },
  {
    "chapter": 18,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Пусть хранит тебя"
  },
  {
    "chapter": 18,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Имя Бога Иакова."
  },
  {
    "chapter": 18,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть пошлет Он"
  },
  {
    "chapter": 18,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "помощь тебе из святилища,"
  },
  {
    "chapter": 18,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "пусть"
  },
  {
    "chapter": 18,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "поддержит тебя с Сиона."
  },
  {
    "chapter": 18,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть вспомнит Он все"
  },
  {
    "chapter": 18,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "жертвы твои"
  },
  {
    "chapter": 18,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и всесожжения твои примет."
  },
  {
    "chapter": 18,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Пауза"
  },
  {
    "chapter": 18,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть даст Он тебе,"
  },
  {
    "chapter": 18,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "чего ты желаешь,"
  },
  {
    "chapter": 18,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и свершит все твои мечты."
  },
  {
    "chapter": 18,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Мы будем кричать от радости"
  },
  {
    "chapter": 18,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "о твоей победе"
  },
  {
    "chapter": 18,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и поднимем знамена во Имя"
  },
  {
    "chapter": 18,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "нашего Бога."
  },
  {
    "chapter": 18,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Пусть исполнит"
  },
  {
    "chapter": 18,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Господь все твои просьбы."
  },
  {
    "chapter": 18,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Теперь я знаю:"
  },
  {
    "chapter": 18,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господь спасает"
  },
  {
    "chapter": 18,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Своего помазанника,"
  },
  {
    "chapter": 18,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "отвечает ему со святых"
  },
  {
    "chapter": 18,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "небес Своих спасающей силой"
  },
  {
    "chapter": 18,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Своей правой руки."
  },
  {
    "chapter": 18,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Кто хвалится колесницами,"
  },
  {
    "chapter": 18,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "а кто конями,"
  },
  {
    "chapter": 18,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "мы же хвалимся"
  },
  {
    "chapter": 18,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Именем Господа,"
  },
  {
    "chapter": 18,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "нашего Бога."
  },
  {
    "chapter": 18,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Они ослабели и падают,"
  },
  {
    "chapter": 18,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "а мы поднимаемся"
  },
  {
    "chapter": 18,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и стоим твердо."
  },
  {
    "chapter": 18,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господи,"
  },
  {
    "chapter": 18,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "даруй царю"
  },
  {
    "chapter": 18,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "победу! Ответь нам,"
  },
  {
    "chapter": 18,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "когда мы взываем!"
  }
]