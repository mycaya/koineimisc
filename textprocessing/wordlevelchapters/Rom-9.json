    "chapter": 9,
    "verse": 8,
    "book_name": "Romans",
    "book_id": "Rom",
    "part": 8,
    "text": "результатом Его обещания,"
  },
  {
    "chapter": 9,
    "verse": 8,
    "book_name": "Romans",
    "book_id": "Rom",
    "part": 9,
    "text": "и есть потомки Авраама."
  },
  {
    "chapter": 9,
    "verse": 9,
    "book_name": "Romans",
    "book_id": "Rom",
