  },
  {
    "chapter": 9,
    "verse": 15,
    "book_name": "Romans",
    "book_id": "Rom",
    "part": 3,
    "text": "кого помилую,"
  },
  {
    "chapter": 9,
    "verse": 15,
    "book_name": "Romans",
    "book_id": "Rom",
    "part": 4,
    "text": "и пожалею того,"
  },
  {
    "chapter": 9,
    "verse": 15,
    "book_name": "Romans",
    "book_id": "Rom",
    "part": 5,
    "text": "кого пожалею."
  },
  {
    "chapter": 9,
    "verse": 16,
    "book_name": "Romans",
    "book_id": "Rom",
