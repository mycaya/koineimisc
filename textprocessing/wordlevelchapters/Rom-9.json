[
  {
    "chapter": 9,
    "verse": 1,
    "book_name": "Romans",
    "book_id": "Rom",
    "part": 1,
    "text": "Я говорю истину и не лгу,"
  },
  {
    "chapter": 9,
    "verse": 1,
    "book_name": "Romans",
