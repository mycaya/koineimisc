[
  {
    "chapter": 104,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Псалом Давида."
  },
  {
    "chapter": 104,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Сказал Господь"
  },
  {
    "chapter": 104,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Господу моему:"
  },
  {
    "chapter": 104,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Сядь по правую"
  },
  {
    "chapter": 104,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "руку от Меня,"
  },
  {
    "chapter": 104,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "пока Я не повергну"
  },
  {
    "chapter": 104,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "врагов Твоих к ногам Твоим."
  },
  {
    "chapter": 104,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Жезл Твоей силы"
  },
  {
    "chapter": 104,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "пошлет Господь с Сиона:"
  },
  {
    "chapter": 104,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "правь среди Твоих врагов!"
  },
  {
    "chapter": 104,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "В тот день Твой народ"
  },
  {
    "chapter": 104,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "добровольно пойдет за Тобой"
  },
  {
    "chapter": 104,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "на битву."
  },
  {
    "chapter": 104,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Облаченная в"
  },
  {
    "chapter": 104,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "святые одеяния,"
  },
  {
    "chapter": 104,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Твоя сила будет"
  },
  {
    "chapter": 104,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "обновляться каждый день,"
  },
  {
    "chapter": 104,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 8,
    "text": "как утренняя роса."
  },
  {
    "chapter": 104,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Поклялся Господь"
  },
  {
    "chapter": 104,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и не откажется:"
  },
  {
    "chapter": 104,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Ты священник навеки"
  },
  {
    "chapter": 104,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "по чину Мелхиседека."
  },
  {
    "chapter": 104,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Владыка по правую Твою руку"
  },
  {
    "chapter": 104,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "сразит царей в день Своего"
  },
  {
    "chapter": 104,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "гнева."
  },
  {
    "chapter": 104,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Будет судить народы,"
  },
  {
    "chapter": 104,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "покроет поле"
  },
  {
    "chapter": 104,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "боя их трупами,"
  },
  {
    "chapter": 104,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "сокрушит головы"
  },
  {
    "chapter": 104,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "по всей земле."
  },
  {
    "chapter": 104,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он будет на пути пить"
  },
  {
    "chapter": 104,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "из потока"
  },
  {
    "chapter": 104,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и потому вознесет голову."
  }
]