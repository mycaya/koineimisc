[
  {
    "chapter": 42,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Дирижеру хора."
  },
  {
    "chapter": 42,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "На мотив Лилии."
  },
  {
    "chapter": 42,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Наставление потомков Корея."
  },
  {
    "chapter": 42,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Свадебная песня."
  },
  {
    "chapter": 42,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Сердце мое"
  },
  {
    "chapter": 42,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "прекрасной речью полнится."
  },
  {
    "chapter": 42,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Для Царя"
  },
  {
    "chapter": 42,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "исполняю я эту песнь;"
  },
  {
    "chapter": 42,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "мой язык перо"
  },
  {
    "chapter": 42,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "искусного писаря."
  },
  {
    "chapter": 42,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты прекраснее всех людей;"
  },
  {
    "chapter": 42,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "величественные слова"
  },
  {
    "chapter": 42,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "сходят с Твоих уст,"
  },
  {
    "chapter": 42,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "ведь Бог навеки"
  },
  {
    "chapter": 42,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "благословил Тебя."
  },
  {
    "chapter": 42,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Сильный,"
  },
  {
    "chapter": 42,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "по бедру мечом"
  },
  {
    "chapter": 42,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Себя препояшь;"
  },
  {
    "chapter": 42,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "облекись в славу и величие."
  },
  {
    "chapter": 42,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "И величия полон,"
  },
  {
    "chapter": 42,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "победоносно поезжай"
  },
  {
    "chapter": 42,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "верхом ради истины,"
  },
  {
    "chapter": 42,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "смирения и праведности."
  },
  {
    "chapter": 42,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Пусть рука Твоя"
  },
  {
    "chapter": 42,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "вершит грозные подвиги."
  },
  {
    "chapter": 42,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть острые стрелы Твои"
  },
  {
    "chapter": 42,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "пронзают сердце царских"
  },
  {
    "chapter": 42,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "врагов;"
  },
  {
    "chapter": 42,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "пусть народы"
  },
  {
    "chapter": 42,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "падут под ноги Твои."
  },
  {
    "chapter": 42,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Вечен престол Твой,"
  },
  {
    "chapter": 42,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Боже,"
  },
  {
    "chapter": 42,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Твой царский скипетр"
  },
  {
    "chapter": 42,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "скипетр правосудия."
  },
  {
    "chapter": 42,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты возлюбил праведность,"
  },
  {
    "chapter": 42,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "а беззаконие возненавидел;"
  },
  {
    "chapter": 42,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "поэтому,"
  },
  {
    "chapter": 42,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "о Боже,"
  },
  {
    "chapter": 42,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Твой Бог помазал Тебя"
  },
  {
    "chapter": 42,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "маслом радости больше,"
  },
  {
    "chapter": 42,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "чем Твоих сотоварищей."
  },
  {
    "chapter": 42,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Благоухают Твои"
  },
  {
    "chapter": 42,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "одежды миррой,"
  },
  {
    "chapter": 42,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "алоэ и кассией."
  },
  {
    "chapter": 42,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Из дворцов,"
  },
  {
    "chapter": 42,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "украшенных костью слоновой,"
  },
  {
    "chapter": 42,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "музыка струн Тебя веселит."
  },
  {
    "chapter": 42,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Среди Твоих придворных"
  },
  {
    "chapter": 42,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "женщин царские дочери."
  },
  {
    "chapter": 42,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "По правую руку от Тебя"
  },
  {
    "chapter": 42,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "царица в золоте из Офира."
  },
  {
    "chapter": 42,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Выслушай,"
  },
  {
    "chapter": 42,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "дочь,"
  },
  {
    "chapter": 42,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "обдумай и прислушайся:"
  },
  {
    "chapter": 42,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "забудь свой народ"
  },
  {
    "chapter": 42,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "и дом отца твоего."
  },
  {
    "chapter": 42,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Царь возжелает"
  },
  {
    "chapter": 42,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "твоей красоты,"
  },
  {
    "chapter": 42,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "покорись Ему"
  },
  {
    "chapter": 42,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Он твой Господь."
  },
  {
    "chapter": 42,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Жители Тира"
  },
  {
    "chapter": 42,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "придут с дарами,"
  },
  {
    "chapter": 42,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "богатейшие из народа"
  },
  {
    "chapter": 42,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "будут искать Твоей милости."
  },
  {
    "chapter": 42,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Внутри этих"
  },
  {
    "chapter": 42,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "покоев дочь царя,"
  },
  {
    "chapter": 42,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "невеста,"
  },
  {
    "chapter": 42,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "ее одежда золотом расшита."
  },
  {
    "chapter": 42,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "В многоцветных"
  },
  {
    "chapter": 42,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "одеждах выводят ее к Царю;"
  },
  {
    "chapter": 42,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "девушек,"
  },
  {
    "chapter": 42,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "ее подруг,"
  },
  {
    "chapter": 42,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "ведут к Царю вслед за ней."
  },
  {
    "chapter": 42,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Их ведут с"
  },
  {
    "chapter": 42,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "весельем и радостью;"
  },
  {
    "chapter": 42,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "они вступают в"
  },
  {
    "chapter": 42,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Царский дворец."
  },
  {
    "chapter": 42,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Место предков Твоих,"
  },
  {
    "chapter": 42,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "о Царь,"
  },
  {
    "chapter": 42,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "займут Твои сыновья;"
  },
  {
    "chapter": 42,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "по всей земле"
  },
  {
    "chapter": 42,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "вождями Ты их поставишь."
  },
  {
    "chapter": 42,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я сделаю памятным"
  },
  {
    "chapter": 42,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Имя Твое в поколениях,"
  },
  {
    "chapter": 42,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и народы будут"
  },
  {
    "chapter": 42,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "славить Тебя вовек."
  }
]