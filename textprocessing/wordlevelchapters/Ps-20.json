[
  {
    "chapter": 20,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Дирижеру хора."
  },
  {
    "chapter": 20,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "На мотив Лань на заре."
  },
  {
    "chapter": 20,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Псалом Давида."
  },
  {
    "chapter": 20,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Боже мой,"
  },
  {
    "chapter": 20,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Боже мой,"
  },
  {
    "chapter": 20,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "почему Ты оставил меня?"
  },
  {
    "chapter": 20,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Почему остаешься вдали,"
  },
  {
    "chapter": 20,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "когда взываю о помощи?"
  },
  {
    "chapter": 20,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Бог мой,"
  },
  {
    "chapter": 20,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "я взываю днем"
  },
  {
    "chapter": 20,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Ты не внемлешь,"
  },
  {
    "chapter": 20,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "ночью и нет мне успокоения."
  },
  {
    "chapter": 20,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты Святой,"
  },
  {
    "chapter": 20,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "восседающий на троне"
  },
  {
    "chapter": 20,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "из восхвалений Израиля."
  },
  {
    "chapter": 20,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "На Тебя уповали наши отцы,"
  },
  {
    "chapter": 20,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "уповали,"
  },
  {
    "chapter": 20,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и Ты избавлял их."
  },
  {
    "chapter": 20,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Они взывали к Тебе"
  },
  {
    "chapter": 20,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и обретали спасение,"
  },
  {
    "chapter": 20,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "на Тебя уповали и"
  },
  {
    "chapter": 20,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "не были разочарованы."
  },
  {
    "chapter": 20,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я же червь,"
  },
  {
    "chapter": 20,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "а не человек,"
  },
  {
    "chapter": 20,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "в бесславии у людей,"
  },
  {
    "chapter": 20,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "в презрении у народа."
  },
  {
    "chapter": 20,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Все,"
  },
  {
    "chapter": 20,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "кто видит меня,"
  },
  {
    "chapter": 20,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "надо мной смеются,"
  },
  {
    "chapter": 20,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "оскорбляют меня,"
  },
  {
    "chapter": 20,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "головами качают:"
  },
  {
    "chapter": 20,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он полагался на Господа,"
  },
  {
    "chapter": 20,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "так пусть Он избавит его,"
  },
  {
    "chapter": 20,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "пусть спасет,"
  },
  {
    "chapter": 20,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "раз он Ему угоден."
  },
  {
    "chapter": 20,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но Ты вывел меня"
  },
  {
    "chapter": 20,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "из утробы матери,"
  },
  {
    "chapter": 20,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Ты вложил в меня"
  },
  {
    "chapter": 20,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "упование у груди материнской."
  },
  {
    "chapter": 20,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "С рождения я на"
  },
  {
    "chapter": 20,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Тебя оставлен;"
  },
  {
    "chapter": 20,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "от чрева матери Ты мой Бог."
  },
  {
    "chapter": 20,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Не будь от меня далек,"
  },
  {
    "chapter": 20,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "ведь скорбь близка,"
  },
  {
    "chapter": 20,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "а помощника нет."
  },
  {
    "chapter": 20,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Множество быков"
  },
  {
    "chapter": 20,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "меня обступило,"
  },
  {
    "chapter": 20,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "крепкие быки"
  },
  {
    "chapter": 20,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Васана меня окружили."
  },
  {
    "chapter": 20,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Раскрыли пасть"
  },
  {
    "chapter": 20,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "свою на меня,"
  },
  {
    "chapter": 20,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "как лев,"
  },
  {
    "chapter": 20,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "что ревет,"
  },
  {
    "chapter": 20,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "терзая добычу."
  },
  {
    "chapter": 20,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пролился я,"
  },
  {
    "chapter": 20,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "как вода;"
  },
  {
    "chapter": 20,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "все кости мои рассыпались."
  },
  {
    "chapter": 20,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Сердце мое превратилось в"
  },
  {
    "chapter": 20,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "воск и растопилось в груди."
  },
  {
    "chapter": 20,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Сила моя иссохла,"
  },
  {
    "chapter": 20,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "как черепок,"
  },
  {
    "chapter": 20,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и к нёбу прилип язык."
  },
  {
    "chapter": 20,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Ты положил меня"
  },
  {
    "chapter": 20,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "в смертный прах."
  },
  {
    "chapter": 20,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Псы меня окружили,"
  },
  {
    "chapter": 20,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "сборище злых"
  },
  {
    "chapter": 20,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "меня обступило,"
  },
  {
    "chapter": 20,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "пронзили мне руки и ноги."
  },
  {
    "chapter": 20,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я все кости мои"
  },
  {
    "chapter": 20,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "могу перечесть,"
  },
  {
    "chapter": 20,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "а они глядят и"
  },
  {
    "chapter": 20,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "злорадствуют надо мной."
  },
  {
    "chapter": 20,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Они делят между"
  },
  {
    "chapter": 20,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "собой мои одежды,"
  },
  {
    "chapter": 20,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и бросают жребий"
  },
  {
    "chapter": 20,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "о моем одеянии."
  },
  {
    "chapter": 20,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но Ты,"
  },
  {
    "chapter": 20,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господь,"
  },
  {
    "chapter": 20,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "не будь от меня далек;"
  },
  {
    "chapter": 20,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "сила моя,"
  },
  {
    "chapter": 20,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "поспеши мне на помощь."
  },
  {
    "chapter": 20,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Душу мою от меча избавь,"
  },
  {
    "chapter": 20,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "защити мою жизнь от псов."
  },
  {
    "chapter": 20,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Спаси меня от львиной"
  },
  {
    "chapter": 20,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "пасти и от рогов диких быков."
  },
  {
    "chapter": 20,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я возвещу Твое Имя братьям"
  },
  {
    "chapter": 20,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "моим"
  },
  {
    "chapter": 20,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и посреди собрания Тебя"
  },
  {
    "chapter": 20,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "восхвалю."
  },
  {
    "chapter": 20,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Хвалите Господа,"
  },
  {
    "chapter": 20,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "боящиеся Его! Славьте Его,"
  },
  {
    "chapter": 20,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "все потомки"
  },
  {
    "chapter": 20,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Иакова! Чтите Его,"
  },
  {
    "chapter": 20,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "все потомки Израиля!"
  },
  {
    "chapter": 20,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ведь Он не презрел и не"
  },
  {
    "chapter": 20,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "пренебрег скорбью страдальца."
  },
  {
    "chapter": 20,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Он не скрыл от"
  },
  {
    "chapter": 20,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "него лица Своего,"
  },
  {
    "chapter": 20,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "но внял его крику о помощи."
  },
  {
    "chapter": 20,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Тебя хвалю я в"
  },
  {
    "chapter": 20,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "большом собрании,"
  },
  {
    "chapter": 20,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "перед боящимися Тебя"
  },
  {
    "chapter": 20,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "я исполню свои обеты."
  },
  {
    "chapter": 20,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Бедняки будут"
  },
  {
    "chapter": 20,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "есть и насытятся,"
  },
  {
    "chapter": 20,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "ищущие Господа"
  },
  {
    "chapter": 20,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "восхвалят Его."
  },
  {
    "chapter": 20,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Пусть живут"
  },
  {
    "chapter": 20,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "сердца ваши вовеки!"
  },
  {
    "chapter": 20,
    "verse": 28,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Все края земли вспомнят"
  },
  {
    "chapter": 20,
    "verse": 28,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господа и обратятся к Нему,"
  },
  {
