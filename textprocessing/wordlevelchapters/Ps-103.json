[
  {
    "chapter": 103,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Дирижеру хора."
  },
  {
    "chapter": 103,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Псалом Давида."
  },
  {
    "chapter": 103,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Боже,"
  },
  {
    "chapter": 103,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Которого я восхваляю,"
  },
  {
    "chapter": 103,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "не будь безмолвен,"
  },
  {
    "chapter": 103,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "ведь нечестивые"
  },
  {
    "chapter": 103,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и коварные люди открыли"
  },
  {
    "chapter": 103,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "свои уста против меня,"
  },
  {
    "chapter": 103,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "говорят со мной"
  },
  {
    "chapter": 103,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "лживым языком;"
  },
  {
    "chapter": 103,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "окружили меня словами"
  },
  {
    "chapter": 103,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "ненависти"
  },
  {
    "chapter": 103,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и без всякой причины"
  },
  {
    "chapter": 103,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "вооружились против меня."
  },
  {
    "chapter": 103,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "За любовь мою"
  },
  {
    "chapter": 103,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "обвиняют меня,"
  },
  {
    "chapter": 103,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "но я предаю себя молитве."
  },
  {
    "chapter": 103,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Воздают мне за добро злом,"
  },
  {
    "chapter": 103,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "за мою любовь ненавистью."
  },
  {
    "chapter": 103,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Поставь над ним"
  },
  {
    "chapter": 103,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "нечестивого человека,"
  },
  {
    "chapter": 103,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и пусть сатана будет"
  },
  {
    "chapter": 103,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "по правую руку от него."
  },
  {
    "chapter": 103,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть на суде он"
  },
  {
    "chapter": 103,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "будет виновным,"
  },
  {
    "chapter": 103,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и пусть его"
  },
  {
    "chapter": 103,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "молитву посчитают грехом."
  },
  {
    "chapter": 103,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть сократятся его дни,"
  },
  {
    "chapter": 103,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и пусть другой"
  },
  {
    "chapter": 103,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "займет его должность."
  },
  {
    "chapter": 103,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пускай его дети"
  },
  {
    "chapter": 103,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "станут сиротами,"
  },
  {
    "chapter": 103,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "а его жена вдовой."
  },
  {
    "chapter": 103,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть его дети"
  },
  {
    "chapter": 103,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "скитаются и нищенствуют,"
  },
  {
    "chapter": 103,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и просят хлеба на"
  },
  {
    "chapter": 103,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "своих развалинах."
  },
  {
    "chapter": 103,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть захватит"
  },
  {
    "chapter": 103,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "кредитор все,"
  },
  {
    "chapter": 103,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "что есть у него"
  },
  {
    "chapter": 103,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и чужие пусть расхитят"
  },
  {
    "chapter": 103,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "плоды его труда."
  },
  {
    "chapter": 103,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть не будет"
  },
  {
    "chapter": 103,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "сострадающего ему"
  },
  {
    "chapter": 103,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и милующего его сирот."
  },
  {
    "chapter": 103,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть погибнет его"
  },
  {
    "chapter": 103,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "потомство и изгладится его"
  },
  {
    "chapter": 103,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "имя в следующем поколении."
  },
  {
    "chapter": 103,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть перед Господом будет"
  },
  {
    "chapter": 103,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "вспомянуто нечестие его"
  },
  {
    "chapter": 103,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "предков,"
  },
  {
    "chapter": 103,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и пусть не"
  },
  {
    "chapter": 103,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "изгладится грех его матери."
  },
  {
    "chapter": 103,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть их грехи будут"
  },
  {
    "chapter": 103,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "постоянно перед Господом,"
  },
  {
    "chapter": 103,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "но пусть Он истребит на"
  },
  {
    "chapter": 103,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "земле память о них самих,"
  },
  {
    "chapter": 103,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "потому что он не"
  },
  {
    "chapter": 103,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "думал оказывать милость;"
  },
  {
    "chapter": 103,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "он преследовал"
  },
  {
    "chapter": 103,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "бедного и нищего,"
  },
  {
    "chapter": 103,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "и огорченного сердцем"
  },
  {
    "chapter": 103,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "хотел свести в могилу."
  },
  {
    "chapter": 103,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он любил насылать"
  },
  {
    "chapter": 103,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "проклятие оно и придет к нему;"
  },
  {
    "chapter": 103,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "не любил давать"
  },
  {
    "chapter": 103,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "благословение оно и удалится от него."
  },
  {
    "chapter": 103,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть облечется проклятием,"
  },
  {
    "chapter": 103,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "как одеянием,"
  },
  {
    "chapter": 103,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и оно войдет в"
  },
  {
    "chapter": 103,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "его внутренности,"
  },
  {
    "chapter": 103,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "как вода,"
  },
  {
    "chapter": 103,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "и как масло в его кости."
  },
  {
    "chapter": 103,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Будет оно ему,"
  },
  {
    "chapter": 103,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "как одежда,"
  },
  {
    "chapter": 103,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "в которую он одевается,"
  },
  {
    "chapter": 103,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "как пояс,"
  },
  {
    "chapter": 103,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "которым он"
  },
  {
    "chapter": 103,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "всегда опоясывается."
  },
  {
    "chapter": 103,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Таково пусть будет"
  },
  {
    "chapter": 103,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "воздаяние от Господа моим врагам,"
  },
  {
    "chapter": 103,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "тем,"
  },
  {
    "chapter": 103,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "кто говорит злое"
  },
  {
    "chapter": 103,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "против души моей."
  },
  {
    "chapter": 103,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты же,"
  },
  {
    "chapter": 103,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи Владыка,"
  },
  {
    "chapter": 103,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "сделай мне доброе"
  },
  {
    "chapter": 103,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "ради Имени Своего;"
  },
  {
    "chapter": 103,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "по Своей благой"
  },
  {
    "chapter": 103,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "милости спаси меня."
  },
  {
    "chapter": 103,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я беден и нищ,"
  },
  {
    "chapter": 103,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и сердце мое"
  },
  {
    "chapter": 103,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "уязвлено во мне."
  },
  {
    "chapter": 103,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я исчезаю,"
  },
  {
    "chapter": 103,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "как уходящая тень,"
  },
  {
    "chapter": 103,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "как саранча отгоняем."
  },
  {
    "chapter": 103,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Мои колени"
  },
  {
    "chapter": 103,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "ослабли от поста,"
  },
  {
    "chapter": 103,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и мое тело истощилось."
  },
  {
    "chapter": 103,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я стал для них посмешищем:"
  },
  {
    "chapter": 103,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "увидев меня,"
  },
  {
    "chapter": 103,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "качают головой."
  },
  {
    "chapter": 103,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Помоги мне,"
  },
  {
    "chapter": 103,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 103,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "мой Бог,"
  },
  {
    "chapter": 103,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "спаси меня по"
  },
  {
    "chapter": 103,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Твоей милости!"
  },
  {
    "chapter": 103,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть узнают,"
  },
  {
    "chapter": 103,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "что в этом Твоя рука,"
  },
  {
    "chapter": 103,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "что Ты,"
  },
  {
    "chapter": 103,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Господи,"
  },
  {
    "chapter": 103,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "сотворил это."
