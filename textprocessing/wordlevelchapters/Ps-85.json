[
  {
    "chapter": 85,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Наставление"
  },
  {
    "chapter": 85,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Ефана езрахитянина."
  },
  {
    "chapter": 85,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я всегда буду петь"
  },
  {
    "chapter": 85,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "о милости Господа;"
  },
  {
    "chapter": 85,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "устами своими возвещу"
  },
  {
    "chapter": 85,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "верность Твою всем поколениям."
  },
  {
    "chapter": 85,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я говорю:"
  },
  {
    "chapter": 85,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "навсегда"
  },
  {
    "chapter": 85,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "основана Твоя милость,"
  },
  {
    "chapter": 85,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "верность Твоя"
  },
  {
    "chapter": 85,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "утверждена на небесах."
  },
  {
    "chapter": 85,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты сказал:"
  },
  {
    "chapter": 85,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "С избранным Моим"
  },
  {
    "chapter": 85,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "заключил Я завет,"
  },
  {
    "chapter": 85,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "с Давидом,"
  },
  {
    "chapter": 85,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Моим слугой."
  },
  {
    "chapter": 85,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Я дал клятву:"
  },
  {
    "chapter": 85,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "“Семя твое навек утвержу,"
  },
  {
    "chapter": 85,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "из поколения в поколение"
  },
  {
    "chapter": 85,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "пребудет престол Твой”."
  },
  {
    "chapter": 85,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Пауза"
  },
  {
    "chapter": 85,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Небеса"
  },
  {
    "chapter": 85,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "прославляют Твои чудеса,"
  },
  {
    "chapter": 85,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Господи,"
  },
  {
    "chapter": 85,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и Твою верность в"
  },
  {
    "chapter": 85,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "собрании святых."
  },
  {
    "chapter": 85,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ведь кто на небесах"
  },
  {
    "chapter": 85,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "сравнится с Господом? Кто"
  },
  {
    "chapter": 85,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "из сынов властителей"
  },
  {
    "chapter": 85,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "уподобится Господу?"
  },
  {
    "chapter": 85,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Бог внушает страх святым,"
  },
  {
    "chapter": 85,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и грозен для"
  },
  {
    "chapter": 85,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "всех окружающих Его."
  },
  {
    "chapter": 85,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господи,"
  },
  {
    "chapter": 85,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Бог сил,"
  },
  {
    "chapter": 85,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "кто сравнится с Тобой в"
  },
  {
    "chapter": 85,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "силе Твоей? Ты силен"
  },
  {
    "chapter": 85,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "и верность Твоя окружает"
  },
  {
    "chapter": 85,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Тебя."
  },
  {
    "chapter": 85,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты владычествуешь"
  },
  {
    "chapter": 85,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "над яростью моря;"
  },
  {
    "chapter": 85,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "когда волны вздымаются,"
  },
  {
    "chapter": 85,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Ты укрощаешь их."
  },
  {
    "chapter": 85,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты поразил"
  },
  {
    "chapter": 85,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Раава и убил его;"
  },
  {
    "chapter": 85,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "крепкой рукою Ты"
  },
  {
    "chapter": 85,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "рассеял врагов Своих."
  },
  {
    "chapter": 85,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Небеса Твои и земля Твоя;"
  },
  {
    "chapter": 85,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Ты основал вселенную и все,"
  },
  {
    "chapter": 85,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "что ее наполняет."
  },
  {
    "chapter": 85,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Север и юг Ты сотворил;"
  },
  {
    "chapter": 85,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "горы Фавор и Ермон"
  },
  {
    "chapter": 85,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "радуются Имени Твоему."
  },
  {
    "chapter": 85,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Твоя мышца крепка,"
  },
  {
    "chapter": 85,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "длань Твоя могуча,"
  },
  {
    "chapter": 85,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "вознесена рука Твоя правая."
  },
  {
    "chapter": 85,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Справедливость и правосудие"
  },
  {
    "chapter": 85,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "основание престола Твоего,"
  },
  {
    "chapter": 85,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "милость и верность"
  },
  {
    "chapter": 85,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "идут пред Твоим лицом."
  },
  {
    "chapter": 85,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Благословен народ,"
  },
  {
    "chapter": 85,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "знающий радостный крик,"
  },
  {
    "chapter": 85,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "во свете Твоего"
  },
  {
    "chapter": 85,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "лица они будут ходить."
  },
  {
    "chapter": 85,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Во Имя Твое они радуются"
  },
  {
    "chapter": 85,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "целый день"
  },
  {
    "chapter": 85,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и праведностью Твоей"
  },
  {
    "chapter": 85,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "возвышаются,"
  },
  {
    "chapter": 85,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "ведь Ты их слава и мощь,"
  },
  {
    "chapter": 85,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и по Своему"
  },
  {
    "chapter": 85,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "благоволению наш рог вознесется."
  },
  {
    "chapter": 85,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "От Господа щит наш,"
  },
  {
    "chapter": 85,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "от Святого"
  },
  {
    "chapter": 85,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Израиля царь наш."
  },
  {
    "chapter": 85,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Некогда говорил Ты в"
  },
  {
    "chapter": 85,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "видении Своим верным служителям:"
  },
  {
    "chapter": 85,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Я оказал помощь воину,"
  },
  {
    "chapter": 85,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "вознес"
  },
  {
    "chapter": 85,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "избранного из народа."
  },
  {
    "chapter": 85,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я нашел Давида,"
  },
  {
    "chapter": 85,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Моего слугу,"
  },
  {
    "chapter": 85,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и Моим святым"
  },
  {
    "chapter": 85,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "маслом помазал его."
  },
  {
    "chapter": 85,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Рука Моя пребудет с ним"
  },
  {
    "chapter": 85,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и мышца Моя укрепит его."
  },
  {
    "chapter": 85,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Враг не одолеет его,"
  },
  {
    "chapter": 85,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и нечестивый не"
  },
  {
    "chapter": 85,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "притеснит его."
  },
  {
    "chapter": 85,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Сокрушу перед ним всех его"
  },
  {
    "chapter": 85,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "врагов"
  },
  {
    "chapter": 85,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и поражу ненавидящих его."
  },
  {
    "chapter": 85,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Верность и"
  },
  {
    "chapter": 85,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "милость Моя будут с ним,"
  },
  {
    "chapter": 85,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и во Имя Мое"
  },
  {
    "chapter": 85,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "возвысится его рог."
  },
  {
    "chapter": 85,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Левую руку его Я"
  },
  {
    "chapter": 85,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "положу на море,"
  },
  {
    "chapter": 85,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "а правую на реки."
  },
  {
    "chapter": 85,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он будет звать Меня:"
  },
  {
    "chapter": 85,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "“Ты Отец мой,"
  },
  {
    "chapter": 85,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Бог мой и скала"
  },
  {
    "chapter": 85,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "моего спасения!”"
  },
  {
    "chapter": 85,
    "verse": 28,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Сделаю его первенцем,"
  },
  {
    "chapter": 85,
    "verse": 28,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "превыше всех царей земных."
  },
  {
    "chapter": 85,
    "verse": 29,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Милость Мою сохраню для"
  },
  {
    "chapter": 85,
    "verse": 29,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "него вовек"
  },
  {
    "chapter": 85,
    "verse": 29,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и завет Мой с ним твердым"
  },
  {
    "chapter": 85,
    "verse": 29,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "будет."
  },
  {
    "chapter": 85,
    "verse": 30,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Семя его будет вечно,"
  },
  {
    "chapter": 85,
    "verse": 30,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и престол его как дни неба."
  },
  {
    "chapter": 85,
    "verse": 31,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Если потомки его забудут"
  },
  {
    "chapter": 85,
    "verse": 31,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Мой Закон"
  },
  {
    "chapter": 85,
    "verse": 31,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и не будут следовать Моим"
  },
  {
    "chapter": 85,
    "verse": 31,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "постановлениям,"
  },
  {
    "chapter": 85,
    "verse": 32,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "если преступят уставы Мои"
  },
  {
    "chapter": 85,
    "verse": 32,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и не будут хранить Моих"
  },
  {
    "chapter": 85,
    "verse": 32,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "повелений,"
  },
  {
    "chapter": 85,
    "verse": 33,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "то накажу их жезлом за"
  },
  {
    "chapter": 85,
    "verse": 33,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "их беззакония и ударами за"
  },
  {
    "chapter": 85,
    "verse": 33,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "их грехи."
  },
  {
    "chapter": 85,
    "verse": 34,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Милости же Моей его не лишу"
  },
  {
    "chapter": 85,
    "verse": 34,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "совсем и не"
  },
  {
    "chapter": 85,
    "verse": 34,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "изменю верности Моей."
  },
  {
    "chapter": 85,
    "verse": 35,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Завета Моего Я не нарушу"
  },
  {
    "chapter": 85,
    "verse": 35,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и не отменю сказанного"
  },
  {
    "chapter": 85,
    "verse": 35,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "устами Моими."
  },
  {
    "chapter": 85,
    "verse": 36,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Однажды Я"
  },
  {
    "chapter": 85,
    "verse": 36,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "поклялся Своею святостью,"
  },
  {
    "chapter": 85,
    "verse": 36,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и не солгу Давиду:"
  },
  {
    "chapter": 85,
    "verse": 37,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "семя его пребудет навеки,"
  },
  {
    "chapter": 85,
    "verse": 37,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и престол его,"
  },
  {
    "chapter": 85,
    "verse": 37,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "как солнце предо Мною,"
  },
  {
    "chapter": 85,
    "verse": 38,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "будет утвержден навеки,"
  },
  {
    "chapter": 85,
    "verse": 38,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "как луна;"
  },
  {
    "chapter": 85,
    "verse": 38,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "он верный"
  },
  {
    "chapter": 85,
    "verse": 38,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "свидетель на небесах."
  },
  {
    "chapter": 85,
    "verse": 38,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Пауза"
  },
  {
    "chapter": 85,
    "verse": 39,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но теперь Ты"
  },
  {
    "chapter": 85,
    "verse": 39,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "отринул и презрел,"
  },
  {
    "chapter": 85,
    "verse": 39,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "прогневался на"
  },
  {
    "chapter": 85,
    "verse": 39,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "помазанника Твоего."
  },
  {
    "chapter": 85,
    "verse": 40,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты отменил завет,"
  },
  {
    "chapter": 85,
    "verse": 40,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "что заключил со слугой"
  },
  {
    "chapter": 85,
    "verse": 40,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Твоим"
  },
  {
    "chapter": 85,
    "verse": 40,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и поверг на землю венец"
  },
  {
    "chapter": 85,
    "verse": 40,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "его."
  },
  {
    "chapter": 85,
    "verse": 41,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты разрушил все его ограды,"
  },
  {
    "chapter": 85,
    "verse": 41,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "крепости его"
  },
  {
    "chapter": 85,
    "verse": 41,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "превратил в развалины."
  },
  {
    "chapter": 85,
    "verse": 42,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Крадут у него все"
  },
  {
    "chapter": 85,
    "verse": 42,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "проходящие мимо;"
  },
  {
    "chapter": 85,
    "verse": 42,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "он стал посмешищем"
  },
  {
    "chapter": 85,
    "verse": 42,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "для соседей своих."
  },
  {
    "chapter": 85,
    "verse": 43,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты вознес правую"
  },
  {
    "chapter": 85,
    "verse": 43,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "руку его противников,"
  },
  {
    "chapter": 85,
    "verse": 43,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "всех его врагов обрадовал."
  },
  {
    "chapter": 85,
    "verse": 44,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты повернул вспять острие"
  },
  {
    "chapter": 85,
    "verse": 44,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "его меча"
  },
  {
    "chapter": 85,
    "verse": 44,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и не укрепил его в"
  },
  {
    "chapter": 85,
    "verse": 44,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "сражении."
  },
  {
    "chapter": 85,
    "verse": 45,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Затмил его славу и"
  },
  {
    "chapter": 85,
    "verse": 45,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "поверг его престол на землю."
  },
  {
    "chapter": 85,
    "verse": 46,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты сократил дни его"
  },
  {
    "chapter": 85,
    "verse": 46,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "юности и покрыл его стыдом."
  },
  {
    "chapter": 85,
    "verse": 46,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Пауза"
  },
  {
    "chapter": 85,
    "verse": 47,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Долго ли,"
  },
  {
    "chapter": 85,
    "verse": 47,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 85,
    "verse": 47,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "будешь постоянно"
  },
  {
    "chapter": 85,
    "verse": 47,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "скрываться? Долго ли будет"
  },
  {
    "chapter": 85,
    "verse": 47,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "пылать Твой гнев,"
  },
  {
    "chapter": 85,
    "verse": 47,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "как огонь?"
  },
  {
    "chapter": 85,
    "verse": 48,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Вспомни,"
  },
  {
    "chapter": 85,
    "verse": 48,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "как коротка моя жизнь!"
  },
  {
    "chapter": 85,
    "verse": 48,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Ужели напрасно Ты сотворил"
  },
  {
    "chapter": 85,
    "verse": 48,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "человека?"
  },
  {
    "chapter": 85,
    "verse": 49,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Какой человек жил"
  },
  {
    "chapter": 85,
    "verse": 49,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и не видел смерти,"
  },
  {
    "chapter": 85,
    "verse": 49,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "избавил ли он душу свою"
  },
  {
    "chapter": 85,
    "verse": 49,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "от руки мира мертвых? Пауза"
  },
  {
    "chapter": 85,
    "verse": 50,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Где Твоя прежняя милость,"
  },
  {
    "chapter": 85,
    "verse": 50,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Владыка,"
  },
  {
    "chapter": 85,
    "verse": 50,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "о которой Ты клялся"
  },
  {
    "chapter": 85,
    "verse": 50,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Давиду верностью Своею?"
  },
  {
    "chapter": 85,
    "verse": 51,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Вспомни,"
  },
  {
    "chapter": 85,
    "verse": 51,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Владыка,"
  },
  {
    "chapter": 85,
    "verse": 51,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "поношение слуг"
  },
  {
    "chapter": 85,
    "verse": 51,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Твоих от многих народов,"
  },
  {
    "chapter": 85,
    "verse": 51,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "которое ношу я в себе,"
  },
  {
    "chapter": 85,
    "verse": 52,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "как злословят Твои враги,"
  },
  {
    "chapter": 85,
    "verse": 52,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 85,
    "verse": 52,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "как бесславят они"
  },
  {
    "chapter": 85,
    "verse": 52,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "следы Твоего помазанника."
  },
  {
    "chapter": 85,
    "verse": 53,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Благословен будь,"
  },
  {
    "chapter": 85,
    "verse": 53,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 85,
    "verse": 53,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "навек! Аминь и аминь."
  },
  {
    "chapter": 85,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Молитва Моисея,"
  },
  {
    "chapter": 85,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "человека Божьего."
  },
  {
    "chapter": 85,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Владыка,"
  },
  {
    "chapter": 85,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Ты наше прибежище из"
  },
  {
    "chapter": 85,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "поколения в поколение."
  },
  {
    "chapter": 85,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Прежде чем родились горы"
  },
  {
    "chapter": 85,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и Ты образовал землю"
  },
  {
    "chapter": 85,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и вселенную,"
  },
  {
    "chapter": 85,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "от века и до века Ты Бог."
  },
  {
    "chapter": 85,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты возвращаешь"
  },
  {
    "chapter": 85,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "человека в тление и говоришь:"
  },
  {
    "chapter": 85,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Вернитесь,"
  },
  {
    "chapter": 85,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "сыны человеческие."
  },
  {
    "chapter": 85,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Тысяча лет в глазах Твоих,"
  },
  {
    "chapter": 85,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "как день вчерашний,"
  },
  {
    "chapter": 85,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "что минул,"
  },
  {
    "chapter": 85,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "как стража в ночи."
  },
  {
    "chapter": 85,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Как наводнением"
  },
  {
    "chapter": 85,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "уносишь людей;"
  },
  {
    "chapter": 85,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "они проходят,"
  },
  {
    "chapter": 85,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "как сон."
  },
  {
    "chapter": 85,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Они как"
  },
  {
    "chapter": 85,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "пробившаяся утром трава:"
  },
  {
    "chapter": 85,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "утром она"
  },
  {
    "chapter": 85,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 8,
    "text": "цветет и зеленеет,"
  },
  {
    "chapter": 85,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 9,
    "text": "а вечером вянет и засыхает."
  },
  {
    "chapter": 85,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Мы исчезаем от Твоего гнева"
  },
  {
    "chapter": 85,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и в смятении от Твоей"
  },
  {
    "chapter": 85,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "ярости."
  },
  {
    "chapter": 85,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты поставил наши беззакония"
  },
  {
    "chapter": 85,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "перед Собой"
  },
  {
    "chapter": 85,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и наши тайные грехи перед"
  },
  {
    "chapter": 85,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "светом Своего лица."
  },
  {
    "chapter": 85,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Наши дни"
  },
  {
    "chapter": 85,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "проходят в Твоем гневе;"
  },
  {
    "chapter": 85,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "мы завершаем"
  },
  {
    "chapter": 85,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "свои годы со стоном."
  },
  {
    "chapter": 85,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Наших дней лет семьдесят,"
  },
  {
    "chapter": 85,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "а для более"
  },
  {
    "chapter": 85,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "сильных восемьдесят,"
  },
  {
    "chapter": 85,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и то большая часть"
  },
  {
    "chapter": 85,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "их труд и скорбь;"
  },
  {
    "chapter": 85,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "они проходят быстро,"
  },
  {
    "chapter": 85,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "и мы улетим."
  },
  {
    "chapter": 85,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Кто познал силу Твоего"
  },
  {
    "chapter": 85,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "гнева? Ярость Твоя столь же"
  },
  {
    "chapter": 85,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "велика,"
  },
  {
    "chapter": 85,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "сколь велик"
  },
  {
    "chapter": 85,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "страх перед Тобой."
  },
  {
    "chapter": 85,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Научи нас вести"
  },
  {
    "chapter": 85,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "счет нашим дням,"
  },
  {
    "chapter": 85,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "чтобы мы обрели"
  },
  {
    "chapter": 85,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "сердце мудрое."
  },
  {
    "chapter": 85,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Возвратись,"
  },
  {
    "chapter": 85,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи! Как долго это"
  },
  {
    "chapter": 85,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "будет длиться? Сжалься над"
  },
  {
    "chapter": 85,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Твоими рабами!"
  },
  {
    "chapter": 85,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Насыщай нас по"
  },
  {
    "chapter": 85,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "утрам милостью Своей,"
  },
  {
    "chapter": 85,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "чтобы мы радовались и"
  },
  {
    "chapter": 85,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "веселились все наши дни."
  },
  {
    "chapter": 85,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Возвесели нас за дни,"
  },
  {
    "chapter": 85,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "когда Ты наказывал нас,"
  },
  {
    "chapter": 85,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и за годы,"
  },
  {
    "chapter": 85,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "в которые мы"
  },
  {
    "chapter": 85,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "испытывали бедствие."
  },
  {
    "chapter": 85,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Да откроется взору"
  },
  {
    "chapter": 85,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "рабов Твоих дело Твое,"
  },
  {
    "chapter": 85,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и слава Твоя их детям."
  },
  {
    "chapter": 85,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "И да пребудет на нас"
  },
  {
    "chapter": 85,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "благоволение Владыки,"
  },
  {
    "chapter": 85,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "нашего Бога."
  },
  {
    "chapter": 85,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Укрепи дело"
  },
  {
    "chapter": 85,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "наших рук для нас,"
  },
  {
    "chapter": 85,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "укрепи дело наших рук."
  }
]