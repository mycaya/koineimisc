[
  {
    "chapter": 75,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Псалом Асафа."
  },
  {
    "chapter": 75,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "О Боже,"
  },
  {
    "chapter": 75,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "чужие народы"
  },
  {
    "chapter": 75,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "вторглись в удел Твой,"
  },
  {
    "chapter": 75,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "осквернили"
  },
  {
    "chapter": 75,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "святой храм Твой,"
  },
  {
    "chapter": 75,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "и превратили"
  },
  {
    "chapter": 75,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 8,
    "text": "Иерусалим в развалины."
  },
  {
    "chapter": 75,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Они отдали трупы Твоих"
  },
  {
    "chapter": 75,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "рабов на съедение небесным"
  },
  {
    "chapter": 75,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "птицам,"
  },
  {
    "chapter": 75,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "тела Твоих"
  },
  {
    "chapter": 75,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "святых земным зверям."
  },
  {
    "chapter": 75,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Они пролили кровь их,"
  },
  {
    "chapter": 75,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "как воду,"
  },
  {
    "chapter": 75,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "вокруг всего Иерусалима,"
  },
  {
    "chapter": 75,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и некому было"
  },
  {
    "chapter": 75,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "похоронить их."
  },
  {
    "chapter": 75,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Мы сделались"
  },
  {
    "chapter": 75,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "посмешищем у наших соседей;"
  },
  {
    "chapter": 75,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "окружающие нас"
  },
  {
    "chapter": 75,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "выказывают лишь упрек и презрение."
  },
  {
    "chapter": 75,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Как долго,"
  },
  {
    "chapter": 75,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 75,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Твой гнев будет длиться"
  },
  {
    "chapter": 75,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "бесконечно? Как долго Твоя"
  },
  {
    "chapter": 75,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "ревность будет пылать,"
  },
  {
    "chapter": 75,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "как огонь?"
  },
  {
    "chapter": 75,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пролей Твой гнев на народы,"
  },
  {
    "chapter": 75,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "которые не знают"
  },
  {
    "chapter": 75,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Тебя и на царства,"
  },
  {
    "chapter": 75,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "которые не"
  },
  {
    "chapter": 75,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "призывают Твоего Имени,"
  },
  {
    "chapter": 75,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "потому что они"
  },
  {
    "chapter": 75,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "истребили народ Иакова"
  },
  {
    "chapter": 75,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и его землю опустошили."
  },
  {
    "chapter": 75,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Не вспоминай нам"
  },
  {
    "chapter": 75,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "грехов наших предков;"
  },
  {
    "chapter": 75,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "пусть милость Твоя"
  },
  {
    "chapter": 75,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "поспешит к нам навстречу,"
  },
  {
    "chapter": 75,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "потому что мы в"
  },
  {
    "chapter": 75,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "полном отчаянии."
  },
  {
    "chapter": 75,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Помоги нам,"
  },
  {
    "chapter": 75,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Боже,"
  },
  {
    "chapter": 75,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "наш Спаситель,"
  },
  {
    "chapter": 75,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "ради славы Твоего Имени."
  },
  {
    "chapter": 75,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Избавь нас и прости нам"
  },
  {
    "chapter": 75,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "наши грехи ради Твоего Имени."
  },
  {
    "chapter": 75,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Для чего народам говорить:"
  },
  {
    "chapter": 75,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Где их Бог? Пусть станет"
  },
  {
    "chapter": 75,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "известным между народами"
  },
  {
    "chapter": 75,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "отмщение Твое за пролитую"
  },
  {
    "chapter": 75,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "кровь Твоего народа,"
  },
  {
    "chapter": 75,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "и пусть это"
  },
  {
    "chapter": 75,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "увидят наши глаза."
  },
  {
    "chapter": 75,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть дойдет до"
  },
  {
    "chapter": 75,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Тебя стенание узника;"
  },
  {
    "chapter": 75,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "сильной рукою Своей"
  },
  {
    "chapter": 75,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "сохрани обреченных на смерть."
  },
  {
    "chapter": 75,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Семикратно возврати"
  },
  {
    "chapter": 75,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "нашим соседям их оскорбление,"
  },
  {
    "chapter": 75,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "которое они Тебе нанесли,"
  },
  {
    "chapter": 75,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "о Владыка."
  },
  {
    "chapter": 75,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "А мы,"
  },
  {
    "chapter": 75,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "народ Твой и"
  },
  {
    "chapter": 75,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "овцы пастбищ Твоих,"
  },
  {
    "chapter": 75,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "вечно будем восхвалять Тебя"
  },
  {
    "chapter": 75,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "и возвещать о славе Твоей"
  },
  {
    "chapter": 75,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "из поколения в поколение."
  }
]