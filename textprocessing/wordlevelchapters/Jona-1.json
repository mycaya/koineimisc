    "verse": 14,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 2,
    "text": "призывать Господа:"
  },
  {
    "chapter": 1,
    "verse": 14,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 3,
