    "verse": 4,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 4,
    "text": "такой сильный шторм,"
  },
  {
    "chapter": 1,
    "verse": 4,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 5,
    "text": "что корабль был"
  },
