  },
  {
    "chapter": 5,
    "verse": 8,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 6,
    "text": "словно молодой лев"
  },
  {
    "chapter": 5,
    "verse": 8,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 7,
    "text": "между овечьих отар,"
  },
  {
    "chapter": 5,
    "verse": 8,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 8,
    "text": "что,"
