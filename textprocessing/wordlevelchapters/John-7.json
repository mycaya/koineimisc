  },
  {
    "chapter": 7,
    "verse": 28,
    "book_name": "John",
    "book_id": "John",
    "part": 8,
    "text": "И верен Тот,"
  },
  {
    "chapter": 7,
    "verse": 28,
    "book_name": "John",
    "book_id": "John",
    "part": 9,
    "text": "Кто послал Меня,"
  },
  {
    "chapter": 7,
    "verse": 28,
    "book_name": "John",
    "book_id": "John",
    "part": 10,
    "text": "но вы Его не знаете."
