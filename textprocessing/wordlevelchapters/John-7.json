  {
    "chapter": 7,
    "verse": 35,
    "book_name": "John",
    "book_id": "John",
    "part": 3,
    "text": "стали"
  },
  {
    "chapter": 7,
    "verse": 35,
    "book_name": "John",
    "book_id": "John",
