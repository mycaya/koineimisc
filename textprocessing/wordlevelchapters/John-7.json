  {
    "chapter": 7,
    "verse": 37,
    "book_name": "John",
    "book_id": "John",
    "part": 4,
    "text": "и громко возгласил:"
  },
  {
    "chapter": 7,
    "verse": 37,
    "book_name": "John",
    "book_id": "John",
    "part": 5,
    "text": "Кто хочет пить,"
  },
  {
