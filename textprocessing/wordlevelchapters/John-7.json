  {
    "chapter": 7,
    "verse": 36,
    "book_name": "John",
    "book_id": "John",
    "part": 2,
    "text": "когда говорил:"
  },
  {
    "chapter": 7,
    "verse": 36,
    "book_name": "John",
    "book_id": "John",
    "part": 3,
    "text": "Вы будете искать Меня,"
  },
  {
    "chapter": 7,
    "verse": 36,
    "book_name": "John",
    "book_id": "John",
    "part": 4,
    "text": "но не найдете и Туда,"
  },
  {
    "chapter": 7,
    "verse": 36,
    "book_name": "John",
    "book_id": "John",
    "part": 5,
    "text": "где Я буду,"
  },
  {
    "chapter": 7,
    "verse": 36,
    "book_name": "John",
    "book_id": "John",
    "part": 6,
    "text": "вы прийти не сможете?"
  },
  {
    "chapter": 7,
    "verse": 37,
    "book_name": "John",
    "book_id": "John",
    "part": 1,
    "text": "В последний"
  },
  {
    "chapter": 7,
    "verse": 37,
    "book_name": "John",
    "book_id": "John",
    "part": 2,
    "text": "и самый торжественный день"
