[
  {
    "chapter": 7,
    "verse": 1,
    "book_name": "John",
    "book_id": "John",
    "part": 1,
    "text": "После этого Иисус"
  },
  {
    "chapter": 7,
    "verse": 1,
    "book_name": "John",
    "book_id": "John",
    "part": 2,
    "text": "ходил по Галилее."
  },
  {
    "chapter": 7,
    "verse": 1,
    "book_name": "John",
    "book_id": "John",
    "part": 3,
    "text": "В Иудею Он не шел,"
  },
  {
    "chapter": 7,
    "verse": 1,
    "book_name": "John",
    "book_id": "John",
    "part": 4,
    "text": "так как иудеи"
  },
  {
    "chapter": 7,
    "verse": 1,
    "book_name": "John",
    "book_id": "John",
    "part": 5,
    "text": "хотели убить Его."
  },
  {
    "chapter": 7,
    "verse": 2,
    "book_name": "John",
    "book_id": "John",
    "part": 1,
    "text": "Когда приближался"
  },
  {
    "chapter": 7,
    "verse": 2,
    "book_name": "John",
    "book_id": "John",
    "part": 2,
    "text": "иудейский праздник Шалашей,"
  },
  {
    "chapter": 7,
    "verse": 3,
    "book_name": "John",
    "book_id": "John",
    "part": 1,
    "text": "братья Иисуса сказали Ему:"
  },
  {
    "chapter": 7,
    "verse": 3,
    "book_name": "John",
    "book_id": "John",
    "part": 2,
    "text": "Не оставайся здесь,"
  },
  {
    "chapter": 7,
    "verse": 3,
    "book_name": "John",
    "book_id": "John",
    "part": 3,
    "text": "иди в Иудею,"
  },
  {
    "chapter": 7,
    "verse": 3,
    "book_name": "John",
    "book_id": "John",
    "part": 4,
    "text": "чтобы и Твои"
  },
  {
    "chapter": 7,
    "verse": 3,
    "book_name": "John",
    "book_id": "John",
    "part": 5,
    "text": "ученики увидели чудеса,"
  },
  {
    "chapter": 7,
    "verse": 3,
    "book_name": "John",
    "book_id": "John",
    "part": 6,
    "text": "которые Ты совершаешь."
  },
  {
    "chapter": 7,
    "verse": 4,
    "book_name": "John",
    "book_id": "John",
    "part": 1,
    "text": "Ведь никто,"
  },
  {
    "chapter": 7,
    "verse": 4,
    "book_name": "John",
    "book_id": "John",
    "part": 2,
    "text": "стремящийся к известности,"
  },
  {
    "chapter": 7,
    "verse": 4,
    "book_name": "John",
    "book_id": "John",
    "part": 3,
    "text": "не действует втайне."
  },
  {
    "chapter": 7,
    "verse": 4,
    "book_name": "John",
    "book_id": "John",
    "part": 4,
    "text": "Если Ты делаешь такое,"
  },
  {
    "chapter": 7,
    "verse": 4,
    "book_name": "John",
    "book_id": "John",
    "part": 5,
    "text": "покажи Себя миру."
  },
  {
    "chapter": 7,
    "verse": 5,
    "book_name": "John",
    "book_id": "John",
    "part": 1,
    "text": "Даже Его братья"
  },
  {
    "chapter": 7,
    "verse": 5,
    "book_name": "John",
    "book_id": "John",
    "part": 2,
    "text": "не верили Ему."
  },
  {
    "chapter": 7,
    "verse": 6,
    "book_name": "John",
    "book_id": "John",
    "part": 1,
    "text": "Иисус сказал им:"
  },
  {
    "chapter": 7,
    "verse": 6,
    "book_name": "John",
    "book_id": "John",
    "part": 2,
    "text": "Мое время еще не подошло,"
  },
  {
    "chapter": 7,
    "verse": 6,
    "book_name": "John",
    "book_id": "John",
    "part": 3,
    "text": "для вас же"
  },
  {
    "chapter": 7,
    "verse": 6,
    "book_name": "John",
    "book_id": "John",
    "part": 4,
    "text": "подходит любое время."
  },
  {
    "chapter": 7,
    "verse": 7,
    "book_name": "John",
    "book_id": "John",
    "part": 1,
    "text": "Вас мир не"
  },
  {
    "chapter": 7,
    "verse": 7,
    "book_name": "John",
    "book_id": "John",
    "part": 2,
    "text": "может ненавидеть,"
  },
  {
    "chapter": 7,
    "verse": 7,
    "book_name": "John",
    "book_id": "John",
    "part": 3,
    "text": "Меня же он ненавидит,"
  },
  {
    "chapter": 7,
    "verse": 7,
    "book_name": "John",
    "book_id": "John",
    "part": 4,
    "text": "потому что Я"
  },
  {
    "chapter": 7,
    "verse": 7,
    "book_name": "John",
    "book_id": "John",
    "part": 5,
    "text": "свидетельствую о том,"
  },
  {
    "chapter": 7,
    "verse": 7,
    "book_name": "John",
    "book_id": "John",
    "part": 6,
    "text": "что его дела злы."
  },
  {
    "chapter": 7,
    "verse": 8,
    "book_name": "John",
    "book_id": "John",
    "part": 1,
    "text": "Идите на праздник,"
  },
  {
    "chapter": 7,
    "verse": 8,
    "book_name": "John",
    "book_id": "John",
    "part": 2,
    "text": "Я же пока не пойду,"
  },
  {
    "chapter": 7,
    "verse": 8,
    "book_name": "John",
    "book_id": "John",
    "part": 3,
    "text": "потому что Мое"
  },
  {
    "chapter": 7,
    "verse": 8,
    "book_name": "John",
    "book_id": "John",
    "part": 4,
    "text": "время еще не подошло."
  },
  {
    "chapter": 7,
    "verse": 9,
    "book_name": "John",
    "book_id": "John",
    "part": 1,
    "text": "И Иисус остался в Галилее."
  },
  {
    "chapter": 7,
    "verse": 10,
    "book_name": "John",
    "book_id": "John",
    "part": 1,
    "text": "Но когда Его братья"
  },
  {
    "chapter": 7,
    "verse": 10,
    "book_name": "John",
    "book_id": "John",
    "part": 2,
    "text": "пошли на праздник,"
  },
  {
    "chapter": 7,
    "verse": 10,
    "book_name": "John",
    "book_id": "John",
    "part": 3,
    "text": "Он тоже пошел,"
  },
  {
    "chapter": 7,
    "verse": 10,
    "book_name": "John",
    "book_id": "John",
    "part": 4,
    "text": "только не открыто,"
  },
  {
    "chapter": 7,
    "verse": 10,
    "book_name": "John",
    "book_id": "John",
    "part": 5,
    "text": "а так,"
  },
  {
    "chapter": 7,
    "verse": 10,
    "book_name": "John",
    "book_id": "John",
    "part": 6,
    "text": "чтобы никто не знал."
  },
  {
    "chapter": 7,
    "verse": 11,
    "book_name": "John",
    "book_id": "John",
    "part": 1,
    "text": "На празднике иудеи"
  },
  {
    "chapter": 7,
    "verse": 11,
    "book_name": "John",
    "book_id": "John",
    "part": 2,
    "text": "искали Его и спрашивали:"
  },
  {
    "chapter": 7,
    "verse": 11,
    "book_name": "John",
    "book_id": "John",
    "part": 3,
    "text": "Где Он?"
  },
  {
    "chapter": 7,
    "verse": 12,
    "book_name": "John",
    "book_id": "John",
    "part": 1,
    "text": "В народе об Иисусе"
  },
  {
    "chapter": 7,
    "verse": 12,
    "book_name": "John",
    "book_id": "John",
    "part": 2,
    "text": "шло много разговоров."
  },
  {
    "chapter": 7,
    "verse": 12,
    "book_name": "John",
    "book_id": "John",
    "part": 3,
    "text": "Некоторые говорили,"
  },
  {
    "chapter": 7,
    "verse": 12,
    "book_name": "John",
    "book_id": "John",
    "part": 4,
    "text": "что Он хороший Человек,"
  },
  {
    "chapter": 7,
    "verse": 12,
    "book_name": "John",
    "book_id": "John",
    "part": 5,
    "text": "другие что Он"
  },
  {
    "chapter": 7,
    "verse": 12,
    "book_name": "John",
    "book_id": "John",
    "part": 6,
    "text": "обманывает народ."
  },
  {
    "chapter": 7,
    "verse": 13,
    "book_name": "John",
    "book_id": "John",
    "part": 1,
    "text": "Однако открыто никто о Нем"
  },
  {
    "chapter": 7,
    "verse": 13,
    "book_name": "John",
    "book_id": "John",
    "part": 2,
    "text": "ничего не говорил"
  },
  {
    "chapter": 7,
    "verse": 13,
    "book_name": "John",
    "book_id": "John",
    "part": 3,
    "text": "из страха перед иудеями."
  },
  {
    "chapter": 7,
    "verse": 14,
    "book_name": "John",
    "book_id": "John",
    "part": 1,
    "text": "Первая половина"
  },
  {
    "chapter": 7,
    "verse": 14,
    "book_name": "John",
    "book_id": "John",
    "part": 2,
    "text": "праздника уже прошла,"
  },
  {
    "chapter": 7,
    "verse": 14,
    "book_name": "John",
    "book_id": "John",
    "part": 3,
    "text": "когда Иисус пришел"
  },
  {
    "chapter": 7,
    "verse": 14,
    "book_name": "John",
    "book_id": "John",
    "part": 4,
    "text": "в храм и стал учить."
  },
  {
    "chapter": 7,
    "verse": 15,
    "book_name": "John",
    "book_id": "John",
    "part": 1,
    "text": "Иудеи"
  },
  {
    "chapter": 7,
    "verse": 15,
    "book_name": "John",
    "book_id": "John",
    "part": 2,
    "text": "удивлялись и спрашивали:"
  },
  {
    "chapter": 7,
    "verse": 15,
    "book_name": "John",
    "book_id": "John",
    "part": 3,
    "text": "Откуда у Него"
  },
  {
    "chapter": 7,
    "verse": 15,
    "book_name": "John",
    "book_id": "John",
    "part": 4,
    "text": "такая ученость,"
  },
  {
    "chapter": 7,
    "verse": 15,
    "book_name": "John",
    "book_id": "John",
    "part": 5,
    "text": "ведь Он нигде не учился?"
  },
  {
    "chapter": 7,
    "verse": 16,
    "book_name": "John",
    "book_id": "John",
    "part": 1,
    "text": "Иисус ответил:"
  },
  {
    "chapter": 7,
    "verse": 16,
    "book_name": "John",
    "book_id": "John",
    "part": 2,
    "text": "Учение Мое не от Меня,"
  },
  {
    "chapter": 7,
    "verse": 16,
    "book_name": "John",
    "book_id": "John",
    "part": 3,
    "text": "но от Того,"
  },
  {
    "chapter": 7,
    "verse": 16,
    "book_name": "John",
    "book_id": "John",
    "part": 4,
    "text": "Кто послал Меня."
  },
  {
    "chapter": 7,
    "verse": 17,
    "book_name": "John",
    "book_id": "John",
    "part": 1,
    "text": "И кто действительно"
  },
  {
    "chapter": 7,
    "verse": 17,
    "book_name": "John",
    "book_id": "John",
    "part": 2,
    "text": "хочет исполнять волю Бога,"
  },
  {
    "chapter": 7,
    "verse": 17,
    "book_name": "John",
    "book_id": "John",
    "part": 3,
    "text": "тот узнает,"
  },
  {
    "chapter": 7,
    "verse": 17,
    "book_name": "John",
    "book_id": "John",
    "part": 4,
    "text": "от Бога Мое учение или"
  },
  {
    "chapter": 7,
    "verse": 17,
    "book_name": "John",
    "book_id": "John",
    "part": 5,
    "text": "же Я говорю Сам от Себя."
  },
  {
    "chapter": 7,
    "verse": 18,
    "book_name": "John",
    "book_id": "John",
    "part": 1,
    "text": "Тот,"
  },
  {
    "chapter": 7,
    "verse": 18,
    "book_name": "John",
    "book_id": "John",
    "part": 2,
    "text": "кто говорит от себя,"
  },
  {
    "chapter": 7,
    "verse": 18,
    "book_name": "John",
    "book_id": "John",
    "part": 3,
    "text": "ищет похвалы самому себе,"
  },
  {
    "chapter": 7,
    "verse": 18,
    "book_name": "John",
    "book_id": "John",
    "part": 4,
    "text": "Тот же,"
  },
  {
    "chapter": 7,
    "verse": 18,
    "book_name": "John",
    "book_id": "John",
    "part": 5,
    "text": "Кто ищет славы"
  },
  {
    "chapter": 7,
    "verse": 18,
    "book_name": "John",
    "book_id": "John",
    "part": 6,
    "text": "Пославшему Его,"
  },
  {
    "chapter": 7,
    "verse": 18,
    "book_name": "John",
    "book_id": "John",
    "part": 7,
    "text": "говорит истину,"
  },
  {
    "chapter": 7,
    "verse": 18,
    "book_name": "John",
    "book_id": "John",
    "part": 8,
    "text": "и лжи нет в Нем."
  },
  {
    "chapter": 7,
    "verse": 19,
    "book_name": "John",
    "book_id": "John",
    "part": 1,
    "text": "Разве Моисей не дал вам"
  },
  {
    "chapter": 7,
    "verse": 19,
    "book_name": "John",
    "book_id": "John",
    "part": 2,
    "text": "Закон? Однако ни один"
  },
  {
    "chapter": 7,
    "verse": 19,
    "book_name": "John",
    "book_id": "John",
    "part": 3,
    "text": "из вас не соблюдает Закона."
  },
  {
    "chapter": 7,
    "verse": 19,
    "book_name": "John",
    "book_id": "John",
    "part": 4,
    "text": "Зачем вы хотите убить Меня?"
  },
  {
    "chapter": 7,
    "verse": 20,
    "book_name": "John",
    "book_id": "John",
    "part": 1,
    "text": "- Да Ты не одержим ли! Кто"
  },
  {
    "chapter": 7,
    "verse": 20,
    "book_name": "John",
    "book_id": "John",
    "part": 2,
    "text": "хочет Тебя убить? отвечал"
  },
  {
    "chapter": 7,
    "verse": 20,
    "book_name": "John",
    "book_id": "John",
    "part": 3,
    "text": "народ."
  },
  {
    "chapter": 7,
    "verse": 21,
    "book_name": "John",
    "book_id": "John",
    "part": 1,
    "text": "Иисус сказал:"
  },
  {
    "chapter": 7,
    "verse": 21,
    "book_name": "John",
    "book_id": "John",
    "part": 2,
    "text": "Я совершил одно дело,"
  },
  {
    "chapter": 7,
    "verse": 21,
    "book_name": "John",
    "book_id": "John",
    "part": 3,
    "text": "и вы так удивляетесь."
  },
  {
    "chapter": 7,
    "verse": 22,
    "book_name": "John",
    "book_id": "John",
    "part": 1,
    "text": "Моисей дал вам"
  },
  {
    "chapter": 7,
    "verse": 22,
    "book_name": "John",
    "book_id": "John",
    "part": 2,
    "text": "обрезание (хотя,"
  },
  {
    "chapter": 7,
    "verse": 22,
    "book_name": "John",
    "book_id": "John",
    "part": 3,
    "text": "конечно,"
  },
  {
    "chapter": 7,
    "verse": 22,
    "book_name": "John",
    "book_id": "John",
    "part": 4,
    "text": "обрезание идет"
  },
  {
    "chapter": 7,
    "verse": 22,
    "book_name": "John",
    "book_id": "John",
    "part": 5,
    "text": "не от Моисея,"
  },
  {
    "chapter": 7,
    "verse": 22,
    "book_name": "John",
    "book_id": "John",
    "part": 6,
    "text": "а от праотцов),"
  },
  {
    "chapter": 7,
    "verse": 22,
    "book_name": "John",
    "book_id": "John",
    "part": 7,
    "text": "и вы обрезаете"
  },
  {
    "chapter": 7,
    "verse": 22,
    "book_name": "John",
    "book_id": "John",
    "part": 8,
    "text": "детей даже в субботу."
  },
  {
    "chapter": 7,
    "verse": 23,
    "book_name": "John",
    "book_id": "John",
    "part": 1,
    "text": "Так если человек может быть"
  },
  {
    "chapter": 7,
    "verse": 23,
    "book_name": "John",
    "book_id": "John",
    "part": 2,
    "text": "обрезан в субботу ради"
  },
  {
    "chapter": 7,
    "verse": 23,
    "book_name": "John",
    "book_id": "John",
    "part": 3,
    "text": "того,"
  },
  {
    "chapter": 7,
    "verse": 23,
    "book_name": "John",
    "book_id": "John",
    "part": 4,
    "text": "чтобы не был"
  },
  {
    "chapter": 7,
    "verse": 23,
    "book_name": "John",
    "book_id": "John",
    "part": 5,
    "text": "нарушен Закон Моисея,"
  },
  {
    "chapter": 7,
    "verse": 23,
    "book_name": "John",
    "book_id": "John",
    "part": 6,
    "text": "то почему вы"
  },
  {
    "chapter": 7,
    "verse": 23,
    "book_name": "John",
    "book_id": "John",
    "part": 7,
    "text": "злы на Меня за то,"
  },
  {
    "chapter": 7,
    "verse": 23,
    "book_name": "John",
    "book_id": "John",
    "part": 8,
    "text": "что Я всего человека"
  },
  {
    "chapter": 7,
    "verse": 23,
    "book_name": "John",
    "book_id": "John",
    "part": 9,
    "text": "сделал здоровым в субботу?"
  },
  {
    "chapter": 7,
    "verse": 24,
    "book_name": "John",
    "book_id": "John",
    "part": 1,
    "text": "Судите не по виду,"
  },
  {
    "chapter": 7,
    "verse": 24,
    "book_name": "John",
    "book_id": "John",
    "part": 2,
    "text": "а по справедливости."
  },
  {
    "chapter": 7,
    "verse": 25,
    "book_name": "John",
    "book_id": "John",
    "part": 1,
    "text": "В это время некоторые"
  },
  {
    "chapter": 7,
    "verse": 25,
    "book_name": "John",
    "book_id": "John",
    "part": 2,
    "text": "жители Иерусалима стали"
  },
  {
    "chapter": 7,
    "verse": 25,
    "book_name": "John",
    "book_id": "John",
    "part": 3,
    "text": "спрашивать:"
  },
  {
    "chapter": 7,
    "verse": 25,
    "book_name": "John",
    "book_id": "John",
    "part": 4,
    "text": "Не Тот ли это,"
  },
  {
    "chapter": 7,
    "verse": 25,
    "book_name": "John",
    "book_id": "John",
    "part": 5,
    "text": "Которого хотят убить?"
  },
  {
    "chapter": 7,
    "verse": 26,
    "book_name": "John",
    "book_id": "John",
    "part": 1,
    "text": "Вот же Он открыто"
  },
  {
    "chapter": 7,
    "verse": 26,
    "book_name": "John",
    "book_id": "John",
    "part": 2,
    "text": "говорит перед народом,"
  },
  {
