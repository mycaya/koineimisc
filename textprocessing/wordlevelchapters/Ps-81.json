[
  {
    "chapter": 81,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Дирижеру хора."
  },
  {
    "chapter": 81,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Псалом потомков Корея."
  },
  {
    "chapter": 81,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господи,"
  },
  {
    "chapter": 81,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Ты был милостив"
  },
  {
    "chapter": 81,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "к Своей земле;"
  },
  {
    "chapter": 81,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Ты вернул"
  },
  {
    "chapter": 81,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "благополучие Иакову."
  },
  {
    "chapter": 81,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты простил беззаконие"
  },
  {
    "chapter": 81,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Твоего народа"
  },
  {
    "chapter": 81,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и покрыл все его грехи."
  },
  {
    "chapter": 81,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Пауза"
  },
  {
    "chapter": 81,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты отвел Свою ярость,"
  },
  {
    "chapter": 81,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "отвратил Свой"
  },
  {
    "chapter": 81,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "пылающий гнев."
  },
  {
    "chapter": 81,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Восстанови нас,"
  },
  {
    "chapter": 81,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Боже,"
  },
  {
    "chapter": 81,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "наш Спаситель,"
  },
  {
    "chapter": 81,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "прекрати Свое"
  },
  {
    "chapter": 81,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "негодование на нас."
  },
  {
    "chapter": 81,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Неужели Ты будешь"
  },
  {
    "chapter": 81,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "гневаться на нас вечно,"
  },
  {
    "chapter": 81,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "продлевать Свой гнев из"
  },
  {
    "chapter": 81,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "поколения в поколение?"
  },
  {
    "chapter": 81,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Неужели не"
  },
  {
    "chapter": 81,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "оживишь нас вновь,"
  },
  {
    "chapter": 81,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "чтобы народ Твой"
  },
  {
    "chapter": 81,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "возрадовался о Тебе?"
  },
  {
    "chapter": 81,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Яви нам,"
  },
  {
    "chapter": 81,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 81,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "милость Твою и"
  },
  {
    "chapter": 81,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "даруй нам спасение Твое."
  },
  {
    "chapter": 81,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Послушаю,"
  },
  {
    "chapter": 81,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "что скажет Господь Бог,"
  },
  {
    "chapter": 81,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "ведь Он обещает мир"
  },
  {
    "chapter": 81,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Своему народу тем,"
  },
  {
    "chapter": 81,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "кто предан Ему."
  },
  {
    "chapter": 81,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Лишь бы они снова"
  },
  {
    "chapter": 81,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "не впали в безумие."
  },
  {
    "chapter": 81,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Истинно,"
  },
  {
    "chapter": 81,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "близко Его спасение к тем,"
  },
  {
    "chapter": 81,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "кто боится Его,"
  },
  {
    "chapter": 81,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "чтобы слава Его"
  },
  {
    "chapter": 81,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "поселилась на нашей земле."
  },
  {
    "chapter": 81,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Милость и"
  },
  {
    "chapter": 81,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "истина встретятся,"
  },
  {
    "chapter": 81,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "праведность и"
  },
  {
    "chapter": 81,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "мир поцелуются."
  },
  {
    "chapter": 81,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Истина поднимется с земли,"
  },
  {
    "chapter": 81,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и праведность"
  },
  {
    "chapter": 81,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "посмотрит вниз с небес."
  },
  {
    "chapter": 81,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господь воистину"
  },
  {
    "chapter": 81,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "даст нам благо,"
  },
  {
    "chapter": 81,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и наша земля"
  },
  {
    "chapter": 81,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "принесет урожай."
  },
  {
    "chapter": 81,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Праведность идет перед Ним"
  },
  {
    "chapter": 81,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и для Его стоп готовит"
  },
  {
    "chapter": 81,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "путь."
  }
]