[
  {
    "chapter": 41,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Дирижеру хора."
  },
  {
    "chapter": 41,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Наставление потомков Корея."
  },
  {
    "chapter": 41,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Боже,"
  },
  {
    "chapter": 41,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "своими ушами мы слышали,"
  },
  {
    "chapter": 41,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "рассказали нам"
  },
  {
    "chapter": 41,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "наши отцы о том,"
  },
  {
    "chapter": 41,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "что Ты сделал в их дни,"
  },
  {
    "chapter": 41,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "в дни стародавние."
  },
  {
    "chapter": 41,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Рукою Твоей Ты"
  },
  {
    "chapter": 41,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "изгнал народы,"
  },
  {
    "chapter": 41,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "а наших отцов в"
  },
  {
    "chapter": 41,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "земле насадил."
  },
  {
    "chapter": 41,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Ты сокрушил народы и"
  },
  {
    "chapter": 41,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "заставил оставить их землю."
  },
  {
    "chapter": 41,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Не мечом своим"
  },
  {
    "chapter": 41,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "землю отцы добыли,"
  },
  {
    "chapter": 41,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и не сила их"
  },
  {
    "chapter": 41,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "дала им победу,"
  },
  {
    "chapter": 41,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "а Твоя правая"
  },
  {
    "chapter": 41,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "рука и сила Твоя,"
  },
  {
    "chapter": 41,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "и свет Твоего лица,"
  },
  {
    "chapter": 41,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 8,
    "text": "потому что Ты возлюбил их."
  },
  {
    "chapter": 41,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты Царь мой и Бог;"
  },
  {
    "chapter": 41,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "даруй победы Иакову!"
  },
  {
    "chapter": 41,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "С Тобой мы тесним врага"
  },
  {
    "chapter": 41,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и во Имя Твое попираем"
  },
  {
    "chapter": 41,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "противника."
  },
  {
    "chapter": 41,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я не верю в свой лук,"
  },
  {
    "chapter": 41,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и не меч мой"
  },
  {
    "chapter": 41,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "мне дарит победу."
  },
  {
    "chapter": 41,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Это Ты спасаешь"
  },
  {
    "chapter": 41,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "нас от врагов,"
  },
  {
    "chapter": 41,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "ненавидящих нас"
  },
  {
    "chapter": 41,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "предаешь стыду."
  },
  {
    "chapter": 41,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Богом мы хвалимся каждый"
  },
  {
    "chapter": 41,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "день"
  },
  {
    "chapter": 41,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и Имя Твое будем славить"
  },
  {
    "chapter": 41,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "вовек."
  },
  {
    "chapter": 41,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Пауза"
  },
  {
    "chapter": 41,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но теперь Ты"
  },
  {
    "chapter": 41,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "оставил нас и посрамил,"
  },
  {
    "chapter": 41,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и уже не выходишь с"
  },
  {
    "chapter": 41,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "войсками нашими на битву."
  },
  {
    "chapter": 41,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты обратил нас в"
  },
  {
    "chapter": 41,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "бегство перед врагом,"
  },
  {
    "chapter": 41,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и ненавистники"
  },
  {
    "chapter": 41,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "наши нас грабят."
  },
  {
    "chapter": 41,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты отдал нас,"
  },
  {
    "chapter": 41,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "как овец,"
  },
  {
    "chapter": 41,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "на съедение,"
  },
  {
    "chapter": 41,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и рассеял нас"
  },
  {
    "chapter": 41,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "между народами."
  },
  {
    "chapter": 41,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "За бесценок Ты"
  },
  {
    "chapter": 41,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "продал народ Свой,"
  },
  {
    "chapter": 41,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "ничего от"
  },
  {
    "chapter": 41,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "продажи не выручив."
  },
  {
    "chapter": 41,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты сделал нас"
  },
  {
    "chapter": 41,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "глумлением для наших соседей;"
  },
  {
    "chapter": 41,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "все окружающие смеются"
  },
  {
    "chapter": 41,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и издеваются над нами."
  },
  {
    "chapter": 41,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты сделал нас"
  },
  {
    "chapter": 41,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "посмешищем для других народов;"
  },
  {
    "chapter": 41,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "люди качают головой,"
  },
  {
    "chapter": 41,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "глядя на нас."
  },
  {
    "chapter": 41,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Всякий день"
  },
  {
    "chapter": 41,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "бесчестье мое предо мной,"
  },
  {
    "chapter": 41,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и лицо мое горит от стыда"
  },
  {
    "chapter": 41,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "из-за насмешек тех,"
  },
  {
    "chapter": 41,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "кто злословит"
  },
  {
    "chapter": 41,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "меня и бранит,"
  },
  {
    "chapter": 41,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "из-за мстительного врага."
  },
  {
    "chapter": 41,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Все это случилось с нами,"
  },
  {
    "chapter": 41,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "хотя мы Тебя не забыли"
  },
  {
    "chapter": 41,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и не предали Твой завет."
  },
  {
    "chapter": 41,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Не отступали наши сердца,"
  },
  {
    "chapter": 41,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "наши стопы с пути"
  },
  {
    "chapter": 41,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Твоего не сходили."
  },
  {
    "chapter": 41,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но Ты сокрушил нас,"
  },
  {
    "chapter": 41,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "превратил наши жилища в"
  },
  {
    "chapter": 41,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "шакальи логова"
  },
  {
    "chapter": 41,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и облек нас непроглядной"
  },
  {
    "chapter": 41,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "тьмой."
  },
  {
    "chapter": 41,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Если бы забыли мы Имя"
  },
  {
    "chapter": 41,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "нашего Бога"
  },
  {
    "chapter": 41,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и к чужому богу простерли"
  },
  {
    "chapter": 41,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "руки,"
  },
  {
    "chapter": 41,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "неужели Бог не"
  },
  {
    "chapter": 41,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "узнал бы об этом Тот,"
  },
  {
    "chapter": 41,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Кто ведает тайны сердца?"
  },
  {
    "chapter": 41,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ради Тебя убивают нас"
  },
  {
    "chapter": 41,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "всякий день и смотрят на нас,"
  },
  {
    "chapter": 41,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "как на овец перед бойней."
  },
  {
    "chapter": 41,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пробудись,"
  },
  {
    "chapter": 41,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Владыка! Почему Ты спишь?"
  },
  {
    "chapter": 41,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Восстань! Не отвергни нас"
  },
  {
    "chapter": 41,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "навсегда."
  },
  {
    "chapter": 41,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Почему Ты скрываешь лицо,"
  },
  {
    "chapter": 41,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "забывая беду"
  },
  {
    "chapter": 41,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "нашу и гонения?"
  },
  {
    "chapter": 41,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Сведена наша жизнь во прах,"
  },
  {
    "chapter": 41,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и тела наши льнут к земле."
  },
  {
    "chapter": 41,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Поднимись,"
  },
  {
    "chapter": 41,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "помоги нам;"
  },
  {
    "chapter": 41,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "спаси нас по Твоей милости."
  }
]