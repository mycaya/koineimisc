[
  {
    "chapter": 8,
    "verse": 1,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "В те же дни,"
  },
  {
    "chapter": 8,
    "verse": 1,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "когда опять собралась"
  },
  {
    "chapter": 8,
    "verse": 1,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 3,
    "text": "большая толпа"
  },
  {
    "chapter": 8,
    "verse": 1,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 4,
    "text": "и у людей не было еды,"
  },
  {
    "chapter": 8,
    "verse": 1,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 5,
    "text": "Иисус подозвал"
  },
  {
    "chapter": 8,
    "verse": 1,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 6,
    "text": "учеников и сказал:"
  },
  {
    "chapter": 8,
    "verse": 2,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "- Мне жаль этих людей,"
  },
  {
    "chapter": 8,
    "verse": 2,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "они со Мной"
  },
  {
    "chapter": 8,
    "verse": 2,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 3,
    "text": "вот уже три дня,"
  },
  {
    "chapter": 8,
    "verse": 2,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 4,
    "text": "и у них не осталось еды."
  },
  {
    "chapter": 8,
    "verse": 3,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "Если Я отпущу их"
  },
  {
    "chapter": 8,
    "verse": 3,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "по домам голодными,"
  },
  {
    "chapter": 8,
    "verse": 3,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 3,
    "text": "то они по дороге ослабеют,"
  },
  {
    "chapter": 8,
    "verse": 3,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 4,
    "text": "ведь некоторые"
  },
  {
    "chapter": 8,
    "verse": 3,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 5,
    "text": "пришли издалека."
  },
  {
    "chapter": 8,
    "verse": 4,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "- Как же здесь,"
  },
  {
    "chapter": 8,
    "verse": 4,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "в таком безлюдном месте,"
  },
  {
    "chapter": 8,
    "verse": 4,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 3,
    "text": "найти достаточно хлеба,"
  },
  {
    "chapter": 8,
    "verse": 4,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 4,
    "text": "чтобы их накормить?"
  },
  {
    "chapter": 8,
    "verse": 4,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 5,
    "text": "удивились ученики."
  },
  {
    "chapter": 8,
    "verse": 5,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "- Сколько у вас"
  },
  {
    "chapter": 8,
    "verse": 5,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "хлебов? спросил Иисус."
  },
  {
    "chapter": 8,
    "verse": 5,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 3,
    "text": "Семь,"
  },
  {
    "chapter": 8,
    "verse": 5,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 4,
    "text": "ответили ученики."
  },
  {
    "chapter": 8,
    "verse": 6,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "Иисус велел людям"
  },
  {
    "chapter": 8,
    "verse": 6,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "возлечь на земле."
  },
  {
    "chapter": 8,
    "verse": 6,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 3,
    "text": "Потом Он взял"
  },
  {
    "chapter": 8,
    "verse": 6,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 4,
    "text": "семь хлебов и,"
  },
  {
    "chapter": 8,
    "verse": 6,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 5,
    "text": "поблагодарив за них Бога,"
  },
  {
    "chapter": 8,
    "verse": 6,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 6,
    "text": "стал разламывать на куски,"
  },
  {
    "chapter": 8,
    "verse": 6,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 7,
    "text": "передавая ученикам,"
  },
  {
    "chapter": 8,
    "verse": 6,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 8,
    "text": "а те раздавали хлеб народу."
  },
  {
    "chapter": 8,
    "verse": 7,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "Было у них и"
  },
  {
    "chapter": 8,
    "verse": 7,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "несколько рыбок;"
  },
  {
    "chapter": 8,
    "verse": 7,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 3,
    "text": "Иисус благословил их"
  },
  {
    "chapter": 8,
    "verse": 7,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 4,
    "text": "и тоже велел раздать."
  },
  {
    "chapter": 8,
    "verse": 8,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "Люди ели и насытились,"
  },
  {
    "chapter": 8,
    "verse": 8,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "и еще набралось"
  },
  {
    "chapter": 8,
    "verse": 8,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 3,
    "text": "семь корзин остатков."
  },
  {
    "chapter": 8,
    "verse": 9,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "А было там только мужчин"
  },
  {
    "chapter": 8,
    "verse": 9,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "около четырех тысяч человек."
  },
  {
    "chapter": 8,
    "verse": 9,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 3,
    "text": "Иисус отпустил народ"
  },
  {
    "chapter": 8,
    "verse": 10,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "и сразу же,"
  },
  {
    "chapter": 8,
    "verse": 10,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "сев вместе с"
  },
  {
    "chapter": 8,
    "verse": 10,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 3,
    "text": "учениками в лодку,"
  },
  {
    "chapter": 8,
    "verse": 10,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 4,
    "text": "отправился в"
  },
  {
    "chapter": 8,
    "verse": 10,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 5,
    "text": "Далмануфские земли."
  },
  {
    "chapter": 8,
    "verse": 11,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "К Иисусу подошли"
  },
  {
    "chapter": 8,
    "verse": 11,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "фарисеи и стали с Ним спорить."
  },
  {
    "chapter": 8,
    "verse": 11,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 3,
    "text": "Они хотели испытать Его"
  },
  {
    "chapter": 8,
    "verse": 11,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 4,
    "text": "и требовали от Него"
  },
  {
    "chapter": 8,
    "verse": 11,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 5,
    "text": "знамения с неба."
  },
  {
    "chapter": 8,
    "verse": 12,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "Иисус глубоко"
  },
  {
    "chapter": 8,
    "verse": 12,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "вздохнул и спросил:"
  },
  {
    "chapter": 8,
    "verse": 12,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 3,
    "text": "Почему это поколение"
  },
  {
    "chapter": 8,
    "verse": 12,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 4,
    "text": "требует знамения? Говорю"
  },
  {
    "chapter": 8,
    "verse": 12,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 5,
    "text": "вам истину,"
  },
  {
    "chapter": 8,
    "verse": 12,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 6,
    "text": "ему не будет дано"
  },
  {
    "chapter": 8,
    "verse": 12,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 7,
    "text": "никакого знамения."
  },
  {
    "chapter": 8,
    "verse": 13,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "И,"
  },
  {
    "chapter": 8,
    "verse": 13,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "оставив их,"
  },
  {
    "chapter": 8,
    "verse": 13,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 3,
    "text": "Он снова сел в лодку"
  },
  {
    "chapter": 8,
    "verse": 13,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 4,
    "text": "и отправился на другую"
  },
  {
    "chapter": 8,
    "verse": 13,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 5,
    "text": "сторону озера."
  },
  {
    "chapter": 8,
    "verse": 14,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "Ученики забыли"
  },
  {
    "chapter": 8,
    "verse": 14,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "взять с собой хлеба,"
  },
  {
    "chapter": 8,
    "verse": 14,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 3,
    "text": "и в лодке у них"
  },
  {
    "chapter": 8,
    "verse": 14,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 4,
    "text": "был всего один хлеб."
  },
  {
    "chapter": 8,
    "verse": 15,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "А Иисус предостерегал их:"
  },
  {
    "chapter": 8,
    "verse": 15,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "Смотрите,"
  },
  {
    "chapter": 8,
    "verse": 15,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 3,
    "text": "берегитесь закваски"
  },
  {
    "chapter": 8,
    "verse": 15,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 4,
    "text": "фарисеев и закваски Ирода."
  },
  {
    "chapter": 8,
    "verse": 16,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "Ученики стали"
  },
  {
    "chapter": 8,
    "verse": 16,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "рассуждать между собой:"
  },
  {
    "chapter": 8,
    "verse": 16,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 3,
    "text": "Он говорит это потому,"
  },
  {
    "chapter": 8,
    "verse": 16,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 4,
    "text": "что мы не взяли"
  },
  {
    "chapter": 8,
    "verse": 16,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 5,
    "text": "с собой хлеба?"
  },
  {
    "chapter": 8,
    "verse": 17,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "Зная,"
  },
  {
    "chapter": 8,
    "verse": 17,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "о чем они говорят,"
  },
  {
    "chapter": 8,
    "verse": 17,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 3,
    "text": "Иисус сказал:"
  },
  {
    "chapter": 8,
    "verse": 17,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 4,
    "text": "Почему вы"
  },
  {
    "chapter": 8,
    "verse": 17,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 5,
    "text": "рассуждаете о том,"
  },
  {
    "chapter": 8,
    "verse": 17,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 6,
    "text": "что у вас нет хлеба?"
  },
  {
    "chapter": 8,
    "verse": 17,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 7,
    "text": "Неужели вы все еще не"
  },
  {
    "chapter": 8,
    "verse": 17,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 8,
    "text": "сознаете"
  },
  {
    "chapter": 8,
    "verse": 17,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 9,
    "text": "и не понимаете? Неужели"
  },
  {
    "chapter": 8,
    "verse": 17,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 10,
    "text": "сердца ваши совсем"
  },
  {
    "chapter": 8,
    "verse": 17,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 11,
    "text": "окаменели?"
  },
  {
    "chapter": 8,
    "verse": 18,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "У вас есть глаза,"
  },
  {
    "chapter": 8,
    "verse": 18,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "и вы не видите? Есть уши,"
  },
  {
    "chapter": 8,
    "verse": 18,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 3,
    "text": "и вы не слышите?"
  },
  {
    "chapter": 8,
    "verse": 18,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 4,
    "text": "Неужели вы не помните?"
  },
  {
    "chapter": 8,
    "verse": 19,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "Когда Я разделил пять"
  },
  {
    "chapter": 8,
    "verse": 19,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "хлебов на пять тысяч человек,"
  },
  {
    "chapter": 8,
    "verse": 19,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 3,
    "text": "сколько корзин остатков"
  },
  {
    "chapter": 8,
    "verse": 19,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 4,
    "text": "вы набрали? Двенадцать,"
  },
  {
    "chapter": 8,
    "verse": 19,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 5,
    "text": "ответили ученики."
  },
  {
    "chapter": 8,
    "verse": 20,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "- А когда семь"
  },
  {
    "chapter": 8,
    "verse": 20,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "хлебов на четыре тысячи,"
  },
  {
    "chapter": 8,
    "verse": 20,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 3,
    "text": "сколько полных корзин"
  },
  {
    "chapter": 8,
    "verse": 20,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 4,
    "text": "остатков вы набрали? Семь,"
  },
  {
    "chapter": 8,
    "verse": 20,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 5,
    "text": "ответили те."
  },
  {
    "chapter": 8,
    "verse": 21,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "- Так неужели вы все еще"
  },
  {
    "chapter": 8,
    "verse": 21,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "не понимаете? сказал Иисус."
  },
  {
    "chapter": 8,
    "verse": 22,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "Когда они"
  },
  {
    "chapter": 8,
    "verse": 22,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "пришли в Вифсаиду,"
  },
  {
    "chapter": 8,
    "verse": 22,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 3,
    "text": "к Иисусу привели слепого"
  },
  {
    "chapter": 8,
    "verse": 22,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 4,
    "text": "и попросили прикоснуться к"
  },
  {
    "chapter": 8,
    "verse": 22,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 5,
    "text": "нему."
  },
  {
    "chapter": 8,
    "verse": 23,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "Иисус взял слепого за руку,"
  },
  {
    "chapter": 8,
    "verse": 23,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "вывел из селения,"
  },
  {
    "chapter": 8,
    "verse": 23,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 3,
    "text": "плюнул ему на глаза и,"
  },
  {
    "chapter": 8,
    "verse": 23,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 4,
    "text": "возложив на него руки,"
  },
  {
    "chapter": 8,
    "verse": 23,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 5,
    "text": "спросил:"
  },
  {
    "chapter": 8,
    "verse": 23,
    "book_name": "Mark",
    "book_id": "Mark",
