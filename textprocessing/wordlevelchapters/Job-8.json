[
  {
    "chapter": 8,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Тогда ответил"
  },
  {
    "chapter": 8,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Вилдад из Савхы:"
  },
  {
    "chapter": 8,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "- Долго ли будешь ты так"
  },
  {
    "chapter": 8,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "говорить? Твои слова буйный"
  },
  {
    "chapter": 8,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "ветер!"
  },
  {
    "chapter": 8,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Разве Бог"
  },
  {
    "chapter": 8,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "извращает суд? Извращает ли"
  },
  {
    "chapter": 8,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Всемогущий правду?"
  },
  {
    "chapter": 8,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Если твои дети"
  },
  {
    "chapter": 8,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "согрешили против Него,"
  },
  {
    "chapter": 8,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Он отдал их во"
  },
  {
    "chapter": 8,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "власть их греха."
  },
  {
    "chapter": 8,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Но если ты воззовешь к"
  },
  {
    "chapter": 8,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Богу и помолишься Всемогущему,"
  },
  {
    "chapter": 8,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "если ты чист и праведен,"
  },
  {
    "chapter": 8,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "то"
  },
  {
    "chapter": 8,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и ныне поднимется Он для"
  },
  {
    "chapter": 8,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "тебя"
  },
  {
    "chapter": 8,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "и вернет тебя на должное"
  },
  {
    "chapter": 8,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 6,
    "text": "место."
  },
  {
    "chapter": 8,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "И твое начало"
  },
  {
    "chapter": 8,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "покажется ничтожным,"
  },
  {
    "chapter": 8,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "так велик будет твой конец."
  },
  {
    "chapter": 8,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Спроси прежнее поколение,"
  },
  {
    "chapter": 8,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "рассмотри,"
  },
  {
    "chapter": 8,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "что узнали их предки,"
  },
  {
    "chapter": 8,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "ведь мы лишь"
  },
  {
    "chapter": 8,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "вчерашние и ничего не знаем,"
  },
  {
    "chapter": 8,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "наши дни на"
  },
  {
    "chapter": 8,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "земле лишь тень."
  },
  {
    "chapter": 8,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Неужели они не научат тебя,"
  },
  {
    "chapter": 8,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "не наставят и слов"
  },
  {
    "chapter": 8,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "своей мудрости не изрекут?"
  },
  {
    "chapter": 8,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Растет ли папирус,"
  },
  {
    "chapter": 8,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "где нет болот?"
  },
  {
    "chapter": 8,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Поднимается ли тростник без влаги?"
  },
  {
    "chapter": 8,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Пока в цвету они"
  },
  {
    "chapter": 8,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и еще не срезаны,"
  },
  {
    "chapter": 8,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "но засыхают"
  },
  {
    "chapter": 8,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "быстрее всех трав."
  },
  {
    "chapter": 8,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Таков удел всех,"
  },
  {
    "chapter": 8,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "кто забыл Бога,"
  },
  {
    "chapter": 8,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и умирает"
  },
  {
    "chapter": 8,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "надежда безбожных."
  },
  {
    "chapter": 8,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Их упование подсечено,"
  },
  {
    "chapter": 8,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "их"
  },
  {
    "chapter": 8,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "безопасность паучья сеть."
  },
  {
    "chapter": 8,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Обопрутся на сеть"
  },
  {
    "chapter": 8,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "свою она не выдержит,"
  },
  {
    "chapter": 8,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "а если схватятся"
  },
  {
    "chapter": 8,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "сеть не удержит."
  },
  {
    "chapter": 8,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Расцветают они под солнцем,"
  },
  {
    "chapter": 8,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "простирая ветви над садом;"
  },
  {
    "chapter": 8,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "груду камней"
  },
  {
    "chapter": 8,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "оплетая корнями,"
  },
  {
    "chapter": 8,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "пробиваются они меж камней."
  },
  {
    "chapter": 8,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "А когда их"
  },
  {
    "chapter": 8,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "исторгнут с прежнего места,"
  },
  {
    "chapter": 8,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "само место от"
  },
  {
    "chapter": 8,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "них отречется:"
  },
  {
    "chapter": 8,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "Я никогда вас не видело."
  },
  {
    "chapter": 8,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Посмотри,"
  },
  {
    "chapter": 8,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "как счастливы их пути! А"
  },
  {
    "chapter": 8,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "из земли прорастут другие."
  },
  {
    "chapter": 8,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Итак не отвергнет Бог"
  },
  {
    "chapter": 8,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "беспорочного"
  },
  {
    "chapter": 8,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и не поддержит руки злодея."
  },
  {
    "chapter": 8,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Он наполнит еще смехом твои"
  },
  {
    "chapter": 8,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "уста"
  },
  {
    "chapter": 8,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и ликованием губы твои."
  },
  {
    "chapter": 8,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Враги твои"
  },
  {
    "chapter": 8,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "облекутся в стыд,"
  },
  {
    "chapter": 8,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и шатров"
  },
  {
    "chapter": 8,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "нечестивых не станет."
  }
]