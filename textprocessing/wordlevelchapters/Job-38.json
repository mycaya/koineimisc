[
  {
    "chapter": 38,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "И Господь"
  },
  {
    "chapter": 38,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "ответил Иову из бури."
  },
  {
    "chapter": 38,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Он сказал:"
  },
  {
    "chapter": 38,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "- Кто ты,"
  },
  {
    "chapter": 38,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "что в невежестве Мой"
  },
  {
    "chapter": 38,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "замысел омрачаешь словами,"
  },
  {
    "chapter": 38,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "в которых нет знания?"
  },
  {
    "chapter": 38,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Препояшь себя,"
  },
  {
    "chapter": 38,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "как мужчина;"
  },
  {
    "chapter": 38,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Я буду спрашивать,"
  },
  {
    "chapter": 38,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "а ты отвечай."
  },
  {
    "chapter": 38,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Где ты был,"
  },
  {
    "chapter": 38,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "когда Я землю"
  },
  {
    "chapter": 38,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "утверждал? Если ты знаешь,"
  },
  {
    "chapter": 38,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "то отвечай Мне."
  },
  {
    "chapter": 38,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Кто положил ей"
  },
  {
    "chapter": 38,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "предел? Конечно,"
  },
  {
    "chapter": 38,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "ты знаешь! Кто"
  },
  {
    "chapter": 38,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "протянул над ней мерную нить?"
  },
  {
    "chapter": 38,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "На чем покоится"
  },
  {
    "chapter": 38,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "земное основание,"
  },
  {
    "chapter": 38,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и кто заложил ее"
  },
  {
    "chapter": 38,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "краеугольный камень,"
  },
  {
    "chapter": 38,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "когда вместе пели"
  },
  {
    "chapter": 38,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "все звезды утра,"
  },
  {
    "chapter": 38,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и сыны Божьи"
  },
  {
    "chapter": 38,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "ликовали от радости?"
  },
  {
    "chapter": 38,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Кто запер воротами море,"
  },
  {
    "chapter": 38,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "когда оно вышло исторглось,"
  },
  {
    "chapter": 38,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "как бы из чрева,"
  },
  {
    "chapter": 38,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "когда сделал Я тучи его"
  },
  {
    "chapter": 38,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "одеждой"
  },
  {
    "chapter": 38,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и обвил его пеленами мглы,"
  },
  {
    "chapter": 38,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "когда Я"
  },
  {
    "chapter": 38,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "назначил ему рубежи,"
  },
  {
    "chapter": 38,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и поставил"
  },
  {
    "chapter": 38,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "ворота его и запоры,"
  },
  {
    "chapter": 38,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "когда Я сказал:"
  },
  {
    "chapter": 38,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "До этого места дойдешь,"
  },
  {
    "chapter": 38,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "но не дальше,"
  },
  {
    "chapter": 38,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "здесь предел для"
  },
  {
    "chapter": 38,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "гордых твоих валов?"
  },
  {
    "chapter": 38,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Случалось тебе приказывать"
  },
  {
    "chapter": 38,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "утру"
  },
  {
    "chapter": 38,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и указывать место заре,"
  },
  {
    "chapter": 38,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "чтоб она охватила края"
  },
  {
    "chapter": 38,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "земли"
  },
  {
    "chapter": 38,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и стряхнула с нее злодеев?"
  },
  {
    "chapter": 38,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "И земля обретает свое лицо,"
  },
  {
    "chapter": 38,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "как глина под печатью,"
  },
  {
    "chapter": 38,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и расцвечивается,"
  },
  {
    "chapter": 38,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "как риза."
  },
  {
    "chapter": 38,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Но у злодеев отнят их свет"
  },
  {
    "chapter": 38,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и надменная мышца"
  },
  {
    "chapter": 38,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "разорвана."
  },
  {
    "chapter": 38,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Доходил ли ты до"
  },
  {
    "chapter": 38,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "истоков моря"
  },
  {
    "chapter": 38,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и ходил ли по дну пучины?"
  },
  {
    "chapter": 38,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Открывались ли тебе двери"
  },
  {
    "chapter": 38,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "смерти? Видел ли ты врата"
  },
  {
    "chapter": 38,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "мрака?"
  },
  {
    "chapter": 38,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Обозрел ли ты"
  },
  {
    "chapter": 38,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "широту земли? Скажи Мне,"
  },
  {
    "chapter": 38,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "если все это знаешь."
  },
  {
    "chapter": 38,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Где путь к обители"
  },
  {
    "chapter": 38,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "света? Где пребывает тьма?"
  },
  {
    "chapter": 38,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Ты,"
  },
  {
    "chapter": 38,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "верно,"
  },
  {
    "chapter": 38,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "в пределы ее входил и"
  },
  {
    "chapter": 38,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "знаешь путь к ее дому?"
  },
  {
    "chapter": 38,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Конечно же знаешь,"
  },
  {
    "chapter": 38,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "ведь ты тогда родился,"
  },
  {
    "chapter": 38,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и прожил так много лет!"
  },
  {
    "chapter": 38,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Входил ли ты в кладовые"
  },
  {
    "chapter": 38,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "снега"
  },
  {
    "chapter": 38,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и видел ли хранилища града?"
  },
  {
    "chapter": 38,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "На смутное"
  },
  {
    "chapter": 38,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "время Я их берегу,"
  },
  {
    "chapter": 38,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "на дни войны и побоищ."
  },
  {
    "chapter": 38,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "По какому пути"
  },
  {
    "chapter": 38,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "разливается свет,"
  },
  {
    "chapter": 38,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и мчится на землю"
  },
  {
    "chapter": 38,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "восточный ветер?"
  },
  {
    "chapter": 38,
    "verse": 25,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Кто устроил проток для"
  },
  {
    "chapter": 38,
    "verse": 25,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "ливня и путь проложил грозе,"
  },
  {
    "chapter": 38,
    "verse": 26,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "чтобы землю"
  },
  {
    "chapter": 38,
    "verse": 26,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "безлюдную оросить,"
  },
  {
    "chapter": 38,
    "verse": 26,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "пустыню,"
  },
  {
    "chapter": 38,
    "verse": 26,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "где нет людей,"
  },
  {
    "chapter": 38,
    "verse": 27,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "дикую степь напитать водой,"
  },
  {
    "chapter": 38,
    "verse": 27,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и взрастить из земли траву?"
  },
  {
    "chapter": 38,
    "verse": 28,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Есть ли отец у дождя?"
  },
  {
    "chapter": 38,
    "verse": 28,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Кто капли росы родит?"
  },
  {
    "chapter": 38,
    "verse": 29,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Из чьей утробы выходит лед?"
  },
  {
    "chapter": 38,
    "verse": 29,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Кто на свет производит"
  },
  {
    "chapter": 38,
    "verse": 29,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "небесный иней,"
  },
  {
    "chapter": 38,
    "verse": 30,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "когда воды,"
  },
  {
    "chapter": 38,
    "verse": 30,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "как камень,"
  },
  {
    "chapter": 38,
    "verse": 30,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "крепнут,"
  },
  {
    "chapter": 38,
    "verse": 30,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "и твердеет лицо морей?"
  },
  {
    "chapter": 38,
    "verse": 31,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Властен ли ты"
  },
  {
    "chapter": 38,
    "verse": 31,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "связать узел Плеяд,"
  },
  {
    "chapter": 38,
    "verse": 31,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и развязать узы Ориона?"
  },
  {
    "chapter": 38,
    "verse": 32,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Ты можешь созвездия вывести"
  },
  {
    "chapter": 38,
    "verse": 32,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "в срок"
  },
  {
    "chapter": 38,
    "verse": 32,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и вести Большую Медведицу с"
  },
  {
    "chapter": 38,
    "verse": 32,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "медвежатами?"
  },
  {
    "chapter": 38,
    "verse": 33,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Известны тебе уставы небес?"
  },
  {
    "chapter": 38,
    "verse": 33,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Ты можешь"
  },
  {
    "chapter": 38,
    "verse": 33,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "их власть утвердить на"
  },
  {
    "chapter": 38,
    "verse": 33,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "земле?"
  },
  {
    "chapter": 38,
    "verse": 34,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Можешь ли ты возвысить свой"
  },
  {
    "chapter": 38,
    "verse": 34,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "голос к тучам чтобы вода в"
  },
  {
    "chapter": 38,
    "verse": 34,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "обилии покрыла тебя?"
  },
  {
    "chapter": 38,
    "verse": 35,
    "book_name": "Job",
