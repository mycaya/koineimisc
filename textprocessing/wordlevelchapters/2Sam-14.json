  },
  {
    "chapter": 14,
    "verse": 19,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "Верно,"
  },
  {
    "chapter": 14,
    "verse": 19,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 6,
    "text": "как и то,"
  },
  {
    "chapter": 14,
    "verse": 19,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
