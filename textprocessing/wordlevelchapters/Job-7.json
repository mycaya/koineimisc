[
  {
    "chapter": 7,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Не тяжкий ли труд уготовлен"
  },
  {
    "chapter": 7,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "на земле человеку? Дни его"
  },
  {
    "chapter": 7,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "не дни ли батрака?"
  },
  {
    "chapter": 7,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Как раб,"
  },
  {
    "chapter": 7,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "который жаждет"
  },
  {
    "chapter": 7,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "вечерней тени,"
  },
  {
    "chapter": 7,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "как батрак,"
  },
  {
    "chapter": 7,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "который ожидает платы,"
  },
  {
    "chapter": 7,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "так и я"
  },
  {
    "chapter": 7,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "получил месяцы суеты,"
  },
  {
    "chapter": 7,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и горькие ночи"
  },
  {
    "chapter": 7,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "отпущены мне."
  },
  {
    "chapter": 7,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Ложась,"
  },
  {
    "chapter": 7,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "размышляю:"
  },
  {
    "chapter": 7,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Когда я встану?"
  },
  {
    "chapter": 7,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "но тянется ночь,"
  },
  {
    "chapter": 7,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "и я ворочаюсь до зари."
  },
  {
    "chapter": 7,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Червями и"
  },
  {
    "chapter": 7,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "язвами плоть одета,"
  },
  {
    "chapter": 7,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "кожа"
  },
  {
    "chapter": 7,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "потрескалась и гноится."
  },
  {
    "chapter": 7,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Бегут мои дни"
  },
  {
    "chapter": 7,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "быстрее ткацкого челнока,"
  },
  {
    "chapter": 7,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и к концу"
  },
  {
    "chapter": 7,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "устремляются без надежды."
  },
  {
    "chapter": 7,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Вспомни,"
  },
  {
    "chapter": 7,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Боже,"
  },
  {
    "chapter": 7,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "что жизнь моя"
  },
  {
    "chapter": 7,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "лишь дуновение;"
  },
  {
    "chapter": 7,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "уже не увидеть"
  },
  {
    "chapter": 7,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 6,
    "text": "счастья моим глазам."
  },
  {
    "chapter": 7,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Око,"
  },
  {
    "chapter": 7,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "что видит меня,"
  },
  {
    "chapter": 7,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "не увидит меня вскоре;"
  },
  {
    "chapter": 7,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "будешь искать меня,"
  },
  {
    "chapter": 7,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "но меня больше нет."
  },
  {
    "chapter": 7,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Как редеет"
  },
  {
    "chapter": 7,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "облако и исчезает,"
  },
  {
    "chapter": 7,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "так не выйдет и тот,"
  },
  {
    "chapter": 7,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "кто спускается"
  },
  {
    "chapter": 7,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "в мир мертвых."
  },
  {
    "chapter": 7,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Не возвратится он"
  },
  {
    "chapter": 7,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "больше в свой дом,"
  },
  {
    "chapter": 7,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и не вспомнит о"
  },
  {
    "chapter": 7,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "нем его местность."
  },
  {
    "chapter": 7,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Поэтому я не стану молчать"
  },
  {
    "chapter": 7,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "выговорюсь в скорби духа,"
  },
  {
    "chapter": 7,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "в муке души пожалуюсь."
  },
  {
    "chapter": 7,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Разве я море или"
  },
  {
    "chapter": 7,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "чудовище бездны,"
  },
  {
    "chapter": 7,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "что Ты окружил"
  },
  {
    "chapter": 7,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "меня стражей?"
  },
  {
    "chapter": 7,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Как подумаю:"
  },
  {
    "chapter": 7,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Утешит меня постель,"
  },
  {
    "chapter": 7,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "печаль мою ложе развеет,"
  },
  {
    "chapter": 7,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "так Ты снами меня"
  },
  {
    "chapter": 7,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "страшишь и ужасаешь видениями,"
  },
  {
    "chapter": 7,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "и тогда мне лучше,"
  },
  {
    "chapter": 7,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "чтобы прекратилось дыхание,"
  },
  {
    "chapter": 7,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и я умер,"
  },
  {
    "chapter": 7,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "чем пребывать в этом теле."
  },
  {
    "chapter": 7,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Я презираю жизнь все"
  },
  {
    "chapter": 7,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "равно мне не жить вечно."
  },
  {
    "chapter": 7,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Отступи от меня"
  },
  {
    "chapter": 7,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "мои дни суета."
  },
  {
    "chapter": 7,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Кто такой человек,"
  },
  {
    "chapter": 7,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "что Ты так его возвеличил,"
  },
  {
    "chapter": 7,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "что обращаешь"
  },
  {
    "chapter": 7,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "на него внимание,"
  },
  {
    "chapter": 7,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "что каждое утро посещаешь"
  },
  {
    "chapter": 7,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "его и поминутно испытываешь?"
  },
  {
    "chapter": 7,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Неужели не"
  },
  {
    "chapter": 7,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "отступишь Ты от меня,"
  },
  {
    "chapter": 7,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "не дашь сглотнуть слюну?"
  },
  {
    "chapter": 7,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Если я согрешил,"
  },
  {
    "chapter": 7,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "что Тебе я сделал,"
  },
  {
    "chapter": 7,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Тебе,"
  },
  {
    "chapter": 7,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "о Страж человека? За что Ты"
  },
  {
    "chapter": 7,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "поставил меня Своей"
  },
  {
    "chapter": 7,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 6,
    "text": "мишенью? За что я стал Тебе"
  },
  {
    "chapter": 7,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 7,
    "text": "в тягость?"
  },
  {
    "chapter": 7,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Почему не отпустишь моих"
  },
  {
    "chapter": 7,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "проступков"
  },
  {
    "chapter": 7,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и не простишь грехов?"
  },
  {
    "chapter": 7,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "Скоро,"
  },
  {
    "chapter": 7,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "скоро я лягу в землю;"
  },
  {
    "chapter": 7,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 6,
    "text": "будешь искать меня,"
  },
  {
    "chapter": 7,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 7,
    "text": "но меня уже не будет."
  }
]