  {
    "chapter": 15,
    "verse": 18,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 5,
    "text": "истреби этот нечестивый"
  },
  {
    "chapter": 15,
    "verse": 18,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 6,
    "text": "народ амаликитян."
  },
  {
    "chapter": 15,
    "verse": 18,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 7,
    "text": "Веди с ними войну,"
  },
