  {
    "chapter": 15,
    "verse": 32,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 3,
    "text": "царя амаликитян."
  },
  {
    "chapter": 15,
    "verse": 32,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 4,
    "text": "Агаг подошел к"
  },
  {
    "chapter": 15,
    "verse": 32,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 5,
    "text": "нему уверенно,"
  },
  {
    "chapter": 15,
    "verse": 32,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 6,
    "text": "думая:"
  },
  {
    "chapter": 15,
    "verse": 32,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 7,
    "text": "Конечно,"
  },
  {
    "chapter": 15,
    "verse": 32,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 8,
    "text": "горечь смерти"
  },
  {
    "chapter": 15,
    "verse": 32,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 9,
    "text": "прошла стороной."
  },
  {
    "chapter": 15,
    "verse": 33,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 1,
    "text": "Но Самуил сказал:"
  },
  {
    "chapter": 15,
    "verse": 33,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 2,
    "text": "Как меч твой лишал"
  },
  {
    "chapter": 15,
    "verse": 33,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
