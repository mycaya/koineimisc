  {
    "chapter": 15,
    "verse": 29,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 3,
    "text": "ведь Он не человек,"
  },
  {
    "chapter": 15,
    "verse": 29,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 4,
    "text": "чтобы передумывать."
  },
  {
    "chapter": 15,
    "verse": 30,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 1,
    "text": "Саул ответил:"
  },
  {
    "chapter": 15,
    "verse": 30,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 2,
    "text": "Я согрешил."
  },
  {
    "chapter": 15,
    "verse": 30,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 3,
    "text": "Но пожалуйста,"
