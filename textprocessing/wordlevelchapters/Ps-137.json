[
  {
    "chapter": 137,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Псалом Давида."
  },
  {
    "chapter": 137,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 137,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "услышь молитву мою и"
  },
  {
    "chapter": 137,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "внемли моим молениям."
  },
  {
    "chapter": 137,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "По Своей верности и"
  },
  {
    "chapter": 137,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "праведности ответь мне."
  },
  {
    "chapter": 137,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Со слугой Своим"
  },
  {
    "chapter": 137,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "тяжбы не начинай,"
  },
  {
    "chapter": 137,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "потому что никто"
  },
  {
    "chapter": 137,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "из живущих не оправдается"
  },
  {
    "chapter": 137,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "перед Тобой."
  },
  {
    "chapter": 137,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Преследует меня враг,"
  },
  {
    "chapter": 137,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "жизнь мою придавит к земле;"
  },
  {
    "chapter": 137,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "принуждает"
  },
  {
    "chapter": 137,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "меня жить во тьме,"
  },
  {
    "chapter": 137,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "как давно умершие."
  },
  {
    "chapter": 137,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я пал духом,"
  },
  {
    "chapter": 137,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "сердце во мне онемело."
  },
  {
    "chapter": 137,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я вспоминаю"
  },
  {
    "chapter": 137,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "давно прошедшие дни,"
  },
  {
    "chapter": 137,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "размышляю о"
  },
  {
    "chapter": 137,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "всех Твоих делах,"
  },
  {
    "chapter": 137,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "думаю о том,"
  },
  {
    "chapter": 137,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "что сделали Твои руки."
  },
  {
    "chapter": 137,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я простираю"
  },
  {
    "chapter": 137,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "руки мои к Тебе;"
  },
  {
    "chapter": 137,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "душа моя жаждет Тебя,"
  },
  {
    "chapter": 137,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "как высохшая земля."
  },
  {
    "chapter": 137,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Пауза"
  },
  {
    "chapter": 137,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Поспеши,"
  },
  {
    "chapter": 137,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "ответь мне,"
  },
  {
    "chapter": 137,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Господи;"
  },
  {
    "chapter": 137,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "дух мой изнемогает."
  },
  {
    "chapter": 137,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Не скрывай от"
  },
  {
    "chapter": 137,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "меня Твоего лица,"
  },
  {
    "chapter": 137,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "иначе я уподоблюсь тем,"
  },
  {
    "chapter": 137,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 8,
    "text": "кто нисходит в пропасть."
  },
  {
    "chapter": 137,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Дай мне утром"
  },
  {
    "chapter": 137,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "услышать о Твоей милости,"
  },
  {
    "chapter": 137,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "ведь я на Тебя полагаюсь."
  },
  {
    "chapter": 137,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Путь укажи мне,"
  },
  {
    "chapter": 137,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "по которому мне идти,"
  },
  {
    "chapter": 137,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "ведь к Тебе возношу я душу."
  },
  {
    "chapter": 137,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господи,"
  },
  {
    "chapter": 137,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "избавь меня от врагов;"
  },
  {
    "chapter": 137,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "я прибегаю к"
  },
  {
    "chapter": 137,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Тебе за защитой."
  },
  {
    "chapter": 137,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Научи меня"
  },
  {
    "chapter": 137,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "исполнять волю Твою,"
  },
  {
    "chapter": 137,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "потому что Ты мой Бог."
  },
  {
    "chapter": 137,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Пусть выведет меня Дух"
  },
  {
    "chapter": 137,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Твой благой по ровному пути."
  },
  {
    "chapter": 137,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ради Имени Твоего,"
  },
  {
    "chapter": 137,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 137,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "оживи меня;"
  },
  {
    "chapter": 137,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "ради праведности"
  },
  {
    "chapter": 137,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Твоей выручи меня из беды."
  },
  {
    "chapter": 137,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "По милости Твоей"
  },
  {
    "chapter": 137,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "погуби врагов моих,"
  },
  {
    "chapter": 137,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "истреби всех,"
  },
  {
    "chapter": 137,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "кто ищет моей жизни,"
  },
  {
    "chapter": 137,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "ведь я Твой слуга."
  }
