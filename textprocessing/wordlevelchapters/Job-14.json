[
  {
    "chapter": 14,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Человек,"
  },
  {
    "chapter": 14,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "рожденный от женщины,"
  },
  {
    "chapter": 14,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "скуден днями,"
  },
  {
    "chapter": 14,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "но скорбью сыт."
  },
  {
    "chapter": 14,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Он,"
  },
  {
    "chapter": 14,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "как цветок,"
  },
  {
    "chapter": 14,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "прорастает и вянет."
  },
  {
    "chapter": 14,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "Ускользает,"
  },
  {
    "chapter": 14,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "как тень,"
  },
  {
    "chapter": 14,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 6,
    "text": "не задерживается."
  },
  {
    "chapter": 14,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "И на нем задержал Ты"
  },
  {
    "chapter": 14,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "взгляд? И его Ты на суд ведешь?"
  },
  {
    "chapter": 14,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Кто выведет чистое"
  },
  {
    "chapter": 14,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "из нечистого? Никто!"
  },
  {
    "chapter": 14,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Если дни его установлены,"
  },
  {
    "chapter": 14,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и число его"
  },
  {
    "chapter": 14,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "месяцев Ты сосчитал,"
  },
  {
    "chapter": 14,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "и поставил рубеж,"
  },
  {
    "chapter": 14,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "который он не преступит,"
  },
  {
    "chapter": 14,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "то отведи от"
  },
  {
    "chapter": 14,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "него Свой взгляд,"
  },
  {
    "chapter": 14,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "оставь его в покое,"
  },
  {
    "chapter": 14,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "пусть он,"
  },
  {
    "chapter": 14,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "как батрак,"
  },
  {
    "chapter": 14,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 6,
    "text": "порадуется своим дням."
  },
  {
    "chapter": 14,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Ведь и для"
  },
  {
    "chapter": 14,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "дерева есть надежда:"
  },
  {
    "chapter": 14,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "если срубят его,"
  },
  {
    "chapter": 14,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "оно оживет и"
  },
  {
    "chapter": 14,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "снова пустит побеги."
  },
  {
    "chapter": 14,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Пусть корни его"
  },
  {
    "chapter": 14,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "одряхлели в земле,"
  },
  {
    "chapter": 14,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и пень омертвел в пыли,"
  },
  {
    "chapter": 14,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "чуть почует воду расцветет,"
  },
  {
    "chapter": 14,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "пустит ветви,"
  },
  {
    "chapter": 14,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "как молодое растение."
  },
  {
    "chapter": 14,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "А человек умрет и исчезнет,"
  },
  {
    "chapter": 14,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "испустит дух,"
  },
  {
    "chapter": 14,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и где он?"
  },
  {
    "chapter": 14,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Как исчезает вода из озера,"
  },
  {
    "chapter": 14,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "как иссякает река и сохнет,"
  },
  {
    "chapter": 14,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "так и смертный"
  },
  {
    "chapter": 14,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "ляжет и не поднимется;"
  },
  {
    "chapter": 14,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "пока не"
  },
  {
    "chapter": 14,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "исчезнут небеса не"
  },
  {
    "chapter": 14,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "проснется"
  },
  {
    "chapter": 14,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 6,
    "text": "и от сна своего не встанет."
  },
  {
    "chapter": 14,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "О если бы Ты укрыл"
  },
  {
    "chapter": 14,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "меня в мире мертвых,"
  },
  {
    "chapter": 14,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "спрятал меня,"
  },
  {
    "chapter": 14,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "пока не пройдет Твой гнев!"
  },
  {
    "chapter": 14,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "О если бы Ты установил мне"
  },
  {
    "chapter": 14,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 6,
    "text": "срок,"
  },
  {
    "chapter": 14,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 7,
    "text": "а потом"
  },
  {
    "chapter": 14,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 8,
    "text": "вспомнил бы обо мне!"
  },
  {
    "chapter": 14,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Будет ли жить человек,"
  },
  {
    "chapter": 14,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "умерев? Все дни"
  },
  {
    "chapter": 14,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "моей службы я бы ждал,"
  },
  {
    "chapter": 14,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "когда придет"
  },
  {
    "chapter": 14,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "мое избавление."
  },
  {
    "chapter": 14,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Ты бы позвал,"
  },
  {
    "chapter": 14,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и я бы ответил;"
  },
  {
    "chapter": 14,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "творение Твоих"
  },
  {
    "chapter": 14,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "рук тронуло бы Тебя."
  },
  {
    "chapter": 14,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Тогда Ты"
  },
  {
    "chapter": 14,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "считал бы мои шаги,"
  },
  {
    "chapter": 14,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "но греха моего"
  },
  {
    "chapter": 14,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "не выискивал бы;"
  },
  {
    "chapter": 14,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "накрепко"
  },
  {
    "chapter": 14,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "запечатана была бы моя вина,"
  },
  {
    "chapter": 14,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Ты сокрыл бы мои проступки."
  },
  {
    "chapter": 14,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Но как гора"
  },
  {
    "chapter": 14,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "рушится и дробится,"
  },
  {
    "chapter": 14,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и как скала"
  },
  {
    "chapter": 14,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "сходит со своего места,"
  },
  {
    "chapter": 14,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "как вода подтачивает камни,"
  },
  {
    "chapter": 14,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и потоки смывают почву,"
  },
  {
    "chapter": 14,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "так и Ты губишь"
  },
  {
    "chapter": 14,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "надежды смертного."
  },
  {
    "chapter": 14,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Ты теснишь его до конца,"
  },
  {
    "chapter": 14,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и он уходит;"
  },
  {
    "chapter": 14,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Ты искажаешь его"
  },
  {
    "chapter": 14,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "лицо и отсылаешь его."
  },
  {
    "chapter": 14,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "В чести ли дети его,"
  },
  {
    "chapter": 14,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "он не знает;"
  },
  {
    "chapter": 14,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "обижают ли их,"
  },
  {
    "chapter": 14,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "не видит."
  },
  {
    "chapter": 14,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Он лишь чувствует:"
  },
  {
    "chapter": 14,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "тело его болит и"
  },
  {
    "chapter": 14,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "плачет лишь о себе."
  }
]