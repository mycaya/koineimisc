[
  {
    "chapter": 115,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Песнь восхождения."
  },
  {
    "chapter": 115,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Обращаю взгляд"
  },
  {
    "chapter": 115,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "свой к горам,"
  },
  {
    "chapter": 115,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "откуда придет мне помощь?"
  },
  {
    "chapter": 115,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Помощь мне"
  },
  {
    "chapter": 115,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "придет от Господа,"
  },
  {
    "chapter": 115,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "сотворившего небо и землю."
  },
  {
    "chapter": 115,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он не позволит ноге"
  },
  {
    "chapter": 115,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "твоей пошатнуться,"
  },
  {
    "chapter": 115,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "хранящий тебя не задремлет;"
  },
  {
    "chapter": 115,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "истинно,"
  },
  {
    "chapter": 115,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "хранящий Израиль не"
  },
  {
    "chapter": 115,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "задремлет и не уснет."
  },
  {
    "chapter": 115,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господь страж твой;"
  },
  {
    "chapter": 115,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господь тень твоя"
  },
  {
    "chapter": 115,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "от правой руки твоей."
  },
  {
    "chapter": 115,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Солнце не"
  },
  {
    "chapter": 115,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "поразит тебя днем,"
  },
  {
    "chapter": 115,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "ни луна ночью."
  },
  {
    "chapter": 115,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господь сохранит"
  },
  {
    "chapter": 115,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "тебя от всякого зла;"
  },
  {
    "chapter": 115,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Он сохранит твою душу."
  },
  {
    "chapter": 115,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господь будет хранить тебя,"
  },
  {
    "chapter": 115,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "когда ты уходишь"
  },
  {
    "chapter": 115,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и когда приходишь,"
  },
  {
    "chapter": 115,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "сейчас и вовеки."
  }
]