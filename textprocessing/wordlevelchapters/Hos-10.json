[
  {
    "chapter": 10,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Израиль ветвистый виноград."
  },
  {
    "chapter": 10,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "Он производил"
  },
  {
    "chapter": 10,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "плод для себя."
  },
  {
    "chapter": 10,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "Чем больше было"
  },
  {
    "chapter": 10,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "у него плодов,"
  },
  {
    "chapter": 10,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "тем больше он"
  },
  {
    "chapter": 10,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 7,
    "text": "возводил жертвенников."
  },
  {
    "chapter": 10,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 8,
    "text": "Чем плодороднее"
  },
  {
    "chapter": 10,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 9,
    "text": "была его земля,"
  },
  {
    "chapter": 10,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 10,
    "text": "тем лучше он украшал"
  },
  {
    "chapter": 10,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 11,
    "text": "свои священные столбы."
  },
  {
    "chapter": 10,
    "verse": 2,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Их сердце обманчиво,"
  },
  {
    "chapter": 10,
    "verse": 2,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "и теперь они должны"
  },
  {
    "chapter": 10,
    "verse": 2,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "понести наказание."
  },
  {
    "chapter": 10,
    "verse": 2,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "Господь разрушит"
  },
  {
    "chapter": 10,
    "verse": 2,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "их жертвенники и уничтожит"
  },
  {
    "chapter": 10,
    "verse": 2,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "их священные столбы."
  },
  {
    "chapter": 10,
    "verse": 3,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Тогда они скажут:"
  },
  {
    "chapter": 10,
    "verse": 3,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "У нас нет царя,"
  },
  {
    "chapter": 10,
    "verse": 3,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "потому что мы не"
  },
  {
    "chapter": 10,
    "verse": 3,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "почитали Господа."
  },
  {
    "chapter": 10,
    "verse": 3,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "Но даже если бы"
  },
  {
    "chapter": 10,
    "verse": 3,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "и был у нас царь,"
  },
  {
    "chapter": 10,
    "verse": 3,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 7,
    "text": "то что бы он мог"
  },
  {
    "chapter": 10,
    "verse": 3,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 8,
    "text": "сделать для нас?"
  },
  {
    "chapter": 10,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Они говорят пустые слова,"
  },
  {
    "chapter": 10,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "дают ложные клятвы и"
  },
  {
    "chapter": 10,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "заключают соглашения,"
  },
  {
    "chapter": 10,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "поэтому суд"
  },
  {
    "chapter": 10,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "явится над ними,"
  },
  {
    "chapter": 10,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "словно ядовитые"
  },
  {
    "chapter": 10,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 7,
    "text": "сорняки на вспаханном поле."
  },
  {
    "chapter": 10,
    "verse": 5,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Жители Самарии будут"
  },
  {
    "chapter": 10,
    "verse": 5,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "бояться за"
  },
  {
    "chapter": 10,
    "verse": 5,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "идола-тельца Беф-Авена."
  },
  {
    "chapter": 10,
    "verse": 5,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "Его народ и его жрецы,"
  },
  {
    "chapter": 10,
    "verse": 5,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "что радовались славе идола,"
  },
  {
    "chapter": 10,
    "verse": 5,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "будут скорбеть по нему,"
  },
  {
    "chapter": 10,
    "verse": 5,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 7,
    "text": "потому что слава"
  },
  {
    "chapter": 10,
    "verse": 5,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 8,
    "text": "отойдет от него."
  },
  {
    "chapter": 10,
    "verse": 6,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Он будет унесен в Ассирию,"
  },
  {
    "chapter": 10,
    "verse": 6,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "в дар великому царю."
  },
  {
    "chapter": 10,
    "verse": 6,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "Ефрем будет опозорен."
  },
  {
    "chapter": 10,
    "verse": 6,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "Израиль будет"
  },
  {
    "chapter": 10,
    "verse": 6,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "постыжен за свой совет."
  },
  {
    "chapter": 10,
    "verse": 7,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Самария и ее царь уплывут,"
  },
  {
    "chapter": 10,
    "verse": 7,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "как ветка по течению."
  },
  {
    "chapter": 10,
    "verse": 8,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Святилища зла будут"
  },
  {
    "chapter": 10,
    "verse": 8,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "уничтожены они грех Израиля."
  },
  {
    "chapter": 10,
    "verse": 8,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "Вырастут"
  },
  {
    "chapter": 10,
    "verse": 8,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "терновник и колючки,"
  },
  {
    "chapter": 10,
    "verse": 8,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "и покроют их жертвенники."
  },
  {
    "chapter": 10,
    "verse": 8,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "И тогда они скажут горам:"
  },
  {
    "chapter": 10,
    "verse": 8,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 7,
    "text": "Покройте нас! и холмам:"
  },
  {
    "chapter": 10,
    "verse": 8,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 8,
    "text": "Падите на нас!"
  },
  {
    "chapter": 10,
    "verse": 9,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "- Ты грешил со времен Гивы,"
  },
  {
    "chapter": 10,
    "verse": 9,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "Израиль,"
  },
  {
    "chapter": 10,
    "verse": 9,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "таким ты и остался."
  },
  {
    "chapter": 10,
    "verse": 9,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "Разве война не"
  },
  {
    "chapter": 10,
    "verse": 9,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "настигла беззаконников в Гиве?"
  },
  {
    "chapter": 10,
    "verse": 10,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "По Моему"
  },
  {
    "chapter": 10,
    "verse": 10,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "желанию Я накажу их."
  },
  {
    "chapter": 10,
    "verse": 10,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "Народы"
  },
  {
    "chapter": 10,
    "verse": 10,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "соберутся против них,"
  },
  {
    "chapter": 10,
    "verse": 10,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "чтобы надеть на них"
  },
  {
    "chapter": 10,
    "verse": 10,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "оковы за их двойной грех."
  },
  {
    "chapter": 10,
    "verse": 11,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Ефрем приученная телица,"
  },
  {
    "chapter": 10,
    "verse": 11,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "привыкшая молотить зерно."
  },
  {
    "chapter": 10,
    "verse": 11,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "Итак,"
  },
  {
    "chapter": 10,
    "verse": 11,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "Я надену ярмо"
  },
  {
    "chapter": 10,
    "verse": 11,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "на ее тучную шею."
  },
  {
    "chapter": 10,
    "verse": 11,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "Я запрягу Ефрема,"
  },
  {
    "chapter": 10,
    "verse": 11,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 7,
    "text": "Иуда и Иаков должны"
  },
  {
    "chapter": 10,
    "verse": 11,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 8,
    "text": "будут пахать землю."
  },
  {
    "chapter": 10,
    "verse": 12,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Сейте для себя праведность,"
  },
  {
    "chapter": 10,
    "verse": 12,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "пожинайте плоды милости"
  },
  {
    "chapter": 10,
    "verse": 12,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "и возделывайте свою"
  },
  {
    "chapter": 10,
    "verse": 12,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "нераспаханную землю,"
  },
  {
    "chapter": 10,
    "verse": 12,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "потому что настало"
  },
  {
    "chapter": 10,
    "verse": 12,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "время искать Господа,"
  },
  {
    "chapter": 10,
    "verse": 12,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 7,
    "text": "пока Он не придет и не"
  },
  {
    "chapter": 10,
    "verse": 12,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 8,
    "text": "изольет на вас праведность,"
  },
  {
    "chapter": 10,
    "verse": 12,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 9,
    "text": "как дождь."
  },
  {
    "chapter": 10,
    "verse": 13,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Но вы сеяли"
  },
  {
    "chapter": 10,
    "verse": 13,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "беззаконие и пожинали зло,"
  },
  {
    "chapter": 10,
    "verse": 13,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "вы съели плод лжи."
  },
  {
    "chapter": 10,
    "verse": 13,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "Так как вы полагались на"
  },
  {
    "chapter": 10,
    "verse": 13,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "свою силу"
  },
  {
    "chapter": 10,
    "verse": 13,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "и на множество своих"
  },
  {
    "chapter": 10,
    "verse": 13,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 7,
    "text": "воинов,"
  },
  {
    "chapter": 10,
    "verse": 14,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "рев войны поднимется"
  },
  {
    "chapter": 10,
    "verse": 14,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "против вашего народа,"
  },
  {
    "chapter": 10,
    "verse": 14,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "и все ваши"
  },
  {
    "chapter": 10,
    "verse": 14,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "крепости будут разрушены."
  },
  {
    "chapter": 10,
    "verse": 14,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "Как Салман разрушил"
  },
  {
    "chapter": 10,
    "verse": 14,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "Беф-Арбел в день битвы,"
  },
  {
    "chapter": 10,
    "verse": 14,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 7,
    "text": "когда матери вместе со"
  },
  {
    "chapter": 10,
    "verse": 14,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 8,
    "text": "своими детьми были забиты"
  },
  {
    "chapter": 10,
    "verse": 14,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 9,
    "text": "до смерти,"
  },
  {
    "chapter": 10,
    "verse": 15,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "так будет и с тобою,"
  },
  {
    "chapter": 10,
    "verse": 15,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "Вефиль,"
  },
  {
    "chapter": 10,
    "verse": 15,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "потому что"
  },
  {
    "chapter": 10,
    "verse": 15,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "велики твои злодеяния."
  }
]