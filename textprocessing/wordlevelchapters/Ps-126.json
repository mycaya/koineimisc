[
  {
    "chapter": 126,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Песнь восхождения."
  },
  {
    "chapter": 126,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 126,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "вспомни Давида"
  },
  {
    "chapter": 126,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и все огорчения,"
  },
  {
    "chapter": 126,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "которые он перенес."
  },
  {
    "chapter": 126,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Как поклялся он Господу и"
  },
  {
    "chapter": 126,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "дал обет Сильному Иакова:"
  },
  {
    "chapter": 126,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Не войду в свое жилище и"
  },
  {
    "chapter": 126,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "не лягу в свою постель,"
  },
  {
    "chapter": 126,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "не дам сна своим"
  },
  {
    "chapter": 126,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "глазам и векам своим дремы,"
  },
  {
    "chapter": 126,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "пока не найду"
  },
  {
    "chapter": 126,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "места Господу,"
  },
  {
    "chapter": 126,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "жилища Мощному Иакова."
  },
  {
    "chapter": 126,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Вот,"
  },
  {
    "chapter": 126,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "мы слышали о нем в Ефрафе,"
  },
  {
    "chapter": 126,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "нашли его на полях Иарима."
  },
  {
    "chapter": 126,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пойдем к Его жилищу"
  },
  {
    "chapter": 126,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и поклонимся Ему у подножия"
  },
  {
    "chapter": 126,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "ног Его."
  },
  {
    "chapter": 126,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Встань,"
  },
  {
    "chapter": 126,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 126,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и приди на"
  },
  {
    "chapter": 126,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "место Твоего покоя,"
  },
  {
    "chapter": 126,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Ты и ковчег Твоей силы."
  },
  {
    "chapter": 126,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть Твои священники"
  },
  {
    "chapter": 126,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "облекутся в праведность,"
  },
  {
    "chapter": 126,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и воскликнут от"
  },
  {
    "chapter": 126,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "радости Твои святые."
  },
  {
    "chapter": 126,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ради Давида,"
  },
  {
    "chapter": 126,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Твоего слуги,"
  },
  {
    "chapter": 126,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "не отверни лица"
  },
  {
    "chapter": 126,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Твоего помазанника."
  },
  {
    "chapter": 126,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Клялся Господь"
  },
  {
    "chapter": 126,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Давиду в истине,"
  },
  {
    "chapter": 126,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и не отвернется от этого:"
  },
  {
    "chapter": 126,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "От плода твоего тела"
  },
  {
    "chapter": 126,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "посажу на твой престол."
  },
  {
    "chapter": 126,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Если твои сыновья сохранят"
  },
  {
    "chapter": 126,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Мой завет и Мои заповеди,"
  },
  {
    "chapter": 126,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "которым их научу,"
  },
  {
    "chapter": 126,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "их дети также будут"
  },
  {
    "chapter": 126,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "сидеть на твоем престоле вечно."
  },
  {
    "chapter": 126,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ведь Господь избрал Сион"
  },
  {
    "chapter": 126,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и пожелал его для Своего"
  },
  {
    "chapter": 126,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "жилища:"
  },
  {
    "chapter": 126,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Вот покой Мой навечно,"
  },
  {
    "chapter": 126,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "здесь поселюсь,"
  },
  {
    "chapter": 126,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "потому что Я возжелал его."
  },
  {
    "chapter": 126,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Обильно"
  },
  {
    "chapter": 126,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "благословлю его пищей,"
  },
  {
    "chapter": 126,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "нищих его насыщу хлебом."
  },
  {
    "chapter": 126,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Священников его"
  },
  {
    "chapter": 126,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "облеку спасением,"
  },
  {
    "chapter": 126,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и воскликнут от"
  },
  {
    "chapter": 126,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "радости Его святые."
  },
  {
    "chapter": 126,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Там повелю рогу"
  },
  {
    "chapter": 126,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Давидову цвести,"
  },
  {
    "chapter": 126,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "установлю Я"
  },
  {
    "chapter": 126,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "светильник Моему помазаннику."
  },
  {
    "chapter": 126,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Врагов его облеку стыдом,"
  },
  {
    "chapter": 126,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "а на нем"
  },
  {
    "chapter": 126,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "воссияет венец его."
  }
]