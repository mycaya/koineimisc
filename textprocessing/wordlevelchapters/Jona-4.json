[
  {
    "chapter": 4,
    "verse": 1,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 1,
    "text": "Но Иона был этим"
  },
  {
    "chapter": 4,
    "verse": 1,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 2,
    "text": "очень недоволен и злился."
  },
  {
    "chapter": 4,
    "verse": 2,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 1,
    "text": "Он стал молиться Господу:"
  },
  {
    "chapter": 4,
    "verse": 2,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 2,
    "text": "О Господи,"
  },
  {
    "chapter": 4,
    "verse": 2,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 3,
    "text": "разве не это я говорил,"
  },
  {
    "chapter": 4,
    "verse": 2,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 4,
    "text": "когда еще был дома?"
  },
  {
    "chapter": 4,
    "verse": 2,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 5,
    "text": "Поэтому-то я и бежал в Фарсис."
  },
  {
    "chapter": 4,
    "verse": 2,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 6,
    "text": "Я же знал,"
  },
  {
    "chapter": 4,
    "verse": 2,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 7,
    "text": "что Ты Бог"
  },
  {
    "chapter": 4,
    "verse": 2,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 8,
    "text": "милостивый и сострадательный,"
  },
  {
    "chapter": 4,
    "verse": 2,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 9,
    "text": "долготерпеливый"
  },
  {
    "chapter": 4,
    "verse": 2,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 10,
    "text": "и многомилостивый"
  },
  {
    "chapter": 4,
    "verse": 2,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 11,
    "text": "и не желаешь насылать беду."
  },
  {
    "chapter": 4,
    "verse": 3,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 1,
    "text": "А теперь,"
  },
  {
    "chapter": 4,
    "verse": 3,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 4,
    "verse": 3,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 3,
    "text": "забери мою жизнь,"
  },
  {
    "chapter": 4,
    "verse": 3,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 4,
    "text": "потому что мне"
  },
  {
    "chapter": 4,
    "verse": 3,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 5,
    "text": "лучше умереть,"
  },
  {
    "chapter": 4,
    "verse": 3,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 6,
    "text": "чем жить."
  },
  {
    "chapter": 4,
    "verse": 4,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 1,
    "text": "Но Господь ответил:"
  },
  {
    "chapter": 4,
    "verse": 4,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 2,
    "text": "Стоит ли тебе гневаться?"
  },
  {
    "chapter": 4,
    "verse": 5,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 1,
    "text": "Иона вышел и сел к"
  },
  {
    "chapter": 4,
    "verse": 5,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 2,
    "text": "востоку от города."
  },
  {
    "chapter": 4,
    "verse": 5,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 3,
    "text": "Там он сделал себе шалаш,"
  },
  {
    "chapter": 4,
    "verse": 5,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 4,
    "text": "сел под ним в"
  },
  {
    "chapter": 4,
    "verse": 5,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 5,
    "text": "тени и стал ждать,"
  },
  {
    "chapter": 4,
    "verse": 5,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 6,
    "text": "что произойдет с городом."
  },
  {
    "chapter": 4,
    "verse": 6,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 1,
    "text": "А Господь Бог устроил так,"
  },
  {
    "chapter": 4,
    "verse": 6,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 2,
    "text": "что выросло растение,"
  },
  {
    "chapter": 4,
    "verse": 6,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 3,
    "text": "которое"
  },
  {
    "chapter": 4,
    "verse": 6,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 4,
    "text": "поднялось над Ионой,"
  },
  {
    "chapter": 4,
    "verse": 6,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 5,
    "text": "чтобы тень над его головой"
  },
  {
    "chapter": 4,
    "verse": 6,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 6,
    "text": "избавила его от"
  },
  {
    "chapter": 4,
    "verse": 6,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 7,
    "text": "раздражения,"
  },
  {
    "chapter": 4,
    "verse": 6,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 8,
    "text": "и Иона очень"
  },
  {
    "chapter": 4,
    "verse": 6,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 9,
    "text": "обрадовался растению."
  },
  {
    "chapter": 4,
    "verse": 7,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 1,
    "text": "Но на рассвете"
  },
  {
    "chapter": 4,
    "verse": 7,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 2,
    "text": "следующего дня Бог устроил так,"
  },
  {
    "chapter": 4,
    "verse": 7,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 3,
    "text": "что червь"
  },
  {
    "chapter": 4,
    "verse": 7,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 4,
    "text": "подточил растение,"
  },
  {
    "chapter": 4,
    "verse": 7,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 5,
    "text": "и оно засохло."
  },
  {
    "chapter": 4,
    "verse": 8,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 1,
    "text": "Когда солнце взошло,"
  },
  {
    "chapter": 4,
    "verse": 8,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 2,
    "text": "Бог наслал"
  },
  {
    "chapter": 4,
    "verse": 8,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 3,
    "text": "знойный восточный ветер,"
  },
  {
    "chapter": 4,
    "verse": 8,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 4,
    "text": "и солнце стало"
  },
  {
    "chapter": 4,
    "verse": 8,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 5,
    "text": "жечь голову Ионы так,"
  },
  {
    "chapter": 4,
    "verse": 8,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 6,
    "text": "что он изнемог."
  },
  {
    "chapter": 4,
    "verse": 8,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 7,
    "text": "Он захотел"
  },
  {
    "chapter": 4,
    "verse": 8,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 8,
    "text": "умереть и сказал:"
  },
  {
    "chapter": 4,
    "verse": 8,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 9,
    "text": "Лучше мне умереть,"
  },
  {
    "chapter": 4,
    "verse": 8,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 10,
    "text": "чем жить."
  },
  {
    "chapter": 4,
    "verse": 9,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 1,
    "text": "А Бог сказал Ионе:"
  },
  {
    "chapter": 4,
    "verse": 9,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 2,
    "text": "Стоит ли тебе гневаться"
  },
  {
    "chapter": 4,
    "verse": 9,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 3,
    "text": "из-за растения? Стоит,"
  },
  {
    "chapter": 4,
    "verse": 9,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 4,
    "text": "ответил он,"
  },
  {
    "chapter": 4,
    "verse": 9,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 5,
    "text": "я до смерти разгневан."
  },
  {
    "chapter": 4,
    "verse": 10,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 1,
    "text": "Но Господь сказал:"
  },
  {
    "chapter": 4,
    "verse": 10,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 2,
    "text": "Ты сожалеешь о растении,"
  },
  {
    "chapter": 4,
    "verse": 10,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 3,
    "text": "за которым ты не ухаживал,"
  },
  {
    "chapter": 4,
    "verse": 10,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 4,
    "text": "которое не растил."
  },
  {
    "chapter": 4,
    "verse": 10,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 5,
    "text": "Оно за ночь выросло"
  },
  {
    "chapter": 4,
    "verse": 10,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 6,
    "text": "и за ночь погибло."
  },
  {
    "chapter": 4,
    "verse": 11,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 1,
    "text": "Так Мне ли не пожалеть"
  },
  {
    "chapter": 4,
    "verse": 11,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 2,
    "text": "великого города Ниневии,"
  },
  {
    "chapter": 4,
    "verse": 11,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 3,
    "text": "где больше ста двадцати"
  },
  {
    "chapter": 4,
    "verse": 11,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 4,
    "text": "тысяч жителей не знают,"
  },
  {
    "chapter": 4,
    "verse": 11,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 5,
    "text": "где право,"
  },
  {
    "chapter": 4,
    "verse": 11,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 6,
    "text": "где лево,"
  },
  {
    "chapter": 4,
    "verse": 11,
    "book_name": "Jonah",
    "book_id": "Jona",
    "part": 7,
    "text": "а также много скота?"
  }
]