[
  {
    "chapter": 13,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Глаза мои все это видели,"
  },
  {
    "chapter": 13,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "слышали уши и понял ум."
  },
  {
    "chapter": 13,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Что знаете вы,"
  },
  {
    "chapter": 13,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "то знаю и я;"
  },
  {
    "chapter": 13,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "я не хуже вас."
  },
  {
    "chapter": 13,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Но я бы хотел"
  },
  {
    "chapter": 13,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "поговорить со Всемогущим,"
  },
  {
    "chapter": 13,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "с Богом"
  },
  {
    "chapter": 13,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "поспорить о моем деле."
  },
  {
    "chapter": 13,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "А вы замарали меня ложью,"
  },
  {
    "chapter": 13,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "все вы бесполезные лекари."
  },
  {
    "chapter": 13,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "О если бы вы все вместе"
  },
  {
    "chapter": 13,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "замолчали! В этом была бы"
  },
  {
    "chapter": 13,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "мудрость для вас."
  },
  {
    "chapter": 13,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Выслушайте же мои доводы,"
  },
  {
    "chapter": 13,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "внимайте укорам моих уст."
  },
  {
    "chapter": 13,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Неужели вы"
  },
  {
    "chapter": 13,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "станете лгать ради Бога,"
  },
  {
    "chapter": 13,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и обманывать ради Него?"
  },
  {
    "chapter": 13,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Будете ради"
  },
  {
    "chapter": 13,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Него пристрастными,"
  },
  {
    "chapter": 13,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и в суде станете"
  },
  {
    "chapter": 13,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "Его выгораживать?"
  },
  {
    "chapter": 13,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Что с вами будет,"
  },
  {
    "chapter": 13,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "если Он испытает"
  },
  {
    "chapter": 13,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "вас? Обманете ли вы Его,"
  },
  {
    "chapter": 13,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "как обманываете людей?"
  },
  {
    "chapter": 13,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Он непременно осудит вас,"
  },
  {
    "chapter": 13,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "если вы были"
  },
  {
    "chapter": 13,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "втайне пристрастны."
  },
  {
    "chapter": 13,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Не страшит вас Его величие?"
  },
  {
    "chapter": 13,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Ужас перед Ним вас не"
  },
  {
    "chapter": 13,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "объемлет?"
  },
  {
    "chapter": 13,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Изречения ваши зола,"
  },
  {
    "chapter": 13,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и оплот ваш оплот из глины."
  },
  {
    "chapter": 13,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Замолчите,"
  },
  {
    "chapter": 13,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и я буду говорить;"
  },
  {
    "chapter": 13,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "а потом пусть"
  },
  {
    "chapter": 13,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "будет со мной,"
  },
  {
    "chapter": 13,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "что будет."
  },
  {
    "chapter": 13,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Я подвергну себя опасности,"
  },
  {
    "chapter": 13,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и жизнью своею рискну."
  },
  {
    "chapter": 13,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Он убивает меня,"
  },
  {
    "chapter": 13,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "но я буду"
  },
  {
    "chapter": 13,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "надеяться на Него;"
  },
  {
    "chapter": 13,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "перед Его лицом я"
  },
  {
    "chapter": 13,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "защищу свой путь!"
  },
  {
    "chapter": 13,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "И это будет моим спасением,"
  },
  {
    "chapter": 13,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "ведь безбожник предстать"
  },
  {
    "chapter": 13,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "перед Ним не осмелится!"
  },
  {
    "chapter": 13,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Внимайте моим словам;"
  },
  {
    "chapter": 13,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "пусть дойдет моя"
  },
  {
    "chapter": 13,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "речь до ваших ушей."
  },
  {
    "chapter": 13,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Вот,"
  },
  {
    "chapter": 13,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "я завел"
  },
  {
    "chapter": 13,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "судебное дело и знаю,"
  },
  {
    "chapter": 13,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "что буду оправдан."
  },
  {
    "chapter": 13,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Возьмется ли кто"
  },
  {
    "chapter": 13,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "оспаривать меня? Если да,"
  },
  {
    "chapter": 13,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "то я замолчу и умру."
  },
  {
    "chapter": 13,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Лишь о двух вещах я,"
  },
  {
    "chapter": 13,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Боже,"
  },
  {
    "chapter": 13,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "молю,"
  },
  {
    "chapter": 13,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "и не стану я больше"
  },
  {
    "chapter": 13,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "прятаться от Тебя -"
  },
  {
    "chapter": 13,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "удали от меня Свою руку"
  },
  {
    "chapter": 13,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и не страши меня ужасами"
  },
  {
    "chapter": 13,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Твоими."
  },
  {
    "chapter": 13,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Тогда призови меня,"
  },
  {
    "chapter": 13,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и я откликнусь,"
  },
  {
    "chapter": 13,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "или позволь мне говорить,"
  },
  {
    "chapter": 13,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "а Сам отвечай."
  },
  {
    "chapter": 13,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Сколько за мной злодейств"
  },
  {
    "chapter": 13,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и грехов? Покажи мне"
  },
  {
    "chapter": 13,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "проступки мои и грех."
  },
  {
    "chapter": 13,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "За что Ты скрываешь Свое"
  },
  {
    "chapter": 13,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "лицо и считаешь меня врагом?"
  },
  {
    "chapter": 13,
    "verse": 25,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Станешь ли Ты пугать лист"
  },
  {
    "chapter": 13,
    "verse": 25,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "опавший? Пустишься ли за"
  },
  {
    "chapter": 13,
    "verse": 25,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "сухою соломинкой в погоню?"
  },
  {
    "chapter": 13,
    "verse": 26,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Ты записываешь горькое"
  },
  {
    "chapter": 13,
    "verse": 26,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "против меня"
  },
  {
    "chapter": 13,
    "verse": 26,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и грехи моей юности мне"
  },
  {
    "chapter": 13,
    "verse": 26,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "вменяешь."
  },
  {
    "chapter": 13,
    "verse": 27,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Ноги мои Ты"
  },
  {
    "chapter": 13,
    "verse": 27,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "заковал в колодки;"
  },
  {
    "chapter": 13,
    "verse": 27,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "следишь за"
  },
  {
    "chapter": 13,
    "verse": 27,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "всеми моими путями;"
  },
  {
    "chapter": 13,
    "verse": 27,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "по следам моих ног идешь."
  },
  {
    "chapter": 13,
    "verse": 28,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "И распадается человек,"
  },
  {
    "chapter": 13,
    "verse": 28,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "как гниль,"
  },
  {
    "chapter": 13,
    "verse": 28,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "как изъеденная"
  },
  {
    "chapter": 13,
    "verse": 28,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "молью одежда."
  }
]