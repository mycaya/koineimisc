    "verse": 10,
    "book_name": "Revelation",
    "book_id": "Rev",
    "part": 2,
    "text": "пять из которых уже пали,"
  },
  {
    "chapter": 17,
    "verse": 10,
    "book_name": "Revelation",
    "book_id": "Rev",
    "part": 3,
    "text": "один есть,"
  },
  {
    "chapter": 17,
    "verse": 10,
    "book_name": "Revelation",
    "book_id": "Rev",
    "part": 4,
    "text": "а другой еще не пришел,"
  },
  {
    "chapter": 17,
    "verse": 10,
    "book_name": "Revelation",
    "book_id": "Rev",
    "part": 5,
    "text": "а когда придет"
  },
  {
    "chapter": 17,
    "verse": 10,
    "book_name": "Revelation",
    "book_id": "Rev",
    "part": 6,
    "text": "то ненадолго."
  },
  {
    "chapter": 17,
    "verse": 11,
    "book_name": "Revelation",
    "book_id": "Rev",
    "part": 1,
