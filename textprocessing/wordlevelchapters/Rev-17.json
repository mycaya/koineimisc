  {
    "chapter": 17,
    "verse": 4,
    "book_name": "Revelation",
    "book_id": "Rev",
    "part": 1,
    "text": "Женщина была одета во"
  },
  {
    "chapter": 17,
    "verse": 4,
    "book_name": "Revelation",
    "book_id": "Rev",
    "part": 2,
    "text": "все пурпурное и алое,"
  },
  {
    "chapter": 17,
    "verse": 4,
    "book_name": "Revelation",
    "book_id": "Rev",
    "part": 3,
    "text": "была украшена золотом,"
  },
  {
    "chapter": 17,
    "verse": 4,
    "book_name": "Revelation",
    "book_id": "Rev",
    "part": 4,
    "text": "драгоценными"
  },
  {
    "chapter": 17,
    "verse": 4,
    "book_name": "Revelation",
    "book_id": "Rev",
