  },
  {
    "chapter": 17,
    "verse": 12,
    "book_name": "Revelation",
    "book_id": "Rev",
    "part": 2,
    "text": "которые ты видел,"
  },
  {
    "chapter": 17,
    "verse": 12,
    "book_name": "Revelation",
    "book_id": "Rev",
    "part": 3,
    "text": "это десять царей,"
  },
  {
    "chapter": 17,
    "verse": 12,
    "book_name": "Revelation",
    "book_id": "Rev",
