  },
  {
    "chapter": 17,
    "verse": 7,
    "book_name": "Revelation",
    "book_id": "Rev",
    "part": 6,
    "text": "и зверя с семью головами"
  },
  {
    "chapter": 17,
    "verse": 7,
    "book_name": "Revelation",
    "book_id": "Rev",
