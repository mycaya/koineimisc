    "part": 2,
    "text": "поднимите на него крик."
  },
  {
    "chapter": 50,
    "verse": 15,
    "book_name": "Jeremiah",
    "book_id": "Jer",
    "part": 3,
    "text": "Он сдается,"
  },
  {
    "chapter": 50,
    "verse": 15,
    "book_name": "Jeremiah",
    "book_id": "Jer",
    "part": 4,
