  },
  {
    "chapter": 8,
    "verse": 5,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 6,
    "text": "чтобы нам торговать"
  },
  {
    "chapter": 8,
    "verse": 5,
    "book_name": "Amos",
    "book_id": "Amos",
