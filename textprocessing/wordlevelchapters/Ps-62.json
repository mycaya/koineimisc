[
  {
    "chapter": 62,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Дирижеру хора."
  },
  {
    "chapter": 62,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Псалом Давида."
  },
  {
    "chapter": 62,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Песнь."
  },
  {
    "chapter": 62,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Боже,"
  },
  {
    "chapter": 62,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Тебе подобает"
  },
  {
    "chapter": 62,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "хвала на Сионе,"
  },
  {
    "chapter": 62,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "пред Тобой"
  },
  {
    "chapter": 62,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "исполним обеты свои."
  },
  {
    "chapter": 62,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты слышишь молитву;"
  },
  {
    "chapter": 62,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "к Тебе придут все люди."
  },
  {
    "chapter": 62,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Когда одолевают"
  },
  {
    "chapter": 62,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "нас наши грехи,"
  },
  {
    "chapter": 62,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Ты прощаешь нам беззакония."
  },
  {
    "chapter": 62,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Блажен тот,"
  },
  {
    "chapter": 62,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "кого Ты избрал и приблизил,"
  },
  {
    "chapter": 62,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "чтобы он жил при"
  },
  {
    "chapter": 62,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Твоем святилище."
  },
  {
    "chapter": 62,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Мы насытимся"
  },
  {
    "chapter": 62,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "благами дома Твоего,"
  },
  {
    "chapter": 62,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "святого храма Твоего."
  },
  {
    "chapter": 62,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты отвечаешь нам"
  },
  {
    "chapter": 62,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "устрашающими делами,"
  },
  {
    "chapter": 62,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "даруя избавление,"
  },
  {
    "chapter": 62,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Боже,"
  },
  {
    "chapter": 62,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Спаситель наш."
  },
  {
    "chapter": 62,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Ты надежда всех краев"
  },
  {
    "chapter": 62,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "земли и самых дальних морей."
  },
  {
    "chapter": 62,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Силой Своей"
  },
  {
    "chapter": 62,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "утвердил Ты горы;"
  },
  {
    "chapter": 62,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Ты препоясан могуществом."
  },
  {
    "chapter": 62,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты усмиряешь шум морей,"
  },
  {
    "chapter": 62,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "шум их волн,"
  },
  {
    "chapter": 62,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и смуту народов."
  },
  {
    "chapter": 62,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Живущие на краю земли"
  },
  {
    "chapter": 62,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "устрашатся Твоих знамений."
  },
  {
    "chapter": 62,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Ворота зари и сумерек Ты"
  },
  {
    "chapter": 62,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "наполнишь песнями радости."
  },
  {
    "chapter": 62,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты заботишься о"
  },
  {
    "chapter": 62,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "земле и орошаешь ее,"
  },
  {
    "chapter": 62,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Ты обильно"
  },
  {
    "chapter": 62,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "даруешь ей плодородие."
  },
  {
    "chapter": 62,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Потоки Божьи полны воды,"
  },
  {
    "chapter": 62,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "чтобы дать народу зерно,"
  },
  {
    "chapter": 62,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "потому что так"
  },
  {
    "chapter": 62,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 8,
    "text": "Ты землю устроил."
  },
  {
    "chapter": 62,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты наполняешь водой ее"
  },
  {
    "chapter": 62,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "борозды"
  },
  {
    "chapter": 62,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и уравниваешь ее гребни,"
  },
  {
    "chapter": 62,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "смягчая ее дождями,"
  },
  {
    "chapter": 62,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "благословляя ее ростки."
  },
  {
    "chapter": 62,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты венчаешь год"
  },
  {
    "chapter": 62,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Своей щедростью;"
  },
  {
    "chapter": 62,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и повозки Твои"
  },
  {
    "chapter": 62,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "перегружены изобилием."
  },
  {
    "chapter": 62,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Скотом переполнены"
  },
  {
    "chapter": 62,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "пустынные выгоны,"
  },
  {
    "chapter": 62,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и весельем одеты холмы."
  },
  {
    "chapter": 62,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Луга покрыты стадами,"
  },
  {
    "chapter": 62,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и зерном одеты долины."
  },
  {
    "chapter": 62,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Все восклицает и"
  },
  {
    "chapter": 62,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "поет от радости!"
  }
]