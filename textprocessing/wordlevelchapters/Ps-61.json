[
  {
    "chapter": 61,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Дирижеру хора."
  },
  {
    "chapter": 61,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Псалом Давида."
  },
  {
    "chapter": 61,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Боже,"
  },
  {
    "chapter": 61,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "услышь мой голос,"
  },
  {
    "chapter": 61,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "когда я возношу жалобы мои;"
  },
  {
    "chapter": 61,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "сохрани мою"
  },
  {
    "chapter": 61,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "жизнь от угроз врага."
  },
  {
    "chapter": 61,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Сокрой меня от"
  },
  {
    "chapter": 61,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "замыслов нечестивых,"
  },
  {
    "chapter": 61,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "от шумной толпы злодеев."
  },
  {
    "chapter": 61,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Они заострили"
  },
  {
    "chapter": 61,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "свои языки как мечи,"
  },
  {
    "chapter": 61,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и нацелили свои слова"
  },
  {
    "chapter": 61,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "как смертоносные стрелы."
  },
  {
    "chapter": 61,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Они стреляют из"
  },
  {
    "chapter": 61,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "засады в невинного,"
  },
  {
    "chapter": 61,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "стреляют в него"
  },
  {
    "chapter": 61,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "неожиданно и без страха."
  },
  {
    "chapter": 61,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Они воодушевляют друг друга"
  },
  {
    "chapter": 61,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "на злые замыслы"
  },
  {
    "chapter": 61,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и совещаются,"
  },
  {
    "chapter": 61,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "чтобы спрятать свои сети,"
  },
  {
    "chapter": 61,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "говоря:"
  },
  {
    "chapter": 61,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Кто увидит их?"
  },
  {
    "chapter": 61,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Они замышляют"
  },
  {
    "chapter": 61,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "несправедливость и говорят:"
  },
  {
    "chapter": 61,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Мы разработали"
  },
  {
    "chapter": 61,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "идеальный план! Истинно,"
  },
  {
    "chapter": 61,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "коварны разум и"
  },
  {
    "chapter": 61,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "сердце человека."
  },
  {
    "chapter": 61,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но Бог пустит в них стрелы;"
  },
  {
    "chapter": 61,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "внезапно они"
  },
  {
    "chapter": 61,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "будут поражены."
  },
  {
    "chapter": 61,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он обратит"
  },
  {
    "chapter": 61,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "их языки против них самих"
  },
  {
    "chapter": 61,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и приведет их к гибели;"
  },
  {
    "chapter": 61,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "все,"
  },
  {
    "chapter": 61,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "кто увидит их,"
  },
  {
    "chapter": 61,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "будут с презрением"
  },
  {
    "chapter": 61,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "качать своими головами."
  },
  {
    "chapter": 61,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Все люди устрашатся;"
  },
  {
    "chapter": 61,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "они будут возвещать дело"
  },
  {
    "chapter": 61,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Божье и размышлять о том,"
  },
  {
    "chapter": 61,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "что Он сделал."
  },
  {
    "chapter": 61,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть праведные возрадуются"
  },
  {
    "chapter": 61,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "о Господе"
  },
  {
    "chapter": 61,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и найдут в Нем убежище;"
  },
  {
    "chapter": 61,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "пусть все правые"
  },
  {
    "chapter": 61,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "сердцем восславят Его."
  }
]