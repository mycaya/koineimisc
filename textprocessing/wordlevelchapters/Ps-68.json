[
  {
    "chapter": 68,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "У Тебя,"
  },
  {
    "chapter": 68,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 68,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "я ищу прибежища."
  },
  {
    "chapter": 68,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Не дай мне быть"
  },
  {
    "chapter": 68,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "постыженным никогда."
  },
  {
    "chapter": 68,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "По справедливости"
  },
  {
    "chapter": 68,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Твоей выручи и спаси меня;"
  },
  {
    "chapter": 68,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "услышь меня и избавь."
  },
  {
    "chapter": 68,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Будь мне"
  },
  {
    "chapter": 68,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "скалой и прибежищем,"
  },
  {
    "chapter": 68,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "где я мог бы"
  },
  {
    "chapter": 68,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "всегда укрыться."
  },
  {
    "chapter": 68,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Распорядись о"
  },
  {
    "chapter": 68,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "спасении моем,"
  },
  {
    "chapter": 68,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "ведь Ты моя"
  },
  {
    "chapter": 68,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 8,
    "text": "скала и крепость."
  },
  {
    "chapter": 68,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Бог мой,"
  },
  {
    "chapter": 68,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "избавь меня от"
  },
  {
    "chapter": 68,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "руки нечестивого,"
  },
  {
    "chapter": 68,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "от руки злодея"
  },
  {
    "chapter": 68,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "и притеснителя."
  },
  {
    "chapter": 68,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ведь Ты надежда моя,"
  },
  {
    "chapter": 68,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Владыка,"
  },
  {
    "chapter": 68,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Господь;"
  },
  {
    "chapter": 68,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "я уповал на"
  },
  {
    "chapter": 68,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Тебя с юных лет."
  },
  {
    "chapter": 68,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "На Тебя"
  },
  {
    "chapter": 68,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "полагался я от рождения;"
  },
  {
    "chapter": 68,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Ты из утробы"
  },
  {
    "chapter": 68,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "матери меня извлек."
  },
  {
    "chapter": 68,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Тебе хвала моя постоянно."
  },
  {
    "chapter": 68,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Для многих я"
  },
  {
    "chapter": 68,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "был как бы знаком,"
  },
  {
    "chapter": 68,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "но Ты мое крепкое убежище."
  },
  {
    "chapter": 68,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Мои уста полны"
  },
  {
    "chapter": 68,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "хвалы о Тебе;"
  },
  {
    "chapter": 68,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "целый день"
  },
  {
    "chapter": 68,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "воспеваю великолепие Твое."
  },
  {
    "chapter": 68,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Не бросай меня ко"
  },
  {
    "chapter": 68,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "времени старости;"
  },
  {
    "chapter": 68,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "когда иссякнет моя сила,"
  },
  {
    "chapter": 68,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "не покидай меня."
  },
  {
    "chapter": 68,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ведь сговариваются"
  },
  {
    "chapter": 68,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "враги мои против меня,"
  },
  {
    "chapter": 68,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и стерегущие душу мою"
  },
  {
    "chapter": 68,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "советуются между собой,"
  },
  {
    "chapter": 68,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "говоря:"
  },
  {
    "chapter": 68,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Бог покинул его."
  },
  {
    "chapter": 68,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Преследуйте и схватите его,"
  },
  {
    "chapter": 68,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "ведь нет у"
  },
  {
    "chapter": 68,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "него выручающего!"
  },
  {
    "chapter": 68,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Боже,"
  },
  {
    "chapter": 68,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "не удаляйся от меня;"
  },
  {
    "chapter": 68,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Бог мой,"
  },
  {
    "chapter": 68,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "на помощь мне поспеши!"
  },
  {
    "chapter": 68,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть погибнут в"
  },
  {
    "chapter": 68,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "бесчестии противники мои;"
  },
  {
    "chapter": 68,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "пусть облекутся в стыд и"
  },
  {
    "chapter": 68,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "позор желающие мне зла."
  },
  {
    "chapter": 68,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "А я постоянно на Тебя"
  },
  {
    "chapter": 68,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "надеюсь и умножаю хвалу о Тебе."
  },
  {
    "chapter": 68,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Уста мои будут"
  },
  {
    "chapter": 68,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "провозглашать Твою праведность,"
  },
  {
    "chapter": 68,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "целый день Твое спасение,"
  },
  {
    "chapter": 68,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "так как я не знаю числа им."
  },
  {
    "chapter": 68,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я приду и буду говорить"
  },
  {
    "chapter": 68,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "о Твоих великих делах,"
  },
  {
    "chapter": 68,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Владыка Господи,"
  },
  {
    "chapter": 68,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "буду напоминать всем"
  },
  {
    "chapter": 68,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "о Твоей праведности,"
  },
  {
    "chapter": 68,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "какая есть только у Тебя."
  },
  {
    "chapter": 68,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Боже,"
  },
  {
    "chapter": 68,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Ты учил меня с"
  },
  {
    "chapter": 68,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "юных лет моих,"
  },
  {
    "chapter": 68,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и до сих пор я"
  },
  {
    "chapter": 68,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "рассказываю о Твоих чудесах."
  },
  {
    "chapter": 68,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Так и вплоть до"
  },
  {
    "chapter": 68,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "самой старости,"
  },
  {
    "chapter": 68,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "до седины,"
  },
  {
    "chapter": 68,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Боже,"
  },
  {
    "chapter": 68,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "не покидай меня до тех пор,"
  },
  {
    "chapter": 68,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "пока не расскажу о силе"
  },
  {
    "chapter": 68,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "Твоей руки этому поколению"
  },
  {
    "chapter": 68,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 8,
    "text": "и всем грядущим поколениям"
  },
  {
    "chapter": 68,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 9,
    "text": "о Твоем могуществе."
  },
  {
    "chapter": 68,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Твоя праведность,"
  },
  {
    "chapter": 68,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Боже,"
  },
  {
    "chapter": 68,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "огромна,"
  },
  {
    "chapter": 68,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "как сама высота,"
  },
  {
    "chapter": 68,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Твои дела велики."
  },
  {
    "chapter": 68,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Боже,"
  },
  {
    "chapter": 68,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "кто подобен Тебе?"
  },
  {
    "chapter": 68,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты показал мне"
  },
  {
    "chapter": 68,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "много горьких бед,"
  },
  {
    "chapter": 68,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и все же возвращал меня к"
  },
  {
    "chapter": 68,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "жизни и"
  },
  {
    "chapter": 68,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "из пропастей земных вновь"
  },
  {
    "chapter": 68,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "выводил меня."
  },
  {
    "chapter": 68,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты возвышал меня и"
  },
  {
    "chapter": 68,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "давал мне утешение."
  },
  {
    "chapter": 68,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "На лире я буду"
  },
  {
    "chapter": 68,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "славить верность Твою,"
  },
  {
    "chapter": 68,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "мой Бог;"
  },
  {
    "chapter": 68,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "на арфе я"
  },
  {
    "chapter": 68,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "воспою хвалу Тебе,"
  },
  {
    "chapter": 68,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Святой Израиля."
  },
  {
    "chapter": 68,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Восклицают уста"
  },
  {
    "chapter": 68,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "мои от радости,"
  },
  {
    "chapter": 68,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "когда я Тебя воспеваю,"
  },
  {
    "chapter": 68,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и радуется душа моя,"
  },
  {
    "chapter": 68,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "которую Ты искупил."
  },
  {
    "chapter": 68,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
