[
  {
    "chapter": 37,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Дирижеру хора,"
  },
  {
    "chapter": 37,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Идуфуну."
  },
  {
    "chapter": 37,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Псалом Давида."
  },
  {
    "chapter": 37,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я сказал:"
  },
  {
    "chapter": 37,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Я буду следить за своими"
  },
  {
    "chapter": 37,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "путями"
  },
  {
    "chapter": 37,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и язык удерживать от греха,"
  },
  {
    "chapter": 37,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "буду обуздывать уста,"
  },
  {
    "chapter": 37,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "пока нечестивые предо мною."
  },
  {
    "chapter": 37,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но когда я был"
  },
  {
    "chapter": 37,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "нем и безмолвен,"
  },
  {
    "chapter": 37,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и даже о добром молчал,"
  },
  {
    "chapter": 37,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "усилилась моя скорбь"
  },
  {
    "chapter": 37,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "и сердце мое загорелось."
  },
  {
    "chapter": 37,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Пока я размышлял,"
  },
  {
    "chapter": 37,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "вспыхнул огонь,"
  },
  {
    "chapter": 37,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и тогда я"
  },
  {
    "chapter": 37,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "сказал своими устами:"
  },
  {
    "chapter": 37,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Покажи мне,"
  },
  {
    "chapter": 37,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 37,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "кончину мою и"
  },
  {
    "chapter": 37,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "число моих дней скажи;"
  },
  {
    "chapter": 37,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "дай мне знать,"
  },
  {
    "chapter": 37,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "сколь жизнь"
  },
  {
    "chapter": 37,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "моя быстротечна."
  },
  {
    "chapter": 37,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Да,"
  },
  {
    "chapter": 37,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Ты дал мне дней"
  },
  {
    "chapter": 37,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "лишь на ширину ладони;"
  },
  {
    "chapter": 37,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "мой век как"
  },
  {
    "chapter": 37,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "ничто пред Тобой."
  },
  {
    "chapter": 37,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Поистине,"
  },
  {
    "chapter": 37,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "всякая жизнь лишь пар."
  },
  {
    "chapter": 37,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 8,
    "text": "Пауза"
  },
  {
    "chapter": 37,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Поистине,"
  },
  {
    "chapter": 37,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "всякий человек"
  },
  {
    "chapter": 37,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "подобен тени:"
  },
  {
    "chapter": 37,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "напрасно он суетится,"
  },
  {
    "chapter": 37,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "копит,"
  },
  {
    "chapter": 37,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "не зная,"
  },
  {
    "chapter": 37,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "кому все это достанется."
  },
  {
    "chapter": 37,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "И теперь,"
  },
  {
    "chapter": 37,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Владыка,"
  },
  {
    "chapter": 37,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "чего ожидать мне?"
  },
  {
    "chapter": 37,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Надежда моя в Тебе."
  },
  {
    "chapter": 37,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Избавь меня от"
  },
  {
    "chapter": 37,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "всех моих беззаконий,"
  },
  {
    "chapter": 37,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "не предай"
  },
  {
    "chapter": 37,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "безумцам на поругание."
  },
  {
    "chapter": 37,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я молчу;"
  },
  {
    "chapter": 37,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "я не открываю уст,"
  },
  {
    "chapter": 37,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "потому что Ты это сделал."
  },
  {
    "chapter": 37,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Отклони от меня удары Свои;"
  },
  {
    "chapter": 37,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "гибну я от"
  },
  {
    "chapter": 37,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "ударов Твоей руки."
  },
  {
    "chapter": 37,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты коришь и"
  },
  {
    "chapter": 37,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "наказываешь людей за грех;"
  },
  {
    "chapter": 37,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Ты губишь сокровища их,"
  },
  {
    "chapter": 37,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "как губит моль."
  },
  {
    "chapter": 37,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Поистине,"
  },
  {
    "chapter": 37,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "всякий смертный лишь пар."
  },
  {
    "chapter": 37,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "Пауза"
  },
  {
    "chapter": 37,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Услышь молитву мою,"
  },
  {
    "chapter": 37,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи;"
  },
  {
    "chapter": 37,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "внемли моему"
  },
  {
    "chapter": 37,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "крику о помощи;"
  },
  {
    "chapter": 37,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "не будь"
  },
  {
    "chapter": 37,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "безмолвен к моим слезам."
  },
  {
    "chapter": 37,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "Ведь я скиталец у Тебя,"
  },
  {
    "chapter": 37,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 8,
    "text": "чужеземец,"
  },
  {
    "chapter": 37,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 9,
    "text": "как все мои предки."
  },
  {
    "chapter": 37,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Отвернись от меня,"
  },
  {
    "chapter": 37,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "чтобы мне вновь улыбнуться,"
  },
  {
    "chapter": 37,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "прежде чем я уйду"
  },
  {
    "chapter": 37,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и меня не станет."
  }
]