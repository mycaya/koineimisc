[
  {
    "chapter": 6,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Плачевная песнь Давида,"
  },
  {
    "chapter": 6,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "которую он воспел Господу"
  },
  {
    "chapter": 6,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "из-за вениамитянина Куша."
  },
  {
    "chapter": 6,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господи,"
  },
  {
    "chapter": 6,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "мой Бог,"
  },
  {
    "chapter": 6,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "у Тебя я ищу прибежища."
  },
  {
    "chapter": 6,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Спаси меня и"
  },
  {
    "chapter": 6,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "избавь от всех,"
  },
  {
    "chapter": 6,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "кто меня преследует,"
  },
  {
    "chapter": 6,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "иначе,"
  },
  {
    "chapter": 6,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "как лев,"
  },
  {
    "chapter": 6,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "меня разорвут,"
  },
  {
    "chapter": 6,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "растерзают и не"
  },
  {
    "chapter": 6,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "будет спасителя."
  },
  {
    "chapter": 6,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господи,"
  },
  {
    "chapter": 6,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "мой Бог,"
  },
  {
    "chapter": 6,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "если я это сделал,"
  },
  {
    "chapter": 6,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "если есть"
  },
  {
    "chapter": 6,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "несправедливость в моих руках,"
  },
  {
    "chapter": 6,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "если я сделал зло тому,"
  },
  {
    "chapter": 6,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "кто со мною в мире,"
  },
  {
    "chapter": 6,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "или врага без"
  },
  {
    "chapter": 6,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "повода обобрал,"
  },
  {
    "chapter": 6,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "то пусть враг за мною"
  },
  {
    "chapter": 6,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "гонится и настигнет,"
  },
  {
    "chapter": 6,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "пусть он жизнь мою втопчет"
  },
  {
    "chapter": 6,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "в землю"
  },
  {
    "chapter": 6,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "и повергнет славу мою в"
  },
  {
    "chapter": 6,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "прах."
  },
  {
    "chapter": 6,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "Пауза"
  },
  {
    "chapter": 6,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господи,"
  },
  {
    "chapter": 6,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "восстань в гневе Своем,"
  },
  {
    "chapter": 6,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "ополчись на неистовство"
  },
  {
    "chapter": 6,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "моих врагов! Пробудись,"
  },
  {
    "chapter": 6,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "заступись за меня на суде,"
  },
  {
    "chapter": 6,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "который Ты заповедал!"
  },
  {
    "chapter": 6,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть окружат Тебя"
  },
  {
    "chapter": 6,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "собравшиеся народы,"
  },
  {
    "chapter": 6,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "воссядь над ними на высоте."
  },
  {
    "chapter": 6,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Судит Господь народы."
  },
  {
    "chapter": 6,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Оправдай меня,"
  },
  {
    "chapter": 6,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Господи,"
  },
  {
    "chapter": 6,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "по праведности моей,"
  },
  {
    "chapter": 6,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "по моей непорочности,"
  },
  {
    "chapter": 6,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Всевышний."
  },
  {
    "chapter": 6,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Праведный Бог,"
  },
  {
    "chapter": 6,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "судящий разум и сердце,"
  },
  {
    "chapter": 6,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "положи конец"
  },
  {
    "chapter": 6,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "беззаконию нечестивых,"
  },
  {
    "chapter": 6,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "но праведного укрепи."
  },
  {
    "chapter": 6,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Щит мой Бог,"
  },
  {
    "chapter": 6,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "спасающий правых сердцем."
  },
  {
    "chapter": 6,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Бог судья справедливый,"
  },
  {
    "chapter": 6,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Бог,"
  },
  {
    "chapter": 6,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "строго"
  },
  {
    "chapter": 6,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "взыскивающий каждый день."
  },
  {
    "chapter": 6,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Если кто не раскается,"
  },
  {
    "chapter": 6,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Он наточит"
  },
  {
    "chapter": 6,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Свой меч и согнет,"
  },
  {
    "chapter": 6,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "оснастит тетивою лук,"
  },
  {
    "chapter": 6,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "приготовит оружие"
  },
  {
    "chapter": 6,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "смертоносное"
  },
  {
    "chapter": 6,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и горящими сделает стрелы."
  },
  {
    "chapter": 6,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Кто неправду зачал и"
  },
  {
    "chapter": 6,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "носит под сердцем зло,"
  },
  {
    "chapter": 6,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "разрешится от"
  },
  {
    "chapter": 6,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "бремени ложью."
  },
  {
    "chapter": 6,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Кто рыл и выкопал яму,"
  },
  {
    "chapter": 6,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "в нее же и упадет."
  },
  {
    "chapter": 6,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Зло его к нему"
  },
  {
    "chapter": 6,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "же и вернется,"
  },
  {
    "chapter": 6,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "на него"
  },
  {
    "chapter": 6,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "обратится его жестокость."
  },
  {
    "chapter": 6,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Восславлю Господа:"
  },
  {
    "chapter": 6,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "праведен Он,"
  },
  {
    "chapter": 6,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "воспою хвалу Имени Господа,"
  },
  {
    "chapter": 6,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Всевышнего."
  }
]