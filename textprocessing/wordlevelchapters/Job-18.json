[
  {
    "chapter": 18,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Тогда ответил"
  },
  {
    "chapter": 18,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Вилдад из Савхы:"
  },
  {
    "chapter": 18,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "- Когда ты положишь"
  },
  {
    "chapter": 18,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "конец речам? Подумай,"
  },
  {
    "chapter": 18,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "потом будем говорить."
  },
  {
    "chapter": 18,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Почему мы считаемся за"
  },
  {
    "chapter": 18,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "скотов? Почему в глазах"
  },
  {
    "chapter": 18,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "твоих мы глупцы?"
  },
  {
    "chapter": 18,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "О ты,"
  },
  {
    "chapter": 18,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "кто в гневе себя терзает,"
  },
  {
    "chapter": 18,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "опустеть ли ради тебя"
  },
  {
    "chapter": 18,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "земле? Скалам ли с мест"
  },
  {
    "chapter": 18,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "своих сдвинуться?"
  },
  {
    "chapter": 18,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Истинно,"
  },
  {
    "chapter": 18,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "свет у"
  },
  {
    "chapter": 18,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "нечестивого погаснет,"
  },
  {
    "chapter": 18,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "даже искры не останется."
  },
  {
    "chapter": 18,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Померкнет огонь"
  },
  {
    "chapter": 18,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "у него в шатре,"
  },
  {
    "chapter": 18,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и угаснет над"
  },
  {
    "chapter": 18,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "ним светильник."
  },
  {
    "chapter": 18,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Сократится мощь его шагов,"
  },
  {
    "chapter": 18,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и падет он жертвой"
  },
  {
    "chapter": 18,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "своих же замыслов."
  },
  {
    "chapter": 18,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Он своими ногами"
  },
  {
    "chapter": 18,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "наступит в тенета,"
  },
  {
    "chapter": 18,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "будет ходить"
  },
  {
    "chapter": 18,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "по сетям ловца."
  },
  {
    "chapter": 18,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Поймает его за пяту петля,"
  },
  {
    "chapter": 18,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "крепко схватит его ловушка."
  },
  {
    "chapter": 18,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Силки для него"
  },
  {
    "chapter": 18,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "раскинуты по земле,"
  },
  {
    "chapter": 18,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и ждет на пути западня."
  },
  {
    "chapter": 18,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Страшат его ужасы отовсюду,"
  },
  {
    "chapter": 18,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "заставляя метаться"
  },
  {
    "chapter": 18,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "из стороны в сторону."
  },
  {
    "chapter": 18,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Истощается сила"
  },
  {
    "chapter": 18,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "его от голода,"
  },
  {
    "chapter": 18,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и беда на страже,"
  },
  {
    "chapter": 18,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "когда он споткнется."
  },
  {
    "chapter": 18,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Съест его кожу болезнь,"
  },
  {
    "chapter": 18,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "съест первенец"
  },
  {
    "chapter": 18,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "смерти члены его."
  },
  {
    "chapter": 18,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Исторгнут его"
  },
  {
    "chapter": 18,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "из шатра надежды,"
  },
  {
    "chapter": 18,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и это приведут"
  },
  {
    "chapter": 18,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "к царю ужасов."
  },
  {
    "chapter": 18,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "В шатре его"
  },
  {
    "chapter": 18,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "поселится пламя;"
  },
  {
    "chapter": 18,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "горящая сера"
  },
  {
    "chapter": 18,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "изольется на его жилище."
  },
  {
    "chapter": 18,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Корни его засохнут внизу,"
  },
  {
    "chapter": 18,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "а ветви его наверху увянут."
  },
  {
    "chapter": 18,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Память о нем"
  },
  {
    "chapter": 18,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "исчезнет с земли,"
  },
  {
    "chapter": 18,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и не будут о нем"
  },
  {
    "chapter": 18,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "вспоминать на улицах."
  },
  {
    "chapter": 18,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Изгонят его из"
  },
  {
    "chapter": 18,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "света во мрак,"
  },
  {
    "chapter": 18,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "из мира живых прогонят."
  },
  {
    "chapter": 18,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Ни детей,"
  },
  {
    "chapter": 18,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "ни внуков не"
  },
  {
    "chapter": 18,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "останется после него в народе;"
  },
  {
    "chapter": 18,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "никого не"
  },
  {
    "chapter": 18,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "останется после него там,"
  },
  {
    "chapter": 18,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 6,
    "text": "где он жил когда-то."
  },
  {
    "chapter": 18,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "На западе"
  },
  {
    "chapter": 18,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "ужаснутся его судьбе,"
  },
  {
    "chapter": 18,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и на востоке"
  },
  {
    "chapter": 18,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "будут объяты страхом."
  },
  {
    "chapter": 18,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Да,"
  },
  {
    "chapter": 18,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "таков беззаконного дом,"
  },
  {
    "chapter": 18,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "место того,"
  },
  {
    "chapter": 18,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "кто не знает Бога."
  }
]