[
  {
    "chapter": 34,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Дирижеру хора."
  },
  {
    "chapter": 34,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Псалом Давида,"
  },
  {
    "chapter": 34,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "слуги Господа."
  },
  {
    "chapter": 34,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Грех говорит"
  },
  {
    "chapter": 34,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "нечестивому глубоко в его сердце;"
  },
  {
    "chapter": 34,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "нет Божьего"
  },
  {
    "chapter": 34,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "страха в глазах его."
  },
  {
    "chapter": 34,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Льстит он себе в"
  },
  {
    "chapter": 34,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "своих же глазах,"
  },
  {
    "chapter": 34,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "что ищет порок свой,"
  },
  {
    "chapter": 34,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "чтобы возненавидеть его."
  },
  {
    "chapter": 34,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Слова его уст"
  },
  {
    "chapter": 34,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "беззаконие и обман;"
  },
  {
    "chapter": 34,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "он отрекся от"
  },
  {
    "chapter": 34,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "мудрости и не делает добра."
  },
  {
    "chapter": 34,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Даже на ложе своем"
  },
  {
    "chapter": 34,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "замышляет он беззаконие."
  },
  {
    "chapter": 34,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Он встал на недобрый"
  },
  {
    "chapter": 34,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "путь и не отвергает зла."
  },
  {
    "chapter": 34,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господи,"
  },
  {
    "chapter": 34,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "милость Твоя до небес,"
  },
  {
    "chapter": 34,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "до облаков Твоя верность."
  },
  {
    "chapter": 34,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Праведность Твоя высока,"
  },
  {
    "chapter": 34,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "как горы,"
  },
  {
    "chapter": 34,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Твоя"
  },
  {
    "chapter": 34,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "справедливость великая бездна."
  },
  {
    "chapter": 34,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Ты хранишь"
  },
  {
    "chapter": 34,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "человека и зверя,"
  },
  {
    "chapter": 34,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "Господи."
  },
  {
    "chapter": 34,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Боже,"
  },
  {
    "chapter": 34,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "милость Твоя драгоценна! В"
  },
  {
    "chapter": 34,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "тени Твоих крыл укрываются"
  },
  {
    "chapter": 34,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "смертные."
  },
  {
    "chapter": 34,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пируют они от"
  },
  {
    "chapter": 34,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "щедрот Твоего дома;"
  },
  {
    "chapter": 34,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "из реки отрад"
  },
  {
    "chapter": 34,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Твоих Ты их поишь."
  },
  {
    "chapter": 34,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ведь у Тебя источник жизни,"
  },
  {
    "chapter": 34,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и во свете Твоем"
  },
  {
    "chapter": 34,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "мы увидим свет."
  },
  {
    "chapter": 34,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Излей Твою"
  },
  {
    "chapter": 34,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "милость на знающих Тебя,"
  },
  {
    "chapter": 34,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "праведность Свою"
  },
  {
    "chapter": 34,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "на правых сердцем."
  },
  {
    "chapter": 34,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Да не наступит на"
  },
  {
    "chapter": 34,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "меня нога гордеца;"
  },
  {
    "chapter": 34,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "да не изгонит меня"
  },
  {
    "chapter": 34,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "рука нечестивого."
  },
  {
    "chapter": 34,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Посмотри,"
  },
  {
    "chapter": 34,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "как пали беззаконники:"
  },
  {
    "chapter": 34,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "повергнуты они и"
  },
  {
    "chapter": 34,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "не могут подняться."
  }
]