[
  {
    "chapter": 8,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Дирижеру хора."
  },
  {
    "chapter": 8,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "На мотив Смерть сына."
  },
  {
    "chapter": 8,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Псалом Давида."
  },
  {
    "chapter": 8,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Буду славить Тебя,"
  },
  {
    "chapter": 8,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 8,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "от всего сердца,"
  },
  {
    "chapter": 8,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "расскажу о всех"
  },
  {
    "chapter": 8,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Твоих чудесах."
  },
  {
    "chapter": 8,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Буду радоваться и"
  },
  {
    "chapter": 8,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "торжествовать о Тебе;"
  },
  {
    "chapter": 8,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "в песнях Имя"
  },
  {
    "chapter": 8,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Твое прославлю,"
  },
  {
    "chapter": 8,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Всевышний."
  },
  {
    "chapter": 8,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Враги мои"
  },
  {
    "chapter": 8,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "обратились вспять;"
  },
  {
    "chapter": 8,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "пошатнулись они"
  },
  {
    "chapter": 8,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "пред Тобой и погибли."
  },
  {
    "chapter": 8,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты поддержал"
  },
  {
    "chapter": 8,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "меня в правой тяжбе,"
  },
  {
    "chapter": 8,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Ты воссел на престол"
  },
  {
    "chapter": 8,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "творить правый суд."
  },
  {
    "chapter": 8,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты осудил народы и"
  },
  {
    "chapter": 8,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "погубил нечестивых,"
  },
  {
    "chapter": 8,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Ты изгладил из"
  },
  {
    "chapter": 8,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "памяти имена их навеки."
  },
  {
    "chapter": 8,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Вечная гибель"
  },
  {
    "chapter": 8,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "пришла на врага,"
  },
  {
    "chapter": 8,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и города его Ты разрушил,"
  },
  {
    "chapter": 8,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "сама память о нем исчезла."
  },
  {
    "chapter": 8,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но Господь"
  },
  {
    "chapter": 8,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "владычествует вовек,"
  },
  {
    "chapter": 8,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Он для суда"
  },
  {
    "chapter": 8,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "воздвиг Свой престол."
  },
  {
    "chapter": 8,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "По правде будет судить Он"
  },
  {
    "chapter": 8,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "мир"
  },
  {
    "chapter": 8,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и народами править по"
  },
  {
    "chapter": 8,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "справедливости."
  },
  {
    "chapter": 8,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господь"
  },
  {
    "chapter": 8,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "прибежище угнетенным,"
  },
  {
    "chapter": 8,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "прибежище во время смуты."
  },
  {
    "chapter": 8,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Будут уповать на Тебя те,"
  },
  {
    "chapter": 8,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "кто знает Имя Твое,"
  },
  {
    "chapter": 8,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "ведь Ты,"
  },
  {
    "chapter": 8,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Господи,"
  },
  {
    "chapter": 8,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "не оставишь ищущих Тебя."
  },
  {
    "chapter": 8,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Славьте песнями Господа,"
  },
  {
    "chapter": 8,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "царящего на Сионе,"
  },
  {
    "chapter": 8,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "возвещайте"
  },
  {
    "chapter": 8,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "народам Его дела!"
  },
  {
    "chapter": 8,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ведь взыскивая за кровь,"
  },
  {
    "chapter": 8,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Он помнит о беззащитных,"
  },
  {
    "chapter": 8,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "вопля их не забывает."
  },
  {
    "chapter": 8,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господи,"
  },
  {
    "chapter": 8,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "как ненавистники мои"
  },
  {
    "chapter": 8,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "стеснили меня! Помилуй"
  },
  {
    "chapter": 8,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и отдали меня от ворот"
  },
  {
    "chapter": 8,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "смерти,"
  },
  {
    "chapter": 8,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "чтобы хвалу Тебе я"
  },
  {
    "chapter": 8,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "воздал в воротах дочери Сиона,"
  },
  {
    "chapter": 8,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "радуясь о Твоем спасении."
  },
  {
    "chapter": 8,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Народы упали в яму,"
  },
  {
    "chapter": 8,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "которую сами выкопали;"
  },
  {
    "chapter": 8,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "их ноги запутались в сети,"
  },
  {
    "chapter": 8,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "которую они скрыли."
  },
  {
    "chapter": 8,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Открылся Господь как"
  },
  {
    "chapter": 8,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "справедливый судья:"
  },
  {
    "chapter": 8,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "нечестивые попались в"
  },
  {
    "chapter": 8,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "ловушку своих же дел."
  },
  {
    "chapter": 8,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Раздумье."
  },
  {
    "chapter": 8,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Пауза"
  },
  {
    "chapter": 8,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Нечестивые отправятся в"
  },
  {
    "chapter": 8,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "мир мертвых все народы,"
  },
  {
    "chapter": 8,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "что забыли Бога."
  },
  {
    "chapter": 8,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но не навсегда"
  },
  {
    "chapter": 8,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "забыт будет нищий,"
  },
  {
    "chapter": 8,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и надеждам"
  },
  {
    "chapter": 8,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "страдальца не вечно гибнуть."
  },
  {
    "chapter": 8,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господи,"
  },
  {
    "chapter": 8,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "восстань! Не дай человеку"
  },
  {
    "chapter": 8,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "победы! Пусть будут судимы"
  },
  {
    "chapter": 8,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "перед Тобой народы."
  },
  {
    "chapter": 8,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господи,"
  },
  {
    "chapter": 8,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "ужасом их срази,"
  },
  {
    "chapter": 8,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "пусть знают,"
  },
  {
    "chapter": 8,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "что они лишь смертные."
  },
  {
    "chapter": 8,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Пауза"
  },
  {
    "chapter": 8,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Для чего,"
  },
  {
    "chapter": 8,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 8,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Ты стоишь вдали и во"
  },
  {
    "chapter": 8,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "время скорби Себя скрываешь?"
  },
  {
    "chapter": 8,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "В гордости своей нечестивый"
  },
  {
    "chapter": 8,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "преследует бедного пусть"
  },
  {
    "chapter": 8,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "будет он своими же уловками"
  },
  {
    "chapter": 8,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "пойман."
  },
  {
    "chapter": 8,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Кичится нечестивый"
  },
  {
    "chapter": 8,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "страстями своей души,"
  },
  {
    "chapter": 8,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "жадный до наживы"
  },
  {
    "chapter": 8,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "клянет и ругает Господа."
  },
  {
    "chapter": 8,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "В надменности своей"
  },
  {
    "chapter": 8,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "нечестивый Его не ищет,"
  },
  {
    "chapter": 8,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "в его мыслях нет"
  },
  {
    "chapter": 8,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "места для Бога."
  },
  {
    "chapter": 8,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Дела его всегда успешны."
  },
  {
    "chapter": 8,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Он надменен,"
  },
  {
    "chapter": 8,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и Твои законы"
  },
  {
    "chapter": 8,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "от него далеки;"
  },
  {
    "chapter": 8,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "на всех врагов своих"
  },
  {
    "chapter": 8,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "смотрит с пренебрежением."
  },
  {
    "chapter": 8,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он говорит себе:"
  },
  {
    "chapter": 8,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Не поколеблюсь! Никогда"
  },
  {
    "chapter": 8,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "не придет ко мне беда."
  },
  {
    "chapter": 8,
    "verse": 28,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Уста его полны проклятий,"
  },
  {
    "chapter": 8,
    "verse": 28,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "лжи и коварства,"
  },
  {
    "chapter": 8,
    "verse": 28,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "под языком у него"
  },
  {
    "chapter": 8,
    "verse": 28,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "козни и беззаконие."
  },
  {
    "chapter": 8,
    "verse": 29,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он ставит"
  },
  {
    "chapter": 8,
    "verse": 29,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "засаду у поселений;"
  },
  {
    "chapter": 8,
    "verse": 29,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "в глухих местах"
  },
  {
    "chapter": 8,
    "verse": 29,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "убивает невинных,"
  },
  {
    "chapter": 8,
    "verse": 29,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "наблюдая тайком за жертвой."
  },
  {
    "chapter": 8,
    "verse": 30,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он ждет в засаде,"
  },
  {
    "chapter": 8,
    "verse": 30,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "как в логове лев."
  },
  {
    "chapter": 8,
    "verse": 30,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Он ждет в засаде,"
  },
  {
    "chapter": 8,
    "verse": 30,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "чтобы ловить бессильных;"
  },
  {
    "chapter": 8,
    "verse": 30,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "он ловит их в"
  },
  {
    "chapter": 8,
    "verse": 30,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "сеть и уносит прочь."
  },
  {
    "chapter": 8,
    "verse": 31,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Те пригибаются,"
  },
  {
    "chapter": 8,
    "verse": 31,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "навзничь падают и"
  },
  {
    "chapter": 8,
    "verse": 31,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "несчастных крушит мощь нечестивого."
  },
  {
    "chapter": 8,
    "verse": 32,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "А он говорит себе:"
  },
  {
    "chapter": 8,
    "verse": 32,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Бог забыл."
  },
  {
    "chapter": 8,
    "verse": 32,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Он сокрыл лицо,"
  },
  {
    "chapter": 8,
    "verse": 32,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Он вовек не увидит."
  },
  {
    "chapter": 8,
    "verse": 33,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господи,"
  },
  {
    "chapter": 8,
    "verse": 33,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "восстань!"
  },
  {
    "chapter": 8,
    "verse": 33,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Воздень Свою руку,"
  },
  {
    "chapter": 8,
    "verse": 33,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Боже!"
  },
  {
    "chapter": 8,
    "verse": 33,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Беспомощных не забудь!"
  },
  {
    "chapter": 8,
    "verse": 34,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Почему нечестивый"
  },
  {
    "chapter": 8,
    "verse": 34,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "оскорбляет Бога? Почему он"
  },
  {
    "chapter": 8,
    "verse": 34,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "говорит себе:"
  },
  {
    "chapter": 8,
    "verse": 34,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Он не спросит с меня?"
  },
  {
    "chapter": 8,
    "verse": 35,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Нет! Ты видишь"
  },
  {
    "chapter": 8,
    "verse": 35,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "обиды и притеснения,"
  },
  {
    "chapter": 8,
    "verse": 35,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и готов"
  },
  {
    "chapter": 8,
    "verse": 35,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "воздать Твоею рукой."
  },
  {
    "chapter": 8,
    "verse": 35,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "И страдальцы"
  },
  {
    "chapter": 8,
    "verse": 35,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "вверяют себя Тебе."
  },
  {
    "chapter": 8,
    "verse": 35,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "Ты сиротам помощник."
  },
  {
    "chapter": 8,
    "verse": 36,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Сокруши же мощь"
  },
  {
    "chapter": 8,
    "verse": 36,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "нечестивого и злого,"
  },
  {
    "chapter": 8,
    "verse": 36,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и взыщи с него"
  },
  {
    "chapter": 8,
    "verse": 36,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "за его вину так,"
  },
  {
    "chapter": 8,
    "verse": 36,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "чтобы нечего"
  },
  {
    "chapter": 8,
    "verse": 36,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "было уже взыскивать."
  },
  {
    "chapter": 8,
    "verse": 37,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господь Царь вовеки,"
  },
  {
    "chapter": 8,
    "verse": 37,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "исчезнут"
  },
  {
    "chapter": 8,
    "verse": 37,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "народы на Его земле."
  },
  {
    "chapter": 8,
    "verse": 38,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты внемлешь,"
  },
  {
    "chapter": 8,
    "verse": 38,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 8,
    "verse": 38,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "желаниям кротких;"
  },
  {
    "chapter": 8,
    "verse": 38,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Ты ободряешь их,"
  },
  {
    "chapter": 8,
    "verse": 38,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "слышишь их крик."
  },
  {
    "chapter": 8,
    "verse": 39,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты защитишь"
  },
  {
    "chapter": 8,
    "verse": 39,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "сироту и бесправного,"
  },
  {
    "chapter": 8,
    "verse": 39,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "чтобы на земле человек"
  },
  {
    "chapter": 8,
    "verse": 39,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "не внушал больше страха."
  }
]