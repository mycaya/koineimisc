  {
    "chapter": 8,
    "verse": 10,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 7,
    "text": "встречал такой сильной веры!"
  },
  {
    "chapter": 8,
    "verse": 11,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 1,
    "text": "Говорю вам,"
  },
  {
    "chapter": 8,
    "verse": 11,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 2,
    "text": "что многие"
