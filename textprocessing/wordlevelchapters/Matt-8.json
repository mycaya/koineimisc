  {
    "chapter": 8,
    "verse": 13,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 1,
    "text": "И Иисус сказал сотнику:"
  },
  {
    "chapter": 8,
    "verse": 13,
    "book_name": "Matthew",
