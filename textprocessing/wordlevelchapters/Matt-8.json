  {
    "chapter": 8,
    "verse": 9,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 5,
    "text": "Я говорю одному:"
  },
  {
    "chapter": 8,
    "verse": 9,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 6,
    "text": "Иди!,"
  },
  {
    "chapter": 8,
    "verse": 9,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 7,
    "text": "и он идет,"
  },
  {
    "chapter": 8,
    "verse": 9,
    "book_name": "Matthew",
    "book_id": "Matt",
