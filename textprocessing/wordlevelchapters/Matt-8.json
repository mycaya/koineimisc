    "verse": 19,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 2,
    "text": "учителей Закона и сказал:"
  },
  {
    "chapter": 8,
    "verse": 19,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 3,
    "text": "Учитель,"
  },
  {
    "chapter": 8,
    "verse": 19,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 4,
    "text": "я пойду за Тобой,"
  },
  {
    "chapter": 8,
    "verse": 19,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 5,
