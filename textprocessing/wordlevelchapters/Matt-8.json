  {
    "chapter": 8,
    "verse": 3,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 3,
    "text": "сказав:"
  },
  {
    "chapter": 8,
    "verse": 3,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 4,
    "text": "Хочу,"
  },
  {
