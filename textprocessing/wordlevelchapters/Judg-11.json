  },
  {
    "chapter": 11,
    "verse": 23,
    "book_name": "Judges",
    "book_id": "Judg",
    "part": 7,
    "text": "какое у тебя"
  },
  {
    "chapter": 11,
    "verse": 23,
    "book_name": "Judges",
    "book_id": "Judg",
