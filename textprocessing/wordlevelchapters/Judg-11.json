  },
  {
    "chapter": 11,
    "verse": 24,
    "book_name": "Judges",
    "book_id": "Judg",
    "part": 6,
    "text": "наш Бог."
  },
  {
    "chapter": 11,
    "verse": 25,
    "book_name": "Judges",
    "book_id": "Judg",
    "part": 1,
    "text": "Чем ты лучше Валака,"
  },
  {
    "chapter": 11,
    "verse": 25,
    "book_name": "Judges",
    "book_id": "Judg",
    "part": 2,
