  },
  {
    "chapter": 11,
    "verse": 32,
    "book_name": "Judges",
    "book_id": "Judg",
    "part": 3,
    "text": "с аммонитянами,"
  },
  {
    "chapter": 11,
    "verse": 32,
    "book_name": "Judges",
    "book_id": "Judg",
