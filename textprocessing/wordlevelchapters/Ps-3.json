[
  {
    "chapter": 3,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Дирижеру хора."
  },
  {
    "chapter": 3,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "На струнных инструментах."
  },
  {
    "chapter": 3,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Псалом Давида."
  },
  {
    "chapter": 3,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ответь мне,"
  },
  {
    "chapter": 3,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "когда я взываю к Тебе,"
  },
  {
    "chapter": 3,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Бог мой,"
  },
  {
    "chapter": 3,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "оправдывающий меня."
  },
  {
    "chapter": 3,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Когда мне было тесно,"
  },
  {
    "chapter": 3,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Ты вывел меня на простор."
  },
  {
    "chapter": 3,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "Помилуй меня и"
  },
  {
    "chapter": 3,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 8,
    "text": "молитву мою услышь."
  },
  {
    "chapter": 3,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Люди,"
  },
  {
    "chapter": 3,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "как долго вы будете меня"
  },
  {
    "chapter": 3,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "чернить? Как долго будете"
  },
  {
    "chapter": 3,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "любить пустое,"
  },
  {
    "chapter": 3,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "искать ложь? Пауза"
  },
  {
    "chapter": 3,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Знайте,"
  },
  {
    "chapter": 3,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "праведника"
  },
  {
    "chapter": 3,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Господь Себе отделил."
  },
  {
    "chapter": 3,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Господь услышит,"
  },
  {
    "chapter": 3,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "когда я к Нему воззову."
  },
  {
    "chapter": 3,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Гневаясь,"
  },
  {
    "chapter": 3,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "не грешите;"
  },
  {
    "chapter": 3,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "задумайтесь об этом на"
  },
  {
    "chapter": 3,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "ложах ваших"
  },
  {
    "chapter": 3,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "и успокойтесь! Пауза"
  },
  {
    "chapter": 3,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Приносите жертвы правды"
  },
  {
    "chapter": 3,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и уповайте на Господа."
  },
  {
    "chapter": 3,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Многие спрашивают:"
  },
  {
    "chapter": 3,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Кто покажет нам"
  },
  {
    "chapter": 3,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "благо? Пусть воссияет нам,"
  },
  {
    "chapter": 3,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Господи,"
  },
  {
    "chapter": 3,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "свет Твоего лица!"
  },
  {
    "chapter": 3,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты наполнил мое"
  },
  {
    "chapter": 3,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "сердце радостью большей,"
  },
  {
    "chapter": 3,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "чем у тех,"
  },
  {
    "chapter": 3,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "у кого зерно и"
  },
  {
    "chapter": 3,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "вино в изобилии."
  },
  {
    "chapter": 3,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я лягу и с миром засну,"
  },
  {
    "chapter": 3,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "потому что,"
  },
  {
    "chapter": 3,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Господи,"
  },
  {
    "chapter": 3,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "только Ты даешь мне"
  },
  {
    "chapter": 3,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "жить в безопасности."
  }
]