[
  {
    "chapter": 57,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Дирижеру хора."
  },
  {
    "chapter": 57,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "На мотив Лилия завета."
  },
  {
    "chapter": 57,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Мольба Давида."
  },
  {
    "chapter": 57,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Наставление в то время,"
  },
  {
    "chapter": 57,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "когда он воевал с"
  },
  {
    "chapter": 57,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Арам-Нахараимом и с Арам-Цовой,"
  },
  {
    "chapter": 57,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и когда Иоав,"
  },
  {
    "chapter": 57,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "вернувшись,"
  },
  {
    "chapter": 57,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "сразил двенадцать тысяч"
  },
  {
    "chapter": 57,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "идумеян в Соляной долине."
  },
  {
    "chapter": 57,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты отверг нас,"
  },
  {
    "chapter": 57,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Боже,"
  },
  {
    "chapter": 57,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и сокрушил;"
  },
  {
    "chapter": 57,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Ты был в гневе"
  },
  {
    "chapter": 57,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "вернись к нам снова!"
  },
  {
    "chapter": 57,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты заставил землю дрожать,"
  },
  {
    "chapter": 57,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "расколол ее."
  },
  {
    "chapter": 57,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Исцели ее раны"
  },
  {
    "chapter": 57,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "она содрогается."
  },
  {
    "chapter": 57,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты послал Своему"
  },
  {
    "chapter": 57,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "народу безотрадные времена."
  },
  {
    "chapter": 57,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Ты напоил нас вином,"
  },
  {
    "chapter": 57,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "от которого нас шатает."
  },
  {
    "chapter": 57,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но для тех,"
  },
  {
    "chapter": 57,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "кто Тебя боится,"
  },
  {
    "chapter": 57,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "поднял Ты знамя,"
  },
  {
    "chapter": 57,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "чтобы они,"
  },
  {
    "chapter": 57,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "собравшись к нему,"
  },
  {
    "chapter": 57,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "стали для лука недосягаемы."
  },
  {
    "chapter": 57,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "Пауза"
  },
  {
    "chapter": 57,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Сохрани нас правой"
  },
  {
    "chapter": 57,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "рукой Твоей и ответь нам,"
  },
  {
    "chapter": 57,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "чтобы"
  },
  {
    "chapter": 57,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "возлюбленные Тобой спаслись."
  },
  {
    "chapter": 57,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Бог обещал в"
  },
  {
    "chapter": 57,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Своем святилище:"
  },
  {
    "chapter": 57,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Я разделю,"
  },
  {
    "chapter": 57,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "торжествуя,"
  },
  {
    "chapter": 57,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Шехем,"
  },
  {
    "chapter": 57,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "и долину Сокхоф размерю."
  },
  {
    "chapter": 57,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Мой Галаад и Мой Манассия,"
  },
  {
    "chapter": 57,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Ефрем Мой шлем,"
  },
  {
    "chapter": 57,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Иуда Мой скипетр."
  },
  {
    "chapter": 57,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Моав Моя умывальная чаша,"
  },
  {
    "chapter": 57,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "на Едом Я"
  },
  {
    "chapter": 57,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "брошу Мою сандалию,"
  },
  {
    "chapter": 57,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "над землей филистимлян"
  },
  {
    "chapter": 57,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "торжествующе восклицаю."
  },
  {
    "chapter": 57,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Кто приведет меня в"
  },
  {
    "chapter": 57,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "укрепленный город? Кто"
  },
  {
    "chapter": 57,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "доведет меня до Едома?"
  },
  {
    "chapter": 57,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Не Ты ли,"
  },
  {
    "chapter": 57,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Боже,"
  },
  {
    "chapter": 57,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Кто нас отринул,"
  },
  {
    "chapter": 57,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и не выходишь с"
  },
  {
    "chapter": 57,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "войсками нашими?"
  },
  {
    "chapter": 57,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Окажи нам помощь"
  },
  {
    "chapter": 57,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "в борьбе с врагом,"
  },
  {
    "chapter": 57,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "потому что"
  },
  {
    "chapter": 57,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "людская помощь напрасна."
  },
  {
    "chapter": 57,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "С Богом мы одержим победу;"
  },
  {
    "chapter": 57,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Он низвергнет наших врагов."
  }
]