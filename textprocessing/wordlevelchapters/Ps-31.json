[
  {
    "chapter": 31,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Радуйтесь,"
  },
  {
    "chapter": 31,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "праведные,"
  },
  {
    "chapter": 31,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "о Господе! Честным"
  },
  {
    "chapter": 31,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "подобает Его хвалить."
  },
  {
    "chapter": 31,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Под арфу хвалите Господа,"
  },
  {
    "chapter": 31,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "играйте Ему на"
  },
  {
    "chapter": 31,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "лире десятиструнной!"
  },
  {
    "chapter": 31,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пойте Ему новую песнь;"
  },
  {
    "chapter": 31,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "играйте искусно,"
  },
  {
    "chapter": 31,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "от радости восклицая!"
  },
  {
    "chapter": 31,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ведь слово Господне"
  },
  {
    "chapter": 31,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "право и все дела Его верны."
  },
  {
    "chapter": 31,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господь любит"
  },
  {
    "chapter": 31,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "праведность и справедливость."
  },
  {
    "chapter": 31,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Милости"
  },
  {
    "chapter": 31,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Господней полна земля."
  },
  {
    "chapter": 31,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Словом Господним"
  },
  {
    "chapter": 31,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "сотворены небеса,"
  },
  {
    "chapter": 31,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "повелением из уст"
  },
  {
    "chapter": 31,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Его звездное воинство."
  },
  {
    "chapter": 31,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он воды морей собрал,"
  },
  {
    "chapter": 31,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "как в сосуд;"
  },
  {
    "chapter": 31,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "положил пучины в хранилища."
  },
  {
    "chapter": 31,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть боится"
  },
  {
    "chapter": 31,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господа вся земля;"
  },
  {
    "chapter": 31,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "пусть трепещут пред Ним"
  },
  {
    "chapter": 31,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "все живущие во вселенной,"
  },
  {
    "chapter": 31,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "потому что Он"
  },
  {
    "chapter": 31,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "сказал и стало,"
  },
  {
    "chapter": 31,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Он повелел и явилось."
  },
  {
    "chapter": 31,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господь разрушает советы"
  },
  {
    "chapter": 31,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "людей"
  },
  {
    "chapter": 31,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и уничтожает замыслы"
  },
  {
    "chapter": 31,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "народов."
  },
  {
    "chapter": 31,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но Господень"
  },
  {
    "chapter": 31,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "совет пребудет вовек,"
  },
  {
    "chapter": 31,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "замыслы сердца Его из"
  },
  {
    "chapter": 31,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "поколения в поколение."
  },
  {
    "chapter": 31,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Блажен народ,"
  },
  {
    "chapter": 31,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "для кого Бог Господь,"
  },
  {
    "chapter": 31,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "народ,"
  },
  {
    "chapter": 31,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "который избрал Он"
  },
  {
    "chapter": 31,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Себе в наследие."
  },
  {
    "chapter": 31,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господь взирает с небес и"
  },
  {
    "chapter": 31,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "видит весь род человеческий."
  },
  {
    "chapter": 31,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он смотрит со"
  },
  {
    "chapter": 31,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Своего престола на всех,"
  },
  {
    "chapter": 31,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "кто живет на земле -"
  },
  {
    "chapter": 31,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он,"
  },
  {
    "chapter": 31,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Кто создал все их"
  },
  {
    "chapter": 31,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "сердца и вникает во все,"
  },
  {
    "chapter": 31,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "что они творят."
  },
  {
    "chapter": 31,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Не спасет царя"
  },
  {
    "chapter": 31,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "огромное войско;"
  },
  {
    "chapter": 31,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "не спасет"
  },
  {
    "chapter": 31,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "воина великая сила."
  },
  {
    "chapter": 31,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ненадежен конь"
  },
  {
    "chapter": 31,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "для спасения:"
  },
  {
    "chapter": 31,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "не спасет он своею мощью."
  },
  {
    "chapter": 31,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но глаза Господни"
  },
  {
    "chapter": 31,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "обращены на тех,"
  },
  {
    "chapter": 31,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "кто Его боится,"
  },
  {
    "chapter": 31,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "на тех,"
  },
  {
    "chapter": 31,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "чья надежда Его милость,"
  },
  {
    "chapter": 31,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "чтобы от смерти их спасти"
  },
  {
    "chapter": 31,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и прокормить во время"
  },
  {
    "chapter": 31,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "голода."
  },
  {
    "chapter": 31,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Уповает на"
  },
  {
    "chapter": 31,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господа наша душа,"
  },
  {
    "chapter": 31,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Он нам помощь и щит."
  },
  {
    "chapter": 31,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "В нем ликуют наши сердца,"
  },
  {
    "chapter": 31,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "ведь мы уповаем"
  },
  {
    "chapter": 31,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "на святое Имя Его."
  },
  {
    "chapter": 31,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Да будет"
  },
  {
    "chapter": 31,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "милость Твоя с нами,"
  },
  {
    "chapter": 31,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Господи,"
  },
  {
    "chapter": 31,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "ведь мы на Тебя надеемся."
  }
]