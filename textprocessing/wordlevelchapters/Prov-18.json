    "chapter": 18,
    "verse": 20,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 4,
    "text": "жатвой своего языка."
  },
  {
    "chapter": 18,
    "verse": 21,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 1,
    "text": "У языка сила"
  },
  {
    "chapter": 18,
    "verse": 21,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 2,
    "text": "жизни и смерти,"
  },
  {
    "chapter": 18,
    "verse": 21,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 3,
    "text": "те,"
  },
  {
    "chapter": 18,
    "verse": 21,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 4,
    "text": "кто любит его,"
  },
  {
    "chapter": 18,
    "verse": 21,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 5,
    "text": "будут есть его плод."
  },
  {
    "chapter": 18,
    "verse": 22,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 1,
    "text": "Нашедший жену нашел благо"
  },
  {
    "chapter": 18,
    "verse": 22,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 2,
    "text": "и получил от Господа"
  },
  {
    "chapter": 18,
    "verse": 22,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 3,
    "text": "расположение."
  },
  {
    "chapter": 18,
    "verse": 23,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 1,
    "text": "Бедняк о милости молит,"
  },
  {
    "chapter": 18,
    "verse": 23,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 2,
    "text": "а богач отвечает грубо."
  },
  {
