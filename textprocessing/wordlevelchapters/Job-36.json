[
  {
    "chapter": 36,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Затем Елиуй добавил:"
  },
  {
    "chapter": 36,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "- Подожди"
  },
  {
    "chapter": 36,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "немного и покажу тебе,"
  },
  {
    "chapter": 36,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "что мне есть,"
  },
  {
    "chapter": 36,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "что сказать в пользу Бога."
  },
  {
    "chapter": 36,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Я начну"
  },
  {
    "chapter": 36,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "рассуждения издалека,"
  },
  {
    "chapter": 36,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "моему Творцу"
  },
  {
    "chapter": 36,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "справедливость воздам."
  },
  {
    "chapter": 36,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Поистине,"
  },
  {
    "chapter": 36,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "нет в моих словах лжи,"
  },
  {
    "chapter": 36,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "совершенный в"
  },
  {
    "chapter": 36,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "познаниях перед тобой."
  },
  {
    "chapter": 36,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Бог могуществен,"
  },
  {
    "chapter": 36,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "но людей Он не презирает;"
  },
  {
    "chapter": 36,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "силой разума Он могуч."
  },
  {
    "chapter": 36,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Не позволяет Он жить"
  },
  {
    "chapter": 36,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "злодеям и дает угнетенным права."
  },
  {
    "chapter": 36,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "От праведных Он"
  },
  {
    "chapter": 36,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "не отводит глаз;"
  },
  {
    "chapter": 36,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "с царями их на"
  },
  {
    "chapter": 36,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "престол возводит,"
  },
  {
    "chapter": 36,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "и они остаются"
  },
  {
    "chapter": 36,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 6,
    "text": "возвеличенными навсегда."
  },
  {
    "chapter": 36,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "А если люди цепями"
  },
  {
    "chapter": 36,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "скованы и томятся в узах беды,"
  },
  {
    "chapter": 36,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Он объявляет им их дела"
  },
  {
    "chapter": 36,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "как в гордыне они согрешили."
  },
  {
    "chapter": 36,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Он урок им преподает и"
  },
  {
    "chapter": 36,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "велит им в грехах раскаяться."
  },
  {
    "chapter": 36,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Если послушают и"
  },
  {
    "chapter": 36,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "станут Ему служить,"
  },
  {
    "chapter": 36,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "то окончат они свои дни в"
  },
  {
    "chapter": 36,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "благополучии"
  },
  {
    "chapter": 36,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "и в радости свои годы."
  },
  {
    "chapter": 36,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "А если они не услышат,"
  },
  {
    "chapter": 36,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "то переправятся через реку"
  },
  {
    "chapter": 36,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "смерти"
  },
  {
    "chapter": 36,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "и погибнут без знания."
  },
  {
    "chapter": 36,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Сердца"
  },
  {
    "chapter": 36,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "безбожников полны гнева,"
  },
  {
    "chapter": 36,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "не взмолятся о помощи,"
  },
  {
    "chapter": 36,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "даже когда Он"
  },
  {
    "chapter": 36,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "заковывает их в оковы."
  },
  {
    "chapter": 36,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Они умирают в юности,"
  },
  {
    "chapter": 36,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "среди храмовых блудников."
  },
  {
    "chapter": 36,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Но страдающих Он"
  },
  {
    "chapter": 36,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "избавляет страданием"
  },
  {
    "chapter": 36,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "открывает им слух бедой."
  },
  {
    "chapter": 36,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "И тебя бы Он вывел из"
  },
  {
    "chapter": 36,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "тесноты на просторную волю,"
  },
  {
    "chapter": 36,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "где нет преград,"
  },
  {
    "chapter": 36,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "и лакомой пищей"
  },
  {
    "chapter": 36,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "твой стол был бы полон."
  },
  {
    "chapter": 36,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Но ты одержим"
  },
  {
    "chapter": 36,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "судом над злодеями;"
  },
  {
    "chapter": 36,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "суд с"
  },
  {
    "chapter": 36,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "приговором к тебе близки."
  },
  {
    "chapter": 36,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Берегись,"
  },
  {
    "chapter": 36,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "чтобы богатство"
  },
  {
    "chapter": 36,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "не соблазнило тебя,"
  },
  {
    "chapter": 36,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "и внушительный"
  },
  {
    "chapter": 36,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "выкуп тебя не испортил."
  },
  {
    "chapter": 36,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Спасет ли твое"
  },
  {
    "chapter": 36,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "богатство тебя от беды,"
  },
  {
    "chapter": 36,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "вся сила твоя спасет ли?"
  },
  {
    "chapter": 36,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Не желай прихода той ночи,"
  },
  {
    "chapter": 36,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "под покровом"
  },
  {
    "chapter": 36,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "которой народы гибнут."
  },
  {
    "chapter": 36,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Берегись,"
  },
  {
    "chapter": 36,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "не склоняйся к нечестию,"
  },
  {
    "chapter": 36,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "ты за это бедой испытуем."
  },
  {
    "chapter": 36,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Бог высок в"
  },
  {
    "chapter": 36,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Своем могуществе."
  },
  {
    "chapter": 36,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Кто учитель подобный Ему?"
  },
  {
    "chapter": 36,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Кто пути Ему"
  },
  {
    "chapter": 36,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "предписал или сказал Ему:"
  },
  {
    "chapter": 36,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Ты не прав?."
  },
  {
    "chapter": 36,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Помни о том,"
  },
  {
    "chapter": 36,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "чтобы"
  },
  {
    "chapter": 36,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "возвеличивать дела Его,"
  },
  {
    "chapter": 36,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "о которых люди поют."
  },
  {
    "chapter": 36,
    "verse": 25,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Видели их все люди;"
  },
  {
    "chapter": 36,
    "verse": 25,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "издавна удивлялись им."
  },
  {
    "chapter": 36,
    "verse": 26,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Бог велик,"
  },
  {
    "chapter": 36,
    "verse": 26,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "мы не в силах Его познать."
  },
  {
    "chapter": 36,
    "verse": 26,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Непостижимо число Его лет."
  },
  {
    "chapter": 36,
    "verse": 27,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Он собирает капли воды"
  },
  {
    "chapter": 36,
    "verse": 27,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и обращает их в дождь."
  },
  {
    "chapter": 36,
    "verse": 28,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Облака изливают влагу,"
  },
  {
    "chapter": 36,
    "verse": 28,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и обильные"
  },
  {
    "chapter": 36,
    "verse": 28,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "ливни идут на людей."
  },
  {
    "chapter": 36,
    "verse": 29,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Кто в силах постичь"
  },
  {
    "chapter": 36,
    "verse": 29,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "движение туч и грохот грозы"
  },
  {
    "chapter": 36,
    "verse": 29,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "из Его шатра?"
  },
  {
    "chapter": 36,
    "verse": 30,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Вот,"
  },
  {
    "chapter": 36,
    "verse": 30,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Он молнии"
  },
  {
    "chapter": 36,
    "verse": 30,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "мечет вокруг Себя,"
  },
  {
    "chapter": 36,
    "verse": 30,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "и вновь"
  },
  {
    "chapter": 36,
    "verse": 30,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "затворяет истоки бездны."
  },
  {
    "chapter": 36,
    "verse": 31,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Так Он властвует над"
  },
  {
    "chapter": 36,
    "verse": 31,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "народами и дает в изобилии пищу."
  },
  {
    "chapter": 36,
    "verse": 32,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Он руки Свои"
  },
  {
    "chapter": 36,
    "verse": 32,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "наполняет молнией и велит ей,"
  },
  {
    "chapter": 36,
    "verse": 32,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "кого поразить."
  },
  {
    "chapter": 36,
    "verse": 33,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Гром возвещает приход бури;"
  },
  {
    "chapter": 36,
    "verse": 33,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и знает скот,"
  },
  {
    "chapter": 36,
    "verse": 33,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "что близко она."
  }
]