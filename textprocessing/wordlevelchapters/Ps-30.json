[
  {
    "chapter": 30,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Наставление Давида."
  },
  {
    "chapter": 30,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Псалом."
  },
  {
    "chapter": 30,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Блажен тот,"
  },
  {
    "chapter": 30,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "чьи беззакония прощены,"
  },
  {
    "chapter": 30,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "чьи грехи покрыты!"
  },
  {
    "chapter": 30,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Блажен тот,"
  },
  {
    "chapter": 30,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "кому Господь"
  },
  {
    "chapter": 30,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "не вменит греха,"
  },
  {
    "chapter": 30,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "в чьем духе нет коварства!"
  },
  {
    "chapter": 30,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пока я хранил молчание,"
  },
  {
    "chapter": 30,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "тело мое изнемогло от"
  },
  {
    "chapter": 30,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "стонов моих ежедневных."
  },
  {
    "chapter": 30,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ведь день"
  },
  {
    "chapter": 30,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и ночь напролет тяготела"
  },
  {
    "chapter": 30,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "надо мною Твоя рука;"
  },
  {
    "chapter": 30,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "сила моя иссякла,"
  },
  {
    "chapter": 30,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "как от летнего зноя."
  },
  {
    "chapter": 30,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Пауза"
  },
  {
    "chapter": 30,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Тогда я открыл Тебе мой"
  },
  {
    "chapter": 30,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "грех"
  },
  {
    "chapter": 30,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и не скрыл моего"
  },
  {
    "chapter": 30,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "беззакония."
  },
  {
    "chapter": 30,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Я сказал:"
  },
  {
    "chapter": 30,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Признаюсь перед"
  },
  {
    "chapter": 30,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "Господом в своих преступлениях,"
  },
  {
    "chapter": 30,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 8,
    "text": "и Ты простил"
  },
  {
    "chapter": 30,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 9,
    "text": "вину моего греха."
  },
  {
    "chapter": 30,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 10,
    "text": "Пауза"
  },
  {
    "chapter": 30,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Поэтому тот,"
  },
  {
    "chapter": 30,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "кто верен Тебе,"
  },
  {
    "chapter": 30,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "пусть помолится"
  },
  {
    "chapter": 30,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "пока может Тебя найти,"
  },
  {
    "chapter": 30,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "и когда"
  },
  {
    "chapter": 30,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "разольются могучие воды,"
  },
  {
    "chapter": 30,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "они не достигнут его."
  },
  {
    "chapter": 30,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты убежище мне;"
  },
  {
    "chapter": 30,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Ты спасешь меня от беды"
  },
  {
    "chapter": 30,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и окружишь криками радости"
  },
  {
    "chapter": 30,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "об избавлении."
  },
  {
    "chapter": 30,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Пауза"
  },
  {
    "chapter": 30,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господь говорит:"
  },
  {
    "chapter": 30,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Я наставлю тебя и"
  },
  {
    "chapter": 30,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "покажу тебе путь,"
  },
  {
    "chapter": 30,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "по которому тебе идти."
  },
  {
    "chapter": 30,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Я буду вести тебя и"
  },
  {
    "chapter": 30,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "не выпущу тебя из виду."
  },
  {
    "chapter": 30,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Не будь как конь или мул,"
  },
  {
    "chapter": 30,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "что разумом обделен,"
  },
  {
    "chapter": 30,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "чей норов нужно"
  },
  {
    "chapter": 30,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "обуздывать уздечкой и удилами,"
  },
  {
    "chapter": 30,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "иначе они к"
  },
  {
    "chapter": 30,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "тебе не подойдут."
  },
  {
    "chapter": 30,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Много горя у нечестивых,"
  },
  {
    "chapter": 30,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "но надеющихся на"
  },
  {
    "chapter": 30,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Господа окружает Его милость."
  },
  {
    "chapter": 30,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Веселитесь о"
  },
  {
    "chapter": 30,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господе и ликуйте,"
  },
  {
    "chapter": 30,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "праведные! Пойте,"
  },
  {
    "chapter": 30,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "все правые сердцем!"
  }
]