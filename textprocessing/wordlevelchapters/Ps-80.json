[
  {
    "chapter": 80,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Дирижеру хора."
  },
  {
    "chapter": 80,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Под гиттит."
  },
  {
    "chapter": 80,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Песнь сыновей Корея."
  },
  {
    "chapter": 80,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Как желанно Твое жилище,"
  },
  {
    "chapter": 80,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 80,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Бог сил!"
  },
  {
    "chapter": 80,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Истомилась моя душа,"
  },
  {
    "chapter": 80,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "желая быть во"
  },
  {
    "chapter": 80,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "дворах храма Господа,"
  },
  {
    "chapter": 80,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "сердце мое и плоть"
  },
  {
    "chapter": 80,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "стремятся к Богу живому."
  },
  {
    "chapter": 80,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Даже воробей находит"
  },
  {
    "chapter": 80,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "себе дом и ласточка гнездо,"
  },
  {
    "chapter": 80,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "чтобы вывести"
  },
  {
    "chapter": 80,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "своих птенцов,"
  },
  {
    "chapter": 80,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "у Твоего жертвенника,"
  },
  {
    "chapter": 80,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Господи,"
  },
  {
    "chapter": 80,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "Бог сил,"
  },
  {
    "chapter": 80,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 8,
    "text": "Царь мой и Бог мой."
  },
  {
    "chapter": 80,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Благословенны"
  },
  {
    "chapter": 80,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "живущие в доме Твоем,"
  },
  {
    "chapter": 80,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "они непрестанно"
  },
  {
    "chapter": 80,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "будут восхвалять Тебя."
  },
  {
    "chapter": 80,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Пауза"
  },
  {
    "chapter": 80,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Благословен человек,"
  },
  {
    "chapter": 80,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "сила которого в Тебе,"
  },
  {
    "chapter": 80,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "в чьем сердце есть"
  },
  {
    "chapter": 80,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "желание отправиться в Иерусалим."
  },
  {
    "chapter": 80,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Проходя долиною Плача,"
  },
  {
    "chapter": 80,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "они открывают в"
  },
  {
    "chapter": 80,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "ней источники,"
  },
  {
    "chapter": 80,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и дождь"
  },
  {
    "chapter": 80,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "покрывает ее водоемами."
  },
  {
    "chapter": 80,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Они крепнут все"
  },
  {
    "chapter": 80,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "больше и больше,"
  },
  {
    "chapter": 80,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "предстают перед"
  },
  {
    "chapter": 80,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Богом на Сионе."
  },
  {
    "chapter": 80,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господи,"
  },
  {
    "chapter": 80,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Бог сил,"
  },
  {
    "chapter": 80,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "услышь мою молитву;"
  },
  {
    "chapter": 80,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "внемли мне,"
  },
  {
    "chapter": 80,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Бог Иакова."
  },
  {
    "chapter": 80,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Пауза"
  },
  {
    "chapter": 80,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Взгляни,"
  },
  {
    "chapter": 80,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Боже,"
  },
  {
    "chapter": 80,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "на царя,"
  },
  {
    "chapter": 80,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "на щит наш;"
  },
  {
    "chapter": 80,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "посмотри с благоволением"
  },
  {
    "chapter": 80,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "на лицо Твоего помазанника!"
  },
  {
    "chapter": 80,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Один день во дворах Твоих"
  },
  {
    "chapter": 80,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "лучше тысячи вне его стен."
  },
  {
    "chapter": 80,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Лучше быть у"
  },
  {
    "chapter": 80,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "порога дома Божьего,"
  },
  {
    "chapter": 80,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "чем жить в"
  },
  {
    "chapter": 80,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "шатрах у нечестивых."
  },
  {
    "chapter": 80,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Потому что Господь"
  },
  {
    "chapter": 80,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Бог солнце и щит,"
  },
  {
    "chapter": 80,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и награда Господа"
  },
  {
    "chapter": 80,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "благодать и слава."
  },
  {
    "chapter": 80,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Тех,"
  },
  {
    "chapter": 80,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "кто ходит непорочно,"
  },
  {
    "chapter": 80,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "Он не лишает благ."
  },
  {
    "chapter": 80,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господи сил,"
  },
  {
    "chapter": 80,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "блажен человек,"
  },
  {
    "chapter": 80,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "уповающий на Тебя."
  }
]