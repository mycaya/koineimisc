    "chapter": 6,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "положить на весы!"
  },
  {
    "chapter": 6,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Они перевесили бы песок"
  },
  {
    "chapter": 6,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "морей мои слова оттого"
  },
  {
    "chapter": 6,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и бессвязны."
  },
  {
    "chapter": 6,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Стрелы Всемогущего во мне,"
  },
  {
    "chapter": 6,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "их яд пьет мой дух;"
  },
  {
    "chapter": 6,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "ужасы Божьи"
  },
  {
    "chapter": 6,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "ополчились против меня."
  },
  {
    "chapter": 6,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Разве ревет дикий осел на"
  },
  {
    "chapter": 6,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "пастбище? Разве мычит бык"
  },
  {
    "chapter": 6,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "над кормушкой?"
  },
  {
    "chapter": 6,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Разве вкусна еда без соли?"
  },
  {
    "chapter": 6,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "И разве есть вкус в яичном"
  },
  {
    "chapter": 6,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "белке?"
  },
  {
    "chapter": 6,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Я гнушаюсь"
  },
  {
    "chapter": 6,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "прикоснуться к такой пище;"
  },
  {
    "chapter": 6,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "вот что в"
  },
  {
    "chapter": 6,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "несчастье стало моей пищей."
  },
  {
    "chapter": 6,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "О,"
  },
  {
    "chapter": 6,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "когда сбылась"
  },
  {
    "chapter": 6,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "бы просьба моя,"
  },
  {
    "chapter": 6,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "и Бог исполнил"
  },
  {
    "chapter": 6,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "мою надежду -"
  },
  {
    "chapter": 6,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "соизволил бы Бог"
  },
  {
    "chapter": 6,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "сокрушить меня,"
  },
  {
    "chapter": 6,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и,"
  },
  {
    "chapter": 6,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "руку протянув,"
  },
  {
    "chapter": 6,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "сразил бы!"
  },
  {
    "chapter": 6,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "У меня тогда было бы"
  },
  {
    "chapter": 6,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "утешение радость в боли"
  },
  {
    "chapter": 6,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "непрекращающейся:"
  },
  {
    "chapter": 6,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "слов Святого я не отверг."
  },
  {
    "chapter": 6,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Откуда взять силы,"
  },
  {
    "chapter": 6,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "чтобы ждать?"
  },
  {
    "chapter": 6,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Каков мой конец,"
  },
  {
    "chapter": 6,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "чтобы терпеть?"
  },
  {
    "chapter": 6,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Разве моя сила сила камня?"
  },
  {
    "chapter": 6,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Разве из бронзы моя плоть?"
  },
  {
    "chapter": 6,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Разве есть во мне"
  },
  {
    "chapter": 6,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "сила помочь себе,"
  },
  {
    "chapter": 6,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "когда опоры у"
  },
  {
    "chapter": 6,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "меня не стало?"
  },
  {
    "chapter": 6,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Не проявивший"
  },
  {
    "chapter": 6,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "милосердия к другу,"
  },
  {
    "chapter": 6,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "потерял страх"
  },
  {
    "chapter": 6,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "перед Всемогущим."
  },
  {
    "chapter": 6,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Но братья мои ненадежны,"
  },
  {
    "chapter": 6,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "как паводок,"
  },
  {
    "chapter": 6,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "как реки,"
  },
  {
    "chapter": 6,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "которые разливаются,"
  },
  {
    "chapter": 6,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "потемнев от талого льда,"
  },
  {
    "chapter": 6,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "разбухнув от талого снега,"
  },
  {
    "chapter": 6,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "но затем"
  },
  {
    "chapter": 6,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "исчезают во время зноя:"
  },
  {
    "chapter": 6,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "когда припечет пропадают."
  },
  {
    "chapter": 6,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
