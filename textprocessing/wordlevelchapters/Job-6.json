  },
  {
    "chapter": 6,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "идут в пустыню и гибнут."
  },
  {
    "chapter": 6,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Караваны из Фемы ищут воду,"
  },
  {
    "chapter": 6,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "купцы из Савы надеются."
  },
  {
    "chapter": 6,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Они разуверились,"
  },
  {
    "chapter": 6,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "понадеявшись;"
  },
  {
    "chapter": 6,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "пришли туда,"
  },
