    "chapter": 5,
    "verse": 12,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Я словно моль для Ефрема,"
  },
  {
    "chapter": 5,
    "verse": 12,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "как гниль для народа Иудеи."
  },
  {
    "chapter": 5,
    "verse": 13,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Когда Ефрем"
  },
  {
    "chapter": 5,
    "verse": 13,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "увидел свою болезнь,"
  },
  {
    "chapter": 5,
    "verse": 13,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "а Иуда свои раны,"
  },
  {
    "chapter": 5,
    "verse": 13,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "тогда обратился Ефрем к"
  },
  {
    "chapter": 5,
    "verse": 13,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "Ассирии"
  },
