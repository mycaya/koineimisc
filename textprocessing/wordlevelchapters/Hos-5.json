    "chapter": 5,
    "verse": 10,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Вожди Иудеи"
  },
  {
    "chapter": 5,
    "verse": 10,
    "book_name": "Hosea",
    "book_id": "Hos",
