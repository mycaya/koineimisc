[
  {
    "chapter": 7,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Когда Я исцелю Израиль,"
  },
  {
    "chapter": 7,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "откроются грехи Ефрема"
  },
  {
    "chapter": 7,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "и преступления столицы"
  },
  {
    "chapter": 7,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "Самарии будут разоблачены."
  },
  {
    "chapter": 7,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "Люди обманывают,"
  },
  {
    "chapter": 7,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "воры пробираются в дома,"
  },
  {
    "chapter": 7,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 7,
    "text": "разбойники"
  },
  {
    "chapter": 7,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 8,
    "text": "грабят на улицах,"
  },
  {
    "chapter": 7,
    "verse": 2,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "но они не осознают,"
  },
  {
    "chapter": 7,
    "verse": 2,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "что Я помню"
  },
  {
    "chapter": 7,
    "verse": 2,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "все их злые дела."
  },
  {
    "chapter": 7,
    "verse": 2,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "Их"
  },
  {
    "chapter": 7,
    "verse": 2,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "преступления окружают их."
  },
  {
    "chapter": 7,
    "verse": 2,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "Грехи их"
  },
  {
    "chapter": 7,
    "verse": 2,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 7,
    "text": "всегда передо Мной."
  },
  {
    "chapter": 7,
    "verse": 3,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Они веселят царя"
  },
  {
    "chapter": 7,
    "verse": 3,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "своим злодейством,"
  },
  {
    "chapter": 7,
    "verse": 3,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "вождей своей ложью."
  },
  {
    "chapter": 7,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Все они неверные,"
  },
  {
    "chapter": 7,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "пылают,"
  },
  {
    "chapter": 7,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "словно печь,"
  },
  {
    "chapter": 7,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "чей огонь не нужно"
  },
  {
    "chapter": 7,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "поддерживать пекарю с"
  },
  {
    "chapter": 7,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "момента замешивания теста"
  },
  {
    "chapter": 7,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 7,
    "text": "и до тех пор,"
  },
  {
    "chapter": 7,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 8,
    "text": "пока оно не поднимется."
  },
  {
    "chapter": 7,
    "verse": 5,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "В день праздника нашего"
  },
  {
    "chapter": 7,
    "verse": 5,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "царя вожди напиваются вином"
  },
  {
    "chapter": 7,
    "verse": 5,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "до бесчувствия,"
  },
  {
    "chapter": 7,
    "verse": 5,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "а царь протягивает"
  },
  {
    "chapter": 7,
    "verse": 5,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "свою руку насмешникам."
  },
  {
    "chapter": 7,
    "verse": 6,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Их сердца,"
  },
  {
    "chapter": 7,
    "verse": 6,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "словно печь,"
  },
  {
    "chapter": 7,
    "verse": 6,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "горят интригами."
  },
  {
    "chapter": 7,
    "verse": 6,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "Их злоба теплится всю ночь,"
  },
  {
    "chapter": 7,
    "verse": 6,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "а утром она горит,"
  },
  {
    "chapter": 7,
    "verse": 6,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "как пылающий огонь."
  },
  {
    "chapter": 7,
    "verse": 7,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Все они раскалены,"
  },
  {
    "chapter": 7,
    "verse": 7,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "как печь,"
  },
  {
    "chapter": 7,
    "verse": 7,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "истребляют"
  },
  {
    "chapter": 7,
    "verse": 7,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "своих же правителей."
  },
  {
    "chapter": 7,
    "verse": 7,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "Все их цари низвергаются,"
  },
  {
    "chapter": 7,
    "verse": 7,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "и никто из них"
  },
  {
    "chapter": 7,
    "verse": 7,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 7,
    "text": "не взывает ко Мне."
  },
  {
    "chapter": 7,
    "verse": 8,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Ефрем смешался с"
  },
  {
    "chapter": 7,
    "verse": 8,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "нечестивыми народами,"
  },
  {
    "chapter": 7,
    "verse": 8,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "уподобился хлебу,"
  },
  {
    "chapter": 7,
    "verse": 8,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "недопеченному с одной"
  },
  {
    "chapter": 7,
    "verse": 8,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "стороны и подгоревшему с другой."
  },
  {
    "chapter": 7,
    "verse": 9,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Чужестранцы"
  },
  {
    "chapter": 7,
    "verse": 9,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "поедают его силы,"
  },
  {
    "chapter": 7,
    "verse": 9,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "а он и не осознает."
  },
  {
    "chapter": 7,
    "verse": 9,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "Его волос стал седым,"
  },
  {
    "chapter": 7,
    "verse": 9,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "а он и не замечает."
  },
  {
    "chapter": 7,
    "verse": 10,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Гордость Израиля"
  },
  {
    "chapter": 7,
    "verse": 10,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "свидетельствует против него,"
  },
  {
    "chapter": 7,
    "verse": 10,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "и,"
  },
  {
    "chapter": 7,
    "verse": 10,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "при всем этом,"
  },
  {
    "chapter": 7,
    "verse": 10,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "он не"
  },
  {
    "chapter": 7,
    "verse": 10,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "возвращается к Господу,"
  },
  {
    "chapter": 7,
    "verse": 10,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 7,
    "text": "своему Богу,"
  },
  {
    "chapter": 7,
    "verse": 10,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 8,
    "text": "и не ищет Его."
  },
  {
    "chapter": 7,
    "verse": 11,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Ефрем стал как глупый,"
  },
  {
    "chapter": 7,
    "verse": 11,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "ничего не смыслящий голубь."
  },
  {
    "chapter": 7,
    "verse": 11,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "Теперь он взывает к"
  },
  {
    "chapter": 7,
    "verse": 11,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "Египту и идет в Ассирию."
  },
  {
    "chapter": 7,
    "verse": 12,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Когда они пойдут,"
  },
  {
    "chapter": 7,
    "verse": 12,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "Я накину на них Свою сеть."
  },
  {
    "chapter": 7,
    "verse": 12,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "Я низвергну их,"
  },
  {
    "chapter": 7,
    "verse": 12,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "как птиц небесных."
  },
  {
    "chapter": 7,
    "verse": 12,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "Я накажу их,"
  },
  {
    "chapter": 7,
    "verse": 12,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "как им и было"
  },
  {
    "chapter": 7,
    "verse": 12,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 7,
    "text": "сказано в собрании."
  },
  {
    "chapter": 7,
    "verse": 13,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Горе им,"
  },
  {
    "chapter": 7,
    "verse": 13,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "потому что они"
  },
  {
    "chapter": 7,
    "verse": 13,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "удалились от Меня! Гибель им,"
  },
  {
    "chapter": 7,
    "verse": 13,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "потому что они восстали"
  },
  {
    "chapter": 7,
    "verse": 13,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "против Меня! Я желаю"
  },
  {
    "chapter": 7,
    "verse": 13,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "искупить их,"
  },
  {
    "chapter": 7,
    "verse": 13,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 7,
    "text": "а они лгут обо Мне."
  },
  {
    "chapter": 7,
    "verse": 14,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Они не взывают ко"
  },
  {
    "chapter": 7,
    "verse": 14,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "Мне из своих сердец,"
  },
  {
    "chapter": 7,
    "verse": 14,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "а причитают на своих ложах."
  },
  {
    "chapter": 7,
    "verse": 14,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "Они собираются вместе"
  },
  {
    "chapter": 7,
    "verse": 14,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "из-за хлеба и молодого вина,"
  },
  {
    "chapter": 7,
    "verse": 14,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "но отворачиваются от Меня."
  },
  {
    "chapter": 7,
    "verse": 15,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "И хотя Я"
  },
  {
    "chapter": 7,
    "verse": 15,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "вразумлял и укреплял их,"
  },
  {
    "chapter": 7,
    "verse": 15,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "они все равно"
  },
  {
    "chapter": 7,
    "verse": 15,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "затевают зло против Меня."
  },
  {
    "chapter": 7,
    "verse": 16,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Они обращаются,"
  },
  {
    "chapter": 7,
    "verse": 16,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "но не к Богу."
  },
  {
    "chapter": 7,
    "verse": 16,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "Они как неверный лук."
  },
  {
    "chapter": 7,
    "verse": 16,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "Вожди их падут от меча"
  },
  {
    "chapter": 7,
    "verse": 16,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "из-за своих дерзких слов."
  },
  {
    "chapter": 7,
    "verse": 16,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "За это над ними будут"
  },
  {
    "chapter": 7,
    "verse": 16,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 7,
    "text": "смеяться в земле Египта."
  }
]