    "verse": 30,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "головы"
  },
  {
    "chapter": 12,
    "verse": 30,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "их царя корона весила"
  },
  {
    "chapter": 12,
    "verse": 30,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "талант"
  },
  {
    "chapter": 12,
    "verse": 30,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "и была украшена драгоценным"
  },
  {
    "chapter": 12,
    "verse": 30,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
