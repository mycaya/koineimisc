[
  {
    "chapter": 77,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Дирижеру хора."
  },
  {
    "chapter": 77,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Под гиттит."
  },
  {
    "chapter": 77,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Псалом Асафа."
  },
  {
    "chapter": 77,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Радостно пойте Богу,"
  },
  {
    "chapter": 77,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "нашей твердыне,"
  },
  {
    "chapter": 77,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "восклицайте Богу Иакова."
  },
  {
    "chapter": 77,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть грянет музыка!"
  },
  {
    "chapter": 77,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Ударьте в тамбурин"
  },
  {
    "chapter": 77,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и троньте струны"
  },
  {
    "chapter": 77,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "сладкозвучной арфы с лирой."
  },
  {
    "chapter": 77,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Трубите в рог в"
  },
  {
    "chapter": 77,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "праздник Новолуния,"
  },
  {
    "chapter": 77,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и в полнолуние,"
  },
  {
    "chapter": 77,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "в день нашего праздника,"
  },
  {
    "chapter": 77,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "так как это"
  },
  {
    "chapter": 77,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "закон для Израиля,"
  },
  {
    "chapter": 77,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "установление"
  },
  {
    "chapter": 77,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "от Бога Иакова."
  },
  {
    "chapter": 77,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он установил это в"
  },
  {
    "chapter": 77,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "свидетельство для Иосифа,"
  },
  {
    "chapter": 77,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "когда тот вышел из"
  },
  {
    "chapter": 77,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "земли египетской."
  },
  {
    "chapter": 77,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Я услышал голос,"
  },
  {
    "chapter": 77,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "которого не знал:"
  },
  {
    "chapter": 77,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он говорит:"
  },
  {
    "chapter": 77,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Я снял тяжелую"
  },
  {
    "chapter": 77,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "ношу с его плеч,"
  },
  {
    "chapter": 77,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и руки его"
  },
  {
    "chapter": 77,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "освободились от корзин."
  },
  {
    "chapter": 77,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "В бедствии ты призвал Меня,"
  },
  {
    "chapter": 77,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и Я спас тебя;"
  },
  {
    "chapter": 77,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "из тучи грозовой Я ответил"
  },
  {
    "chapter": 77,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "тебе при водах Меривы"
  },
  {
    "chapter": 77,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "испытал тебя."
  },
  {
    "chapter": 77,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Пауза"
  },
  {
    "chapter": 77,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Слушай,"
  },
  {
    "chapter": 77,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Мой народ,"
  },
  {
    "chapter": 77,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и Я буду"
  },
  {
    "chapter": 77,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "свидетельствовать тебе."
  },
  {
    "chapter": 77,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "О Израиль,"
  },
  {
    "chapter": 77,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "если бы ты послушал Меня!"
  },
  {
    "chapter": 77,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть не будет у"
  },
  {
    "chapter": 77,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "тебя иного бога,"
  },
  {
    "chapter": 77,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и не поклоняйся"
  },
  {
    "chapter": 77,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "богу чужеземному."
  },
  {
    "chapter": 77,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я Господь,"
  },
  {
    "chapter": 77,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "твой Бог,"
  },
  {
    "chapter": 77,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Тот,"
  },
  {
    "chapter": 77,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Кто вывел тебя из"
  },
  {
    "chapter": 77,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "земли египетской."
  },
  {
    "chapter": 77,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Открой свои уста,"
  },
  {
    "chapter": 77,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "и Я наполню их."
  },
  {
    "chapter": 77,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но народ Мой не"
  },
  {
    "chapter": 77,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "слушал Моего голоса,"
  },
  {
    "chapter": 77,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Израиль не покорялся Мне."
  },
  {
    "chapter": 77,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Потому Я и оставил"
  },
  {
    "chapter": 77,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "их во власти"
  },
  {
    "chapter": 77,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "их упрямых сердец"
  },
  {
    "chapter": 77,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и позволил"
  },
  {
    "chapter": 77,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "им ходить своими путями."
  },
  {
    "chapter": 77,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "О,"
  },
  {
    "chapter": 77,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "если бы народ Мой слушал"
  },
  {
    "chapter": 77,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Меня"
  },
  {
    "chapter": 77,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и Израиль ходил Моими"
  },
  {
    "chapter": 77,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "путями!"
  },
  {
    "chapter": 77,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я вскоре бы смирил"
  },
  {
    "chapter": 77,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "их врагов"
  },
  {
    "chapter": 77,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и обратил бы Мою руку"
  },
  {
    "chapter": 77,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "против их неприятелей."
  },
  {
    "chapter": 77,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ненавидящие Господа"
  },
  {
    "chapter": 77,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "раболепствовали бы перед Ним,"
  },
  {
    "chapter": 77,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и их наказание"
  },
  {
    "chapter": 77,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "продолжалось бы вечно."
  },
  {
    "chapter": 77,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я питал бы Израиля лучшей"
  },
  {
    "chapter": 77,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "пшеницей"
  },
  {
    "chapter": 77,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и насыщал бы тебя медом"
  },
  {
    "chapter": 77,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "из скалы."
  }
]