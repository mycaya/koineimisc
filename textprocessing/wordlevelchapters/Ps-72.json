[
  {
    "chapter": 72,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Дирижеру хора."
  },
  {
    "chapter": 72,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "На струнных инструментах."
  },
  {
    "chapter": 72,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Псалом Асафа."
  },
  {
    "chapter": 72,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Песнь."
  },
  {
    "chapter": 72,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "В Иудее известен Бог;"
  },
  {
    "chapter": 72,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "велико Его Имя в Израиле."
  },
  {
    "chapter": 72,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Жилище Его в Салиме,"
  },
  {
    "chapter": 72,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и на Сионе Его пребывание."
  },
  {
    "chapter": 72,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Там Он сокрушил"
  },
  {
    "chapter": 72,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "сверкающие стрелы,"
  },
  {
    "chapter": 72,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "щит и меч,"
  },
  {
    "chapter": 72,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "орудия войны."
  },
  {
    "chapter": 72,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Пауза"
  },
  {
    "chapter": 72,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты сияешь во свете;"
  },
  {
    "chapter": 72,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Ты величественнее,"
  },
  {
    "chapter": 72,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "чем горы с добычей."
  },
  {
    "chapter": 72,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Крепкие сердцем"
  },
  {
    "chapter": 72,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "стали добычею,"
  },
  {
    "chapter": 72,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "уснули последним сном;"
  },
  {
    "chapter": 72,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "ни один из них не"
  },
  {
    "chapter": 72,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "смог даже руку поднять."
  },
  {
    "chapter": 72,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Бог Иакова,"
  },
  {
    "chapter": 72,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "от грозного крика"
  },
  {
    "chapter": 72,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Твоего и колесница,"
  },
  {
    "chapter": 72,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и конь упали замертво."
  },
  {
    "chapter": 72,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Лишь Тебя надо бояться,"
  },
  {
    "chapter": 72,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "кто устоит"
  },
  {
    "chapter": 72,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "пред Твоим лицом,"
  },
  {
    "chapter": 72,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "когда Ты в гневе?"
  },
  {
    "chapter": 72,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты произнес суд с небес;"
  },
  {
    "chapter": 72,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "вся земля"
  },
  {
    "chapter": 72,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "испугалась и притихла,"
  },
  {
    "chapter": 72,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "когда Бог восстал на суд,"
  },
  {
    "chapter": 72,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "чтобы спасти всех"
  },
  {
    "chapter": 72,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "угнетенных на земле."
  },
  {
    "chapter": 72,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Пауза"
  },
  {
    "chapter": 72,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Истинно,"
  },
  {
    "chapter": 72,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Твой гнев на людей"
  },
  {
    "chapter": 72,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "принесет Тебе хвалу,"
  },
  {
    "chapter": 72,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и выживших после"
  },
  {
    "chapter": 72,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Твоего гнева Ты смиришь."
  },
  {
    "chapter": 72,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Давайте обеты Господу,"
  },
  {
    "chapter": 72,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "вашему Богу,"
  },
  {
    "chapter": 72,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и исполняйте их;"
  },
  {
    "chapter": 72,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "все,"
  },
  {
    "chapter": 72,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "кто вокруг Него,"
  },
  {
    "chapter": 72,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "приносите дары Грозному."
  },
  {
    "chapter": 72,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он сокрушает дух вождей;"
  },
  {
    "chapter": 72,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Он страшен для"
  },
  {
    "chapter": 72,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "земных царей."
  }
]