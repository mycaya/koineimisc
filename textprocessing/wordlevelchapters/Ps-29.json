[
  {
    "chapter": 29,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Дирижеру хора."
  },
  {
    "chapter": 29,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Псалом Давида."
  },
  {
    "chapter": 29,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "У Тебя,"
  },
  {
    "chapter": 29,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 29,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "я ищу прибежище."
  },
  {
    "chapter": 29,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Не дай мне быть"
  },
  {
    "chapter": 29,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "постыженным никогда;"
  },
  {
    "chapter": 29,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "по Твоей"
  },
  {
    "chapter": 29,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "праведности спаси меня!"
  },
  {
    "chapter": 29,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Услышь меня и"
  },
  {
    "chapter": 29,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "поспеши избавить."
  },
  {
    "chapter": 29,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Будь мне"
  },
  {
    "chapter": 29,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "скалой и прибежищем,"
  },
  {
    "chapter": 29,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "надежной крепостью,"
  },
  {
    "chapter": 29,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "чтобы меня спасти в ней."
  },
  {
    "chapter": 29,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты моя скала и крепость;"
  },
  {
    "chapter": 29,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "ради Имени Твоего"
  },
  {
    "chapter": 29,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "веди меня и направляй."
  },
  {
    "chapter": 29,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Высвободи меня из сети,"
  },
  {
    "chapter": 29,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "которую мне раскинули,"
  },
  {
    "chapter": 29,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "ведь Ты мое прибежище."
  },
  {
    "chapter": 29,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "В Твои руки я"
  },
  {
    "chapter": 29,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "отдаю мой дух;"
  },
  {
    "chapter": 29,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "избавь меня,"
  },
  {
    "chapter": 29,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Господи,"
  },
  {
    "chapter": 29,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Боже истины."
  },
  {
    "chapter": 29,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ненавижу тех,"
  },
  {
    "chapter": 29,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "кто ничтожных идолов чтит,"
  },
  {
    "chapter": 29,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "я на Господа уповаю."
  },
  {
    "chapter": 29,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я возрадуюсь и"
  },
  {
    "chapter": 29,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "возликую о Твоей милости,"
  },
  {
    "chapter": 29,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "потому что Ты горе мое"
  },
  {
    "chapter": 29,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "увидел"
  },
  {
    "chapter": 29,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "и узнал о скорби моей души."
  },
  {
    "chapter": 29,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты не отдал меня врагу,"
  },
  {
    "chapter": 29,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "но поставил меня"
  },
  {
    "chapter": 29,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "на просторном месте."
  },
  {
    "chapter": 29,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Мне трудно,"
  },
  {
    "chapter": 29,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи! Помилуй меня!"
  },
  {
    "chapter": 29,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Ослабли от скорби мои глаза,"
  },
  {
    "chapter": 29,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "душа моя и"
  },
  {
    "chapter": 29,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "тело мое от горя."
  },
  {
    "chapter": 29,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "В скорби кончается моя"
  },
  {
    "chapter": 29,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "жизнь и годы мои в стенаниях."
  },
  {
    "chapter": 29,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Сила моя иссякла"
  },
  {
    "chapter": 29,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "из-за моих грехов,"
  },
  {
    "chapter": 29,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "и кости мои иссохли."
  },
  {
    "chapter": 29,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Из-за моих врагов я в"
  },
  {
    "chapter": 29,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "презрении у соседей,"
  },
  {
    "chapter": 29,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "поношением стал я"
  },
  {
    "chapter": 29,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "для моих друзей,"
  },
  {
    "chapter": 29,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "кто увидит меня"
  },
  {
    "chapter": 29,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "на улице убегает."
  },
  {
    "chapter": 29,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я забыт ими,"
  },
  {
    "chapter": 29,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "словно мертвый;"
  },
  {
    "chapter": 29,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "я как сосуд разбитый."
  },
  {
    "chapter": 29,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Слышу я клевету от многих;"
  },
  {
    "chapter": 29,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "ужас со всех сторон."
  },
  {
    "chapter": 29,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Они"
  },
  {
    "chapter": 29,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "сговариваются против меня:"
  },
  {
    "chapter": 29,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "мою жизнь отнять замышляют."
  },
  {
    "chapter": 29,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "На Тебя,"
  },
  {
    "chapter": 29,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 29,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "надеюсь! Я говорю:"
  },
  {
    "chapter": 29,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Ты мой Бог!"
  },
  {
    "chapter": 29,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "В Твоей руке мои дни;"
  },
  {
    "chapter": 29,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "избавь меня от"
  },
  {
    "chapter": 29,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "врагов и от тех,"
  },
  {
    "chapter": 29,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "кто меня преследует."
  },
  {
    "chapter": 29,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть сияет Твое"
  },
  {
    "chapter": 29,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "лицо над Твоим слугой,"
  },
  {
    "chapter": 29,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "спаси меня по"
  },
  {
    "chapter": 29,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "верной Твоей милости."
  },
  {
    "chapter": 29,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть не постыжусь я,"
  },
  {
    "chapter": 29,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 29,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "ведь к Тебе я воззвал."
  },
  {
    "chapter": 29,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Пусть"
  },
  {
    "chapter": 29,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "нечестивые постыдятся и,"
  },
  {
    "chapter": 29,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "онемев,"
  },
  {
    "chapter": 29,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "сойдут в мир мертвых."
  },
  {
    "chapter": 29,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть умолкнут"
  },
  {
    "chapter": 29,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "их лживые уста,"
  },
  {
    "chapter": 29,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "что дерзко говорят о"
  },
  {
    "chapter": 29,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "праведных с гордынею"
  },
  {
    "chapter": 29,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "и презрением."
  },
  {
    "chapter": 29,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Как велика Твоя благость,"
  },
  {
    "chapter": 29,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "которую хранишь Ты для тех,"
  },
  {
    "chapter": 29,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "кто Тебя боится,"
  },
  {
    "chapter": 29,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "которую даришь у"
  },
  {
    "chapter": 29,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "всех на глазах тем,"
  },
  {
    "chapter": 29,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "кто в Тебе прибежища ищет."
  },
  {
    "chapter": 29,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты скрываешь"
  },
  {
    "chapter": 29,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "их пологом Своего"
  },
  {
    "chapter": 29,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "присутствия от козней"
  },
  {
    "chapter": 29,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "людских."
  },
  {
    "chapter": 29,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Ты охраняешь их в Своем"
  },
  {
    "chapter": 29,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "жилище от языков сварливых."
  },
  {
    "chapter": 29,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Благословен будь,"
  },
  {
    "chapter": 29,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 29,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Который явил мне"
  },
  {
    "chapter": 29,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "чудесную милость Свою,"
  },
  {
    "chapter": 29,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "когда я был в"
  },
  {
    "chapter": 29,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "городе осажденном."
  },
  {
    "chapter": 29,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "В своем смятении я сказал:"
  },
  {
    "chapter": 29,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Я"
  },
  {
    "chapter": 29,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "изгнан от Твоих очей! Но Ты"
  },
  {
    "chapter": 29,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "услышал мой крик о милости,"
  },
  {
    "chapter": 29,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "когда к Тебе я взывал."
  },
  {
    "chapter": 29,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Любите Господа,"
  },
  {
    "chapter": 29,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "все святые Его!"
  },
  {
    "chapter": 29,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
