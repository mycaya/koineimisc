  },
  {
    "chapter": 10,
    "verse": 19,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "Не убивай,"
  },
  {
    "chapter": 10,
    "verse": 19,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 3,
    "text": "Не нарушай"
  },
  {
    "chapter": 10,
    "verse": 19,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 4,
    "text": "супружескую верность,"
  },
  {
    "chapter": 10,
    "verse": 19,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 5,
    "text": "Не кради,"
  },
  {
    "chapter": 10,
    "verse": 19,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 6,
