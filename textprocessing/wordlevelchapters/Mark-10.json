  {
    "chapter": 10,
    "verse": 22,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 1,
    "text": "Услышав это,"
  },
  {
    "chapter": 10,
    "verse": 22,
    "book_name": "Mark",
    "book_id": "Mark",
    "part": 2,
    "text": "человек помрачнел"
  },
