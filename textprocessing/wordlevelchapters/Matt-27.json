    "verse": 41,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 4,
    "text": "насмехались над Иисусом."
  },
  {
    "chapter": 27,
    "verse": 42,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 1,
    "text": "- Спасал других,"
  },
  {
    "chapter": 27,
    "verse": 42,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 2,
    "text": "говорили они,"
  },
  {
    "chapter": 27,
    "verse": 42,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 3,
