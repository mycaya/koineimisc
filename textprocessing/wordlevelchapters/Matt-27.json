    "chapter": 27,
    "verse": 24,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 5,
    "text": "он взял воды,"
  },
  {
    "chapter": 27,
    "verse": 24,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 6,
    "text": "вымыл руки перед"
  },
  {
    "chapter": 27,
    "verse": 24,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 7,
    "text": "всем народом и сказал:"
  },
  {
    "chapter": 27,
    "verse": 24,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 8,
    "text": "Смотрите сами."
  },
  {
    "chapter": 27,
    "verse": 24,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 9,
    "text": "Я же не виновен в"
  },
  {
    "chapter": 27,
    "verse": 24,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 10,
