  },
  {
    "chapter": 27,
    "verse": 46,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 1,
    "text": "Около девятого часа"
  },
  {
    "chapter": 27,
    "verse": 46,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 2,
    "text": "Иисус громко крикнул:"
  },
  {
    "chapter": 27,
    "verse": 46,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 3,
    "text": "Эли,"
  },
  {
    "chapter": 27,
    "verse": 46,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 4,
    "text": "Эли,"
  },
  {
    "chapter": 27,
    "verse": 46,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 5,
    "text": "лема"
  },
  {
    "chapter": 27,
    "verse": 46,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 6,
    "text": "савахтани? (Что значит:"
  },
  {
    "chapter": 27,
    "verse": 46,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 7,
    "text": "Боже Мой,"
  },
  {
    "chapter": 27,
    "verse": 46,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 8,
    "text": "Боже Мой,"
  },
  {
    "chapter": 27,
    "verse": 46,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 9,
    "text": "почему Ты оставил Меня?)"
  },
  {
    "chapter": 27,
    "verse": 47,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 1,
    "text": "Некоторые из"
  },
  {
    "chapter": 27,
    "verse": 47,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 2,
    "text": "стоявших поблизости,"
  },
  {
    "chapter": 27,
    "verse": 47,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 3,
    "text": "услышав это,"
  },
