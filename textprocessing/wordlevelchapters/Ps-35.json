[
  {
    "chapter": 35,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Псалом Давида."
  },
  {
    "chapter": 35,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Не раздражайся"
  },
  {
    "chapter": 35,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "из-за злодеев,"
  },
  {
    "chapter": 35,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и не завидуй"
  },
  {
    "chapter": 35,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "творящим беззаконие,"
  },
  {
    "chapter": 35,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "ведь они,"
  },
  {
    "chapter": 35,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "как трава,"
  },
  {
    "chapter": 35,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "скоро засохнут и,"
  },
  {
    "chapter": 35,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "как зелень,"
  },
  {
    "chapter": 35,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "увянут."
  },
  {
    "chapter": 35,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Надейся на Господа,"
  },
  {
    "chapter": 35,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "делай добро,"
  },
  {
    "chapter": 35,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "живи на земле и"
  },
  {
    "chapter": 35,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "храни верность."
  },
  {
    "chapter": 35,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Радуйся Господу,"
  },
  {
    "chapter": 35,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и Он исполнит"
  },
  {
    "chapter": 35,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "желания твоего сердца."
  },
  {
    "chapter": 35,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Доверь свой путь Господу,"
  },
  {
    "chapter": 35,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "уповай на Него и"
  },
  {
    "chapter": 35,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Он совершит такое:"
  },
  {
    "chapter": 35,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "высветит праведность твою,"
  },
  {
    "chapter": 35,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "как свет,"
  },
  {
    "chapter": 35,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "твою справедливость"
  },
  {
    "chapter": 35,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "как солнце в полдень."
  },
  {
    "chapter": 35,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Будь безмолвен перед"
  },
  {
    "chapter": 35,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господом и надейся на Него."
  },
  {
    "chapter": 35,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Не раздражайся,"
  },
  {
    "chapter": 35,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "когда нечестивые"
  },
  {
    "chapter": 35,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "преуспевают в делах,"
  },
  {
    "chapter": 35,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "когда они исполняют"
  },
  {
    "chapter": 35,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "свои злые замыслы."
  },
  {
    "chapter": 35,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Перестань"
  },
  {
    "chapter": 35,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "гневаться и оставь ярость,"
  },
  {
    "chapter": 35,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "не раздражайся это"
  },
  {
    "chapter": 35,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "ведет к несчастью."
  },
  {
    "chapter": 35,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ведь злодеи искоренятся,"
  },
  {
    "chapter": 35,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "а надеющиеся на"
  },
  {
    "chapter": 35,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Господа унаследуют землю."
  },
  {
    "chapter": 35,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Еще немного,"
  },
  {
    "chapter": 35,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и нечестивых не станет,"
  },
  {
    "chapter": 35,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "будешь искать их,"
  },
  {
    "chapter": 35,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "но не найдешь."
  },
  {
    "chapter": 35,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "А кроткие унаследуют землю"
  },
  {
    "chapter": 35,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и насладятся"
  },
  {
    "chapter": 35,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "благоденствием."
  },
  {
    "chapter": 35,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Нечестивый умышляет на"
  },
  {
    "chapter": 35,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "праведника"
  },
  {
    "chapter": 35,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и скрежещет против него"
  },
  {
    "chapter": 35,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "зубами,"
  },
  {
    "chapter": 35,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "но Владыка смеется над ним,"
  },
  {
    "chapter": 35,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "так как знает,"
  },
  {
    "chapter": 35,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "что день его близок."
  },
  {
    "chapter": 35,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Обнажают злодеи мечи и"
  },
  {
    "chapter": 35,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "натягивают свои луки,"
  },
  {
    "chapter": 35,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "чтобы поразить"
  },
  {
    "chapter": 35,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "бедняков и нищих,"
  },
  {
    "chapter": 35,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "чтобы погубить"
  },
  {
    "chapter": 35,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "идущих прямым путем."
  },
  {
    "chapter": 35,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но мечи их пронзят"
  },
  {
    "chapter": 35,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "их же сердца"
  },
  {
    "chapter": 35,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и луки у них сокрушатся."
  },
  {
    "chapter": 35,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Лучше малое,"
  },
  {
    "chapter": 35,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "что у праведного,"
  },
  {
    "chapter": 35,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "чем богатство"
  },
  {
    "chapter": 35,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "многих нечестивых,"
  },
  {
    "chapter": 35,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "так как сила"
  },
  {
    "chapter": 35,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "нечестивых будет сломлена,"
  },
  {
    "chapter": 35,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "а праведных"
  },
  {
    "chapter": 35,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "укрепит Господь."
  },
  {
    "chapter": 35,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Дни непорочных"
  },
  {
    "chapter": 35,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "известны Господу,"
  },
  {
    "chapter": 35,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и наследие их"
  },
  {
    "chapter": 35,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "устоит вовеки."
  },
  {
    "chapter": 35,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Не постыдятся они во время"
  },
  {
    "chapter": 35,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "беды"
  },
  {
    "chapter": 35,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и в голодные дни будут"
  },
  {
    "chapter": 35,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "сыты."
  },
  {
    "chapter": 35,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но нечестивые сгинут,"
  },
  {
    "chapter": 35,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господни враги исчезнут,"
  },
  {
    "chapter": 35,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "как краса полей,"
  },
  {
    "chapter": 35,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "развеются они,"
  },
  {
    "chapter": 35,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "как дым."
  },
  {
    "chapter": 35,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Нечестивый берет"
  },
  {
    "chapter": 35,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "взаймы и не возвращает,"
  },
  {
    "chapter": 35,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "а праведный дает щедро."
  },
  {
    "chapter": 35,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Благословенные"
  },
  {
    "chapter": 35,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господом унаследуют землю,"
  },
  {
    "chapter": 35,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "а проклятые Им истребятся."
  },
  {
    "chapter": 35,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господь"
  },
  {
    "chapter": 35,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "утверждает шаги человека,"
  },
  {
    "chapter": 35,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "когда его путь угоден Ему."
  },
  {
    "chapter": 35,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Если и споткнется он,"
  },
  {
    "chapter": 35,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "то не упадет:"
  },
  {
    "chapter": 35,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Господь за руку"
  },
  {
    "chapter": 35,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "его поддержит."
  },
  {
    "chapter": 35,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я был молод,"
  },
  {
    "chapter": 35,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "а теперь состарился,"
  },
  {
    "chapter": 35,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "но не видел праведного"
  },
  {
    "chapter": 35,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "оставленным"
  },
  {
    "chapter": 35,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "и детей его просящими"
  },
  {
    "chapter": 35,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "хлеба."
  },
  {
    "chapter": 35,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он всегда щедр"
  },
  {
    "chapter": 35,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и дает охотно,"
  },
  {
    "chapter": 35,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и дети его"
  },
  {
    "chapter": 35,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "будут благословенны."
  },
  {
    "chapter": 35,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Удаляйся от зла"
  },
  {
    "chapter": 35,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и твори добро,"
  },
  {
    "chapter": 35,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "чтобы жить тебе в"
  },
  {
    "chapter": 35,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "этой земле вовек."
  },
  {
    "chapter": 35,
    "verse": 28,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ведь Господь любит правый"
  },
  {
    "chapter": 35,
    "verse": 28,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "суд"
  },
  {
    "chapter": 35,
    "verse": 28,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и верных Своих не оставит."
  },
  {
    "chapter": 35,
    "verse": 28,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "В безопасности"
  },
  {
    "chapter": 35,
    "verse": 28,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "будут они навеки,"
  },
  {
    "chapter": 35,
    "verse": 28,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "а род"
  },
  {
    "chapter": 35,
    "verse": 28,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "нечестивых искоренится."
  },
  {
    "chapter": 35,
    "verse": 29,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Праведные унаследуют землю"
  },
  {
    "chapter": 35,
    "verse": 29,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и поселятся в ней навеки."
  },
  {
    "chapter": 35,
    "verse": 30,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Уста праведника"
  },
  {
    "chapter": 35,
    "verse": 30,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "изрекают мудрость,"
  },
  {
    "chapter": 35,
    "verse": 30,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и язык его"
  },
  {
    "chapter": 35,
    "verse": 30,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "произносит правду."
  },
  {
    "chapter": 35,
    "verse": 31,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Закон его Бога"
  },
  {
    "chapter": 35,
    "verse": 31,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "в сердце у него,"
  },
  {
    "chapter": 35,
    "verse": 31,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и стопы его не"
  },
  {
    "chapter": 35,
    "verse": 31,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "поколеблются."
  },
  {
    "chapter": 35,
    "verse": 32,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Нечестивые подстерегают"
  },
  {
    "chapter": 35,
    "verse": 32,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "праведного"
  },
  {
    "chapter": 35,
    "verse": 32,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и стараются его убить."
  },
  {
    "chapter": 35,
    "verse": 33,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но Господь не отдаст его в"
  },
  {
    "chapter": 35,
    "verse": 33,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "их руки"
  },
  {
    "chapter": 35,
    "verse": 33,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и не даст обвинить его на"
  },
  {
    "chapter": 35,
    "verse": 33,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "суде."
  },
  {
    "chapter": 35,
    "verse": 34,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Надейся на Господа"
  },
  {
    "chapter": 35,
    "verse": 34,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и храни Его путь."
  },
  {
    "chapter": 35,
    "verse": 34,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Он вознесет тебя,"
  },
  {
    "chapter": 35,
    "verse": 34,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и ты унаследуешь землю и"
  },
  {
    "chapter": 35,
    "verse": 34,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "увидишь гибель нечестивых."
  },
  {
    "chapter": 35,
    "verse": 35,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я видел нечестивого злодея,"
  },
  {
    "chapter": 35,
    "verse": 35,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "что возвышался,"
  },
  {
    "chapter": 35,
    "verse": 35,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "как ветвистое дерево."
  },
  {
    "chapter": 35,
    "verse": 36,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но он прошел,"
  },
  {
    "chapter": 35,
    "verse": 36,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и его не стало;"
  },
  {
    "chapter": 35,
    "verse": 36,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "я искал его,"
  },
  {
    "chapter": 35,
    "verse": 36,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "но не нашел."
  },
  {
    "chapter": 35,
    "verse": 37,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Наблюдай за непорочным,"
  },
  {
    "chapter": 35,
    "verse": 37,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "смотри на праведного,"
  },
  {
    "chapter": 35,
    "verse": 37,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "потому что у"
  },
  {
    "chapter": 35,
    "verse": 37,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "любящих мир есть будущее."
  },
  {
    "chapter": 35,
    "verse": 38,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но грешники"
  },
  {
    "chapter": 35,
    "verse": 38,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "будут истреблены,"
  },
  {
    "chapter": 35,
    "verse": 38,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и погибнет будущее злодеев."
  },
  {
    "chapter": 35,
    "verse": 39,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "От Господа"
  },
  {
    "chapter": 35,
    "verse": 39,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "спасение праведных."
  },
  {
    "chapter": 35,
    "verse": 39,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Он их крепость"
  },
  {
    "chapter": 35,
    "verse": 39,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "во время беды."
  },
  {
    "chapter": 35,
    "verse": 40,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господь помогает"
  },
  {
    "chapter": 35,
    "verse": 40,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "им и избавляет их;"
  },
  {
    "chapter": 35,
    "verse": 40,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Он избавляет их от"
  },
  {
    "chapter": 35,
    "verse": 40,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "злодеев и спасает их,"
  },
  {
    "chapter": 35,
    "verse": 40,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "ведь они нашли"
  },
  {
    "chapter": 35,
    "verse": 40,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "в Нем прибежище."
  }
]