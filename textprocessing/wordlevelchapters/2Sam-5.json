    "verse": 15,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Ивхар,"
  },
  {
    "chapter": 5,
    "verse": 15,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
