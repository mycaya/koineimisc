[
  {
    "chapter": 141,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Аллилуйя! Как хорошо петь"
  },
  {
    "chapter": 141,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "хвалу нашему Богу!"
  },
  {
    "chapter": 141,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Сладостна подобающая хвала."
  },
  {
    "chapter": 141,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господь"
  },
  {
    "chapter": 141,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "отстраивает Иерусалим,"
  },
  {
    "chapter": 141,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "собирает"
  },
  {
    "chapter": 141,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "изгнанников Израиля."
  },
  {
    "chapter": 141,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он исцеляет разбитых"
  },
  {
    "chapter": 141,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "сердцем и перевязывает их раны."
  },
  {
    "chapter": 141,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он исчисляет количество"
  },
  {
    "chapter": 141,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "звезд и называет их по именам."
  },
  {
    "chapter": 141,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Велик наш"
  },
  {
    "chapter": 141,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Владыка и могуществен;"
  },
  {
    "chapter": 141,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Его разум непостижим."
  },
  {
    "chapter": 141,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господь возвышает кроткого,"
  },
  {
    "chapter": 141,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "нечестивого же"
  },
  {
    "chapter": 141,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "низвергает на землю."
  },
  {
    "chapter": 141,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Воспойте Господу"
  },
  {
    "chapter": 141,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "с благодарностью,"
  },
  {
    "chapter": 141,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "сыграйте"
  },
  {
    "chapter": 141,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "нашему Богу на арфе."
  },
  {
    "chapter": 141,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он небо покрывает тучами,"
  },
  {
    "chapter": 141,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "готовит дождь для земли"
  },
  {
    "chapter": 141,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и растит траву на горах."
  },
  {
    "chapter": 141,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он дает пищу животным и"
  },
  {
    "chapter": 141,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "кричащим птенцам ворона."
  },
  {
    "chapter": 141,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Не силе лошади Он радуется"
  },
  {
    "chapter": 141,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и не человеческим ногам"
  },
  {
    "chapter": 141,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "благоволит,"
  },
  {
    "chapter": 141,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господь"
  },
  {
    "chapter": 141,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "благосклонен к боящимся Его,"
  },
  {
    "chapter": 141,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "к надеющимся"
  },
  {
    "chapter": 141,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "на Его милость."
  }
]