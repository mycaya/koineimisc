[
  {
    "chapter": 69,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Для Соломона."
  },
  {
    "chapter": 69,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Боже,"
  },
  {
    "chapter": 69,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "справедливостью Твоей"
  },
  {
    "chapter": 69,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "надели царя"
  },
  {
    "chapter": 69,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "и праведностью Твоей сына"
  },
  {
    "chapter": 69,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "царского,"
  },
  {
    "chapter": 69,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "чтобы он судил народ Твой"
  },
  {
    "chapter": 69,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "праведно"
  },
  {
    "chapter": 69,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и страдальцев Твоих"
  },
  {
    "chapter": 69,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "справедливо."
  },
  {
    "chapter": 69,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Горы принесут"
  },
  {
    "chapter": 69,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "процветание народу,"
  },
  {
    "chapter": 69,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и холмы плоды праведности."
  },
  {
    "chapter": 69,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он защитит"
  },
  {
    "chapter": 69,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "страдальцев из народа,"
  },
  {
    "chapter": 69,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "спасет сыновей нищих и"
  },
  {
    "chapter": 69,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "сокрушит притеснителя."
  },
  {
    "chapter": 69,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Будут бояться Тебя,"
  },
  {
    "chapter": 69,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "пока существуют"
  },
  {
    "chapter": 69,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "солнце и луна,"
  },
  {
    "chapter": 69,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "из поколения в поколение."
  },
  {
    "chapter": 69,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он будет как дождь,"
  },
  {
    "chapter": 69,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "идущий над скошенным полем,"
  },
  {
    "chapter": 69,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "словно ливень,"
  },
  {
    "chapter": 69,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "орошающий землю."
  },
  {
    "chapter": 69,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "В дни его будет"
  },
  {
    "chapter": 69,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "процветать праведник,"
  },
  {
    "chapter": 69,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и благоденствие"
  },
  {
    "chapter": 69,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "не прекратится,"
  },
  {
    "chapter": 69,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "пока не исчезнет луна."
  },
  {
    "chapter": 69,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он будет владычествовать от"
  },
  {
    "chapter": 69,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "моря и до моря"
  },
  {
    "chapter": 69,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и от реки Евфрата до краев"
  },
  {
    "chapter": 69,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "земли."
  },
  {
    "chapter": 69,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Жители пустынь"
  },
  {
    "chapter": 69,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "преклонятся перед ним,"
  },
  {
    "chapter": 69,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и враги его"
  },
  {
    "chapter": 69,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "будут лизать пыль."
  },
  {
    "chapter": 69,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Цари Фарсиса и отдаленных"
  },
  {
    "chapter": 69,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "побережий принесут ему дань,"
  },
  {
    "chapter": 69,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "цари Шевы и"
  },
  {
    "chapter": 69,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Севы принесут дары."
  },
  {
    "chapter": 69,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Поклонятся ему все цари;"
  },
  {
    "chapter": 69,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "все народы"
  },
  {
    "chapter": 69,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "будут ему служить."
  },
  {
    "chapter": 69,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он спасет нищего,"
  },
  {
    "chapter": 69,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "когда тот взывает,"
  },
  {
    "chapter": 69,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и угнетенного,"
  },
  {
    "chapter": 69,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "у которого нет помощника."
  },
  {
    "chapter": 69,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он будет милосерден"
  },
  {
    "chapter": 69,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "к бедному и нищему;"
  },
  {
    "chapter": 69,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "души нищих он спасет."
  },
  {
    "chapter": 69,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "От угнетения и"
  },
  {
    "chapter": 69,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "насилия избавит их души,"
  },
  {
    "chapter": 69,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "ведь драгоценна их"
  },
  {
    "chapter": 69,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "кровь в глазах его."
  },
  {
    "chapter": 69,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть будет долог его век;"
  },
  {
    "chapter": 69,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и пусть будет"
  },
  {
    "chapter": 69,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "дано ему золото Шевы."
  },
  {
    "chapter": 69,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "И пусть непрестанно"
  },
  {
    "chapter": 69,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "возносят молитвы за него,"
  },
  {
    "chapter": 69,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "весь день прославляя его."
  },
  {
    "chapter": 69,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть будет обилие"
  },
  {
    "chapter": 69,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "хлеба на всей земле,"
  },
  {
    "chapter": 69,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и ветер колышет"
  },
  {
    "chapter": 69,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "колосья на вершинах холмов,"
  },
  {
    "chapter": 69,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "подобно процветанию Ливана"
  },
  {
    "chapter": 69,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "пусть будет его изобилие,"
  },
  {
    "chapter": 69,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "и пусть оно покроет землю,"
  },
  {
    "chapter": 69,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 8,
    "text": "подобно траве."
  },
  {
    "chapter": 69,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть имя его"
  },
  {
    "chapter": 69,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "пребудет вовек,"
  },
  {
    "chapter": 69,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "пока пребывает солнце."
  },
  {
    "chapter": 69,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "В нем"
  },
  {
    "chapter": 69,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "благословятся все народы земли,"
  },
  {
    "chapter": 69,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "и они назовут"
  },
  {
    "chapter": 69,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "его благословенным."
  },
  {
    "chapter": 69,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Благословен будь Господь,"
  },
  {
    "chapter": 69,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Бог Израиля,"
  },
  {
    "chapter": 69,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Который один творит чудеса!"
  },
  {
    "chapter": 69,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть всегда"
  },
  {
    "chapter": 69,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "превозносится Его величественное Имя,"
  },
  {
    "chapter": 69,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и наполнится вся земля"
  },
  {
    "chapter": 69,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Его славою! Аминь и аминь!"
  },
  {
    "chapter": 69,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Закончились молитвы Давида,"
  },
  {
    "chapter": 69,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "сына Иессея."
  },
  {
    "chapter": 69,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Псалом Асафа."
  },
  {
    "chapter": 69,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Воистину Бог"
  },
  {
    "chapter": 69,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "благ к Израилю,"
  },
  {
    "chapter": 69,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "к чистым сердцем!"
  },
  {
    "chapter": 69,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "А мои ноги едва"
  },
  {
    "chapter": 69,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "не поскользнулись,"
  },
  {
    "chapter": 69,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "стопы мои чуть"
  },
  {
    "chapter": 69,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "не потеряли опору,"
  },
  {
    "chapter": 69,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "потому что я"
  },
  {
    "chapter": 69,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "позавидовал надменным,"
  },
  {
    "chapter": 69,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "увидев"
  },
  {
    "chapter": 69,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "процветание нечестивых."
  },
  {
    "chapter": 69,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Нет им страданий;"
  },
  {
    "chapter": 69,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "они полны здоровья и силы."
  },
  {
    "chapter": 69,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Они свободны от"
  },
  {
    "chapter": 69,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "тягот человеческих,"
  },
  {
    "chapter": 69,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и трудностей"
  },
  {
    "chapter": 69,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "людских они не знают."
  },
  {
    "chapter": 69,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "И поэтому"
  },
  {
    "chapter": 69,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "гордость их ожерелье,"
  },
  {
    "chapter": 69,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "а жестокость их одеяние."
  },
  {
    "chapter": 69,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Вылезли от"
  },
  {
    "chapter": 69,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "тучности глаза их;"
  },
  {
    "chapter": 69,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "их тщеславие"
  },
  {
    "chapter": 69,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "не знает границ."
  },
  {
    "chapter": 69,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Слова их полны"
  },
  {
    "chapter": 69,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "издевательства и злобы,"
  },
  {
    "chapter": 69,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и в своей"
  },
  {
    "chapter": 69,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "надменности угрожают насилием."
  },
  {
    "chapter": 69,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Открывают уста"
  },
  {
    "chapter": 69,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "свои против небес,"
  },
  {
    "chapter": 69,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и слова их"
  },
  {
    "chapter": 69,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "расхаживают по земле."
  },
  {
    "chapter": 69,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Потому к ним же обращается"
  },
  {
    "chapter": 69,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Его народ и пьет слова их,"
  },
  {
    "chapter": 69,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "как воду из полной чаши."
  },
  {
    "chapter": 69,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Они говорят:"
  },
  {
    "chapter": 69,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Как узнает Бог? Есть"
  },
  {
    "chapter": 69,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "ли знание у Всевышнего?"
  },
  {
    "chapter": 69,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Таковы нечестивые:"
  },
  {
    "chapter": 69,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "они всегда беззаботны и"
  },
  {
    "chapter": 69,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "умножают свое богатство."
  },
  {
    "chapter": 69,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Так не напрасно ли я хранил"
  },
  {
    "chapter": 69,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "сердце свое в чистоте"
  },
  {
    "chapter": 69,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и омывал руки свои в"
  },
  {
    "chapter": 69,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "невинности?"
  },
  {
    "chapter": 69,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Целый день я подвергался"
  },
  {
    "chapter": 69,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "мучениям"
  },
  {
    "chapter": 69,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и был наказан каждое утро."
  },
  {
    "chapter": 69,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но если бы я решил,"
  },
  {
    "chapter": 69,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "что буду рассуждать так,"
  },
  {
    "chapter": 69,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "то предал бы детей Твоих."
  },
  {
    "chapter": 69,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Когда я пытался"
  },
  {
    "chapter": 69,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "все это понять,"
  },
  {
    "chapter": 69,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "то мне это"
  },
  {
    "chapter": 69,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "казалось слишком трудным,"
  },
  {
    "chapter": 69,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "пока я не вошел в"
  },
  {
    "chapter": 69,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "святилище Бога и не понял конца их."
  },
  {
    "chapter": 69,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Истинно,"
  },
  {
    "chapter": 69,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "на скользких путях Ты"
  },
  {
    "chapter": 69,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "поставил их и предаешь"
  },
  {
    "chapter": 69,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "их полному разрушению."
  },
  {
    "chapter": 69,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Как неожиданно"
  },
  {
    "chapter": 69,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "они были истреблены,"
  },
  {
    "chapter": 69,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "погибли от ужаса!"
  },
  {
    "chapter": 69,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Как сновидение"
  },
  {
    "chapter": 69,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "исчезает при пробуждении,"
  },
  {
    "chapter": 69,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "так Ты,"
  },
  {
    "chapter": 69,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Владыка,"
  },
  {
    "chapter": 69,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "восстав,"
  },
  {
    "chapter": 69,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "презришь их образ."
  },
  {
    "chapter": 69,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Когда мое сердце наполняла"
  },
  {
    "chapter": 69,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "горечь"
  },
  {
    "chapter": 69,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и терзалась моя душа,"
  },
  {
    "chapter": 69,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "я был"
  },
  {
    "chapter": 69,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "невежествен и не понимал;"
  },
  {
    "chapter": 69,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "как неразумное"
  },
  {
    "chapter": 69,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "животное я был пред Тобою."
  },
  {
    "chapter": 69,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "И все же я всегда с Тобою:"
  },
  {
    "chapter": 69,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Ты держишь меня"
  },
  {
    "chapter": 69,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "за правую руку;"
  },
  {
    "chapter": 69,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты руководишь мною Своим"
  },
  {
    "chapter": 69,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "советом"
  },
  {
    "chapter": 69,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и потом к славе поведешь"
  },
  {
    "chapter": 69,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "меня."
  },
  {
    "chapter": 69,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Кто мне еще на небесах,"
  },
  {
    "chapter": 69,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "кроме Тебя? И на"
  },
  {
    "chapter": 69,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "земле я ничего,"
  },
  {
    "chapter": 69,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "кроме Тебя,"
  },
  {
    "chapter": 69,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "не желаю."
  },
  {
    "chapter": 69,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ослабевает моя"
  },
  {
    "chapter": 69,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "плоть и мое сердце,"
  },
  {
    "chapter": 69,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "но Бог твердыня сердца"
  },
  {
    "chapter": 69,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "моего и часть моя навсегда."
  },
  {
    "chapter": 69,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Удаляющиеся от"
  },
  {
    "chapter": 69,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Тебя погибнут,"
  },
  {
    "chapter": 69,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Ты губишь всех,"
  },
  {
    "chapter": 69,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "кто неверен Тебе."
  },
  {
    "chapter": 69,
    "verse": 28,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "А для меня благо"
  },
  {
    "chapter": 69,
    "verse": 28,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "приближаться к Богу;"
  },
  {
    "chapter": 69,
    "verse": 28,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Владыку Господа я"
  },
  {
    "chapter": 69,
    "verse": 28,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "сделал своим убежищем."
  },
  {
    "chapter": 69,
    "verse": 28,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Я возвещу о"
  },
  {
    "chapter": 69,
    "verse": 28,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "всех делах Твоих."
  }
]