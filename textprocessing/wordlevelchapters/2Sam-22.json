    "book_id": "2Sam",
    "part": 1,
    "text": "С Твоей помощью я"
  },
  {
    "chapter": 22,
    "verse": 30,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "сокрушаю войско,"
  },
  {
    "chapter": 22,
    "verse": 30,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "с Богом моим"
  },
  {
    "chapter": 22,
    "verse": 30,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "поднимаюсь на стену."
  },
  {
    "chapter": 22,
    "verse": 31,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Бог! Путь Его безупречен;"
  },
  {
    "chapter": 22,
    "verse": 31,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "слово Господа чисто."
  },
  {
    "chapter": 22,
    "verse": 31,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "Он щит для всех,"
  },
  {
    "chapter": 22,
    "verse": 31,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "кто ищет в Нем прибежища."
  },
  {
    "chapter": 22,
    "verse": 32,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Потому что кто Бог,"
  },
  {
    "chapter": 22,
    "verse": 32,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "кроме Господа? И кто скала,"
  },
  {
    "chapter": 22,
    "verse": 32,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "кроме нашего Бога?"
  },
  {
    "chapter": 22,
    "verse": 33,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Бог мое крепкое прибежище,"
  },
  {
    "chapter": 22,
    "verse": 33,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "Он делает верным мой путь,"
  },
  {
    "chapter": 22,
    "verse": 33,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "."
  },
  {
    "chapter": 22,
    "verse": 34,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Он делает ноги мои,"
  },
  {
    "chapter": 22,
    "verse": 34,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "как ноги оленя,"
  },
  {
    "chapter": 22,
    "verse": 34,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "и ставит меня на высотах."
  },
  {
    "chapter": 22,
    "verse": 35,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Он учит руки мои войне,"
  },
  {
    "chapter": 22,
    "verse": 35,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "так что гнут мои"
  },
  {
    "chapter": 22,
    "verse": 35,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "мышцы бронзовый лук."
  },
  {
    "chapter": 22,
    "verse": 36,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Ты вручил мне щит"
  },
  {
    "chapter": 22,
    "verse": 36,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "спасения Твоего,"
  },
  {
    "chapter": 22,
    "verse": 36,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "Твоя милость"
  },
  {
    "chapter": 22,
    "verse": 36,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "меня возвеличивает."
  },
  {
    "chapter": 22,
    "verse": 37,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Ты расширил мой"
  },
  {
    "chapter": 22,
    "verse": 37,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "шаг подо мной,"
  },
  {
    "chapter": 22,
    "verse": 37,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "чтобы ногам"
  },
  {
    "chapter": 22,
    "verse": 37,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "моим не оступиться."
  },
  {
    "chapter": 22,
    "verse": 38,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Я преследовал врагов"
  },
  {
    "chapter": 22,
    "verse": 38,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "моих и сокрушил их;"
  },
  {
    "chapter": 22,
    "verse": 38,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "я не повернул назад,"
  },
  {
    "chapter": 22,
    "verse": 38,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "пока не истребил их."
  },
  {
    "chapter": 22,
    "verse": 39,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Я поразил и сокрушил их,"
  },
  {
    "chapter": 22,
    "verse": 39,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "и им не встать;"
  },
  {
    "chapter": 22,
    "verse": 39,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "под ноги мне они пали."
  },
  {
    "chapter": 22,
    "verse": 40,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Ты препоясал меня"
  },
  {
    "chapter": 22,
    "verse": 40,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "силой для битвы;"
  },
  {
    "chapter": 22,
    "verse": 40,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "Ты поверг к моим"
  },
  {
    "chapter": 22,
    "verse": 40,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "ногам восставших на меня."
  },
  {
    "chapter": 22,
    "verse": 41,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Врагов обратил"
  },
  {
    "chapter": 22,
    "verse": 41,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "Ты ко мне спиной,"
  },
  {
    "chapter": 22,
    "verse": 41,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "и я истребил"
  },
  {
    "chapter": 22,
    "verse": 41,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "ненавидящих меня."
  },
  {
    "chapter": 22,
    "verse": 42,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Они поднимали свой взор,"
  },
  {
    "chapter": 22,
    "verse": 42,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "но не было никого,"
  },
  {
    "chapter": 22,
    "verse": 42,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "чтобы спасти их к"
  },
  {
    "chapter": 22,
    "verse": 42,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "Господу взывали,"
  },
  {
    "chapter": 22,
    "verse": 42,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "но Он не ответил им."
  },
  {
    "chapter": 22,
    "verse": 43,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Я рассеял их,"
  },
  {
    "chapter": 22,
    "verse": 43,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "как земную пыль,"
  },
  {
    "chapter": 22,
    "verse": 43,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "я толок их и топтал,"
  },
  {
    "chapter": 22,
    "verse": 43,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "как уличную грязь."
  },
  {
    "chapter": 22,
    "verse": 44,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Ты избавил меня от"
  },
  {
    "chapter": 22,
    "verse": 44,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "мятежа народа моего;"
  },
  {
    "chapter": 22,
    "verse": 44,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "Ты сохранил меня"
  },
  {
    "chapter": 22,
    "verse": 44,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "главой чужеземцев."
  },
  {
    "chapter": 22,
    "verse": 44,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "Народы,"
  },
  {
    "chapter": 22,
    "verse": 44,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 6,
    "text": "которых я не знал,"
  },
  {
    "chapter": 22,
    "verse": 44,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 7,
    "text": "служат мне."
  },
  {
    "chapter": 22,
    "verse": 45,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Чужеземцы"
  },
  {
    "chapter": 22,
    "verse": 45,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "раболепствуют предо мной;"
  },
  {
    "chapter": 22,
    "verse": 45,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "они покоряются,"
  },
  {
    "chapter": 22,
    "verse": 45,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "едва обо мне услышав."
  },
  {
    "chapter": 22,
    "verse": 46,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Все они пали"
  },
  {
    "chapter": 22,
    "verse": 46,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "духом и выходят,"
  },
  {
    "chapter": 22,
    "verse": 46,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "дрожа,"
  },
  {
    "chapter": 22,
    "verse": 46,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "из своих крепостей."
  },
  {
    "chapter": 22,
    "verse": 47,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Жив Господь! Хвала моей"
  },
  {
    "chapter": 22,
    "verse": 47,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "скале! Да будет превознесен"
  },
  {
    "chapter": 22,
    "verse": 47,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "мой Бог,"
  },
  {
    "chapter": 22,
    "verse": 47,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "скала моего спасения!"
  },
  {
    "chapter": 22,
    "verse": 48,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Он Бог,"
  },
  {
    "chapter": 22,
    "verse": 48,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "Который мстит за меня,"
  },
  {
    "chapter": 22,
    "verse": 48,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "Который народы"
  },
  {
    "chapter": 22,
    "verse": 48,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "мне покоряет,"
  },
  {
    "chapter": 22,
    "verse": 49,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "и спасает меня"
  },
  {
    "chapter": 22,
    "verse": 49,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "от моих врагов."
  },
  {
    "chapter": 22,
    "verse": 49,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "Ты вознес меня над"
  },
  {
    "chapter": 22,
    "verse": 49,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "моими противниками;"
  },
  {
    "chapter": 22,
    "verse": 49,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "Ты спас меня от"
  },
  {
    "chapter": 22,
    "verse": 49,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 6,
    "text": "врагов моих жестоких."
  },
  {
    "chapter": 22,
    "verse": 50,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Итак,"
  },
  {
    "chapter": 22,
    "verse": 50,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "буду славить Тебя,"
  },
  {
    "chapter": 22,
    "verse": 50,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "Господи,"
  },
  {
    "chapter": 22,
    "verse": 50,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "среди других народов;"
  },
  {
    "chapter": 22,
    "verse": 50,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "Имени Твоему"
  },
  {
    "chapter": 22,
    "verse": 50,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 6,
    "text": "воспою я хвалу."
  },
  {
    "chapter": 22,
    "verse": 51,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 1,
    "text": "Он дает Своему"
  },
  {
    "chapter": 22,
    "verse": 51,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 2,
    "text": "царю большие победы;"
  },
  {
    "chapter": 22,
    "verse": 51,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "непреложную милость Он"
  },
  {
    "chapter": 22,
    "verse": 51,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "являет помазаннику Своему,"
  },
  {
    "chapter": 22,
    "verse": 51,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 5,
    "text": "Давиду и семени его вовеки."
  }
]