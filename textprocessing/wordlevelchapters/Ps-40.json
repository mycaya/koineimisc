[
  {
    "chapter": 40,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Оправдай меня,"
  },
  {
    "chapter": 40,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Боже,"
  },
  {
    "chapter": 40,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "вступись в мою тяжбу с"
  },
  {
    "chapter": 40,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "народом безбожным от лживых"
  },
  {
    "chapter": 40,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "и злобных спаси меня."
  },
  {
    "chapter": 40,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты Бог,"
  },
  {
    "chapter": 40,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "крепость моя."
  },
  {
    "chapter": 40,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Почему Ты отверг"
  },
  {
    "chapter": 40,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "меня? Почему я скитаюсь,"
  },
  {
    "chapter": 40,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "плача,"
  },
  {
    "chapter": 40,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "оскорбленный моим врагом?"
  },
  {
    "chapter": 40,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пошли Свой свет и истину"
  },
  {
    "chapter": 40,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "пусть они меня направляют;"
  },
  {
    "chapter": 40,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "пусть приведут на"
  },
  {
    "chapter": 40,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "святую гору Твою,"
  },
  {
    "chapter": 40,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "к месту,"
  },
  {
    "chapter": 40,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "где Ты обитаешь."
  },
  {
    "chapter": 40,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Тогда приду я к"
  },
  {
    "chapter": 40,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Божьему жертвеннику,"
  },
  {
    "chapter": 40,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "к Богу радости"
  },
  {
    "chapter": 40,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и веселья моего."
  },
  {
    "chapter": 40,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Буду славить Тебя на арфе,"
  },
  {
    "chapter": 40,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Боже,"
  },
  {
    "chapter": 40,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "мой Бог."
  },
  {
    "chapter": 40,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Что унываешь,"
  },
  {
    "chapter": 40,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "моя душа? Зачем"
  },
  {
    "chapter": 40,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "тревожишься? Возложи"
  },
  {
    "chapter": 40,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "надежду свою на Бога,"
  },
  {
    "chapter": 40,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "ведь я еще буду славить"
  },
  {
    "chapter": 40,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Его моего Спасителя и Бога."
  }
]