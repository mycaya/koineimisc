[
  {
    "chapter": 28,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "- Есть рудник для серебра"
  },
  {
    "chapter": 28,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и для золота горн"
  },
  {
    "chapter": 28,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "плавильный."
  },
  {
    "chapter": 28,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Из земли добывают железо,"
  },
  {
    "chapter": 28,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и плавят медь из руды."
  },
  {
    "chapter": 28,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Тьме рудокоп"
  },
  {
    "chapter": 28,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "полагает предел."
  },
  {
    "chapter": 28,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Ищет он в отдаленных"
  },
  {
    "chapter": 28,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "пределах руду в"
  },
  {
    "chapter": 28,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "непроглядной мгле."
  },
  {
    "chapter": 28,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Далеко от жилья"
  },
  {
    "chapter": 28,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "он роет колодец,"
  },
  {
    "chapter": 28,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "в местах,"
  },
  {
    "chapter": 28,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "что забыла стопа человека;"
  },
  {
    "chapter": 28,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "висит он и"
  },
  {
    "chapter": 28,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 6,
    "text": "зыблется вдали от людей."
  },
  {
    "chapter": 28,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Земля,"
  },
  {
    "chapter": 28,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "на которой вырастает пища,"
  },
  {
    "chapter": 28,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "изнутри изрыта,"
  },
  {
    "chapter": 28,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "будто огнем."
  },
  {
    "chapter": 28,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Сапфиров"
  },
  {
    "chapter": 28,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "россыпь в ее камнях,"
  },
  {
    "chapter": 28,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и в ней золотой песок."
  },
  {
    "chapter": 28,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Пути к ним не"
  },
  {
    "chapter": 28,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "знает хищная птица,"
  },
  {
    "chapter": 28,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "соколиный глаз их не видел."
  },
  {
    "chapter": 28,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Гордые звери их не топтали,"
  },
  {
    "chapter": 28,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и лев по ним не ходил."
  },
  {
    "chapter": 28,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Человек на гранит"
  },
  {
    "chapter": 28,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "простирает руку,"
  },
  {
    "chapter": 28,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "с корнем вырывает горы."
  },
  {
    "chapter": 28,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Он прорубает"
  },
  {
    "chapter": 28,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "проходы в скалах,"
  },
  {
    "chapter": 28,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и глаза его видят"
  },
  {
    "chapter": 28,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "все их сокровища."
  },
  {
    "chapter": 28,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Он"
  },
  {
    "chapter": 28,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "останавливает истоки рек,"
  },
  {
    "chapter": 28,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и сокровенное"
  },
  {
    "chapter": 28,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "выносит на свет."
  },
  {
    "chapter": 28,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Но где можно найти"
  },
  {
    "chapter": 28,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "мудрость? И где обитает разум?"
  },
  {
    "chapter": 28,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Не знает смертный ее дома;"
  },
  {
    "chapter": 28,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "на земле живых ее не найти."
  },
  {
    "chapter": 28,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Пучина скажет:"
  },
  {
    "chapter": 28,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Во мне ее нет;"
  },
  {
    "chapter": 28,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и молвит море:"
  },
  {
    "chapter": 28,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "Не у меня она."
  },
  {
    "chapter": 28,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Ее не купить за"
  },
  {
    "chapter": 28,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "червонное золото,"
  },
  {
    "chapter": 28,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "не отвесить"
  },
  {
    "chapter": 28,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "цену ее серебром."
  },
  {
    "chapter": 28,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Не купить ее ни"
  },
  {
    "chapter": 28,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "за золото Офира,"
  },
  {
    "chapter": 28,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "ни за"
  },
  {
    "chapter": 28,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "драгоценный оникс и сапфир."
  },
  {
    "chapter": 28,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Не сравнить ее с"
  },
  {
    "chapter": 28,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "золотом и кристаллом,"
  },
  {
    "chapter": 28,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "на утварь из чистого"
  },
  {
    "chapter": 28,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "золота не обменять."
  },
  {
    "chapter": 28,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Что говорить о"
  },
  {
    "chapter": 28,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "кораллах и яшме;"
  },
  {
    "chapter": 28,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "превыше рубинов"
  },
  {
    "chapter": 28,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "стоимость мудрости."
  },
  {
    "chapter": 28,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Не сравнить с"
  },
  {
    "chapter": 28,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "ней топазы из Куша;"
  },
  {
    "chapter": 28,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "не купить ее за"
  },
  {
    "chapter": 28,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "отменное золото."
  },
  {
    "chapter": 28,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Откуда приходит"
  },
  {
    "chapter": 28,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "мудрость? Где обитает разум?"
  },
  {
    "chapter": 28,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Скрыта она от глаз всех"
  },
  {
    "chapter": 28,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "живущих"
  },
  {
    "chapter": 28,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и от птиц небесных утаена."
  },
  {
    "chapter": 28,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Погибель и Смерть говорят:"
  },
  {
    "chapter": 28,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Мы слышали"
  },
  {
    "chapter": 28,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "только слух о ней."
  },
  {
    "chapter": 28,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Только Богу"
  },
  {
    "chapter": 28,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "ведом к ней путь,"
  },
  {
    "chapter": 28,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Он знает ее жилище,"
  },
  {
    "chapter": 28,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "ведь Он озирает"
  },
  {
    "chapter": 28,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "края земли и видит все,"
  },
  {
    "chapter": 28,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "что под небесами."
  },
  {
    "chapter": 28,
    "verse": 25,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Когда Он давал ветру"
  },
  {
    "chapter": 28,
    "verse": 25,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "силу и водам ставил пределы,"
  },
  {
    "chapter": 28,
    "verse": 26,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "когда Он дождю предписал"
  },
  {
    "chapter": 28,
    "verse": 26,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "устав"
  },
  {
    "chapter": 28,
    "verse": 26,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и путь проложил для молний,"
  },
  {
    "chapter": 28,
    "verse": 27,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Он увидел"
  },
  {
    "chapter": 28,
    "verse": 27,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "мудрость и восхвалил ее,"
  },
  {
    "chapter": 28,
    "verse": 27,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "утвердил ее,"
  },
  {
    "chapter": 28,
    "verse": 27,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "испытал"
  },
  {
    "chapter": 28,
    "verse": 28,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "и сказал человеку так:"
  },
  {
    "chapter": 28,
    "verse": 28,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Воистину,"
  },
  {
    "chapter": 28,
    "verse": 28,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "страх Владыки вот мудрость,"
  },
  {
    "chapter": 28,
    "verse": 28,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "сторониться зла это разум."
  }
]