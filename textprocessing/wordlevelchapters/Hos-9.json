[
  {
    "chapter": 9,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Не радуйся,"
  },
  {
    "chapter": 9,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "Израиль."
  },
  {
    "chapter": 9,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "Не ликуй,"
  },
  {
    "chapter": 9,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "как другие народы."
  },
  {
    "chapter": 9,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "Потому что ты не"
  },
  {
    "chapter": 9,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "был верен своему Богу,"
  },
  {
    "chapter": 9,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 7,
    "text": "ты любишь заработок"
  },
  {
    "chapter": 9,
    "verse": 1,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 8,
    "text": "блудниц на каждом гумне."
  },
  {
    "chapter": 9,
    "verse": 2,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Гумно и точило"
  },
  {
    "chapter": 9,
    "verse": 2,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "не накормят народ,"
  },
  {
    "chapter": 9,
    "verse": 2,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "молодое вино их подведет."
  },
  {
    "chapter": 9,
    "verse": 3,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Они не останутся"
  },
  {
    "chapter": 9,
    "verse": 3,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "в земле Господа."
  },
  {
    "chapter": 9,
    "verse": 3,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "Ефрем вернется в Египет"
  },
  {
    "chapter": 9,
    "verse": 3,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "и будет есть нечистую пищу"
  },
  {
    "chapter": 9,
    "verse": 3,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "в Ассирии."
  },
  {
    "chapter": 9,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Они не будут возливать"
  },
  {
    "chapter": 9,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "винное приношение Господу,"
  },
  {
    "chapter": 9,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "и их жертвоприношения"
  },
  {
    "chapter": 9,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "не будут угодны Ему."
  },
  {
    "chapter": 9,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "Такие жертвоприношения"
  },
  {
    "chapter": 9,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "будут для них как хлеб на"
  },
  {
    "chapter": 9,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 7,
    "text": "похоронах:"
  },
  {
    "chapter": 9,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 8,
    "text": "каждый,"
  },
  {
    "chapter": 9,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 9,
    "text": "кто будет есть его,"
  },
  {
    "chapter": 9,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 10,
    "text": "осквернится."
  },
  {
    "chapter": 9,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 11,
    "text": "Эта пища будет"
  },
  {
    "chapter": 9,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 12,
    "text": "предназначена только для них самих."
  },
  {
    "chapter": 9,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 13,
    "text": "Она не войдет"
  },
  {
    "chapter": 9,
    "verse": 4,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 14,
    "text": "в храм Господа."
  },
  {
    "chapter": 9,
    "verse": 5,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Что же вы будете делать в"
  },
  {
    "chapter": 9,
    "verse": 5,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "день ваших установленных"
  },
  {
    "chapter": 9,
    "verse": 5,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "праздников,"
  },
  {
    "chapter": 9,
    "verse": 5,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "в празднование"
  },
  {
    "chapter": 9,
    "verse": 5,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "дней Господа?"
  },
  {
    "chapter": 9,
    "verse": 6,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Даже если вы"
  },
  {
    "chapter": 9,
    "verse": 6,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "избежите истребления,"
  },
  {
    "chapter": 9,
    "verse": 6,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "Египет соберет вас,"
  },
  {
    "chapter": 9,
    "verse": 6,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "а Мемфис похоронит."
  },
  {
    "chapter": 9,
    "verse": 6,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "Ваши сокровища из"
  },
  {
    "chapter": 9,
    "verse": 6,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "серебра зарастут колючками,"
  },
  {
    "chapter": 9,
    "verse": 6,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 7,
    "text": "а жилища терновником."
  },
  {
    "chapter": 9,
    "verse": 7,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Наступают дни наказания,"
  },
  {
    "chapter": 9,
    "verse": 7,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "близятся дни расплаты."
  },
  {
    "chapter": 9,
    "verse": 7,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "Пусть знает это Израиль."
  },
  {
    "chapter": 9,
    "verse": 7,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "Из-за того,"
  },
  {
    "chapter": 9,
    "verse": 7,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "что твоих грехов так много,"
  },
  {
    "chapter": 9,
    "verse": 7,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "а твоя"
  },
  {
    "chapter": 9,
    "verse": 7,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 7,
    "text": "враждебность столь велика,"
  },
  {
    "chapter": 9,
    "verse": 7,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 8,
    "text": "пророк считается глупцом,"
  },
  {
    "chapter": 9,
    "verse": 7,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 9,
    "text": "а вдохновенный"
  },
  {
    "chapter": 9,
    "verse": 7,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 10,
    "text": "человек безумцем."
  },
  {
    "chapter": 9,
    "verse": 8,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Пророк вместе с моим"
  },
  {
    "chapter": 9,
    "verse": 8,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "Богом страж над Ефремом;"
  },
  {
    "chapter": 9,
    "verse": 8,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "все же силки ожидают"
  },
  {
    "chapter": 9,
    "verse": 8,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "его на всех его путях,"
  },
  {
    "chapter": 9,
    "verse": 8,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "и враждебность"
  },
  {
    "chapter": 9,
    "verse": 8,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "в доме Бога его."
  },
  {
    "chapter": 9,
    "verse": 9,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Глубоко погрязли"
  },
  {
    "chapter": 9,
    "verse": 9,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "они в развращенности,"
  },
  {
    "chapter": 9,
    "verse": 9,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "как во времена города Гивы."
  },
  {
    "chapter": 9,
    "verse": 9,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "Бог вспомнит об их зле"
  },
  {
    "chapter": 9,
    "verse": 9,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "и накажет их за грехи."
  },
  {
    "chapter": 9,
    "verse": 10,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "- Я нашел Израиль,"
  },
  {
    "chapter": 9,
    "verse": 10,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "как виноград в пустыне,"
  },
  {
    "chapter": 9,
    "verse": 10,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "Я увидел ваших отцов,"
  },
  {
    "chapter": 9,
    "verse": 10,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "как ранний плод инжира."
  },
  {
    "chapter": 9,
    "verse": 10,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "Но когда они пошли к"
  },
  {
    "chapter": 9,
    "verse": 10,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "Ваал-Фегору"
  },
  {
    "chapter": 9,
    "verse": 10,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 7,
    "text": "и посвятили себя этому"
  },
  {
    "chapter": 9,
    "verse": 10,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 8,
    "text": "постыдному идолу,"
  },
  {
    "chapter": 9,
    "verse": 10,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 9,
    "text": "то и сами стали"
  },
  {
    "chapter": 9,
    "verse": 10,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 10,
    "text": "столь же отвратительными,"
  },
  {
    "chapter": 9,
    "verse": 10,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 11,
    "text": "как их идол."
  },
  {
    "chapter": 9,
    "verse": 11,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Будущая слава"
  },
  {
    "chapter": 9,
    "verse": 11,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "Ефрема улетит,"
  },
  {
    "chapter": 9,
    "verse": 11,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "словно птица."
  },
  {
    "chapter": 9,
    "verse": 11,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "Не будет ни рождения,"
  },
  {
    "chapter": 9,
    "verse": 11,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "ни беременности,"
  },
  {
    "chapter": 9,
    "verse": 11,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "ни зачатия."
  },
  {
    "chapter": 9,
    "verse": 12,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Даже если они"
  },
  {
    "chapter": 9,
    "verse": 12,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "воспитают детей,"
  },
  {
    "chapter": 9,
    "verse": 12,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "Я отниму их."
  },
  {
    "chapter": 9,
    "verse": 12,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "Горе им,"
  },
  {
    "chapter": 9,
    "verse": 12,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "когда Я отвернусь от них!"
  },
  {
    "chapter": 9,
    "verse": 13,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Я видел,"
  },
  {
    "chapter": 9,
    "verse": 13,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "Ефрем,"
  },
  {
    "chapter": 9,
    "verse": 13,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "как Тир,"
  },
  {
    "chapter": 9,
    "verse": 13,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "насажден на"
  },
  {
    "chapter": 9,
    "verse": 13,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "плодородном лугу,"
  },
  {
    "chapter": 9,
    "verse": 13,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "но Ефрем приведет"
  },
  {
    "chapter": 9,
    "verse": 13,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 7,
    "text": "своих детей к убийце."
  },
  {
    "chapter": 9,
    "verse": 14,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "- Дай им,"
  },
  {
    "chapter": 9,
    "verse": 14,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 9,
    "verse": 14,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "то,"
  },
  {
    "chapter": 9,
    "verse": 14,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "что они"
  },
  {
    "chapter": 9,
    "verse": 14,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "заслужили! Дай им утробу,"
  },
  {
    "chapter": 9,
    "verse": 14,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "неспособную родить,"
  },
  {
    "chapter": 9,
    "verse": 14,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 7,
    "text": "и грудь,"
  },
  {
    "chapter": 9,
    "verse": 14,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 8,
    "text": "в которой нет молока."
  },
  {
    "chapter": 9,
    "verse": 15,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "- Из-за всего"
  },
  {
    "chapter": 9,
    "verse": 15,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "их зла в Гилгале,"
  },
  {
    "chapter": 9,
    "verse": 15,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "Я там возненавидел их."
  },
  {
    "chapter": 9,
    "verse": 15,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 4,
    "text": "Из-за их злых дел Я"
  },
  {
    "chapter": 9,
    "verse": 15,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 5,
    "text": "изгоню их из Моего дома."
  },
  {
    "chapter": 9,
    "verse": 15,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 6,
    "text": "Я не возлюблю их более,"
  },
  {
    "chapter": 9,
    "verse": 15,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 7,
    "text": "все их вожди отступники."
  },
  {
    "chapter": 9,
    "verse": 16,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 1,
    "text": "Ефрем поражен,"
  },
  {
    "chapter": 9,
    "verse": 16,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "их корень засох,"
  },
  {
    "chapter": 9,
    "verse": 16,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "они не приносят плод."
