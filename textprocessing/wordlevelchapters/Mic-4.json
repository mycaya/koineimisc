[
  {
    "chapter": 4,
    "verse": 1,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "В последние дни гора храма"
  },
  {
    "chapter": 4,
    "verse": 1,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "Господня станет высочайшею"
  },
  {
    "chapter": 4,
    "verse": 1,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "среди гор;"
  },
  {
    "chapter": 4,
    "verse": 1,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "вознесется она над холмами,"
  },
  {
    "chapter": 4,
    "verse": 1,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 5,
    "text": "и устремятся к ней народы."
  },
  {
    "chapter": 4,
    "verse": 2,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "Многие народы"
  },
  {
    "chapter": 4,
    "verse": 2,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "пойдут и скажут:"
  },
  {
    "chapter": 4,
    "verse": 2,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "Идем,"
  },
  {
    "chapter": 4,
    "verse": 2,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "поднимемся на"
  },
  {
    "chapter": 4,
    "verse": 2,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 5,
    "text": "Господню гору,"
  },
  {
    "chapter": 4,
    "verse": 2,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 6,
    "text": "к дому Бога Иакова."
  },
  {
    "chapter": 4,
    "verse": 2,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 7,
    "text": "Он научит нас Своим путям,"
  },
  {
    "chapter": 4,
    "verse": 2,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 8,
    "text": "и будем ходить"
  },
  {
    "chapter": 4,
    "verse": 2,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 9,
    "text": "по Его тропам."
  },
  {
    "chapter": 4,
    "verse": 2,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 10,
    "text": "Ведь из Сиона выйдет Закон,"
  },
  {
    "chapter": 4,
    "verse": 2,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 11,
    "text": "и слово"
  },
  {
    "chapter": 4,
    "verse": 2,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 12,
    "text": "Господне из Иерусалима."
  },
  {
    "chapter": 4,
    "verse": 3,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "Он рассудит меж"
  },
  {
    "chapter": 4,
    "verse": 3,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "многими народами,"
  },
  {
    "chapter": 4,
    "verse": 3,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "разрешит тяжбы сильным"
  },
  {
    "chapter": 4,
    "verse": 3,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "племенам в далеких краях."
  },
  {
    "chapter": 4,
    "verse": 3,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 5,
    "text": "И перекуют они мечи на"
  },
  {
    "chapter": 4,
    "verse": 3,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 6,
    "text": "плуги и копья на серпы."
  },
  {
    "chapter": 4,
    "verse": 3,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 7,
    "text": "Народ на народ"
  },
  {
    "chapter": 4,
    "verse": 3,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 8,
    "text": "не поднимет меча,"
  },
  {
    "chapter": 4,
    "verse": 3,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 9,
    "text": "и не будут больше"
  },
  {
    "chapter": 4,
    "verse": 3,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 10,
    "text": "учиться воевать."
  },
  {
    "chapter": 4,
    "verse": 4,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "Каждый будет сидеть под"
  },
  {
    "chapter": 4,
    "verse": 4,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "своей виноградной лозой"
  },
  {
    "chapter": 4,
    "verse": 4,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "и под своим инжиром,"
  },
  {
    "chapter": 4,
    "verse": 4,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "и никто не будет"
  },
  {
    "chapter": 4,
    "verse": 4,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 5,
    "text": "более их устрашать,"
  },
  {
    "chapter": 4,
    "verse": 4,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 6,
    "text": "потому что так"
  },
  {
    "chapter": 4,
    "verse": 4,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 7,
    "text": "сказал Господь сил."
  },
  {
    "chapter": 4,
    "verse": 5,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "Пусть народы живут"
  },
  {
    "chapter": 4,
    "verse": 5,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "во имя своих богов,"
  },
  {
    "chapter": 4,
    "verse": 5,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "мы будем жить"
  },
  {
    "chapter": 4,
    "verse": 5,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "во Имя Господа,"
  },
  {
    "chapter": 4,
    "verse": 5,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 5,
    "text": "нашего Бога,"
  },
  {
    "chapter": 4,
    "verse": 5,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 6,
    "text": "во веки и веки."
  },
  {
    "chapter": 4,
    "verse": 6,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "- В тот день,"
  },
  {
    "chapter": 4,
    "verse": 6,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "возвещает Господь,"
  },
  {
    "chapter": 4,
    "verse": 6,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "Я созову хромых,"
  },
  {
    "chapter": 4,
    "verse": 6,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "Я соберу"
  },
  {
    "chapter": 4,
    "verse": 6,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 5,
    "text": "изгнанников с теми,"
  },
  {
    "chapter": 4,
    "verse": 6,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 6,
    "text": "кого заставил горевать."
  },
  {
    "chapter": 4,
    "verse": 7,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "Я сделаю хромых уцелевшими,"
  },
  {
    "chapter": 4,
    "verse": 7,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "а изгнанников"
  },
  {
    "chapter": 4,
    "verse": 7,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "сильным народом."
  },
  {
    "chapter": 4,
    "verse": 7,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "Господь будет править"
  },
  {
    "chapter": 4,
    "verse": 7,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 5,
    "text": "над ними с горы Сион,"
  },
  {
    "chapter": 4,
    "verse": 7,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 6,
    "text": "со дня того и вовек."
  },
  {
    "chapter": 4,
    "verse": 8,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "Что до тебя,"
  },
  {
    "chapter": 4,
    "verse": 8,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "башня стада,"
  },
  {
    "chapter": 4,
    "verse": 8,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "холм дочери Сиона,"
  },
  {
    "chapter": 4,
    "verse": 8,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "к тебе"
  },
  {
    "chapter": 4,
    "verse": 8,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 5,
    "text": "возвратится прежняя власть;"
  },
  {
    "chapter": 4,
    "verse": 8,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 6,
    "text": "царская власть придет"
  },
  {
    "chapter": 4,
    "verse": 8,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 7,
    "text": "к дочери Иерусалима."
  },
  {
    "chapter": 4,
    "verse": 9,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "Что же ты теперь"
  },
  {
    "chapter": 4,
    "verse": 9,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "заходишься криком,"
  },
  {
    "chapter": 4,
    "verse": 9,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "разве нет у тебя царя?"
  },
  {
    "chapter": 4,
    "verse": 9,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "Разве погиб твой советник,"
  },
  {
    "chapter": 4,
    "verse": 9,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 5,
    "text": "что муки схватили тебя,"
  },
  {
    "chapter": 4,
    "verse": 9,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 6,
    "text": "точно женщину при родах?"
  },
  {
    "chapter": 4,
    "verse": 10,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "Мечись и стони,"
  },
  {
    "chapter": 4,
    "verse": 10,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "дочь Сиона,"
  },
  {
    "chapter": 4,
    "verse": 10,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "как женщина при родах,"
  },
  {
    "chapter": 4,
    "verse": 10,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "ведь теперь ты покинешь"
  },
  {
    "chapter": 4,
    "verse": 10,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 5,
    "text": "город"
  },
  {
    "chapter": 4,
    "verse": 10,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 6,
    "text": "и в поле разобьешь лагерь;"
  },
  {
    "chapter": 4,
    "verse": 10,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 7,
    "text": "ты уйдешь в Вавилон."
  },
  {
    "chapter": 4,
    "verse": 10,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 8,
    "text": "Там ты будешь избавлена,"
  },
  {
    "chapter": 4,
    "verse": 10,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 9,
    "text": "там Господь тебя"
  },
  {
    "chapter": 4,
    "verse": 10,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 10,
    "text": "выкупит из рук твоих врагов."
  },
  {
    "chapter": 4,
    "verse": 11,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "А сейчас много народов"
  },
  {
    "chapter": 4,
    "verse": 11,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "против тебя собралось."
  },
  {
    "chapter": 4,
    "verse": 11,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "Говорят:"
  },
  {
    "chapter": 4,
    "verse": 11,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "Пусть будет она осквернена!"
  },
  {
    "chapter": 4,
    "verse": 11,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 5,
    "text": "Пусть наши глаза увидят"
  },
  {
    "chapter": 4,
    "verse": 11,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 6,
    "text": "несчастье Сиона!"
  },
  {
    "chapter": 4,
    "verse": 12,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "Но они не знают"
  },
  {
    "chapter": 4,
    "verse": 12,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "Господних мыслей,"
  },
  {
    "chapter": 4,
    "verse": 12,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "не понимают Его замысла,"
  },
  {
    "chapter": 4,
    "verse": 12,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "что собрал Он их,"
  },
  {
    "chapter": 4,
    "verse": 12,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 5,
    "text": "как снопы на гумно."
  },
  {
    "chapter": 4,
    "verse": 13,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "- Поднимайся и молоти,"
  },
  {
    "chapter": 4,
    "verse": 13,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "дочь Сиона;"
  },
  {
    "chapter": 4,
    "verse": 13,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "Я сделаю рог твой железным,"
  },
  {
    "chapter": 4,
    "verse": 13,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "бронзовыми копыта,"
  },
  {
    "chapter": 4,
    "verse": 13,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 5,
    "text": "и сокрушишь ты много"
  },
  {
    "chapter": 4,
    "verse": 13,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 6,
    "text": "народов и Господу посвятишь"
  },
  {
    "chapter": 4,
    "verse": 13,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 7,
    "text": "их имущество,"
  },
  {
    "chapter": 4,
    "verse": 13,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 8,
    "text": "их богатства"
  },
  {
    "chapter": 4,
    "verse": 13,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 9,
    "text": "Владыке всей земли."
  }
]