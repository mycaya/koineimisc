[
  {
    "chapter": 9,
    "verse": 1,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 1,
    "text": "Я видел Владыку,"
  },
  {
    "chapter": 9,
    "verse": 1,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 2,
    "text": "стоящим у жертвенника."
  },
  {
    "chapter": 9,
    "verse": 1,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 3,
    "text": "Он сказал:"
  },
  {
    "chapter": 9,
    "verse": 1,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 4,
    "text": "Бей по верхушкам колонн,"
  },
  {
    "chapter": 9,
    "verse": 1,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 5,
    "text": "пусть задрожит порог."
  },
  {
    "chapter": 9,
    "verse": 1,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 6,
    "text": "Обрушь их на головы им всем"
  },
  {
    "chapter": 9,
    "verse": 1,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 7,
    "text": "уцелевших Я перебью мечом."
  },
  {
    "chapter": 9,
    "verse": 1,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 8,
    "text": "Ни один не укроется,"
  },
  {
    "chapter": 9,
    "verse": 1,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 9,
    "text": "никто не спасется."
  },
  {
    "chapter": 9,
    "verse": 2,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 1,
    "text": "Пусть зароются хоть в мир"
  },
  {
    "chapter": 9,
    "verse": 2,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 2,
    "text": "мертвых и оттуда достанет"
  },
  {
    "chapter": 9,
    "verse": 2,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 3,
    "text": "их Моя рука."
  },
  {
    "chapter": 9,
    "verse": 2,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 4,
    "text": "Пусть взберутся на"
  },
  {
    "chapter": 9,
    "verse": 2,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 5,
    "text": "небеса и оттуда Я свергну их."
  },
  {
    "chapter": 9,
    "verse": 3,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 1,
    "text": "Пусть укроются на вершине"
  },
  {
    "chapter": 9,
    "verse": 3,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 2,
    "text": "Кармила Я и там отыщу"
  },
  {
    "chapter": 9,
    "verse": 3,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 3,
    "text": "и схвачу их."
  },
  {
    "chapter": 9,
    "verse": 3,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 4,
    "text": "Пусть на дне"
  },
  {
    "chapter": 9,
    "verse": 3,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 5,
    "text": "морском спрячутся от Меня,"
  },
  {
    "chapter": 9,
    "verse": 3,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 6,
    "text": "Я велю там змею,"
  },
  {
    "chapter": 9,
    "verse": 3,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 7,
    "text": "и он ужалит их."
  },
  {
    "chapter": 9,
    "verse": 4,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 1,
    "text": "Пусть враги"
  },
  {
    "chapter": 9,
    "verse": 4,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 2,
    "text": "уведут их в плен,"
  },
  {
    "chapter": 9,
    "verse": 4,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 3,
    "text": "прикажу там мечу,"
  },
  {
    "chapter": 9,
    "verse": 4,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 4,
    "text": "и он убьет их."
  },
  {
    "chapter": 9,
    "verse": 4,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 5,
    "text": "Устремлю на них"
  },
  {
    "chapter": 9,
    "verse": 4,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 6,
    "text": "свои глаза им на беду,"
  },
  {
    "chapter": 9,
    "verse": 4,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 7,
    "text": "а не во благо."
  },
  {
    "chapter": 9,
    "verse": 5,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 1,
    "text": "Владыка,"
  },
  {
    "chapter": 9,
    "verse": 5,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 2,
    "text": "Господь сил,"
  },
  {
    "chapter": 9,
    "verse": 5,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 3,
    "text": "касается земли и она тает,"
  },
  {
    "chapter": 9,
    "verse": 5,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 4,
    "text": "а все живущие"
  },
  {
    "chapter": 9,
    "verse": 5,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 5,
    "text": "на ней плачут."
  },
  {
    "chapter": 9,
    "verse": 5,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 6,
    "text": "Вся земля поднимается,"
  },
  {
    "chapter": 9,
    "verse": 5,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 7,
    "text": "словно Нил,"
  },
  {
    "chapter": 9,
    "verse": 5,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 8,
    "text": "и опускается,"
  },
  {
    "chapter": 9,
    "verse": 5,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 9,
    "text": "словно река Египта -"
  },
  {
    "chapter": 9,
    "verse": 6,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 1,
    "text": "Он воздвиг Свой"
  },
  {
    "chapter": 9,
    "verse": 6,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 2,
    "text": "величественный дворец на"
  },
  {
    "chapter": 9,
    "verse": 6,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 3,
    "text": "небесах"
  },
  {
    "chapter": 9,
    "verse": 6,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 4,
    "text": "и основания егоутвердил на"
  },
  {
    "chapter": 9,
    "verse": 6,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 5,
    "text": "земле,"
  },
  {
    "chapter": 9,
    "verse": 6,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 6,
    "text": "призывающий воды морские"
  },
  {
    "chapter": 9,
    "verse": 6,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 7,
    "text": "и разливающий"
  },
  {
    "chapter": 9,
    "verse": 6,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 8,
    "text": "их по лицу земли Господь"
  },
  {
    "chapter": 9,
    "verse": 6,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 9,
    "text": "Его Имя."
  },
  {
    "chapter": 9,
    "verse": 7,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 1,
    "text": "- Разве вы,"
  },
  {
    "chapter": 9,
    "verse": 7,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 2,
    "text": "израильтяне,"
  },
  {
    "chapter": 9,
    "verse": 7,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 3,
    "text": "для Меня не то же,"
  },
  {
    "chapter": 9,
    "verse": 7,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 4,
    "text": "что и кушиты?"
  },
  {
    "chapter": 9,
    "verse": 7,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 5,
    "text": "возвещает Господь."
  },
  {
    "chapter": 9,
    "verse": 7,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 6,
    "text": "Да,"
  },
  {
    "chapter": 9,
    "verse": 7,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 7,
    "text": "Я вывел Израиль из Египта,"
  },
  {
    "chapter": 9,
    "verse": 7,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 8,
    "text": "но Я вывел и"
  },
  {
    "chapter": 9,
    "verse": 7,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 9,
    "text": "филистимлян из Кафтора,"
  },
  {
    "chapter": 9,
    "verse": 7,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 10,
    "text": "и сирийцев из Кира?"
  },
  {
    "chapter": 9,
    "verse": 8,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 1,
    "text": "Несомненно,"
  },
  {
    "chapter": 9,
    "verse": 8,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 2,
    "text": "очи Владыки Господа"
  },
  {
    "chapter": 9,
    "verse": 8,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 3,
    "text": "обращены на грешное царство."
  },
  {
    "chapter": 9,
    "verse": 8,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 4,
    "text": "Я сотру его с лица земли,"
  },
  {
    "chapter": 9,
    "verse": 8,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 5,
    "text": "но не истреблю до"
  },
  {
    "chapter": 9,
    "verse": 8,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 6,
    "text": "конца дом Иакова,"
  },
  {
    "chapter": 9,
    "verse": 8,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 7,
    "text": "возвещает Господь."
  },
  {
    "chapter": 9,
    "verse": 8,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 8,
    "text": "-"
  },
  {
    "chapter": 9,
    "verse": 9,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 1,
    "text": "Я дам повеление просеять"
  },
  {
    "chapter": 9,
    "verse": 9,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 2,
    "text": "дом Израиля среди всех"
  },
  {
    "chapter": 9,
    "verse": 9,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 3,
    "text": "народов,"
  },
  {
    "chapter": 9,
    "verse": 9,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 4,
    "text": "как просеивают"
  },
  {
    "chapter": 9,
    "verse": 9,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 5,
    "text": "зерно решетом,"
  },
  {
    "chapter": 9,
    "verse": 9,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 6,
    "text": "и ни одно не"
  },
  {
    "chapter": 9,
    "verse": 9,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 7,
    "text": "достигнет земли."
  },
  {
    "chapter": 9,
    "verse": 10,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 1,
    "text": "Все грешники среди Моего"
  },
  {
    "chapter": 9,
    "verse": 10,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 2,
    "text": "народа умрут от меча все те,"
  },
  {
    "chapter": 9,
    "verse": 10,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 3,
    "text": "кто говорит:"
  },
  {
    "chapter": 9,
    "verse": 10,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 4,
    "text": "Беда нас не постигнет,"
  },
  {
    "chapter": 9,
    "verse": 10,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 5,
    "text": "нас не коснется."
  },
  {
    "chapter": 9,
    "verse": 11,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 1,
    "text": "- В тот день Я"
  },
  {
    "chapter": 9,
    "verse": 11,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 2,
    "text": "восстановлю павшую скинию Давида."
  },
  {
    "chapter": 9,
    "verse": 11,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 3,
    "text": "Заделаю в нем бреши,"
  },
  {
    "chapter": 9,
    "verse": 11,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 4,
    "text": "восстановлю руины,"
  },
  {
    "chapter": 9,
    "verse": 11,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 5,
    "text": "отстрою,"
  },
  {
    "chapter": 9,
    "verse": 11,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 6,
    "text": "как в дни древние,"
  },
  {
    "chapter": 9,
    "verse": 12,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 1,
    "text": "чтобы им обладать"
  },
  {
    "chapter": 9,
    "verse": 12,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 2,
    "text": "остатком Едома и всеми народами,"
  },
  {
    "chapter": 9,
    "verse": 12,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 3,
    "text": "которые были"
  },
  {
    "chapter": 9,
    "verse": 12,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 4,
    "text": "названы Моим Именем."
  },
  {
    "chapter": 9,
    "verse": 12,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 5,
    "text": "Так говорит Господь,"
  },
  {
    "chapter": 9,
    "verse": 12,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 6,
    "text": "Который сделает это."
  },
  {
    "chapter": 9,
    "verse": 13,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 1,
    "text": "Близятся дни,"
  },
  {
    "chapter": 9,
    "verse": 13,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 2,
    "text": "возвещает Господь,"
  },
  {
    "chapter": 9,
    "verse": 13,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 3,
    "text": "когда жнеца"
  },
  {
    "chapter": 9,
    "verse": 13,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 4,
    "text": "будет сменять пахарь,"
  },
  {
    "chapter": 9,
    "verse": 13,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 5,
    "text": "а сажающего"
  },
  {
    "chapter": 9,
    "verse": 13,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 6,
    "text": "виноградник топчущий ягоды."
  },
  {
    "chapter": 9,
    "verse": 13,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 7,
    "text": "Молодое вино будет сочиться"
  },
  {
    "chapter": 9,
    "verse": 13,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 8,
    "text": "из гор"
  },
  {
    "chapter": 9,
    "verse": 13,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 9,
    "text": "и течь со всех холмов."
  },
  {
    "chapter": 9,
    "verse": 14,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 1,
    "text": "Я возвращу из"
  },
  {
    "chapter": 9,
    "verse": 14,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 2,
    "text": "плена народ Мой,"
  },
  {
    "chapter": 9,
    "verse": 14,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 3,
    "text": "Израиль;"
  },
  {
    "chapter": 9,
    "verse": 14,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 4,
    "text": "они отстроят разрушенные"
  },
  {
    "chapter": 9,
    "verse": 14,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 5,
    "text": "города и будут в них жить."
  },
  {
    "chapter": 9,
    "verse": 14,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 6,
    "text": "Они посадят"
  },
  {
    "chapter": 9,
    "verse": 14,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 7,
    "text": "виноградники и будут пить их вино;"
  },
  {
    "chapter": 9,
    "verse": 14,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 8,
    "text": "разведут сады и"
  },
  {
    "chapter": 9,
    "verse": 14,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 9,
    "text": "будут есть их плоды."
  },
  {
    "chapter": 9,
    "verse": 15,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 1,
    "text": "Насажу Мой"
  },
  {
    "chapter": 9,
    "verse": 15,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 2,
    "text": "народ на его земле,"
  },
  {
    "chapter": 9,
    "verse": 15,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 3,
    "text": "никогда впредь он не"
  },
  {
    "chapter": 9,
    "verse": 15,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 4,
    "text": "будет исторгнут из земли,"
  },
  {
    "chapter": 9,
    "verse": 15,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 5,
    "text": "которую Я дал им,"
  },
  {
    "chapter": 9,
