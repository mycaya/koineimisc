[
  {
    "chapter": 11,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Тогда ответил"
  },
  {
    "chapter": 11,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Софар из Наамы:"
  },
  {
    "chapter": 11,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "- Остаться ли множеству"
  },
  {
    "chapter": 11,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "слов без ответа? Должен ли"
  },
  {
    "chapter": 11,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "быть краснобай оправдан?"
  },
  {
    "chapter": 11,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Разве болтовня твоя"
  },
  {
    "chapter": 11,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "заставит людей замолчать?"
  },
  {
    "chapter": 11,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Неужели не посрамят тебя,"
  },
  {
    "chapter": 11,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "когда ты глумишься?"
  },
  {
    "chapter": 11,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Ты говоришь:"
  },
  {
    "chapter": 11,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Учение мое верно,"
  },
  {
    "chapter": 11,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и чист я в Твоих глазах."
  },
  {
    "chapter": 11,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Неужто? О если"
  },
  {
    "chapter": 11,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "бы Бог ответил,"
  },
  {
    "chapter": 11,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "открыл бы против тебя уста"
  },
  {
    "chapter": 11,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "и сокровенную"
  },
  {
    "chapter": 11,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "мудрость тебе открыл,"
  },
  {
    "chapter": 11,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "ведь двулика"
  },
  {
    "chapter": 11,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "подлинная премудрость."
  },
  {
    "chapter": 11,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "Знай же,"
  },
  {
    "chapter": 11,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 6,
    "text": "что часть твоих"
  },
  {
    "chapter": 11,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 7,
    "text": "грехов Бог предал забвению."
  },
  {
    "chapter": 11,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Можешь ли Божьи глубины"
  },
  {
    "chapter": 11,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "постигнуть? До конца"
  },
  {
    "chapter": 11,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "познаешь ли Всемогущего?"
  },
  {
    "chapter": 11,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Он выше небес что ты можешь"
  },
  {
    "chapter": 11,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "сделать? Глубже мира"
  },
  {
    "chapter": 11,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "мертвых что ты узнаешь?"
  },
  {
    "chapter": 11,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Мера длиннее земли,"
  },
  {
    "chapter": 11,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "шире моря."
  },
  {
    "chapter": 11,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Если пройдет Он,"
  },
  {
    "chapter": 11,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "бросит в темницу и"
  },
  {
    "chapter": 11,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "созовет суд кто Ему помешает?"
  },
  {
    "chapter": 11,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Потому что известно Ему,"
  },
  {
    "chapter": 11,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "кто лжив;"
  },
  {
    "chapter": 11,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Он видит зло неужели"
  },
  {
    "chapter": 11,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "оставит его без внимания?"
  },
  {
    "chapter": 11,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Но глупец"
  },
  {
    "chapter": 11,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "наберется ума лишь тогда,"
  },
  {
    "chapter": 11,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "когда дикий осел"
  },
  {
    "chapter": 11,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "родится человеком."
  },
  {
    "chapter": 11,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Но если ты сердце Ему"
  },
  {
    "chapter": 11,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "отдашь и руки к Нему прострешь,"
  },
  {
    "chapter": 11,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "если грех,"
  },
  {
    "chapter": 11,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "что в руке у тебя,"
  },
  {
    "chapter": 11,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "отбросишь и прогонишь"
  },
  {
    "chapter": 11,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "порок из своих шатров,"
  },
  {
    "chapter": 11,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "то поднимешь"
  },
  {
    "chapter": 11,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "лицо свое без стыда,"
  },
  {
    "chapter": 11,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "встанешь прямо и"
  },
  {
    "chapter": 11,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "не будешь бояться."
  },
  {
    "chapter": 11,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Тогда ты"
  },
  {
    "chapter": 11,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "несчастье свое забудешь,"
  },
  {
    "chapter": 11,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "будешь помнить о нем,"
  },
  {
    "chapter": 11,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "как об утекшей воде."
  },
  {
    "chapter": 11,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Жизнь твоя"
  },
  {
    "chapter": 11,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "станет яснее полдня,"
  },
  {
    "chapter": 11,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "а тьму рассеет заря."
  },
  {
    "chapter": 11,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Ты будешь спокоен,"
  },
  {
    "chapter": 11,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "ведь есть надежда."
  },
  {
    "chapter": 11,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Ты огражден,"
  },
  {
    "chapter": 11,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "будешь спать безопасно."
  },
  {
    "chapter": 11,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Будешь ложиться,"
  },
  {
    "chapter": 11,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и некому будет"
  },
  {
    "chapter": 11,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "тебя страшить."
  },
  {
    "chapter": 11,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "И многие будут"
  },
  {
    "chapter": 11,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "заискивать перед тобой."
  },
  {
    "chapter": 11,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "А глаза"
  },
  {
    "chapter": 11,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "нечестивых померкнут,"
  },
  {
    "chapter": 11,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и убежища им не будет."
  },
  {
    "chapter": 11,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "Их надежда"
  },
  {
    "chapter": 11,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "предсмертный вздох."
  }
]