[
  {
    "chapter": 3,
    "verse": 1,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 1,
    "text": "- Слушай это слово,"
  },
  {
    "chapter": 3,
    "verse": 1,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 2,
    "text": "которое Господь"
  },
  {
    "chapter": 3,
    "verse": 1,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 3,
    "text": "изрек против тебя,"
  },
  {
    "chapter": 3,
    "verse": 1,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 4,
    "text": "о народ Израиля,"
  },
  {
    "chapter": 3,
    "verse": 1,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 5,
    "text": "против всех родов,"
  },
  {
    "chapter": 3,
    "verse": 1,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 6,
    "text": "которые Я вывел из Египта:"
  },
  {
    "chapter": 3,
    "verse": 2,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 1,
    "text": "Вы единственные,"
  },
  {
    "chapter": 3,
    "verse": 2,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 2,
    "text": "кого Я познал из"
  },
  {
    "chapter": 3,
    "verse": 2,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 3,
    "text": "всех народов земли;"
  },
  {
    "chapter": 3,
    "verse": 2,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 4,
    "text": "поэтому Я накажу"
  },
  {
    "chapter": 3,
    "verse": 2,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 5,
    "text": "вас за все ваши грехи."
  },
  {
    "chapter": 3,
    "verse": 3,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 1,
    "text": "Разве пойдут двое вместе,"
  },
  {
    "chapter": 3,
    "verse": 3,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 2,
    "text": "если не сговорятся заранее?"
  },
  {
    "chapter": 3,
    "verse": 4,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 1,
    "text": "Разве ревет в чаще лев,"
  },
  {
    "chapter": 3,
    "verse": 4,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 2,
    "text": "когда нет у него добычи?"
  },
  {
    "chapter": 3,
    "verse": 4,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 3,
    "text": "Разве рычит львенок в своем"
  },
  {
    "chapter": 3,
    "verse": 4,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 4,
    "text": "логове,"
  },
  {
    "chapter": 3,
    "verse": 4,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 5,
    "text": "когда он ничего не поймал?"
  },
  {
    "chapter": 3,
    "verse": 5,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 1,
    "text": "Разве птица"
  },
  {
    "chapter": 3,
    "verse": 5,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 2,
    "text": "попадется в силок,"
  },
  {
    "chapter": 3,
    "verse": 5,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 3,
    "text": "там,"
  },
  {
    "chapter": 3,
    "verse": 5,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 4,
    "text": "где для нее не поставили"
  },
  {
    "chapter": 3,
    "verse": 5,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 5,
    "text": "приманку? Разве захлопнется"
  },
  {
    "chapter": 3,
    "verse": 5,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 6,
    "text": "ловушка на земле,"
  },
  {
    "chapter": 3,
    "verse": 5,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 7,
    "text": "когда нечего ловить?"
  },
  {
    "chapter": 3,
    "verse": 6,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 1,
    "text": "Когда в городе звучит рог,"
  },
  {
    "chapter": 3,
    "verse": 6,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 2,
    "text": "разве народ не трепещет?"
  },
  {
    "chapter": 3,
    "verse": 6,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 3,
    "text": "Когда в город приходит беда,"
  },
  {
    "chapter": 3,
    "verse": 6,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 4,
    "text": "разве не Господь наслал ее?"
  },
  {
    "chapter": 3,
    "verse": 7,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 1,
    "text": "Также Владыка"
  },
  {
    "chapter": 3,
    "verse": 7,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 2,
    "text": "Господь не делает ничего,"
  },
  {
    "chapter": 3,
    "verse": 7,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 3,
    "text": "не открыв Своего"
  },
  {
    "chapter": 3,
    "verse": 7,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 4,
    "text": "замысла слугам Своим,"
  },
  {
    "chapter": 3,
    "verse": 7,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 5,
    "text": "пророкам."
  },
  {
    "chapter": 3,
    "verse": 8,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 1,
    "text": "Лев заревел кто не"
  },
  {
    "chapter": 3,
    "verse": 8,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 2,
    "text": "испугается? Владыка Господь"
  },
  {
    "chapter": 3,
    "verse": 8,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 3,
    "text": "сказал кто не станет"
  },
  {
    "chapter": 3,
    "verse": 8,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 4,
    "text": "пророчествовать?"
  },
  {
    "chapter": 3,
    "verse": 9,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 1,
    "text": "Объявите твердыням"
  },
  {
    "chapter": 3,
    "verse": 9,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 2,
    "text": "Азота и твердыням Египта:"
  },
  {
    "chapter": 3,
    "verse": 9,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 3,
    "text": "Соберитесь на"
  },
  {
    "chapter": 3,
    "verse": 9,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 4,
    "text": "горах Самарии;"
  },
  {
    "chapter": 3,
    "verse": 9,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 5,
    "text": "посмотрите на"
  },
  {
    "chapter": 3,
    "verse": 9,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 6,
    "text": "великие бесчинства в ней,"
  },
  {
    "chapter": 3,
    "verse": 9,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 7,
    "text": "на притеснения"
  },
  {
    "chapter": 3,
    "verse": 9,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 8,
