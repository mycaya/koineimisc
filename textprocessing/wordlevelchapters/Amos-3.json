    "chapter": 3,
    "verse": 10,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 1,
    "text": "- Они не знают,"
  },
  {
    "chapter": 3,
    "verse": 10,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 2,
    "text": "как поступать праведно,"
  },
  {
    "chapter": 3,
    "verse": 10,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 3,
    "text": "возвещает Господь,"
  },
  {
    "chapter": 3,
    "verse": 10,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 4,
    "text": "те,"
  },
  {
    "chapter": 3,
    "verse": 10,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 5,
    "text": "кто копят в своих"
  },
  {
    "chapter": 3,
    "verse": 10,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 6,
    "text": "крепостях добро,"
  },
  {
    "chapter": 3,
    "verse": 10,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 7,
    "text": "собранное"
  },
  {
    "chapter": 3,
    "verse": 10,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 8,
    "text": "насилием и грабежом."
  },
  {
    "chapter": 3,
    "verse": 11,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 1,
    "text": "Поэтому так"
  },
  {
