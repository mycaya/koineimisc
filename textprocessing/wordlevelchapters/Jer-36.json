  },
  {
    "chapter": 36,
    "verse": 29,
    "book_name": "Jeremiah",
    "book_id": "Jer",
    "part": 8,
    "text": "что царь Вавилона"
  },
  {
    "chapter": 36,
    "verse": 29,
    "book_name": "Jeremiah",
    "book_id": "Jer",
