    "verse": 23,
    "book_name": "Jeremiah",
    "book_id": "Jer",
    "part": 3,
    "text": "или четыре столбца свитка,"
  },
  {
    "chapter": 36,
    "verse": 23,
    "book_name": "Jeremiah",
    "book_id": "Jer",
