  },
  {
    "chapter": 36,
    "verse": 30,
    "book_name": "Jeremiah",
    "book_id": "Jer",
    "part": 8,
    "text": "брошено незахороненным на"
  },
  {
    "chapter": 36,
    "verse": 30,
    "book_name": "Jeremiah",
    "book_id": "Jer",
    "part": 9,
    "text": "зной дневной"
  },
  {
    "chapter": 36,
    "verse": 30,
    "book_name": "Jeremiah",
    "book_id": "Jer",
