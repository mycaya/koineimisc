    "chapter": 36,
    "verse": 31,
    "book_name": "Jeremiah",
    "book_id": "Jer",
    "part": 8,
    "text": "которыми Я им грозил,"
  },
  {
    "chapter": 36,
    "verse": 31,
    "book_name": "Jeremiah",
    "book_id": "Jer",
    "part": 9,
    "text": "потому что они"
  },
