  {
    "chapter": 36,
    "verse": 28,
    "book_name": "Jeremiah",
    "book_id": "Jer",
    "part": 4,
    "text": "который сжег Иоаким,"
  },
  {
    "chapter": 36,
    "verse": 28,
    "book_name": "Jeremiah",
    "book_id": "Jer",
