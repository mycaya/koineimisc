  {
    "chapter": 36,
    "verse": 31,
    "book_name": "Jeremiah",
    "book_id": "Jer",
    "part": 1,
    "text": "Я накажу его,"
  },
  {
    "chapter": 36,
    "verse": 31,
    "book_name": "Jeremiah",
    "book_id": "Jer",
    "part": 2,
