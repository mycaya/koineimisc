    "chapter": 23,
    "verse": 15,
    "book_name": "Jeremiah",
    "book_id": "Jer",
    "part": 2,
    "text": "Господь сил о пророках:"
  },
  {
    "chapter": 23,
    "verse": 15,
    "book_name": "Jeremiah",
    "book_id": "Jer",
    "part": 3,
    "text": "Горькой пищей их накормлю,"
  },
