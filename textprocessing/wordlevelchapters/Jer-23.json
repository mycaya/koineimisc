  },
  {
    "chapter": 23,
    "verse": 17,
    "book_name": "Jeremiah",
    "book_id": "Jer",
    "part": 3,
    "text": "Господь говорит:"
  },
  {
    "chapter": 23,
    "verse": 17,
    "book_name": "Jeremiah",
    "book_id": "Jer",
    "part": 4,
