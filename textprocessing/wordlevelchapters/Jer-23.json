  },
  {
    "chapter": 23,
    "verse": 23,
    "book_name": "Jeremiah",
    "book_id": "Jer",
    "part": 4,
    "text": "-"
  },
  {
    "chapter": 23,
    "verse": 24,
    "book_name": "Jeremiah",
    "book_id": "Jer",
    "part": 1,
    "text": "Разве может кто-нибудь"
