  },
  {
    "chapter": 23,
    "verse": 16,
    "book_name": "Jeremiah",
    "book_id": "Jer",
    "part": 1,
    "text": "Так говорит Господь сил:"
  },
  {
    "chapter": 23,
    "verse": 16,
    "book_name": "Jeremiah",
    "book_id": "Jer",
    "part": 2,
