[
  {
    "chapter": 86,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Живущий под кровом"
  },
  {
    "chapter": 86,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Всевышнего в тени"
  },
  {
    "chapter": 86,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Всемогущего покоится."
  },
  {
    "chapter": 86,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Скажу о Господе:"
  },
  {
    "chapter": 86,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Он мое прибежище"
  },
  {
    "chapter": 86,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и крепость моя,"
  },
  {
    "chapter": 86,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Бог мой,"
  },
  {
    "chapter": 86,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "на Которого уповаю."
  },
  {
    "chapter": 86,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он избавит тебя"
  },
  {
    "chapter": 86,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "от сети ловца,"
  },
  {
    "chapter": 86,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и от гибельной язвы."
  },
  {
    "chapter": 86,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он укроет тебя"
  },
  {
    "chapter": 86,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Своими перьями,"
  },
  {
    "chapter": 86,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и под Его крыльями"
  },
  {
    "chapter": 86,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "ты найдешь прибежище."
  },
  {
    "chapter": 86,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Его истина будет"
  },
  {
    "chapter": 86,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "тебе щитом и броней."
  },
  {
    "chapter": 86,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Не убоишься ни"
  },
  {
    "chapter": 86,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "ужасов в ночи,"
  },
  {
    "chapter": 86,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "ни стрелы,"
  },
  {
    "chapter": 86,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "летящей днем,"
  },
  {
    "chapter": 86,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "ни язвы,"
  },
  {
    "chapter": 86,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "ходящей во мраке,"
  },
  {
    "chapter": 86,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "ни заразы,"
  },
  {
    "chapter": 86,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "опустошающей в полдень."
  },
  {
    "chapter": 86,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Тысяча падет около тебя,"
  },
  {
    "chapter": 86,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и десять тысяч"
  },
  {
    "chapter": 86,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "справа от тебя,"
  },
  {
    "chapter": 86,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "но к тебе не приблизится."
  },
  {
    "chapter": 86,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Только глазами своими"
  },
  {
    "chapter": 86,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "будешь смотреть"
  },
  {
    "chapter": 86,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и увидишь возмездие"
  },
  {
    "chapter": 86,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "нечестивым."
  },
  {
    "chapter": 86,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Потому что ты"
  },
  {
    "chapter": 86,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "избрал Господа,"
  },
  {
    "chapter": 86,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "прибежище мое,"
  },
  {
    "chapter": 86,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "своей обителью;"
  },
  {
    "chapter": 86,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "не пристанет к тебе зло,"
  },
  {
    "chapter": 86,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и язва не"
  },
  {
    "chapter": 86,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "приблизится к твоему жилищу."
  },
  {
    "chapter": 86,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Своим ангелам повелит"
  },
  {
    "chapter": 86,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "охранять тебя на всех твоих"
  },
  {
    "chapter": 86,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "путях."
  },
  {
    "chapter": 86,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Они понесут тебя на руках,"
  },
  {
    "chapter": 86,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "чтобы ноги твои не"
  },
  {
    "chapter": 86,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "ударились о камень."
  },
  {
    "chapter": 86,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "На льва и на"
  },
  {
    "chapter": 86,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "змею наступишь,"
  },
  {
    "chapter": 86,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "затопчешь"
  },
  {
    "chapter": 86,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "молодого льва и дракона."
  },
  {
    "chapter": 86,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Сохраню его,"
  },
  {
    "chapter": 86,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "потому что он любит Меня,"
  },
  {
    "chapter": 86,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "защищу его,"
  },
  {
    "chapter": 86,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "потому что он"
  },
  {
    "chapter": 86,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "знает Мое Имя."
  },
  {
    "chapter": 86,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Когда воззовет ко Мне,"
  },
  {
    "chapter": 86,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Я отвечу:"
  },
  {
    "chapter": 86,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "в беде буду с ним,"
  },
  {
    "chapter": 86,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "избавлю его и прославлю."
  },
  {
    "chapter": 86,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Насыщу его долголетием"
  },
  {
    "chapter": 86,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и явлю ему Мое спасение."
  }
]