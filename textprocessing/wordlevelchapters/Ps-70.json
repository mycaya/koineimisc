[
  {
    "chapter": 70,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Наставление Асафа."
  },
  {
    "chapter": 70,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "О Боже,"
  },
  {
    "chapter": 70,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "зачем Ты навсегда отверг"
  },
  {
    "chapter": 70,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "нас? Почему гнев Твой"
  },
  {
    "chapter": 70,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "возгорелся на овец пастбищ"
  },
  {
    "chapter": 70,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Твоих?"
  },
  {
    "chapter": 70,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Вспомни народ,"
  },
  {
    "chapter": 70,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "который Ты"
  },
  {
    "chapter": 70,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "приобрел с давних времен,"
  },
  {
    "chapter": 70,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "который Ты искупил,"
  },
  {
    "chapter": 70,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "чтобы был Твоим наследием;"
  },
  {
    "chapter": 70,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "вспомни гору Сион,"
  },
  {
    "chapter": 70,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "на которой Ты обитаешь."
  },
  {
    "chapter": 70,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Направь Свои шаги к вековым"
  },
  {
    "chapter": 70,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "развалинам все разрушил"
  },
  {
    "chapter": 70,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "враг во святилище!"
  },
  {
    "chapter": 70,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Враги Твои рычали"
  },
  {
    "chapter": 70,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "посреди собрания Твоего,"
  },
  {
    "chapter": 70,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "установили там"
  },
  {
    "chapter": 70,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "свои знамена."
  },
  {
    "chapter": 70,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Словно в чаще"
  },
  {
    "chapter": 70,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "лесной звучали их топоры,"
  },
  {
    "chapter": 70,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "без остатка разрушили"
  },
  {
    "chapter": 70,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "резные стены их секиры"
  },
  {
    "chapter": 70,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и бердыши."
  },
  {
    "chapter": 70,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Они сожгли"
  },
  {
    "chapter": 70,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "святилище Твое дотла,"
  },
  {
    "chapter": 70,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "осквернили они"
  },
  {
    "chapter": 70,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "жилище Имени Твоего."
  },
  {
    "chapter": 70,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Решили они в сердце своем:"
  },
  {
    "chapter": 70,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Уничтожим их полностью"
  },
  {
    "chapter": 70,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и по всей стране сожгли все"
  },
  {
    "chapter": 70,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "места,"
  },
  {
    "chapter": 70,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "где поклонялись мы Тебе."
  },
  {
    "chapter": 70,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Знамений не"
  },
  {
    "chapter": 70,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "видят наши глаза,"
  },
  {
    "chapter": 70,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и не осталось пророков,"
  },
  {
    "chapter": 70,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "нет никого,"
  },
  {
    "chapter": 70,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "кто знал бы,"
  },
  {
    "chapter": 70,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "когда этому наступит конец."
  },
  {
    "chapter": 70,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "О Боже,"
  },
  {
    "chapter": 70,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "как долго еще"
  },
  {
    "chapter": 70,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "будет враг глумиться,"
  },
  {
    "chapter": 70,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и вечно ли будет"
  },
  {
    "chapter": 70,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "противник оскорблять Твое Имя?"
  },
  {
    "chapter": 70,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Почему Ты"
  },
  {
    "chapter": 70,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "убираешь назад Свою руку,"
  },
  {
    "chapter": 70,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Свою правую руку?"
  },
  {
    "chapter": 70,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Извлеки ее на них и порази их!"
  },
  {
    "chapter": 70,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Бог,"
  },
  {
    "chapter": 70,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "мой Царь от начала,"
  },
  {
    "chapter": 70,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Твое спасение"
  },
  {
    "chapter": 70,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "наполняет землю."
  },
  {
    "chapter": 70,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты разделил"
  },
  {
    "chapter": 70,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Твоей силою море,"
  },
  {
    "chapter": 70,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Ты сокрушил"
  },
  {
    "chapter": 70,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "головы морских чудовищ."
  },
  {
    "chapter": 70,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты сокрушил"
  },
  {
    "chapter": 70,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "головы Левиафана,"
  },
  {
    "chapter": 70,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "жителям пустынь"
  },
  {
    "chapter": 70,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "отдав его в пищу."
  },
  {
    "chapter": 70,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты иссек источник и поток,"
  },
  {
    "chapter": 70,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Ты иссушил бегущие реки."
  },
  {
    "chapter": 70,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "День и ночь Твои;"
  },
  {
    "chapter": 70,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Ты создал солнце и луну."
  },
  {
    "chapter": 70,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты определил границы земли,"
  },
  {
    "chapter": 70,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "сотворил лето и зиму."
  },
  {
    "chapter": 70,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Вспомни,"
  },
  {
    "chapter": 70,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 70,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "как глумится враг"
  },
  {
    "chapter": 70,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и как безумный народ"
  },
  {
    "chapter": 70,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "оскорбляет Твое Имя."
  },
  {
    "chapter": 70,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Не отдавай зверям"
  },
  {
    "chapter": 70,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "душу Твоей горлицы;"
  },
  {
    "chapter": 70,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "жизней Твоих"
  },
  {
    "chapter": 70,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "страдальцев не забудь никогда."
  },
  {
    "chapter": 70,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Взгляни на Твой завет,"
  },
  {
    "chapter": 70,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "потому что насилие во"
  },
  {
    "chapter": 70,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "всех темных уголках земли."
  },
  {
    "chapter": 70,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть не возвратится"
  },
  {
    "chapter": 70,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "угнетенный с позором;"
  },
  {
    "chapter": 70,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "пусть бедный и нищий"
  },
  {
    "chapter": 70,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "восхвалят Твое Имя."
  },
  {
    "chapter": 70,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Восстань,"
  },
  {
    "chapter": 70,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Боже,"
  },
  {
    "chapter": 70,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и защити Твое дело;"
  },
  {
    "chapter": 70,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "вспомни,"
  },
  {
    "chapter": 70,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "как глупый"
  },
  {
    "chapter": 70,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "оскорбляет Тебя целый день."
  },
  {
    "chapter": 70,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Не забудь"
  },
  {
    "chapter": 70,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "крика Твоих врагов,"
  },
  {
    "chapter": 70,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "шума противников Твоих,"
  },
  {
    "chapter": 70,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "поднимаемого непрестанно."
  }
]