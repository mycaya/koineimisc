    "chapter": 15,
    "verse": 14,
    "book_name": "Judges",
    "book_id": "Judg",
    "part": 5,
    "text": "стремительно сошел на него,"
  },
  {
    "chapter": 15,
    "verse": 14,
    "book_name": "Judges",
    "book_id": "Judg",
    "part": 6,
    "text": "и веревки на его руках"
