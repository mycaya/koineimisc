[
  {
    "chapter": 15,
    "verse": 1,
    "book_name": "Judges",
    "book_id": "Judg",
    "part": 1,
    "text": "Некоторое время спустя,"
  },
  {
    "chapter": 15,
    "verse": 1,
    "book_name": "Judges",
    "book_id": "Judg",
    "part": 2,
    "text": "во время жатвы пшеницы,"
  },
  {
    "chapter": 15,
    "verse": 1,
    "book_name": "Judges",
    "book_id": "Judg",
    "part": 3,
    "text": "Самсон взял козленка и"
