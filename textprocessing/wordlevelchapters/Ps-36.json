[
  {
    "chapter": 36,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Псалом Давида."
  },
  {
    "chapter": 36,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Мольба."
  },
  {
    "chapter": 36,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господи,"
  },
  {
    "chapter": 36,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "не в ярости упрекай меня"
  },
  {
    "chapter": 36,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и не в гневе наказывай."
  },
  {
    "chapter": 36,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ведь стрелы"
  },
  {
    "chapter": 36,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Твои пронзили меня,"
  },
  {
    "chapter": 36,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и рука Твоя на мне тяжела."
  },
  {
    "chapter": 36,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "От гнева Твоего нет на"
  },
  {
    "chapter": 36,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "теле моем здорового места."
  },
  {
    "chapter": 36,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "От греха моего не"
  },
  {
    "chapter": 36,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "осталось здоровья в костях моих."
  },
  {
    "chapter": 36,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Грехи мои меня поглотили;"
  },
  {
    "chapter": 36,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "они как бремя,"
  },
  {
    "chapter": 36,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "слишком тяжкое для меня."
  },
  {
    "chapter": 36,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Мои раны смердят и"
  },
  {
    "chapter": 36,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "гноятся из-за безумия моего."
  },
  {
    "chapter": 36,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я согбен и совсем поник;"
  },
  {
    "chapter": 36,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "весь день хожу скорбя."
  },
  {
    "chapter": 36,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Палящей болью"
  },
  {
    "chapter": 36,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "полны мои бедра,"
  },
  {
    "chapter": 36,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "нет на теле моем"
  },
  {
    "chapter": 36,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "здорового места."
  },
  {
    "chapter": 36,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я изнемог и"
  },
  {
    "chapter": 36,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "полностью сокрушен,"
  },
  {
    "chapter": 36,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и от муки сердца кричу."
  },
  {
    "chapter": 36,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Все мои"
  },
  {
    "chapter": 36,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "желания перед Тобой,"
  },
  {
    "chapter": 36,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Владыка,"
  },
  {
    "chapter": 36,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и вздохи мои от"
  },
  {
    "chapter": 36,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Тебя не скрыты."
  },
  {
    "chapter": 36,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Сердце мое колотится,"
  },
  {
    "chapter": 36,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "силы мне изменили,"
  },
  {
    "chapter": 36,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "даже свет в"
  },
  {
    "chapter": 36,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "глазах моих меркнет."
  },
  {
    "chapter": 36,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Мои друзья и те,"
  },
  {
    "chapter": 36,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "кто любит меня,"
  },
  {
    "chapter": 36,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "из-за язв меня избегают,"
  },
  {
    "chapter": 36,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и соседи мои стоят вдалеке."
  },
  {
    "chapter": 36,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Те,"
  },
  {
    "chapter": 36,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "кто хочет убить меня,"
  },
  {
    "chapter": 36,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "ставят мне сети;"
  },
  {
    "chapter": 36,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "те,"
  },
  {
    "chapter": 36,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "кто хочет мне зла,"
  },
  {
    "chapter": 36,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "говорят о моей погибели;"
  },
  {
    "chapter": 36,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "целый день они"
  },
  {
    "chapter": 36,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 8,
    "text": "строят козни."
  },
  {
    "chapter": 36,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "А я,"
  },
  {
    "chapter": 36,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "как глухой,"
  },
  {
    "chapter": 36,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "не слышу,"
  },
  {
    "chapter": 36,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "как немой,"
  },
  {
    "chapter": 36,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "не размыкаю уст."
  },
  {
    "chapter": 36,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Да,"
  },
  {
    "chapter": 36,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "я стал как тот,"
  },
  {
    "chapter": 36,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "кто не слышит,"
  },
  {
    "chapter": 36,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "в чьих устах"
  },
  {
    "chapter": 36,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "не найти ответа."
  },
  {
    "chapter": 36,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "На Тебя я,"
  },
  {
    "chapter": 36,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 36,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "уповаю;"
  },
  {
    "chapter": 36,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Ты ответишь,"
  },
  {
    "chapter": 36,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Владыка,"
  },
  {
    "chapter": 36,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "мой Бог."
  },
  {
    "chapter": 36,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я сказал:"
  },
  {
    "chapter": 36,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Не дай им торжествовать"
  },
  {
    "chapter": 36,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и кичиться передо мной,"
  },
  {
    "chapter": 36,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "когда мои ноги колеблются."
  },
  {
    "chapter": 36,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я близок к падению,"
  },
  {
    "chapter": 36,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и боль моя"
  },
  {
    "chapter": 36,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "передо мной всегда."
  },
  {
    "chapter": 36,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я признаю мое беззаконие"
  },
  {
    "chapter": 36,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и скорблю о моем грехе."
  },
  {
    "chapter": 36,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Могущественны и крепки те,"
  },
  {
    "chapter": 36,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "кто враждует со мной;"
  },
  {
    "chapter": 36,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "умножились"
  },
  {
    "chapter": 36,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "ненавидящие меня без причины."
  },
  {
    "chapter": 36,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Злом воздающие мне за"
  },
  {
    "chapter": 36,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "добро враждуют со мною за то,"
  },
  {
    "chapter": 36,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "что добру я следую."
  },
  {
    "chapter": 36,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господи,"
  },
  {
    "chapter": 36,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "не оставь меня! Не"
  },
  {
    "chapter": 36,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "удаляйся от меня,"
  },
  {
    "chapter": 36,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "мой Бог."
  },
  {
    "chapter": 36,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Поспеши мне на помощь,"
  },
  {
    "chapter": 36,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Владыка,"
  },
  {
    "chapter": 36,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "мой Спаситель!"
  }
]