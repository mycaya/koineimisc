[
  {
    "chapter": 22,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Тогда ответил"
  },
  {
    "chapter": 22,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Елифаз из Фемана:"
  },
  {
    "chapter": 22,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "- Может ли человек принести"
  },
  {
    "chapter": 22,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "пользу Богу? Даже самый"
  },
  {
    "chapter": 22,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "разумный может ли принести"
  },
  {
    "chapter": 22,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "Ему пользу?"
  },
  {
    "chapter": 22,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Что за отрада"
  },
  {
    "chapter": 22,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "для Всемогущего,"
  },
  {
    "chapter": 22,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "если бы ты был праведен?"
  },
  {
    "chapter": 22,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "Что за выгода для Него,"
  },
  {
    "chapter": 22,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "даже если бы пути"
  },
  {
    "chapter": 22,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 6,
    "text": "твои были безгрешны?"
  },
  {
    "chapter": 22,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "За благочестие ли Он тебя"
  },
  {
    "chapter": 22,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "осуждает"
  },
  {
    "chapter": 22,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и вступает с тобою в суд?"
  },
  {
    "chapter": 22,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Разве порочность"
  },
  {
    "chapter": 22,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "твоя не безмерна,"
  },
  {
    "chapter": 22,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и проступки не бесконечны?"
  },
  {
    "chapter": 22,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Ты брал без"
  },
  {
    "chapter": 22,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "причины с братьев залог;"
  },
  {
    "chapter": 22,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "ты снимал"
  },
  {
    "chapter": 22,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "одежду с полунагих."
  },
  {
    "chapter": 22,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Ты усталому не давал воды"
  },
  {
    "chapter": 22,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и отказывал в пище"
  },
  {
    "chapter": 22,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "голодному."
  },
  {
    "chapter": 22,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Ты был властным человеком,"
  },
  {
    "chapter": 22,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "владел землей,"
  },
  {
    "chapter": 22,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и знатный на ней селился."
  },
  {
    "chapter": 22,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Ты и вдов отсылал ни с чем,"
  },
  {
    "chapter": 22,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и сирот оставлял"
  },
  {
    "chapter": 22,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "с пустыми руками."
  },
  {
    "chapter": 22,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Потому и сети вокруг тебя,"
  },
  {
    "chapter": 22,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "потому и"
  },
  {
    "chapter": 22,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "внезапный ужас страшит,"
  },
  {
    "chapter": 22,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "потому и глаза"
  },
  {
    "chapter": 22,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "тебе застилает тьма,"
  },
  {
    "chapter": 22,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и разлив многих"
  },
  {
    "chapter": 22,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "вод тебя захлестнул."
  },
  {
    "chapter": 22,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Разве Бог не превыше"
  },
  {
    "chapter": 22,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "небес? Взгляни на звезды,"
  },
  {
    "chapter": 22,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "как они высоки!"
  },
  {
    "chapter": 22,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Но ты говоришь:"
  },
  {
    "chapter": 22,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Что знает Бог? Разве"
  },
  {
    "chapter": 22,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "может судить Он сквозь мглу?"
  },
  {
    "chapter": 22,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Сокрыт облаками,"
  },
  {
    "chapter": 22,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Он нас не видит,"
  },
  {
    "chapter": 22,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "проходя по своду небес."
  },
  {
    "chapter": 22,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Неужели ты"
  },
  {
    "chapter": 22,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "держишься древнего пути,"
  },
  {
    "chapter": 22,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "по которому"
  },
  {
    "chapter": 22,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "шли беззаконники?"
  },
  {
    "chapter": 22,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Они были до"
  },
  {
    "chapter": 22,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "срока истреблены,"
  },
  {
    "chapter": 22,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "их основания унес поток."
  },
  {
    "chapter": 22,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Они говорили Богу:"
  },
  {
    "chapter": 22,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Оставь нас! Что может"
  },
  {
    "chapter": 22,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "сделать нам Всемогущий? -"
  },
  {
    "chapter": 22,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "а Он наполнял"
  },
  {
    "chapter": 22,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "добром их дома."
  },
  {
    "chapter": 22,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Пусть будет далек"
  },
  {
    "chapter": 22,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "от меня совет злых!"
  },
  {
    "chapter": 22,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Увидев их гибель,"
  },
  {
    "chapter": 22,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "ликуют праведные;"
  },
  {
    "chapter": 22,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "непорочные"
  },
  {
    "chapter": 22,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "смеются над ними,"
  },
  {
    "chapter": 22,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "говоря:"
  },
  {
    "chapter": 22,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Поистине,"
  },
  {
    "chapter": 22,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "истреблен наш враг,"
  },
  {
    "chapter": 22,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и огонь пожирает его добро."
  },
  {
    "chapter": 22,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Примирись же с Богом,"
  },
  {
    "chapter": 22,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и обретешь мир;"
  },
  {
    "chapter": 22,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "через это придет к"
  },
  {
    "chapter": 22,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "тебе благополучие."
  },
  {
    "chapter": 22,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Прими наставление Его уст"
  },
  {
    "chapter": 22,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и в сердце слова Его"
  },
  {
    "chapter": 22,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "сохрани."
  },
  {
    "chapter": 22,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Если ты"
  },
  {
    "chapter": 22,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "вернешься к Всемогущему,"
  },
  {
    "chapter": 22,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "то обновишься и прогонишь"
  },
  {
    "chapter": 22,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "неправду от своего шатра,"
  },
  {
    "chapter": 22,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "если пылью"
  },
  {
    "chapter": 22,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "сочтешь ты золото,"
  },
  {
    "chapter": 22,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "камнями ущелий"
  },
  {
    "chapter": 22,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "золото из Офира,"
  },
  {
    "chapter": 22,
    "verse": 25,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "то Всемогущий"
  },
  {
    "chapter": 22,
    "verse": 25,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "станет твоим золотом,"
  },
  {
    "chapter": 22,
    "verse": 25,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "твоим серебром отменным."
  },
  {
    "chapter": 22,
    "verse": 26,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Ты возликуешь о Всемогущем,"
  },
  {
    "chapter": 22,
    "verse": 26,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и поднимешь к"
  },
  {
    "chapter": 22,
    "verse": 26,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Богу свое лицо."
  },
  {
    "chapter": 22,
    "verse": 27,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Когда ты помолишься,"
  },
  {
    "chapter": 22,
    "verse": 27,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Он услышит;"
  },
  {
    "chapter": 22,
    "verse": 27,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и ты исполнишь свои обеты."
  },
  {
    "chapter": 22,
    "verse": 28,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Как ты задумаешь,"
  },
  {
    "chapter": 22,
    "verse": 28,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "так и сбудется,"
  },
  {
    "chapter": 22,
    "verse": 28,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и на пути твои"
  },
  {
    "chapter": 22,
    "verse": 28,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "воссияет свет."
  },
  {
    "chapter": 22,
    "verse": 29,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Кто унижен будет,"
  },
  {
    "chapter": 22,
    "verse": 29,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "ты скажешь:"
  },
  {
    "chapter": 22,
    "verse": 29,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Возвысь!,"
  },
  {
    "chapter": 22,
    "verse": 29,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "и спасет Он павшего духом."
  },
  {
    "chapter": 22,
    "verse": 30,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Он спасет даже виновного,"
  },
  {
    "chapter": 22,
    "verse": 30,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "спасен он будет"
  },
  {
    "chapter": 22,
    "verse": 30,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "чистотой твоих рук."
  }
]