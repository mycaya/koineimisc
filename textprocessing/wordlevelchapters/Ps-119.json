[
  {
    "chapter": 119,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Песнь восхождения."
  },
  {
    "chapter": 119,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Те,"
  },
  {
    "chapter": 119,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "кто надеются на Господа,"
  },
  {
    "chapter": 119,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "как гора Сион:"
  },
  {
    "chapter": 119,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "не поколеблются,"
  },
  {
    "chapter": 119,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "пребудут вовек."
  },
  {
    "chapter": 119,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Как горы"
  },
  {
    "chapter": 119,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "окружают Иерусалим,"
  },
  {
    "chapter": 119,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "так Господь окружает"
  },
  {
    "chapter": 119,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Свой народ сейчас и вовеки."
  },
  {
    "chapter": 119,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Жезл нечестивых не"
  },
  {
    "chapter": 119,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "останется над землей праведных,"
  },
  {
    "chapter": 119,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "чтобы праведные не"
  },
  {
    "chapter": 119,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "протянули свои руки к беззаконию."
  },
  {
    "chapter": 119,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Будь добр,"
  },
  {
    "chapter": 119,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 119,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "к добрым и к"
  },
  {
    "chapter": 119,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "правым в сердцах своих."
  },
  {
    "chapter": 119,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но тех,"
  },
  {
    "chapter": 119,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "кто следует кривым путям,"
  },
  {
    "chapter": 119,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "да отвергнет Господь,"
  },
  {
    "chapter": 119,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "вместе с злодеями."
  },
  {
    "chapter": 119,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Мир Израилю!"
  }
]