[
  {
    "chapter": 4,
    "verse": 1,
    "book_name": "Revelation",
    "book_id": "Rev",
    "part": 1,
    "text": "Потом я посмотрел"
  },
  {
    "chapter": 4,
    "verse": 1,
    "book_name": "Revelation",
    "book_id": "Rev",
    "part": 2,
    "text": "и увидел перед собой"
  },
  {
    "chapter": 4,
    "verse": 1,
    "book_name": "Revelation",
    "book_id": "Rev",
    "part": 3,
    "text": "открытую дверь в небеса"
  },
  {
    "chapter": 4,
    "verse": 1,
    "book_name": "Revelation",
    "book_id": "Rev",
    "part": 4,
    "text": "и услышал тот первый голос,"
  },
  {
    "chapter": 4,
    "verse": 1,
    "book_name": "Revelation",
    "book_id": "Rev",
    "part": 5,
    "text": "напоминающий звук трубы,"
  },
  {
    "chapter": 4,
    "verse": 1,
    "book_name": "Revelation",
    "book_id": "Rev",
    "part": 6,
    "text": "который говорил со мной."
  },
  {
    "chapter": 4,
    "verse": 1,
    "book_name": "Revelation",
    "book_id": "Rev",
