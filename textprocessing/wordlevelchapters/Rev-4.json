  {
    "chapter": 4,
    "verse": 6,
    "book_name": "Revelation",
    "book_id": "Rev",
    "part": 4,
    "text": "искрящегося,"
  },
  {
    "chapter": 4,
    "verse": 6,
    "book_name": "Revelation",
    "book_id": "Rev",
    "part": 5,
    "text": "как кристалл,"
  },
