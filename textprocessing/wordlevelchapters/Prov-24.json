[
  {
    "chapter": 24,
    "verse": 1,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 1,
    "text": "Не завидуй"
  },
  {
    "chapter": 24,
    "verse": 1,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 2,
    "text": "неправедным людям,"
  },
  {
    "chapter": 24,
    "verse": 1,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 3,
    "text": "к обществу их не стремись,"
  },
  {
    "chapter": 24,
    "verse": 2,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 1,
    "text": "ведь насилие у них на уме,"
  },
  {
    "chapter": 24,
    "verse": 2,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 2,
    "text": "и уста говорят"
  },
  {
    "chapter": 24,
    "verse": 2,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 3,
    "text": "о злодействе."
  },
  {
    "chapter": 24,
    "verse": 3,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 1,
    "text": "Мудростью строится дом"
  },
  {
    "chapter": 24,
    "verse": 3,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 2,
    "text": "и разумом утверждается;"
  },
  {
    "chapter": 24,
    "verse": 4,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 1,
    "text": "познание наполняет его"
  },
  {
    "chapter": 24,
    "verse": 4,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 2,
    "text": "комнаты дорогим"
  },
  {
    "chapter": 24,
    "verse": 4,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 3,
    "text": "и красивым добром."
  },
  {
    "chapter": 24,
    "verse": 5,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 1,
    "text": "Мудрый человек силен,"
  },
  {
    "chapter": 24,
    "verse": 5,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 2,
    "text": "и тот,"
  },
  {
    "chapter": 24,
    "verse": 5,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 3,
    "text": "кто сведущ,"
  },
  {
    "chapter": 24,
    "verse": 5,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 4,
    "text": "увеличивает свою силу;"
  },
  {
    "chapter": 24,
    "verse": 6,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 1,
    "text": "чтобы воевать,"
  },
  {
    "chapter": 24,
    "verse": 6,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 2,
    "text": "тебе нужен мудрый совет,"
  },
  {
    "chapter": 24,
    "verse": 6,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 3,
    "text": "и для победы"
  },
  {
    "chapter": 24,
    "verse": 6,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 4,
    "text": "много советников."
  },
  {
    "chapter": 24,
    "verse": 7,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 1,
    "text": "Мудрость слишком"
  },
  {
    "chapter": 24,
    "verse": 7,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 2,
    "text": "возвышенна для глупцов;"
  },
  {
    "chapter": 24,
    "verse": 7,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 3,
    "text": "в собрании городских"
  },
  {
    "chapter": 24,
    "verse": 7,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 4,
    "text": "старейшин нечего им сказать."
  },
  {
    "chapter": 24,
    "verse": 8,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 1,
    "text": "Кто замышляет дурное,"
  },
  {
    "chapter": 24,
    "verse": 8,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 2,
    "text": "того назовут"
  },
  {
    "chapter": 24,
    "verse": 8,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 3,
    "text": "злоумышленником."
  },
  {
    "chapter": 24,
    "verse": 9,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 1,
    "text": "Замыслы глупости грех,"
  },
  {
    "chapter": 24,
    "verse": 9,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 2,
    "text": "и глумливым гнушаются люди."
  },
  {
    "chapter": 24,
    "verse": 10,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 1,
    "text": "Если ты дал"
  },
  {
    "chapter": 24,
    "verse": 10,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 2,
    "text": "слабину в день беды,"
  },
  {
    "chapter": 24,
    "verse": 10,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 3,
    "text": "сила твоя ничтожна;"
  },
  {
    "chapter": 24,
    "verse": 10,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 4,
    "text": "-"
  },
  {
    "chapter": 24,
    "verse": 11,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 1,
    "text": "если ты"
  },
  {
    "chapter": 24,
    "verse": 11,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 2,
    "text": "воздержался и не избавил тех,"
  },
  {
    "chapter": 24,
    "verse": 11,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 3,
    "text": "кто на смерть взят,"
  },
  {
    "chapter": 24,
    "verse": 11,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 4,
    "text": "кто бредет,"
  },
  {
    "chapter": 24,
    "verse": 11,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 5,
    "text": "спотыкаясь,"
  },
  {
    "chapter": 24,
    "verse": 11,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 6,
    "text": "на бойню,"
  },
  {
    "chapter": 24,
    "verse": 12,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 1,
    "text": "если ты говоришь:"
  },
  {
    "chapter": 24,
    "verse": 12,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 2,
    "text": "Ой,"
  },
  {
    "chapter": 24,
    "verse": 12,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 3,
    "text": "мы об этом не знали,"
  },
  {
    "chapter": 24,
    "verse": 12,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 4,
    "text": "то разве Взвешивающий"
  },
  {
    "chapter": 24,
    "verse": 12,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 5,
    "text": "сердца не поймет? Разве"
  },
  {
    "chapter": 24,
    "verse": 12,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 6,
    "text": "Хранящий твою жизнь не"
  },
  {
    "chapter": 24,
    "verse": 12,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 7,
    "text": "узнает? И разве Он не"
  },
  {
    "chapter": 24,
    "verse": 12,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 8,
    "text": "воздаст каждому по делам?"
  },
  {
    "chapter": 24,
    "verse": 13,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 1,
    "text": "Ешь мед,"
  },
  {
    "chapter": 24,
    "verse": 13,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 2,
    "text": "мой сын,"
  },
  {
    "chapter": 24,
    "verse": 13,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 3,
    "text": "ведь он хорош;"
  },
  {
    "chapter": 24,
    "verse": 13,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 4,
    "text": "сотовый мед тебе сладок."
  },
  {
    "chapter": 24,
    "verse": 14,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 1,
    "text": "Знай:"
  },
  {
    "chapter": 24,
    "verse": 14,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 2,
    "text": "такова душе твоей мудрость;"
  },
  {
    "chapter": 24,
    "verse": 14,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 3,
    "text": "если найдешь ее"
  },
  {
    "chapter": 24,
    "verse": 14,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 4,
    "text": "есть у тебя будущее,"
  },
  {
    "chapter": 24,
    "verse": 14,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 5,
    "text": "и надежда твоя не погибнет."
  },
  {
    "chapter": 24,
    "verse": 15,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 1,
    "text": "Не устраивай,"
  },
  {
    "chapter": 24,
    "verse": 15,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 2,
    "text": "как разбойник,"
  },
  {
    "chapter": 24,
    "verse": 15,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 3,
    "text": "засады у дома праведника,"
  },
  {
    "chapter": 24,
    "verse": 15,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 4,
    "text": "не разоряй его крова,"
  },
  {
    "chapter": 24,
    "verse": 16,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 1,
    "text": "ведь праведник,"
  },
  {
    "chapter": 24,
    "verse": 16,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 2,
    "text": "пусть семь раз упадет,"
  },
  {
    "chapter": 24,
    "verse": 16,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 3,
    "text": "поднимется,"
  },
  {
    "chapter": 24,
    "verse": 16,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 4,
    "text": "а злодеев беда погубит."
  },
  {
    "chapter": 24,
    "verse": 17,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 1,
    "text": "Не веселись,"
  },
  {
    "chapter": 24,
    "verse": 17,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 2,
    "text": "когда враг твой упал;"
  },
  {
    "chapter": 24,
    "verse": 17,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 3,
    "text": "когда он споткнулся,"
  },
  {
    "chapter": 24,
    "verse": 17,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 4,
    "text": "пусть сердце"
  },
  {
    "chapter": 24,
    "verse": 17,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 5,
    "text": "твое не ликует,"
  },
  {
    "chapter": 24,
    "verse": 18,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 1,
    "text": "иначе Господь увидит,"
  },
  {
    "chapter": 24,
    "verse": 18,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 2,
    "text": "это будет Ему неугодно"
  },
  {
    "chapter": 24,
    "verse": 18,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 3,
    "text": "и Он отвратит от него Свой"
  },
  {
    "chapter": 24,
    "verse": 18,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 4,
    "text": "гнев."
  },
  {
    "chapter": 24,
    "verse": 19,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 1,
    "text": "Не негодуй из-за злодеев"
  },
  {
    "chapter": 24,
    "verse": 19,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 2,
    "text": "и не завидуй нечестивым,"
  },
  {
    "chapter": 24,
    "verse": 20,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 1,
    "text": "ведь у злодея нет будущего,"
  },
  {
    "chapter": 24,
    "verse": 20,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 2,
    "text": "и светильник"
  },
  {
    "chapter": 24,
    "verse": 20,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 3,
    "text": "нечестивых погаснет."
  },
  {
    "chapter": 24,
    "verse": 21,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 1,
    "text": "Мой сын,"
  },
  {
    "chapter": 24,
    "verse": 21,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 2,
    "text": "бойся Господа и царя и"
  },
  {
    "chapter": 24,
    "verse": 21,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 3,
    "text": "не общайся с мятежниками."
  },
  {
    "chapter": 24,
    "verse": 22,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 1,
    "text": "Ведь внезапно"
  },
  {
    "chapter": 24,
    "verse": 22,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 2,
    "text": "придет к ним беда,"
  },
  {
    "chapter": 24,
    "verse": 22,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 3,
    "text": "и кто знает,"
  },
  {
    "chapter": 24,
    "verse": 22,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 4,
    "text": "какие невзгоды"
  },
  {
    "chapter": 24,
    "verse": 22,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 5,
    "text": "они им пошлют?"
  },
  {
    "chapter": 24,
    "verse": 23,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 1,
    "text": "Вот еще изречения мудрых:"
  },
  {
    "chapter": 24,
    "verse": 23,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 2,
    "text": "Выказывать"
  },
  {
    "chapter": 24,
    "verse": 23,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 3,
    "text": "лицеприятие на суде нехорошо."
  },
  {
    "chapter": 24,
    "verse": 24,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 1,
    "text": "На сказавшего виноватому:"
  },
  {
    "chapter": 24,
    "verse": 24,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 2,
    "text": "Ты невиновен,"
  },
  {
    "chapter": 24,
    "verse": 24,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 3,
    "text": "будут слать"
  },
  {
    "chapter": 24,
    "verse": 24,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 4,
    "text": "племена проклятья,"
  },
  {
    "chapter": 24,
    "verse": 24,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 5,
    "text": "и народы будут негодовать."
  },
  {
    "chapter": 24,
    "verse": 25,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 1,
    "text": "А те,"
  },
  {
    "chapter": 24,
    "verse": 25,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 2,
    "text": "кто осудят виновного,"
  },
  {
    "chapter": 24,
    "verse": 25,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 3,
    "text": "будут преуспевать,"
  },
  {
    "chapter": 24,
    "verse": 25,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 4,
    "text": "и придет на них"
  },
  {
    "chapter": 24,
    "verse": 25,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 5,
    "text": "доброе благословение."
  },
  {
    "chapter": 24,
    "verse": 26,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 1,
    "text": "Честный ответ,"
  },
  {
    "chapter": 24,
    "verse": 26,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 2,
    "text": "что поцелуй в губы."
  },
  {
    "chapter": 24,
    "verse": 27,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 1,
    "text": "Заверши работу на улице,"
  },
  {
    "chapter": 24,
    "verse": 27,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 2,
    "text": "приведи в порядок поля,"
  },
  {
    "chapter": 24,
    "verse": 27,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 3,
    "text": "а потом можешь"
  },
  {
    "chapter": 24,
    "verse": 27,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 4,
    "text": "строить свой дом."
  },
  {
    "chapter": 24,
    "verse": 28,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 1,
    "text": "Не свидетельствуй против"
  },
  {
    "chapter": 24,
    "verse": 28,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 2,
    "text": "ближнего без вины"
  },
  {
    "chapter": 24,
    "verse": 28,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 3,
    "text": "и не лги своими устами."
  },
  {
    "chapter": 24,
    "verse": 29,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 1,
    "text": "Не говори:"
  },
  {
    "chapter": 24,
    "verse": 29,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 2,
    "text": "Поступлю с ним,"
  },
  {
    "chapter": 24,
    "verse": 29,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 3,
    "text": "как он со мной;"
  },
  {
    "chapter": 24,
    "verse": 29,
    "book_name": "Proverbs",
    "book_id": "Prov",
    "part": 4,
    "text": "отплачу ему за то,"
  },
  {
    "chapter": 24,
    "verse": 29,
