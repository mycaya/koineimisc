[
  {
    "chapter": 28,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Псалом Давида."
  },
  {
    "chapter": 28,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Песнь на освящение храма."
  },
  {
    "chapter": 28,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Превозношу Тебя,"
  },
  {
    "chapter": 28,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 28,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "за то,"
  },
  {
    "chapter": 28,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "что Ты поднял меня"
  },
  {
    "chapter": 28,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "из бездны"
  },
  {
    "chapter": 28,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "и не дал моим врагам"
  },
  {
    "chapter": 28,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "возрадоваться надо мной."
  },
  {
    "chapter": 28,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господи,"
  },
  {
    "chapter": 28,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "мой Бог,"
  },
  {
    "chapter": 28,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "я на помощь Тебя призвал,"
  },
  {
    "chapter": 28,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и Ты меня исцелил."
  },
  {
    "chapter": 28,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господи,"
  },
  {
    "chapter": 28,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Ты вывел душу мою"
  },
  {
    "chapter": 28,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "из мира мертвых."
  },
  {
    "chapter": 28,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Ты сохранил мне жизнь,"
  },
  {
    "chapter": 28,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "чтобы я не"
  },
  {
    "chapter": 28,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "сошел в пропасть."
  },
  {
    "chapter": 28,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пойте Господу,"
  },
  {
    "chapter": 28,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "святые Его,"
  },
  {
    "chapter": 28,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "славьте святое Имя Его!"
  },
  {
    "chapter": 28,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Потому что гнев"
  },
  {
    "chapter": 28,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Его длится миг,"
  },
  {
    "chapter": 28,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "а милость Его всю жизнь."
  },
  {
    "chapter": 28,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "На ночлег останется плач,"
  },
  {
    "chapter": 28,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "а утром приходит радость."
  },
  {
    "chapter": 28,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "В благоденствии я сказал:"
  },
  {
    "chapter": 28,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Вовек не поколеблюсь."
  },
  {
    "chapter": 28,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Милостив Ты был ко мне,"
  },
  {
    "chapter": 28,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 28,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и утвердил меня,"
  },
  {
    "chapter": 28,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "словно гору."
  },
  {
    "chapter": 28,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Но лишь сокрыл"
  },
  {
    "chapter": 28,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Ты Свое лицо,"
  },
  {
    "chapter": 28,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "как ужас объял меня."
  },
  {
    "chapter": 28,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "К Тебе я взывал,"
  },
  {
    "chapter": 28,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи;"
  },
  {
    "chapter": 28,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Владыку о милости я умолял:"
  },
  {
    "chapter": 28,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Что за польза"
  },
  {
    "chapter": 28,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "от смерти моей,"
  },
  {
    "chapter": 28,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "если я низойду в бездну?"
  },
  {
    "chapter": 28,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Будет ли прах Тебя славить?"
  },
  {
    "chapter": 28,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Будет ли верность Твою"
  },
  {
    "chapter": 28,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "возвещать?"
  },
  {
    "chapter": 28,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Услышь,"
  },
  {
    "chapter": 28,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 28,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и помилуй меня!"
  },
  {
    "chapter": 28,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Будь мне помощником,"
  },
  {
    "chapter": 28,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Господи!"
  },
  {
    "chapter": 28,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "В танец радости Ты"
  },
  {
    "chapter": 28,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "обратил мой плач,"
  },
  {
    "chapter": 28,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "снял лохмотья с меня и"
  },
  {
    "chapter": 28,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "весельем меня препоясал,"
  },
  {
    "chapter": 28,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "чтобы сердце мое"
  },
  {
    "chapter": 28,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Тебе пело и не смолкало."
  },
  {
    "chapter": 28,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Господи,"
  },
  {
    "chapter": 28,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "мой Бог,"
  },
  {
    "chapter": 28,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "буду славить Тебя вовеки!"
  }
]