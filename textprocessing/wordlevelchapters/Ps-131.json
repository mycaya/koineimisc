[
  {
    "chapter": 131,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "У рек Вавилона мы"
  },
  {
    "chapter": 131,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "сидели и плакали,"
  },
  {
    "chapter": 131,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "когда вспоминали о Сионе."
  },
  {
    "chapter": 131,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Там,"
  },
  {
    "chapter": 131,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "на тополях,"
  },
  {
    "chapter": 131,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "мы повесили наши арфы."
  },
  {
    "chapter": 131,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Там пленившие нас"
  },
  {
    "chapter": 131,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "требовали от нас песен,"
  },
  {
    "chapter": 131,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "притеснители наши"
  },
  {
    "chapter": 131,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "требовали от нас веселья,"
  },
  {
    "chapter": 131,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "говоря:"
  },
  {
    "chapter": 131,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Спойте нам одну"
  },
  {
    "chapter": 131,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "из песен сионских."
  },
  {
    "chapter": 131,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Как нам петь песнь"
  },
  {
    "chapter": 131,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господа в чужой земле?"
  },
  {
    "chapter": 131,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Если забуду тебя,"
  },
  {
    "chapter": 131,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Иерусалим,"
  },
  {
    "chapter": 131,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "пусть забудет моя"
  },
  {
    "chapter": 131,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "правая рукасвои умения."
  },
  {
    "chapter": 131,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Если не буду помнить тебя,"
  },
  {
    "chapter": 131,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "пусть прилипнет"
  },
  {
    "chapter": 131,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "язык мой к нёбу,"
  },
  {
    "chapter": 131,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "если не поставлю"
  },
  {
    "chapter": 131,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Иерусалим во главе своего веселья."
  },
  {
    "chapter": 131,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Припомни,"
  },
  {
    "chapter": 131,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 131,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "сыновей Едома в день"
  },
  {
    "chapter": 131,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "захвата вавилонянами Иерусалима,"
  },
  {
    "chapter": 131,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "когда они говорили:"
  },
  {
    "chapter": 131,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Разрушайте,"
  },
  {
    "chapter": 131,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "разрушайте его"
  },
  {
    "chapter": 131,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 8,
    "text": "до основания!"
  },
  {
    "chapter": 131,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Дочь Вавилона,"
  },
  {
    "chapter": 131,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "обреченная на разорение:"
  },
  {
    "chapter": 131,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "блажен тот,"
  },
  {
    "chapter": 131,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "кто воздаст тебе"
  },
  {
    "chapter": 131,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "за сделанное с нами."
  },
  {
    "chapter": 131,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Блажен тот,"
  },
  {
    "chapter": 131,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "кто возьмет и разобьет"
  },
  {
    "chapter": 131,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "твоих младенцев о камень."
  }
]