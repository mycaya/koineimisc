[
  {
    "chapter": 38,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Дирижеру хора."
  },
  {
    "chapter": 38,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Псалом Давида."
  },
  {
    "chapter": 38,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Твердо"
  },
  {
    "chapter": 38,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "надеялся я на Господа,"
  },
  {
    "chapter": 38,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и Он склонился ко"
  },
  {
    "chapter": 38,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "мне и услышал мой крик."
  },
  {
    "chapter": 38,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он поднял меня из"
  },
  {
    "chapter": 38,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "страшной пропасти,"
  },
  {
    "chapter": 38,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "из зыбкой трясины."
  },
  {
    "chapter": 38,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Он поставил ноги мои на"
  },
  {
    "chapter": 38,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "камне и стопы мои утвердил."
  },
  {
    "chapter": 38,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он вложил мне в уста"
  },
  {
    "chapter": 38,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "новую песнь хвалу нашему Богу."
  },
  {
    "chapter": 38,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Увидят многие и устрашатся,"
  },
  {
    "chapter": 38,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и будут на Господа уповать."
  },
  {
    "chapter": 38,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Блажен тот человек,"
  },
  {
    "chapter": 38,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "кто надежду свою"
  },
  {
    "chapter": 38,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "возлагает на Господа,"
  },
  {
    "chapter": 38,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "кто не обращается к"
  },
  {
    "chapter": 38,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "гордым и к уклоняющимся ко лжи."
  },
  {
    "chapter": 38,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Многочисленны,"
  },
  {
    "chapter": 38,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 38,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "мой Бог,"
  },
  {
    "chapter": 38,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "чудеса,"
  },
  {
    "chapter": 38,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "которые Ты сотворил,"
  },
  {
    "chapter": 38,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "и замыслы Твои о нас! Кто с"
  },
  {
    "chapter": 38,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "Тобою сравнится! Я бы стал"
  },
  {
    "chapter": 38,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 8,
    "text": "возвещать о них"
  },
  {
    "chapter": 38,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 9,
    "text": "и рассказывать,"
  },
  {
    "chapter": 38,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 10,
    "text": "но их больше,"
  },
  {
    "chapter": 38,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 11,
    "text": "чем можно счесть."
  },
  {
    "chapter": 38,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Не захотел Ты ни жертв,"
  },
  {
    "chapter": 38,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "ни даров,"
  },
  {
    "chapter": 38,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "но Ты открыл мне уши."
  },
  {
    "chapter": 38,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Ты не потребовал"
  },
  {
    "chapter": 38,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "ни всесожжения,"
  },
  {
    "chapter": 38,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "ни жертвы за грех."
  },
  {
    "chapter": 38,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Тогда я сказал:"
  },
  {
    "chapter": 38,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Вот,"
  },
  {
    "chapter": 38,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "я иду,"
  },
  {
    "chapter": 38,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "как и написано в"
  },
  {
    "chapter": 38,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "свитке обо мне."
  },
  {
    "chapter": 38,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Бог мой,"
  },
  {
    "chapter": 38,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "я желаю волю"
  },
  {
    "chapter": 38,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Твою исполнить,"
  },
  {
    "chapter": 38,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и в сердце моем Твой Закон."
  },
  {
    "chapter": 38,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "В большом собрании я"
  },
  {
    "chapter": 38,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "возвещал Твою праведность;"
  },
  {
    "chapter": 38,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "я не удерживал моих"
  },
  {
    "chapter": 38,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "уст Ты это знаешь,"
  },
  {
    "chapter": 38,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Господи."
  },
  {
    "chapter": 38,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я не скрыл Твоей"
  },
  {
    "chapter": 38,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "праведности в своем сердце,"
  },
  {
    "chapter": 38,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "но я возвещал"
  },
  {
    "chapter": 38,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "верность Твою и спасение."
  },
  {
    "chapter": 38,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Я не таил Твою милость и"
  },
  {
    "chapter": 38,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "истину перед большим"
  },
  {
    "chapter": 38,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "собранием."
  },
  {
    "chapter": 38,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Не удерживай,"
  },
  {
    "chapter": 38,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 38,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "щедрот Твоих от меня,"
  },
  {
    "chapter": 38,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "пусть милость Твоя и истина"
  },
  {
    "chapter": 38,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "охраняют меня непрестанно."
  },
  {
    "chapter": 38,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ведь беды окружили,"
  },
  {
    "chapter": 38,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и нет им числа;"
  },
  {
    "chapter": 38,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "грехи мои мной овладели,"
  },
  {
    "chapter": 38,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и не могу я видеть."
  },
  {
    "chapter": 38,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Их больше,"
  },
  {
    "chapter": 38,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "чем волос на моей голове;"
  },
  {
    "chapter": 38,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "храбрость меня оставила."
  },
  {
    "chapter": 38,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть будет угодно Тебе,"
  },
  {
    "chapter": 38,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 38,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "избавить меня! Господи,"
  },
  {
    "chapter": 38,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "поспеши мне на помощь!"
  },
  {
    "chapter": 38,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть все,"
  },
  {
    "chapter": 38,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "кто ищет жизни моей,"
  },
  {
    "chapter": 38,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "будут"
  },
  {
    "chapter": 38,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "пристыжены и посрамлены."
  },
  {
    "chapter": 38,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Пусть все,"
  },
  {
    "chapter": 38,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "кто хочет моей погибели,"
  },
  {
    "chapter": 38,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "в бесчестии повернут назад."
  },
  {
    "chapter": 38,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть ужаснутся своему"
  },
  {
    "chapter": 38,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "позору говорящие мне:"
  },
  {
    "chapter": 38,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Ага! Ага!"
  },
  {
    "chapter": 38,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть ликуют и радуются"
  },
  {
    "chapter": 38,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "о Тебе все ищущие Тебя."
  },
  {
    "chapter": 38,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Пусть те,"
  },
  {
    "chapter": 38,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "кто любит Твое спасение,"
  },
  {
    "chapter": 38,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "говорят непрестанно:"
  },
  {
    "chapter": 38,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Велик Господь!"
  },
  {
    "chapter": 38,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я же беден и нищ,"
  },
  {
    "chapter": 38,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "пусть Владыка"
  },
  {
    "chapter": 38,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "позаботится обо мне."
  },
  {
    "chapter": 38,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Ты помощь моя и"
  },
  {
    "chapter": 38,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "мой избавитель."
  },
  {
    "chapter": 38,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Бог мой,"
  },
  {
    "chapter": 38,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "не замедли!"
  }
]