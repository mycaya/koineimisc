[
  {
    "chapter": 49,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Дирижеру хора."
  },
  {
    "chapter": 49,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Наставление Давида,"
  },
  {
    "chapter": 49,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Об обличении"
  },
  {
    "chapter": 49,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Давидом идумеянина Доика,"
  },
  {
    "chapter": 49,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "после того,"
  },
  {
    "chapter": 49,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "как тот предал Давида,"
  },
  {
    "chapter": 49,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "сказав Саулу:"
  },
  {
    "chapter": 49,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Давид в доме Ахимелеха."
  },
  {
    "chapter": 49,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Что хвалишься злодейством,"
  },
  {
    "chapter": 49,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "сильный? Весь день"
  },
  {
    "chapter": 49,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "со мной милость Божья!"
  },
  {
    "chapter": 49,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Твой язык замышляет гибель;"
  },
  {
    "chapter": 49,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "он подобен"
  },
  {
    "chapter": 49,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "отточенной бритве,"
  },
  {
    "chapter": 49,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "о коварный."
  },
  {
    "chapter": 49,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Зло ты любишь больше добра,"
  },
  {
    "chapter": 49,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и ложь сильнее,"
  },
  {
    "chapter": 49,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "чем слова правды."
  },
  {
    "chapter": 49,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Пауза"
  },
  {
    "chapter": 49,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты любишь гибельные слова,"
  },
  {
    "chapter": 49,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "о ты,"
  },
  {
    "chapter": 49,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "язык вероломный!"
  },
  {
    "chapter": 49,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но Бог погубит тебя навек;"
  },
  {
    "chapter": 49,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Он схватит тебя и"
  },
  {
    "chapter": 49,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "изринет прочь из шатра,"
  },
  {
    "chapter": 49,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "исторгнет твой"
  },
  {
    "chapter": 49,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "корень из земли живых."
  },
  {
    "chapter": 49,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Пауза"
  },
  {
    "chapter": 49,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Увидят"
  },
  {
    "chapter": 49,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "праведники и устрашатся,"
  },
  {
    "chapter": 49,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "посмеются над ним,"
  },
  {
    "chapter": 49,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "говоря:"
  },
  {
    "chapter": 49,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Вот человек,"
  },
  {
    "chapter": 49,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "который не сделал"
  },
  {
    "chapter": 49,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Бога своей крепостью,"
  },
  {
    "chapter": 49,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "а верил в свои"
  },
  {
    "chapter": 49,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "сокровища и укреплялся,"
  },
  {
    "chapter": 49,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "уничтожая других!"
  },
  {
    "chapter": 49,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "А я подобен маслине,"
  },
  {
    "chapter": 49,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "зеленеющей в Божьем доме,"
  },
  {
    "chapter": 49,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "я верю в Божью"
  },
  {
    "chapter": 49,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "милость вовеки."
  },
  {
    "chapter": 49,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Буду славить Тебя"
  },
  {
    "chapter": 49,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "вовек за сделанное Тобой."
  },
  {
    "chapter": 49,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Перед Твоими верными я"
  },
  {
    "chapter": 49,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "буду уповать на Твое Имя,"
  },
  {
    "chapter": 49,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "потому что оно прекрасно."
  }
]