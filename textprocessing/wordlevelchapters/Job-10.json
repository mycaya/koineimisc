[
  {
    "chapter": 10,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Мне опротивела жизнь;"
  },
  {
    "chapter": 10,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "дам волю моей жалобе,"
  },
  {
    "chapter": 10,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "в горечи души"
  },
  {
    "chapter": 10,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "буду говорить."
  },
  {
    "chapter": 10,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Я Богу скажу:"
  },
  {
    "chapter": 10,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Не осуждай меня,"
  },
  {
    "chapter": 10,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "но скажи же,"
  },
  {
    "chapter": 10,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "что Ты против меня имеешь?"
  },
  {
    "chapter": 10,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Или Тебе"
  },
  {
    "chapter": 10,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "нравится быть жестоким,"
  },
  {
    "chapter": 10,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "презирать"
  },
  {
    "chapter": 10,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "созданье Твоих рук,"
  },
  {
    "chapter": 10,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "одобряя замыслы нечестивых?"
  },
  {
    "chapter": 10,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Разве плотские у Тебя"
  },
  {
    "chapter": 10,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "глаза? Разве смотришь Ты,"
  },
  {
    "chapter": 10,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "словно смертный смотрит?"
  },
  {
    "chapter": 10,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Разве Твои дни,"
  },
  {
    "chapter": 10,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "как у смертных,"
  },
  {
    "chapter": 10,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и года,"
  },
  {
    "chapter": 10,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "как у человека,"
  },
  {
    "chapter": 10,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "что Ты торопишься найти мою"
  },
  {
    "chapter": 10,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "вину"
  },
  {
    "chapter": 10,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и выискиваешь мой грех -"
  },
  {
    "chapter": 10,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "хоть и знаешь,"
  },
  {
    "chapter": 10,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "что я невиновен? Никто"
  },
  {
    "chapter": 10,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "меня от Тебя не спасет."
  },
  {
    "chapter": 10,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Руки Твои"
  },
  {
    "chapter": 10,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "вылепили и создали меня,"
  },
  {
    "chapter": 10,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "а Ты,"
  },
  {
    "chapter": 10,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "обратившись,"
  },
  {
    "chapter": 10,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "губишь меня?"
  },
  {
    "chapter": 10,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Вспомни,"
  },
  {
    "chapter": 10,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "прошу,"
  },
  {
    "chapter": 10,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Ты вылепил меня,"
  },
  {
    "chapter": 10,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "как из глины."
  },
  {
    "chapter": 10,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "Неужели ныне"
  },
  {
    "chapter": 10,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 6,
    "text": "вернешь меня в прах?"
  },
  {
    "chapter": 10,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Не Ты ли излил меня,"
  },
  {
    "chapter": 10,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "как молоко,"
  },
  {
    "chapter": 10,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и сгустил меня,"
  },
  {
    "chapter": 10,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "как творог;"
  },
  {
    "chapter": 10,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "одел меня кожей и плотью,"
  },
  {
    "chapter": 10,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "скрепил костями и жилами?"
  },
  {
    "chapter": 10,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Ты мне жизнь даровал"
  },
  {
    "chapter": 10,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и явил мне милость,"
  },
  {
    "chapter": 10,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и Твоей заботой"
  },
  {
    "chapter": 10,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "хранился мой дух."
  },
  {
    "chapter": 10,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Но вот то,"
  },
  {
    "chapter": 10,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "что Ты в"
  },
  {
    "chapter": 10,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "сердце Своем сокрыл,"
  },
  {
    "chapter": 10,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "и я знаю,"
  },
  {
    "chapter": 10,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "что Ты это задумал:"
  },
  {
    "chapter": 10,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "если я согрешу,"
  },
  {
    "chapter": 10,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Ты выследишь и не"
  },
  {
    "chapter": 10,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "отпустишь мне грех."
  },
  {
    "chapter": 10,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Если я виновен горе"
  },
  {
    "chapter": 10,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "мне! Но если и прав,"
  },
  {
    "chapter": 10,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "мне головы не поднять,"
  },
  {
    "chapter": 10,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "потому что горю от"
  },
  {
    "chapter": 10,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "стыда и пресыщен бедствием."
  },
  {
    "chapter": 10,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Если голову подниму,"
  },
  {
    "chapter": 10,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Ты бросаешься на меня,"
  },
  {
    "chapter": 10,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "как лев,"
  },
  {
    "chapter": 10,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "вновь и вновь"
  },
  {
    "chapter": 10,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "поражая меня чудесами."
  },
  {
    "chapter": 10,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Приводишь новых"
  },
  {
    "chapter": 10,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "свидетелей против меня,"
  },
  {
    "chapter": 10,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "умножаешь против"
  },
  {
    "chapter": 10,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "меня Свой гнев,"
  },
  {
    "chapter": 10,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "и все новые и новые"
  },
  {
    "chapter": 10,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 6,
    "text": "войска бросаешь на меня."
  },
  {
    "chapter": 10,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Зачем Ты вывел меня из"
  },
  {
    "chapter": 10,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "чрева? Лучше бы я умер,"
  },
  {
    "chapter": 10,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и никто не увидел бы меня."
  },
  {
    "chapter": 10,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Как бы я хотел никогда не"
  },
  {
    "chapter": 10,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "жить и сразу"
  },
  {
    "chapter": 10,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "из утробы в могилу"
  },
  {
    "chapter": 10,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "перенестись!"
  },
  {
    "chapter": 10,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Разве дни моей жизни не"
  },
  {
    "chapter": 10,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "кратки? Отступи от меня,"
  },
  {
    "chapter": 10,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "чтобы утешиться мне на миг,"
  },
  {
    "chapter": 10,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "прежде чем в путь"
  },
  {
    "chapter": 10,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "безвозвратный отправлюсь,"
  },
  {
    "chapter": 10,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "в край мрака и"
  },
  {
    "chapter": 10,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "смертной мглы,"
  },
  {
    "chapter": 10,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "в землю кромешной ночи,"
  },
  {
    "chapter": 10,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "в тот неустроенный"
  },
  {
    "chapter": 10,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "край смертной мглы,"
  },
  {
    "chapter": 10,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "где сам свет подобен тьме."
  }
