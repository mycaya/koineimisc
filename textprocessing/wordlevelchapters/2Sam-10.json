    "verse": 4,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 3,
    "text": "половину бороды и,"
  },
  {
    "chapter": 10,
    "verse": 4,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 4,
    "text": "отрезав их"
  },
  {
    "chapter": 10,
    "verse": 4,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
