  },
  {
    "chapter": 10,
    "verse": 4,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
    "part": 6,
    "text": "по ягодицы,"
  },
  {
    "chapter": 10,
    "verse": 4,
    "book_name": "2 Samuel",
    "book_id": "2Sam",
