[
  {
    "chapter": 54,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Дирижеру хора."
  },
  {
    "chapter": 54,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "На мотив Не погуби."
  },
  {
    "chapter": 54,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Мольба Давида."
  },
  {
    "chapter": 54,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Когда он убежал"
  },
  {
    "chapter": 54,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "от Саула в пещеру."
  },
  {
    "chapter": 54,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Боже,"
  },
  {
    "chapter": 54,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "помилуй меня,"
  },
  {
    "chapter": 54,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "помилуй! У Тебя"
  },
  {
    "chapter": 54,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "я ищу прибежища."
  },
  {
    "chapter": 54,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Я укроюсь в"
  },
  {
    "chapter": 54,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "тени Твоих крыльев,"
  },
  {
    "chapter": 54,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "пока не пройдет беда."
  },
  {
    "chapter": 54,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я взываю к Богу Всевышнему,"
  },
  {
    "chapter": 54,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "к Богу,"
  },
  {
    "chapter": 54,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "вершащему"
  },
  {
    "chapter": 54,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "замысел Свой обо мне."
  },
  {
    "chapter": 54,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он пошлет с"
  },
  {
    "chapter": 54,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "небес и спасет меня,"
  },
  {
    "chapter": 54,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "посрамит Он моих"
  },
  {
    "chapter": 54,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "преследователей;"
  },
  {
    "chapter": 54,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Пауза пошлет Бог"
  },
  {
    "chapter": 54,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Свою милость и верность."
  },
  {
    "chapter": 54,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Львы меня окружили,"
  },
  {
    "chapter": 54,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "я лежу средь людей,"
  },
  {
    "chapter": 54,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "чьи зубы копья и стрелы,"
  },
  {
    "chapter": 54,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "чьи языки наточенные мечи."
  },
  {
    "chapter": 54,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Будь превознесен,"
  },
  {
    "chapter": 54,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Боже,"
  },
  {
    "chapter": 54,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "выше небес,"
  },
  {
    "chapter": 54,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "над всей землей да"
  },
  {
    "chapter": 54,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "будет слава Твоя!"
  },
  {
    "chapter": 54,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ногам моим они"
  },
  {
    "chapter": 54,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "сеть раскинули,"
  },
  {
    "chapter": 54,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "сникла от горя моя душа."
  },
  {
    "chapter": 54,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "На пути моем"
  },
  {
    "chapter": 54,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "они вырыли яму,"
  },
  {
    "chapter": 54,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "но сами в нее упали."
  },
  {
    "chapter": 54,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "Пауза"
  },
  {
    "chapter": 54,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Сердце мое твердо,"
  },
  {
    "chapter": 54,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "о Боже,"
  },
  {
    "chapter": 54,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "сердце мое твердо."
  },
  {
    "chapter": 54,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Буду петь и играть."
  },
  {
    "chapter": 54,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пробудись,"
  },
  {
    "chapter": 54,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "моя душа! Пробудитесь,"
  },
  {
    "chapter": 54,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "лира и арфа! Я"
  },
  {
    "chapter": 54,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "разбужу зарю."
  },
  {
    "chapter": 54,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Прославлю Тебя,"
  },
  {
    "chapter": 54,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Владыка,"
  },
  {
    "chapter": 54,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "среди народов"
  },
  {
    "chapter": 54,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "воспою Тебя среди племен,"
  },
  {
    "chapter": 54,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "потому что милость Твоя"
  },
  {
    "chapter": 54,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "превыше небес"
  },
  {
    "chapter": 54,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и верность Твоя достигает"
  },
  {
    "chapter": 54,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "облаков."
  },
  {
    "chapter": 54,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Будь превознесен,"
  },
  {
    "chapter": 54,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Боже,"
  },
  {
    "chapter": 54,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "выше небес,"
  },
  {
    "chapter": 54,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "над всей землей пусть"
  },
  {
    "chapter": 54,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "пребудет слава Твоя!"
  }
]