[
  {
    "chapter": 7,
    "verse": 1,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "Горе мне,"
  },
  {
    "chapter": 7,
    "verse": 1,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "горе! Я словно тот,"
  },
  {
    "chapter": 7,
    "verse": 1,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "кто после сбора"
  },
  {
    "chapter": 7,
    "verse": 1,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "летних плодов,"
  },
  {
    "chapter": 7,
    "verse": 1,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 5,
    "text": "после уборки винограда,"
  },
  {
    "chapter": 7,
    "verse": 1,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 6,
    "text": "не находит и грозди,"
  },
  {
    "chapter": 7,
    "verse": 1,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 7,
    "text": "чтобы поесть,"
  },
  {
    "chapter": 7,
    "verse": 1,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 8,
    "text": "ни инжирины"
  },
  {
    "chapter": 7,
    "verse": 1,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 9,
    "text": "ранней зрелости,"
  },
  {
    "chapter": 7,
    "verse": 1,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 10,
    "text": "которой мне хочется."
  },
  {
    "chapter": 7,
    "verse": 2,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "Исчезли верные на земле;"
  },
  {
    "chapter": 7,
    "verse": 2,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "нет праведных среди людей."
  },
  {
    "chapter": 7,
    "verse": 2,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "Все сидят в засаде,"
  },
  {
    "chapter": 7,
    "verse": 2,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "чтобы лить кровь;"
  },
  {
    "chapter": 7,
    "verse": 2,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 5,
    "text": "все расставляют"
  },
  {
    "chapter": 7,
    "verse": 2,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 6,
    "text": "друг другу сети."
  },
  {
    "chapter": 7,
    "verse": 3,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "Руки их"
  },
  {
    "chapter": 7,
    "verse": 3,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "искусны творить зло;"
  },
  {
    "chapter": 7,
    "verse": 3,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "правитель подарков требует,"
  },
  {
    "chapter": 7,
    "verse": 3,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "судья принимает взятки,"
  },
  {
    "chapter": 7,
    "verse": 3,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 5,
    "text": "и сильные волю свою диктуют"
  },
  {
    "chapter": 7,
    "verse": 3,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 6,
    "text": "так"
  },
  {
    "chapter": 7,
    "verse": 3,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 7,
    "text": "извращают они правосудие."
  },
  {
    "chapter": 7,
    "verse": 4,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "Лучший из них"
  },
  {
    "chapter": 7,
    "verse": 4,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "что терновник,"
  },
  {
    "chapter": 7,
    "verse": 4,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "самый праведный"
  },
  {
    "chapter": 7,
    "verse": 4,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "хуже колючей изгороди."
  },
  {
    "chapter": 7,
    "verse": 4,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 5,
    "text": "Наступает день,"
  },
  {
    "chapter": 7,
    "verse": 4,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 6,
    "text": "который"
  },
  {
    "chapter": 7,
    "verse": 4,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 7,
    "text": "предсказывали их стражи,"
  },
  {
    "chapter": 7,
    "verse": 4,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 8,
    "text": "день их наказания;"
  },
  {
    "chapter": 7,
    "verse": 4,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 9,
    "text": "ныне охватит их смятение."
  },
  {
    "chapter": 7,
    "verse": 5,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "Не полагайтесь на ближнего,"
  },
  {
    "chapter": 7,
    "verse": 5,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "не доверяйте другу."
  },
  {
    "chapter": 7,
    "verse": 5,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "Даже с той,"
  },
  {
    "chapter": 7,
    "verse": 5,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "что лежит в твоих объятьях,"
  },
  {
    "chapter": 7,
    "verse": 5,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 5,
    "text": "будь осторожен в словах."
  },
  {
    "chapter": 7,
    "verse": 6,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "Сын позорит отца,"
  },
  {
    "chapter": 7,
    "verse": 6,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "дочь восстает"
  },
  {
    "chapter": 7,
    "verse": 6,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "против матери,"
  },
  {
    "chapter": 7,
    "verse": 6,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "и невестка против свекрови:"
  },
  {
    "chapter": 7,
    "verse": 6,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 5,
    "text": "враги человеку"
  },
  {
    "chapter": 7,
    "verse": 6,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 6,
    "text": "его домашние."
  },
  {
    "chapter": 7,
    "verse": 7,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "А что до меня,"
  },
  {
    "chapter": 7,
    "verse": 7,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "я буду взирать на Господа,"
  },
  {
    "chapter": 7,
    "verse": 7,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "ждать Бога спасения моего;"
  },
  {
    "chapter": 7,
    "verse": 7,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "Бог мой меня услышит."
  },
  {
    "chapter": 7,
    "verse": 8,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "Не ликуй надо мной,"
  },
  {
    "chapter": 7,
    "verse": 8,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "неприятель! Пусть"
  },
  {
    "chapter": 7,
    "verse": 8,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "упал я встану опять."
  },
  {
    "chapter": 7,
    "verse": 8,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "Пусть во мраке сижу"
  },
  {
    "chapter": 7,
    "verse": 8,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 5,
    "text": "светом будет мне Господь."
  },
  {
    "chapter": 7,
    "verse": 9,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "Гнев Господень"
  },
  {
    "chapter": 7,
    "verse": 9,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "я должен нести,"
  },
  {
    "chapter": 7,
    "verse": 9,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "ведь я согрешил перед Ним,"
  },
  {
    "chapter": 7,
    "verse": 9,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "пока Он не разрешит мое"
  },
  {
    "chapter": 7,
    "verse": 9,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 5,
    "text": "дело и не оправдает меня."
  },
  {
    "chapter": 7,
    "verse": 9,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 6,
    "text": "Он меня выведет к свету;"
  },
  {
    "chapter": 7,
    "verse": 9,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 7,
    "text": "я увижу Его праведность."
  },
  {
    "chapter": 7,
    "verse": 10,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "Увидит это неприятель"
  },
  {
    "chapter": 7,
    "verse": 10,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "и бесчестьем покроется;"
  },
  {
    "chapter": 7,
    "verse": 10,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "тот,"
  },
  {
    "chapter": 7,
    "verse": 10,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "кто мне говорила:"
  },
  {
    "chapter": 7,
    "verse": 10,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 5,
    "text": "Где Господь,"
  },
  {
    "chapter": 7,
    "verse": 10,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 6,
    "text": "твой Бог? Я своими глазами"
  },
  {
    "chapter": 7,
    "verse": 10,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 7,
    "text": "увижу его падение топтать"
  },
  {
    "chapter": 7,
    "verse": 10,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 8,
    "text": "его будут,"
  },
  {
    "chapter": 7,
    "verse": 10,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 9,
    "text": "словно грязь на улицах."
  },
  {
    "chapter": 7,
    "verse": 11,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "Придет день"
  },
  {
    "chapter": 7,
    "verse": 11,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "отстроить твои стены,"
  },
  {
    "chapter": 7,
    "verse": 11,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "расширить твои границы."
  },
  {
    "chapter": 7,
    "verse": 12,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "В тот день к тебе придут"
  },
  {
    "chapter": 7,
    "verse": 12,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "из Ассирии и"
  },
  {
    "chapter": 7,
    "verse": 12,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "из городов Египта,"
  },
  {
    "chapter": 7,
    "verse": 12,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "от Египта и до"
  },
  {
    "chapter": 7,
    "verse": 12,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 5,
    "text": "реки Евфрата,"
  },
  {
    "chapter": 7,
    "verse": 12,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 6,
    "text": "от моря и до моря,"
  },
  {
    "chapter": 7,
    "verse": 12,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 7,
    "text": "от горы и до горы."
  },
  {
    "chapter": 7,
    "verse": 13,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "Но земля подвергнется"
  },
  {
    "chapter": 7,
    "verse": 13,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "разорению из-за жителей и"
  },
  {
    "chapter": 7,
    "verse": 13,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "их злых дел."
  },
  {
    "chapter": 7,
    "verse": 14,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "Паси Своим"
  },
  {
    "chapter": 7,
    "verse": 14,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "посохом народ Свой,"
  },
  {
    "chapter": 7,
    "verse": 14,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "отару наследия Твоего,"
  },
  {
    "chapter": 7,
    "verse": 14,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "что живет"
  },
  {
    "chapter": 7,
    "verse": 14,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 5,
    "text": "одиноко в зарослях,"
  },
  {
    "chapter": 7,
    "verse": 14,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 6,
    "text": "а вокруг"
  },
  {
    "chapter": 7,
    "verse": 14,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 7,
    "text": "плодородные пастбища."
  },
  {
    "chapter": 7,
    "verse": 14,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 8,
    "text": "Пусть пасутся на"
  },
  {
    "chapter": 7,
    "verse": 14,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 9,
    "text": "Васане и Галааде,"
  },
  {
    "chapter": 7,
    "verse": 14,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 10,
    "text": "как в давно минувшие дни."
  },
  {
    "chapter": 7,
    "verse": 15,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "- Как в те дни,"
  },
  {
    "chapter": 7,
    "verse": 15,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "когда ты вышел из Египта,"
  },
  {
    "chapter": 7,
    "verse": 15,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "Я покажу тебе Свои чудеса."
  },
  {
    "chapter": 7,
    "verse": 16,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "Народы увидят и устыдятся,"
  },
  {
    "chapter": 7,
    "verse": 16,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "при всей своей силе."
  },
  {
    "chapter": 7,
    "verse": 16,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "Рукой они"
  },
  {
    "chapter": 7,
    "verse": 16,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "прикроют себе рот,"
  },
  {
    "chapter": 7,
    "verse": 16,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 5,
    "text": "уши их станут глухи."
  },
  {
    "chapter": 7,
    "verse": 17,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "Они будут лизать пыль,"
  },
  {
    "chapter": 7,
    "verse": 17,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "как змея,"
  },
  {
    "chapter": 7,
    "verse": 17,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "как ползучие твари земные."
  },
  {
    "chapter": 7,
    "verse": 17,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 4,
    "text": "Они выйдут,"
  },
  {
    "chapter": 7,
    "verse": 17,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 5,
    "text": "дрожа,"
  },
  {
    "chapter": 7,
    "verse": 17,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 6,
    "text": "из своих крепостей,"
  },
  {
    "chapter": 7,
    "verse": 17,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 7,
    "text": "обратятся в"
  },
  {
    "chapter": 7,
    "verse": 17,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 8,
    "text": "страхе к Господу,"
  },
  {
    "chapter": 7,
    "verse": 17,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 9,
    "text": "нашему Богу,"
  },
  {
    "chapter": 7,
    "verse": 17,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 10,
    "text": "и будут Тебя бояться."
  },
  {
    "chapter": 7,
    "verse": 18,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 1,
    "text": "Есть ли Бог,"
  },
  {
    "chapter": 7,
    "verse": 18,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 2,
    "text": "подобный Тебе,"
  },
  {
    "chapter": 7,
    "verse": 18,
    "book_name": "Micah",
    "book_id": "Mic",
    "part": 3,
    "text": "Кто отпускает грех"
  },
  {
    "chapter": 7,
    "verse": 18,
    "book_name": "Micah",
    "book_id": "Mic",
