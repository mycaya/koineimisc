[
  {
    "chapter": 25,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Псалом Давида."
  },
  {
    "chapter": 25,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господь мой свет"
  },
  {
    "chapter": 25,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и мое спасение,"
  },
  {
    "chapter": 25,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "кого мне бояться?"
  },
  {
    "chapter": 25,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Господь крепость жизни моей,"
  },
  {
    "chapter": 25,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "кого мне страшиться?"
  },
  {
    "chapter": 25,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Когда злодеи"
  },
  {
    "chapter": 25,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "пойдут на меня,"
  },
  {
    "chapter": 25,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "чтобы плоть мою пожирать,"
  },
  {
    "chapter": 25,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "когда мои враги"
  },
  {
    "chapter": 25,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "и противники против меня"
  },
  {
    "chapter": 25,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "ополчатся споткнутся они"
  },
  {
    "chapter": 25,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "и падут."
  },
  {
    "chapter": 25,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Пусть войско в осаду меня"
  },
  {
    "chapter": 25,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "возьмет сердце мое не"
  },
  {
    "chapter": 25,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "дрогнет;"
  },
  {
    "chapter": 25,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "пусть вспыхнет против меня"
  },
  {
    "chapter": 25,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "война"
  },
  {
    "chapter": 25,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "и тогда я буду спокоен."
  },
  {
    "chapter": 25,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Одного я прошу у Господа,"
  },
  {
    "chapter": 25,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "только этого я ищу:"
  },
  {
    "chapter": 25,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "чтобы жить мне в доме"
  },
  {
    "chapter": 25,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Господнем во все дни моей жизни,"
  },
  {
    "chapter": 25,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "созерцать красоту Господню"
  },
  {
    "chapter": 25,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "и размышлять в Его храме."
  },
  {
    "chapter": 25,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "В день беды Он"
  },
  {
    "chapter": 25,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "даст мне приют,"
  },
  {
    "chapter": 25,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "скроет меня под"
  },
  {
    "chapter": 25,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "пологом Своего шатра,"
  },
  {
    "chapter": 25,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "вознесет меня на скалу."
  },
  {
    "chapter": 25,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Тогда вознесется моя"
  },
  {
    "chapter": 25,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "глава над противниками,"
  },
  {
    "chapter": 25,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "окружающими меня,"
  },
  {
    "chapter": 25,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и принесу у"
  },
  {
    "chapter": 25,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Его шатра жертвы,"
  },
  {
    "chapter": 25,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "восклицая от радости."
  },
  {
    "chapter": 25,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "Буду петь и"
  },
  {
    "chapter": 25,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 8,
    "text": "играть для Господа."
  },
  {
    "chapter": 25,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Услышь,"
  },
  {
    "chapter": 25,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 25,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "когда я зову,"
  },
  {
    "chapter": 25,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "помилуй меня и ответь."
  },
  {
    "chapter": 25,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты говоришь моему сердцу:"
  },
  {
    "chapter": 25,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Ищи Моего лица! Господи,"
  },
  {
    "chapter": 25,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "я буду искать лица Твоего."
  },
  {
    "chapter": 25,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Не скрывай от"
  },
  {
    "chapter": 25,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "меня Свое лицо."
  },
  {
    "chapter": 25,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Не отвергни в"
  },
  {
    "chapter": 25,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "гневе Своего слугу,"
  },
  {
    "chapter": 25,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Ты,"
  },
  {
    "chapter": 25,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "Кто был мне помощником."
  },
  {
    "chapter": 25,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "Не отвергни меня,"
  },
  {
    "chapter": 25,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 8,
    "text": "не оставь,"
  },
  {
    "chapter": 25,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 9,
    "text": "Бог,"
  },
  {
    "chapter": 25,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 10,
    "text": "мой Спаситель."
  },
  {
    "chapter": 25,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Если даже отец мой"
  },
  {
    "chapter": 25,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и мать оставят меня,"
  },
  {
    "chapter": 25,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "то Господь меня примет."
  },
  {
    "chapter": 25,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господи,"
  },
  {
    "chapter": 25,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "научи меня Твоему пути,"
  },
  {
    "chapter": 25,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "веди меня по прямой"
  },
  {
    "chapter": 25,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "стезе из-за врагов моих."
  },
  {
    "chapter": 25,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Не отдавай меня в"
  },
  {
    "chapter": 25,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "руки моих противников,"
  },
  {
    "chapter": 25,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "ведь восстали на меня"
  },
  {
    "chapter": 25,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "лжесвидетели и злобою дышат."
  },
  {
    "chapter": 25,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но верю:"
  },
  {
    "chapter": 25,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "увижу я благость"
  },
  {
    "chapter": 25,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Господню на земле живых."
  },
  {
    "chapter": 25,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Надейся на Господа,"
  },
  {
    "chapter": 25,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "мужайся и"
  },
  {
    "chapter": 25,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "сердце свое укрепи,"
  },
  {
    "chapter": 25,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "надейся на Господа!"
  }
]