[
  {
    "chapter": 108,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Когда вышел Израиль"
  },
  {
    "chapter": 108,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "из Египта"
  },
  {
    "chapter": 108,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и дом Иакова от народа"
  },
  {
    "chapter": 108,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "чужеземного,"
  },
  {
    "chapter": 108,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Иуда стал Его святилищем,"
  },
  {
    "chapter": 108,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Израиль Его владением."
  },
  {
    "chapter": 108,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Море увидело"
  },
  {
    "chapter": 108,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "это и отступило,"
  },
  {
    "chapter": 108,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "река Иордан"
  },
  {
    "chapter": 108,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "обратилась вспять."
  },
  {
    "chapter": 108,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Горы запрыгали,"
  },
  {
    "chapter": 108,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "словно бараны,"
  },
  {
    "chapter": 108,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и холмы,"
  },
  {
    "chapter": 108,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "словно ягнята."
  },
  {
    "chapter": 108,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Что с тобой,"
  },
  {
    "chapter": 108,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "море,"
  },
  {
    "chapter": 108,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "что ты отступило,"
  },
  {
    "chapter": 108,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и с тобой,"
  },
  {
    "chapter": 108,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Иордан,"
  },
  {
    "chapter": 108,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "что ты обратился вспять?"
  },
  {
    "chapter": 108,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Что вы,"
  },
  {
    "chapter": 108,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "горы,"
  },
  {
    "chapter": 108,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "запрыгали,"
  },
  {
    "chapter": 108,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "словно бараны,"
  },
  {
    "chapter": 108,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "и вы,"
  },
  {
    "chapter": 108,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "холмы,"
  },
  {
    "chapter": 108,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "словно ягнята?"
  },
  {
    "chapter": 108,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Трепещи,"
  },
  {
    "chapter": 108,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "земля,"
  },
  {
    "chapter": 108,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "перед Владыкой,"
  },
  {
    "chapter": 108,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "перед Богом Иакова,"
  },
  {
    "chapter": 108,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Который превратил"
  },
  {
    "chapter": 108,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "скалу в озеро воды,"
  },
  {
    "chapter": 108,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "камень в источник воды."
  },
  {
    "chapter": 108,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Не нам,"
  },
  {
    "chapter": 108,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "не нам,"
  },
  {
    "chapter": 108,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Господи,"
  },
  {
    "chapter": 108,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "а Имени Твоему да будет"
  },
  {
    "chapter": 108,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "слава ради милости"
  },
  {
    "chapter": 108,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "и верности Твоей."
  },
  {
    "chapter": 108,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Зачем чужеземцам говорить:"
  },
  {
    "chapter": 108,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Где же их Бог?"
  },
  {
    "chapter": 108,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Наш Бог на небесах,"
  },
  {
    "chapter": 108,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "делает все,"
  },
  {
    "chapter": 108,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "что Ему угодно."
  },
  {
    "chapter": 108,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "А их идолы"
  },
  {
    "chapter": 108,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "серебро и золото,"
  },
  {
    "chapter": 108,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "дело рук человеческих."
  },
  {
    "chapter": 108,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "У них есть уста,"
  },
  {
    "chapter": 108,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "но не говорят они,"
  },
  {
    "chapter": 108,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "у них есть глаза,"
  },
  {
    "chapter": 108,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "но не видят они,"
  },
  {
    "chapter": 108,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "есть уши,"
  },
  {
    "chapter": 108,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "но не слышат они,"
  },
  {
    "chapter": 108,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "есть ноздри,"
  },
  {
    "chapter": 108,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "но не чувствуют запах они,"
  },
  {
    "chapter": 108,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "у них есть руки,"
  },
  {
    "chapter": 108,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "но они не осязают,"
  },
  {
    "chapter": 108,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "есть ноги,"
  },
  {
    "chapter": 108,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "но они не ходят,"
  },
  {
    "chapter": 108,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "и гортанью своей"
  },
  {
    "chapter": 108,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "не издают голоса."
  },
  {
    "chapter": 108,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Делающие их и"
  },
  {
    "chapter": 108,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "надеющиеся на них,"
  },
  {
    "chapter": 108,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "да будут подобны им."
  },
  {
    "chapter": 108,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Израиль,"
  },
  {
    "chapter": 108,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "уповай на Господа!"
  },
  {
    "chapter": 108,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Он их помощь и щит."
  },
  {
    "chapter": 108,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Дом Ааронов,"
  },
  {
    "chapter": 108,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "уповай на Господа!"
  },
  {
    "chapter": 108,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Он их помощь и щит."
  },
  {
    "chapter": 108,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Боящиеся Господа,"
  },
  {
    "chapter": 108,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "уповайте на Господа!"
  },
  {
    "chapter": 108,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Он их помощь и щит."
  },
  {
    "chapter": 108,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господь нас помнит,"
  },
  {
    "chapter": 108,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и благословит:"
  },
  {
    "chapter": 108,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "благословит дом Израилев"
  },
  {
    "chapter": 108,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "благословит дом Ааронов,"
  },
  {
    "chapter": 108,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "благословит всех"
  },
  {
    "chapter": 108,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "боящихся Господа,"
  },
  {
    "chapter": 108,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "малых и великих."
  },
  {
    "chapter": 108,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господь умножит"
  },
  {
    "chapter": 108,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "вас более и более,"
  },
  {
    "chapter": 108,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "вас и ваших детей."
  },
  {
    "chapter": 108,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Благословенны вы Господом,"
  },
  {
    "chapter": 108,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "сотворившим небо и землю."
  },
  {
    "chapter": 108,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Небеса принадлежат Господу,"
  },
  {
    "chapter": 108,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "а землю Он отдал"
  },
  {
    "chapter": 108,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "сынам человеческим."
  },
  {
    "chapter": 108,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Не мертвые восхваляют"
  },
  {
    "chapter": 108,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господа"
  },
  {
    "chapter": 108,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и не нисходящие в тишину,"
  },
  {
    "chapter": 108,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "а мы прославим"
  },
  {
    "chapter": 108,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господа отныне и навеки."
  },
  {
    "chapter": 108,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Аллилуйя!"
  }
]