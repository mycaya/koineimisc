[
  {
    "chapter": 16,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Тогда Иов ответил:"
  },
  {
    "chapter": 16,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "- Я слышал много подобного,"
  },
  {
    "chapter": 16,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "вы все жалкие утешители!"
  },
  {
    "chapter": 16,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Настанет ли ветреным словам"
  },
  {
    "chapter": 16,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "конец? Что заставляет тебя"
  },
  {
    "chapter": 16,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "возражать?"
  },
  {
    "chapter": 16,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "И я бы мог говорить так,"
  },
  {
    "chapter": 16,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "как вы,"
  },
  {
    "chapter": 16,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "если бы вы были"
  },
  {
    "chapter": 16,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "на моем месте;"
  },
  {
    "chapter": 16,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "я сплетал бы речи против"
  },
  {
    "chapter": 16,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 6,
    "text": "вас"
  },
  {
    "chapter": 16,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 7,
    "text": "и неодобрительно качал бы"
  },
  {
    "chapter": 16,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 8,
    "text": "головой;"
  },
  {
    "chapter": 16,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "я укреплял бы"
  },
  {
    "chapter": 16,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "вас своими устами,"
  },
  {
    "chapter": 16,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "унимая вашу"
  },
  {
    "chapter": 16,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "боль движением губ."
  },
  {
    "chapter": 16,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Если я говорю,"
  },
  {
    "chapter": 16,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "не унимается моя боль,"
  },
  {
    "chapter": 16,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и когда перестаю не уходит."
  },
  {
    "chapter": 16,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "О как Ты меня изнурил."
  },
  {
    "chapter": 16,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Ты погубил всех"
  },
  {
    "chapter": 16,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "моих домашних."
  },
  {
    "chapter": 16,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Ты схватил меня во"
  },
  {
    "chapter": 16,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "свидетельство против меня самого;"
  },
  {
    "chapter": 16,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "восстает на меня худоба моя"
  },
  {
    "chapter": 16,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "и против меня"
  },
  {
    "chapter": 16,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "свидетельствует."
  },
  {
    "chapter": 16,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Бог терзает меня во гневе,"
  },
  {
    "chapter": 16,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Он ненавидит меня;"
  },
  {
    "chapter": 16,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Он скрежещет"
  },
  {
    "chapter": 16,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "на меня зубами;"
  },
  {
    "chapter": 16,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "враг мой следит зорко"
  },
  {
    "chapter": 16,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 6,
    "text": "и неотступно за мной."
  },
  {
    "chapter": 16,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Люди открывают рты и"
  },
  {
    "chapter": 16,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "издеваются надо мной,"
  },
  {
    "chapter": 16,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "бьют меня по щекам,"
  },
  {
    "chapter": 16,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "ругаясь,"
  },
  {
    "chapter": 16,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "объединились против меня."
  },
  {
    "chapter": 16,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Бог отдал меня в"
  },
  {
    "chapter": 16,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "руки неправедных,"
  },
  {
    "chapter": 16,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "бросил меня к нечестивым."
  },
  {
    "chapter": 16,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Я был спокоен,"
  },
  {
    "chapter": 16,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "но Он разбил меня,"
  },
  {
    "chapter": 16,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "взял за шею и"
  },
  {
    "chapter": 16,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "раздробил меня."
  },
  {
    "chapter": 16,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "Поставил меня"
  },
  {
    "chapter": 16,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 6,
    "text": "Своей мишенью,"
  },
  {
    "chapter": 16,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Его лучники меня окружили."
  },
  {
    "chapter": 16,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Он рассекает мне почки,"
  },
  {
    "chapter": 16,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "не щадит,"
  },
  {
    "chapter": 16,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "изливает на"
  },
  {
    "chapter": 16,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "землю мою желчь."
  },
  {
    "chapter": 16,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Пробивает во мне"
  },
  {
    "chapter": 16,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "пролом за проломом,"
  },
  {
    "chapter": 16,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "устремляется на меня,"
  },
  {
    "chapter": 16,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "как воин."
  },
  {
    "chapter": 16,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Я сшил для себя одежду"
  },
  {
    "chapter": 16,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "из мешковины"
  },
  {
    "chapter": 16,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и лбом своим уткнулся я в"
  },
  {
    "chapter": 16,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "прах."
  },
  {
    "chapter": 16,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Покраснело от"
  },
  {
    "chapter": 16,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "плача мое лицо,"
  },
  {
    "chapter": 16,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "пелена заволокла глаза,"
  },
  {
    "chapter": 16,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "хоть нет у меня"
  },
  {
    "chapter": 16,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "в руках неправды,"
  },
  {
    "chapter": 16,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и молитва моя чиста."
  },
  {
    "chapter": 16,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "О земля,"
  },
  {
    "chapter": 16,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "не скрывай мою кровь пусть"
  },
  {
    "chapter": 16,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "не найдет покоя мой крик!"
  },
  {
    "chapter": 16,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Но даже теперь мой"
  },
  {
