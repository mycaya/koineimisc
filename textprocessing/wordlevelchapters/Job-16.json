    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Моим годам приходит конец;"
  },
  {
    "chapter": 16,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "я ухожу в путь"
  },
  {
    "chapter": 16,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "безвозвратный."
  }
]