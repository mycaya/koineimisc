[
  {
    "chapter": 20,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Тогда ответил"
  },
  {
    "chapter": 20,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "Софар из Наамы:"
  },
  {
    "chapter": 20,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "- Мои тревожные мысли"
  },
  {
    "chapter": 20,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "заставляют меня ответить,"
  },
  {
    "chapter": 20,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "потому что я"
  },
  {
    "chapter": 20,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "объят смятением."
  },
  {
    "chapter": 20,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Я слышу упрек,"
  },
  {
    "chapter": 20,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "который позорит меня,"
  },
  {
    "chapter": 20,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и мой ум"
  },
  {
    "chapter": 20,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "побуждает меня ответить."
  },
  {
    "chapter": 20,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Разве ты не знаешь,"
  },
  {
    "chapter": 20,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "как было издревле,"
  },
  {
    "chapter": 20,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "с тех пор,"
  },
  {
    "chapter": 20,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "как поставлен на"
  },
  {
    "chapter": 20,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "землю смертный,"
  },
  {
    "chapter": 20,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "что кратко"
  },
  {
    "chapter": 20,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "веселье нечестивцев,"
  },
  {
    "chapter": 20,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и радость"
  },
  {
    "chapter": 20,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "безбожных лишь на миг?"
  },
  {
    "chapter": 20,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Пусть гордыня его"
  },
  {
    "chapter": 20,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "достигает небес,"
  },
  {
    "chapter": 20,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "а голова касается облаков,"
  },
  {
    "chapter": 20,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "он сгинет навек,"
  },
  {
    "chapter": 20,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "как его же нечистоты;"
  },
  {
    "chapter": 20,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и спросят видевшие его:"
  },
  {
    "chapter": 20,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "Где он?"
  },
  {
    "chapter": 20,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Как сон улетит,"
  },
  {
    "chapter": 20,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и его не найти,"
  },
  {
    "chapter": 20,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "как ночное"
  },
  {
    "chapter": 20,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "видение пропадет."
  },
  {
    "chapter": 20,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Глаз,"
  },
  {
    "chapter": 20,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "что видел его,"
  },
  {
    "chapter": 20,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "впредь его не увидит;"
  },
  {
    "chapter": 20,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "место его на"
  },
  {
    "chapter": 20,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "него не посмотрит."
  },
  {
    "chapter": 20,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Его дети будут"
  },
  {
    "chapter": 20,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "заискивать перед нищими,"
  },
  {
    "chapter": 20,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "своими руками"
  },
  {
    "chapter": 20,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "вернет он все,"
  },
  {
    "chapter": 20,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "что похитил,"
  },
  {
    "chapter": 20,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "молодецкая сила,"
  },
  {
    "chapter": 20,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "что наполняет его кости,"
  },
  {
    "chapter": 20,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "вместе с ним ляжет в прах."
  },
  {
    "chapter": 20,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Если его устам сладко зло,"
  },
  {
    "chapter": 20,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и под языком его скрывает,"
  },
  {
    "chapter": 20,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "если он бережет его,"
  },
  {
    "chapter": 20,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "не бросает,"
  },
  {
    "chapter": 20,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "держит его во рту,"
  },
  {
    "chapter": 20,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "то станет пища в его"
  },
  {
    "chapter": 20,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "желудке ядом змеиным внутри него."
  },
  {
    "chapter": 20,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Отрыгнет он богатства,"
  },
  {
    "chapter": 20,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "которые проглотил;"
  },
  {
    "chapter": 20,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Бог исторгнет их"
  },
  {
    "chapter": 20,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "из его желудка."
  },
  {
    "chapter": 20,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Змеиный яд он будет сосать,"
  },
  {
    "chapter": 20,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "жало гадюки его убьет."
  },
  {
    "chapter": 20,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Не видать ему"
  },
  {
    "chapter": 20,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "потоков водных,"
  },
  {
    "chapter": 20,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "рек,"
  },
  {
    "chapter": 20,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "текущих медом и молоком."
  },
  {
    "chapter": 20,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Он вернет нажитое,"
  },
  {
    "chapter": 20,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "не отведав;"
  },
  {
    "chapter": 20,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "доходам от труда"
  },
  {
    "chapter": 20,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "своего не порадуется."
  },
  {
    "chapter": 20,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Теснил он бедных"
  },
  {
    "chapter": 20,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и пренебрегал ими;"
  },
  {
    "chapter": 20,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "захватывал дома,"
  },
  {
    "chapter": 20,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "которые не строил."
  },
  {
    "chapter": 20,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "В его желудке нет покоя,"
  },
  {
    "chapter": 20,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "а в богатстве нет спасения."
  },
  {
    "chapter": 20,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Ничего не спаслось"
  },
  {
    "chapter": 20,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "от его обжорства,"
  },
  {
    "chapter": 20,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "зато и удача"
  },
  {
    "chapter": 20,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "его не удержится."
  },
  {
    "chapter": 20,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Среди достатка"
  },
  {
    "chapter": 20,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "его настигнет беда;"
  },
  {
    "chapter": 20,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "все руки обиженных"
  },
  {
    "chapter": 20,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "поднимутся на него."
  },
  {
    "chapter": 20,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Когда он свой"
  },
  {
    "chapter": 20,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "живот набьет до отказа,"
  },
  {
    "chapter": 20,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Бог пошлет на него"
  },
  {
    "chapter": 20,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "Свой пылающий гнев,"
  },
  {
    "chapter": 20,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "и прольет дождем"
  },
  {
    "chapter": 20,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 6,
    "text": "Свои удары на его плоть."
  },
  {
    "chapter": 20,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "От железного"
  },
  {
    "chapter": 20,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "оружия он побежит,"
  },
  {
    "chapter": 20,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "но его пронзит"
  },
  {
    "chapter": 20,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "бронзовая стрела."
  },
  {
    "chapter": 20,
    "verse": 25,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Стрела войдет в"
  },
  {
    "chapter": 20,
    "verse": 25,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "его спину и пройдет,"
  },
  {
    "chapter": 20,
    "verse": 25,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "сверкая,"
  },
  {
    "chapter": 20,
    "verse": 25,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "сквозь его печень."
  },
  {
    "chapter": 20,
    "verse": 25,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "Двинутся ужасы на него."
  },
  {
    "chapter": 20,
    "verse": 26,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Кромешная тьма"
  },
  {
    "chapter": 20,
    "verse": 26,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "ожидает его сокровища."
  },
  {
    "chapter": 20,
    "verse": 26,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Никем не зажженный"
  },
  {
    "chapter": 20,
    "verse": 26,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "огонь пожрет его,"
  },
  {
    "chapter": 20,
    "verse": 26,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "и сожжет последнее,"
  },
  {
    "chapter": 20,
    "verse": 26,
    "book_name": "Job",
    "book_id": "Job",
    "part": 6,
    "text": "что осталось в его шатре."
  },
  {
    "chapter": 20,
    "verse": 27,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Беззакония его"
  },
  {
    "chapter": 20,
    "verse": 27,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "небеса откроют,"
  },
  {
    "chapter": 20,
    "verse": 27,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и восстанет"
  },
  {
    "chapter": 20,
    "verse": 27,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "против него земля."
  },
  {
    "chapter": 20,
    "verse": 28,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Богатство его"
  },
  {
    "chapter": 20,
    "verse": 28,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "дома расточится,"
  },
  {
    "chapter": 20,
    "verse": 28,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "в день Божьего"
  },
  {
    "chapter": 20,
    "verse": 28,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "гнева расплещется,"
  },
  {
    "chapter": 20,
    "verse": 28,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "как вода."
  },
  {
    "chapter": 20,
    "verse": 29,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Вот участь"
  },
  {
    "chapter": 20,
    "verse": 29,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "нечестивому от Бога,"
  },
  {
    "chapter": 20,
    "verse": 29,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "удел,"
  },
  {
    "chapter": 20,
    "verse": 29,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "который определил ему Бог."
  }
]