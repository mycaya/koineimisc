[
  {
    "chapter": 41,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Пуста надежда его поймать,"
  },
  {
    "chapter": 41,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "от одного его вида падешь."
  },
  {
    "chapter": 41,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Нет таких смельчаков,"
  },
  {
    "chapter": 41,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "кто бы потревожил его;"
  },
  {
    "chapter": 41,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "кто же тогда способен"
  },
  {
    "chapter": 41,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "предстать передо Мной?"
  },
  {
    "chapter": 41,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Кто способен"
  },
  {
    "chapter": 41,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "сразиться со Мной,"
  },
  {
    "chapter": 41,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "чтобы Я воздал? Во"
  },
  {
    "chapter": 41,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "всей поднебесной все Мое."
  },
  {
    "chapter": 41,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Не умолчу о членах его,"
  },
  {
    "chapter": 41,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "о силе его и о"
  },
  {
    "chapter": 41,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "дивной стати."
  },
  {
    "chapter": 41,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Кто снимет с него верхнюю"
  },
  {
    "chapter": 41,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "одежду? Кто пронзит его"
  },
  {
    "chapter": 41,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "двойную броню?"
  },
  {
    "chapter": 41,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Кто распахнет"
  },
  {
    "chapter": 41,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "врата его пасти,"
  },
  {
    "chapter": 41,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "что зубами"
  },
  {
    "chapter": 41,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "ужасными окружена?"
  },
  {
    "chapter": 41,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "На спине у него щитов ряды,"
  },
  {
    "chapter": 41,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "скрепленные намертво,"
  },
  {
    "chapter": 41,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "как печатью."
  },
  {
    "chapter": 41,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Каждый из них так"
  },
  {
    "chapter": 41,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "близок к другому,"
  },
  {
    "chapter": 41,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "что не проходит"
  },
  {
    "chapter": 41,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "меж ними воздух."
  },
  {
    "chapter": 41,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Накрепко сбиты"
  },
  {
    "chapter": 41,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "они друг с другом,"
  },
  {
    "chapter": 41,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "примкнули друг к другу,"
  },
  {
    "chapter": 41,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "и не расторгнуть их."
  },
  {
    "chapter": 41,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Когда он чихает"
  },
  {
    "chapter": 41,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "блистает свет,"
  },
  {
    "chapter": 41,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "его глаза,"
  },
  {
    "chapter": 41,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "как лучи зари."
  },
  {
    "chapter": 41,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Пышет из его пасти"
  },
  {
    "chapter": 41,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "огонь и разлетаются искры."
  },
  {
    "chapter": 41,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Из его ноздрей валит дым,"
  },
  {
    "chapter": 41,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "как из котла,"
  },
  {
    "chapter": 41,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "что клокочет над"
  },
  {
    "chapter": 41,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "пламенем тростника."
  },
  {
    "chapter": 41,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Раздувает угли его дыхание,"
  },
  {
    "chapter": 41,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и из пасти его пышет пламя."
  },
  {
    "chapter": 41,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "В его шее обитает сила,"
  },
  {
    "chapter": 41,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "ужас бежит перед ним."
  },
  {
    "chapter": 41,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Крепко спаяна его плоть,"
  },
  {
    "chapter": 41,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "словно литая,"
  },
  {
    "chapter": 41,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "не поколеблется."
  },
  {
    "chapter": 41,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Сердце его твердо,"
  },
  {
    "chapter": 41,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "как камень,"
  },
  {
    "chapter": 41,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "твердо,"
  },
  {
    "chapter": 41,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "как нижний"
  },
  {
    "chapter": 41,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "мельничный жернов."
  },
  {
    "chapter": 41,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Когда он встает,"
  },
  {
    "chapter": 41,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "содрогаются сильные,"
  },
  {
    "chapter": 41,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "они от ужаса обезумели."
  },
  {
    "chapter": 41,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Меч,"
  },
  {
    "chapter": 41,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "что коснется его,"
  },
  {
    "chapter": 41,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "бессилен не"
  },
  {
    "chapter": 41,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "возьмут его ни копье,"
  },
  {
    "chapter": 41,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "ни дротик,"
  },
  {
    "chapter": 41,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 6,
    "text": "ни пика."
  },
  {
    "chapter": 41,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Железо с"
  },
  {
    "chapter": 41,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "соломою он равняет,"
  },
  {
    "chapter": 41,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "а бронзу с"
  },
  {
    "chapter": 41,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "трухлявым деревом."
  },
  {
    "chapter": 41,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Не обратят его в"
  },
  {
    "chapter": 41,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "бегство стрелы,"
  },
  {
    "chapter": 41,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "камни пращников для него,"
  },
  {
    "chapter": 41,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "как мякина."
  },
  {
    "chapter": 41,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Как солома ему булава,"
  },
  {
    "chapter": 41,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "он над свистом"
  },
  {
    "chapter": 41,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "копья смеется."
  },
  {
    "chapter": 41,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Его брюхо покрыто чешуей,"
  },
  {
    "chapter": 41,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "как острые черепки,"
  },
  {
    "chapter": 41,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "что на грязь налегают,"
  },
  {
    "chapter": 41,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "как молотильная доска."
  },
  {
    "chapter": 41,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Кипятит он пучину,"
  },
  {
    "chapter": 41,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "как котел,"
  },
  {
    "chapter": 41,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "обращает море"
  },
  {
    "chapter": 41,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "в кипящую мазь."
  },
  {
    "chapter": 41,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "За ним остается"
  },
  {
    "chapter": 41,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "светящийся след,"
  },
  {
    "chapter": 41,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "бездна кажется седовласой."
  },
  {
    "chapter": 41,
    "verse": 25,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Нет ему равного на земле"
  },
  {
    "chapter": 41,
    "verse": 25,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "он сотворен бесстрашным,"
  },
  {
    "chapter": 41,
    "verse": 26,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "На все надменное свысока он"
  },
  {
    "chapter": 41,
    "verse": 26,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "смотрит он царит над всем"
  },
  {
    "chapter": 41,
    "verse": 26,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "горделивым."
  }
]