[
  {
    "chapter": 12,
    "verse": 1,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Тогда Иов ответил:"
  },
  {
    "chapter": 12,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "- Воистину,"
  },
  {
    "chapter": 12,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "только вы люди,"
  },
  {
    "chapter": 12,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и мудрость"
  },
  {
    "chapter": 12,
    "verse": 2,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "умрет вместе с вами!"
  },
  {
    "chapter": 12,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Но у меня есть ум,"
  },
  {
    "chapter": 12,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "как у вас;"
  },
  {
    "chapter": 12,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "я ничем не хуже вас,"
  },
  {
    "chapter": 12,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "да и кто же"
  },
  {
    "chapter": 12,
    "verse": 3,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "всего этого не знает?"
  },
  {
    "chapter": 12,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Я стал для"
  },
  {
    "chapter": 12,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "друзей посмешищем."
  },
  {
    "chapter": 12,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "Я,"
  },
  {
    "chapter": 12,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "кто к Богу взывал,"
  },
  {
    "chapter": 12,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "и которому Он отвечал,"
  },
  {
    "chapter": 12,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 6,
    "text": "я,"
  },
  {
    "chapter": 12,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 7,
    "text": "праведный и безупречный,"
  },
  {
    "chapter": 12,
    "verse": 4,
    "book_name": "Job",
    "book_id": "Job",
    "part": 8,
    "text": "стал посмешищем!"
  },
  {
    "chapter": 12,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Кто,"
  },
  {
    "chapter": 12,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "находясь в благополучии,"
  },
  {
    "chapter": 12,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "презирает несчастного,"
  },
  {
    "chapter": 12,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "тот вскоре и"
  },
  {
    "chapter": 12,
    "verse": 5,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "сам поскользнется."
  },
  {
    "chapter": 12,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Шатры грабителей"
  },
  {
    "chapter": 12,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "в безопасности,"
  },
  {
    "chapter": 12,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и те,"
  },
  {
    "chapter": 12,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "кто гневит Бога,"
  },
  {
    "chapter": 12,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "спокойны,"
  },
  {
    "chapter": 12,
    "verse": 6,
    "book_name": "Job",
    "book_id": "Job",
    "part": 6,
    "text": "словно держат Бога в руках."
  },
  {
    "chapter": 12,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Но спроси у животных,"
  },
  {
    "chapter": 12,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "они научат тебя,"
  },
  {
    "chapter": 12,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "у небесных птиц,"
  },
  {
    "chapter": 12,
    "verse": 7,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "они тебе скажут."
  },
  {
    "chapter": 12,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Побеседуй с землей,"
  },
  {
    "chapter": 12,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и наставит тебя,"
  },
  {
    "chapter": 12,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и рыбы морские"
  },
  {
    "chapter": 12,
    "verse": 8,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "тебе возвестят."
  },
  {
    "chapter": 12,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Кто среди них не знает,"
  },
  {
    "chapter": 12,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "что все это"
  },
  {
    "chapter": 12,
    "verse": 9,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "сделала рука Господня?"
  },
  {
    "chapter": 12,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Жизнь всякой"
  },
  {
    "chapter": 12,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "твари в Его руке,"
  },
  {
    "chapter": 12,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "как и дыхание"
  },
  {
    "chapter": 12,
    "verse": 10,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "всякого человека."
  },
  {
    "chapter": 12,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Разве не ухо"
  },
  {
    "chapter": 12,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "различает слова,"
  },
  {
    "chapter": 12,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и не язык"
  },
  {
    "chapter": 12,
    "verse": 11,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "распознает вкус пищи?"
  },
  {
    "chapter": 12,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Разве мудрость не у"
  },
  {
    "chapter": 12,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "старейших? Разве долгая"
  },
  {
    "chapter": 12,
    "verse": 12,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "жизнь не приносит разум?"
  },
  {
    "chapter": 12,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "У Бога мудрость и сила;"
  },
  {
    "chapter": 12,
    "verse": 13,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "совет и разум Его."
  },
  {
    "chapter": 12,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Что Он разрушил,"
  },
  {
    "chapter": 12,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "не восстановится,"
  },
  {
    "chapter": 12,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "заключенные им"
  },
  {
    "chapter": 12,
    "verse": 14,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "не выйдут на волю."
  },
  {
    "chapter": 12,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Он удержит воды,"
  },
  {
    "chapter": 12,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и будет засуха;"
  },
  {
    "chapter": 12,
    "verse": 15,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "отпустит они затопят землю."
  },
  {
    "chapter": 12,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "У Него всесилие"
  },
  {
    "chapter": 12,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и премудрость;"
  },
  {
    "chapter": 12,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "в Его власти и обманутый,"
  },
  {
    "chapter": 12,
    "verse": 16,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "и обманщик."
  },
  {
    "chapter": 12,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Советчиков Он гонит"
  },
  {
    "chapter": 12,
    "verse": 17,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "босыми и глупцами делает судей."
  },
  {
    "chapter": 12,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "У царей Он"
  },
  {
    "chapter": 12,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "развязывает пояса мантии,"
  },
  {
    "chapter": 12,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и обвязывает им бедра"
  },
  {
    "chapter": 12,
    "verse": 18,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "набедренной повязкой раба."
  },
  {
    "chapter": 12,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Священников Он"
  },
  {
    "chapter": 12,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "гонит босыми,"
  },
  {
    "chapter": 12,
    "verse": 19,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и низвергает сильных."
  },
  {
    "chapter": 12,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Он лишает речи"
  },
  {
    "chapter": 12,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "искусных советников"
  },
  {
    "chapter": 12,
    "verse": 20,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "и отбирает разум у старцев."
  },
  {
    "chapter": 12,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Он покрывает позором"
  },
  {
    "chapter": 12,
    "verse": 21,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "знатных и лишает оружия могучих."
  },
  {
    "chapter": 12,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Он открывает глубины тьмы"
  },
  {
    "chapter": 12,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "и выводит на свет тень"
  },
  {
    "chapter": 12,
    "verse": 22,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "смертную."
  },
  {
    "chapter": 12,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Он возвышает и"
  },
  {
    "chapter": 12,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "губит народы,"
  },
  {
    "chapter": 12,
    "verse": 23,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "умножает их и рассеивает."
  },
  {
    "chapter": 12,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Он лишает рассудка земных"
  },
  {
    "chapter": 12,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "владык и шлет их в пустыню,"
  },
  {
    "chapter": 12,
    "verse": 24,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "где нет пути."
  },
  {
    "chapter": 12,
    "verse": 25,
    "book_name": "Job",
    "book_id": "Job",
    "part": 1,
    "text": "Бредут они на"
  },
  {
    "chapter": 12,
    "verse": 25,
    "book_name": "Job",
    "book_id": "Job",
    "part": 2,
    "text": "ощупь в темноте,"
  },
  {
    "chapter": 12,
    "verse": 25,
    "book_name": "Job",
    "book_id": "Job",
    "part": 3,
    "text": "без света,"
  },
  {
    "chapter": 12,
    "verse": 25,
    "book_name": "Job",
    "book_id": "Job",
    "part": 4,
    "text": "и шатаются,"
  },
  {
    "chapter": 12,
    "verse": 25,
    "book_name": "Job",
    "book_id": "Job",
    "part": 5,
    "text": "словно пьяные."
  }
]