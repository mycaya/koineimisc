[
  {
    "chapter": 1,
    "verse": 1,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Пророчество о Ниневии."
  },
  {
    "chapter": 1,
    "verse": 1,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "Книга видений"
  },
  {
    "chapter": 1,
    "verse": 1,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "Наума елкосеянина."
  },
  {
    "chapter": 1,
    "verse": 2,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Господь ревнивый"
  },
  {
    "chapter": 1,
    "verse": 2,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "и мстительный Бог;"
  },
  {
    "chapter": 1,
    "verse": 2,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "Господь"
  },
  {
    "chapter": 1,
    "verse": 2,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "мстительный и гневливый."
  },
  {
    "chapter": 1,
    "verse": 2,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 5,
    "text": "Господь мстит Своим врагам"
  },
  {
    "chapter": 1,
    "verse": 2,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 6,
    "text": "Он держит гнев против Своих"
  },
  {
    "chapter": 1,
    "verse": 2,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 7,
    "text": "врагов."
  },
  {
    "chapter": 1,
    "verse": 3,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Господь долготерпелив и"
  },
  {
    "chapter": 1,
    "verse": 3,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "велик в Своем могуществе;"
  },
  {
    "chapter": 1,
    "verse": 3,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "Господь не оставит"
  },
  {
    "chapter": 1,
    "verse": 3,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "виновных без наказания."
  },
  {
    "chapter": 1,
    "verse": 3,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 5,
    "text": "Его шествие в буре и вихре,"
  },
  {
    "chapter": 1,
    "verse": 3,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 6,
    "text": "облака пыль от Его ног."
  },
  {
    "chapter": 1,
    "verse": 4,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Он приказывает морю,"
  },
  {
    "chapter": 1,
    "verse": 4,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "и оно высыхает,"
  },
  {
    "chapter": 1,
    "verse": 4,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "рекам и они иссякают."
  },
  {
    "chapter": 1,
    "verse": 4,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "Васан и Кармил увядают,"
  },
  {
    "chapter": 1,
    "verse": 4,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 5,
    "text": "и блекнут цветы на Ливане."
  },
  {
    "chapter": 1,
    "verse": 5,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Сотрясаются перед"
  },
  {
    "chapter": 1,
    "verse": 5,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "Ним горы и тают холмы."
  },
  {
    "chapter": 1,
    "verse": 5,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "Земля трепещет"
  },
  {
    "chapter": 1,
    "verse": 5,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "перед Его лицом,"
  },
  {
    "chapter": 1,
    "verse": 5,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 5,
    "text": "трепещет мир и"
  },
  {
    "chapter": 1,
    "verse": 5,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 6,
    "text": "все живущее в нем."
  },
  {
    "chapter": 1,
    "verse": 6,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Кто устоит перед Его"
  },
  {
    "chapter": 1,
    "verse": 6,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "негодованием? Кто сможет"
  },
  {
    "chapter": 1,
    "verse": 6,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "выдержать Его пылающий"
  },
  {
    "chapter": 1,
    "verse": 6,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "гнев? Подобно огню"
  },
  {
    "chapter": 1,
    "verse": 6,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 5,
    "text": "разливается Его гнев,"
  },
  {
    "chapter": 1,
    "verse": 6,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 6,
    "text": "скалы"
  },
  {
    "chapter": 1,
    "verse": 6,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 7,
    "text": "рассыпаются перед Ним."
  },
  {
    "chapter": 1,
    "verse": 7,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Господь благ,"
  },
  {
    "chapter": 1,
    "verse": 7,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "Он убежище в дни бедствий."
  },
  {
    "chapter": 1,
    "verse": 7,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "Он заботится о тех,"
  },
  {
    "chapter": 1,
    "verse": 7,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "кто в Нем ищет прибежища,"
  },
  {
    "chapter": 1,
    "verse": 8,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "но всепотопляющим"
  },
  {
    "chapter": 1,
    "verse": 8,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "наводнением Он разрушит"
  },
  {
    "chapter": 1,
    "verse": 8,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "Ниневию до основания;"
  },
  {
    "chapter": 1,
    "verse": 8,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "мрак настигнет Его врагов."
  },
  {
    "chapter": 1,
    "verse": 9,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Что вы замышляете против"
  },
  {
    "chapter": 1,
    "verse": 9,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "Господа? Он"
  },
  {
    "chapter": 1,
    "verse": 9,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "истребит вас до конца"
  },
  {
    "chapter": 1,
    "verse": 9,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "и бедствие уже не"
  },
  {
    "chapter": 1,
    "verse": 9,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 5,
    "text": "повторится."
  },
  {
    "chapter": 1,
    "verse": 10,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Они запутаются в"
  },
  {
    "chapter": 1,
    "verse": 10,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "терновнике и будут пьяны от вина,"
  },
  {
    "chapter": 1,
    "verse": 10,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "они будут уничтожены,"
  },
  {
    "chapter": 1,
    "verse": 10,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "как сухое жнивье."
  },
  {
    "chapter": 1,
    "verse": 11,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Из тебя,"
  },
  {
    "chapter": 1,
    "verse": 11,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "Ниневия,"
  },
  {
    "chapter": 1,
    "verse": 11,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "вышел тот,"
  },
  {
    "chapter": 1,
    "verse": 11,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "кто замышляет"
  },
  {
    "chapter": 1,
    "verse": 11,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 5,
    "text": "злое против Господа,"
  },
  {
    "chapter": 1,
    "verse": 11,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 6,
    "text": "кто советует беззаконное."
  },
  {
    "chapter": 1,
    "verse": 12,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Так говорит Господь:"
  },
  {
    "chapter": 1,
    "verse": 12,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "Хотя у них есть союзники"
  },
  {
    "chapter": 1,
    "verse": 12,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "и их великое множество,"
  },
  {
    "chapter": 1,
    "verse": 12,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "они будут"
  },
  {
    "chapter": 1,
    "verse": 12,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 5,
    "text": "уничтожены и исчезнут,"
  },
  {
    "chapter": 1,
    "verse": 12,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 6,
    "text": "а тебя,"
  },
  {
    "chapter": 1,
    "verse": 12,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 7,
    "text": "Иудея,"
  },
  {
    "chapter": 1,
    "verse": 12,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 8,
    "text": "раньше Я отягощал но"
  },
  {
    "chapter": 1,
    "verse": 12,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 9,
    "text": "впредь уже не стану."
  },
  {
    "chapter": 1,
    "verse": 13,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "И теперь Я сокрушу ярмо,"
  },
  {
    "chapter": 1,
    "verse": 13,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "что на твоей шее и"
  },
  {
    "chapter": 1,
    "verse": 13,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "разорву твои оковы."
  },
  {
    "chapter": 1,
    "verse": 14,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Господь дал"
  },
  {
    "chapter": 1,
    "verse": 14,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "следующее повеление о тебе:"
  },
  {
    "chapter": 1,
    "verse": 14,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "Не станет у тебя потомков,"
  },
  {
    "chapter": 1,
    "verse": 14,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "носящих твое имя."
  },
  {
    "chapter": 1,
    "verse": 14,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 5,
    "text": "Я разрушу твоих"
  },
  {
    "chapter": 1,
    "verse": 14,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 6,
    "text": "идолов и уничтожу истуканы,"
  },
  {
    "chapter": 1,
    "verse": 14,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 7,
    "text": "что находятся в"
  },
  {
    "chapter": 1,
    "verse": 14,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 8,
    "text": "храмах твоих богов."
  },
  {
    "chapter": 1,
    "verse": 14,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 9,
    "text": "Я приготовлю тебе могилу,"
  },
  {
    "chapter": 1,
    "verse": 14,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 10,
    "text": "потому что ты презираема."
  },
  {
    "chapter": 1,
    "verse": 15,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 1,
    "text": "Смотри,"
  },
  {
    "chapter": 1,
    "verse": 15,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 2,
    "text": "идет по горам гонец,"
  },
  {
    "chapter": 1,
    "verse": 15,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 3,
    "text": "возвещающий"
  },
  {
    "chapter": 1,
    "verse": 15,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 4,
    "text": "радостную весть,"
  },
  {
    "chapter": 1,
    "verse": 15,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 5,
    "text": "провозглашающий мир!"
  },
  {
    "chapter": 1,
    "verse": 15,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 6,
    "text": "Отмечай свои праздники,"
  },
  {
    "chapter": 1,
    "verse": 15,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 7,
    "text": "Иудея исполняй"
  },
  {
    "chapter": 1,
    "verse": 15,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 8,
    "text": "свои обещания."
  },
  {
    "chapter": 1,
    "verse": 15,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 9,
    "text": "Не вторгнется больше"
  },
  {
    "chapter": 1,
    "verse": 15,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 10,
    "text": "беззаконный в твои владения,"
  },
  {
    "chapter": 1,
    "verse": 15,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 11,
    "text": "потому что будет"
  },
  {
    "chapter": 1,
    "verse": 15,
    "book_name": "Nahum",
    "book_id": "Nah",
    "part": 12,
    "text": "полностью уничтожен."
  }
]