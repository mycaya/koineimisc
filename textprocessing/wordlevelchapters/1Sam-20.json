    "chapter": 20,
    "verse": 16,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 1,
    "text": "Ионафан заключил"
  },
  {
    "chapter": 20,
    "verse": 16,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 2,
    "text": "союз с домом Давида,"
  },
  {
    "chapter": 20,
    "verse": 16,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 3,
    "text": "говоря:"
  },
  {
    "chapter": 20,
    "verse": 16,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 4,
    "text": "Пусть Господь призовет"
  },
  {
    "chapter": 20,
    "verse": 16,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 5,
    "text": "к ответу врагов Давида."
  },
  {
    "chapter": 20,
    "verse": 17,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 1,
    "text": "Ионафан заставил Давида"
  },
  {
    "chapter": 20,
    "verse": 17,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 2,
    "text": "поклясться вновь"
  },
  {
    "chapter": 20,
    "verse": 17,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 3,
    "text": "из-за любви к нему,"
  },
  {
    "chapter": 20,
    "verse": 17,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 4,
    "text": "потому что он любил его,"
  },
  {
    "chapter": 20,
    "verse": 17,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 5,
    "text": "как самого себя."
  },
  {
    "chapter": 20,
    "verse": 18,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 1,
    "text": "Затем Ионафан"
  },
  {
    "chapter": 20,
    "verse": 18,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 2,
    "text": "сказал Давиду:"
  },
  {
    "chapter": 20,
    "verse": 18,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 3,
    "text": "Завтра праздник Новолуния."
  },
  {
    "chapter": 20,
    "verse": 18,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 4,
    "text": "Твое отсутствие"
  },
  {
    "chapter": 20,
    "verse": 18,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 5,
    "text": "будет замечено,"
  },
  {
    "chapter": 20,
    "verse": 18,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 6,
    "text": "потому что твое"
  },
  {
    "chapter": 20,
    "verse": 18,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 7,
    "text": "место будет пустым."
  },
  {
    "chapter": 20,
    "verse": 19,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 1,
    "text": "Послезавтра под"
  },
  {
    "chapter": 20,
    "verse": 19,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 2,
    "text": "вечер ступай к месту,"
  },
  {
    "chapter": 20,
    "verse": 19,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 3,
    "text": "где ты прятался прежде,"
  },
  {
    "chapter": 20,
    "verse": 19,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 4,
    "text": "и жди там у каменной груды."
  },
  {
    "chapter": 20,
    "verse": 20,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 1,
    "text": "Я пущу в ту"
  },
  {
    "chapter": 20,
    "verse": 20,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 2,
    "text": "сторону три стрелы,"
  },
  {
    "chapter": 20,
    "verse": 20,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 3,
    "text": "словно стреляя в цель."
  },
  {
    "chapter": 20,
    "verse": 21,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 1,
    "text": "Затем я пошлю"
  },
  {
    "chapter": 20,
    "verse": 21,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 2,
    "text": "мальчика и скажу:"
  },
  {
    "chapter": 20,
    "verse": 21,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 3,
    "text": "Ступай,"
  },
  {
    "chapter": 20,
    "verse": 21,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 4,
    "text": "найди стрелы."
  },
  {
    "chapter": 20,
    "verse": 21,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 5,
    "text": "Если я скажу ему:"
  },
  {
    "chapter": 20,
    "verse": 21,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 6,
    "text": "Стрелы позади тебя,"
  },
  {
    "chapter": 20,
    "verse": 21,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 7,
    "text": "принеси их сюда,"
  },
  {
    "chapter": 20,
    "verse": 21,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 8,
    "text": "тогда приходи,"
  },
  {
    "chapter": 20,
    "verse": 21,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 9,
    "text": "потому что верно,"
  },
  {
    "chapter": 20,
    "verse": 21,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 10,
    "text": "как и то,"
  },
  {
    "chapter": 20,
    "verse": 21,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 11,
    "text": "что Господь жив,"
  },
  {
    "chapter": 20,
    "verse": 21,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 12,
    "text": "ты в безопасности;"
  },
  {
    "chapter": 20,
    "verse": 21,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 13,
    "text": "угрозы нет."
  },
  {
    "chapter": 20,
    "verse": 22,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 1,
    "text": "Но если я скажу мальчику:"
  },
  {
    "chapter": 20,
    "verse": 22,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 2,
    "text": "Стрелы впереди тебя,"
  },
  {
    "chapter": 20,
    "verse": 22,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 3,
    "text": "тогда уходи,"
  },
  {
    "chapter": 20,
    "verse": 22,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 4,
    "text": "потому что"
  },
  {
    "chapter": 20,
    "verse": 22,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 5,
    "text": "Господь отсылает тебя."
  },
  {
    "chapter": 20,
    "verse": 23,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 1,
    "text": "А что до дела,"
  },
  {
    "chapter": 20,
    "verse": 23,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 2,
    "text": "о котором мы говорили,"
  },
  {
    "chapter": 20,
    "verse": 23,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 3,
    "text": "помни,"
  },
  {
    "chapter": 20,
    "verse": 23,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 4,
    "text": "Господь свидетель между"
  },
  {
    "chapter": 20,
    "verse": 23,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 5,
    "text": "мной и тобой навсегда."
  },
  {
    "chapter": 20,
    "verse": 24,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 1,
    "text": "Давид спрятался в поле."
  },
  {
    "chapter": 20,
    "verse": 24,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 2,
    "text": "Когда начался"
  },
  {
    "chapter": 20,
    "verse": 24,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 3,
    "text": "праздник Новолуния,"
  },
  {
    "chapter": 20,
    "verse": 24,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 4,
    "text": "царь сел есть."
  },
  {
    "chapter": 20,
    "verse": 25,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 1,
    "text": "Царь сел на свое"
  },
  {
    "chapter": 20,
    "verse": 25,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 2,
    "text": "обычное место у стены,"
  },
  {
    "chapter": 20,
    "verse": 25,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 3,
    "text": "напротив Ионафана,"
  },
  {
    "chapter": 20,
    "verse": 25,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 4,
    "text": "Авенир сел рядом с Саулом,"
  },
  {
    "chapter": 20,
    "verse": 25,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 5,
    "text": "но место Давида"
  },
  {
    "chapter": 20,
    "verse": 25,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 6,
    "text": "осталось пустым."
  },
  {
    "chapter": 20,
    "verse": 26,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 1,
    "text": "В тот день Саул"
  },
  {
    "chapter": 20,
    "verse": 26,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 2,
    "text": "ничего не сказал,"
  },
  {
    "chapter": 20,
    "verse": 26,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 3,
    "text": "потому что думал:"
  },
  {
    "chapter": 20,
    "verse": 26,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 4,
    "text": "Должно быть,"
  },
  {
    "chapter": 20,
    "verse": 26,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 5,
    "text": "с Давидом"
  },
  {
    "chapter": 20,
    "verse": 26,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 6,
    "text": "случилось что-нибудь,"
  },
  {
    "chapter": 20,
    "verse": 26,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 7,
    "text": "что сделало его"
  },
  {
    "chapter": 20,
    "verse": 26,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 8,
    "text": "ритуально нечистым;"
  },
  {
    "chapter": 20,
    "verse": 26,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 9,
    "text": "ну конечно же,"
  },
  {
    "chapter": 20,
    "verse": 26,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 10,
    "text": "он нечист."
  },
  {
    "chapter": 20,
    "verse": 27,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 1,
    "text": "Но на следующий день,"
  },
  {
    "chapter": 20,
    "verse": 27,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 2,
    "text": "второй день месяца,"
  },
  {
    "chapter": 20,
    "verse": 27,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 3,
    "text": "место Давида"
  },
  {
    "chapter": 20,
    "verse": 27,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 4,
    "text": "опять было пустым."
  },
  {
    "chapter": 20,
    "verse": 27,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 5,
    "text": "Тогда Саул сказал"
  },
  {
    "chapter": 20,
    "verse": 27,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 6,
    "text": "своему сыну Ионафану:"
  },
  {
    "chapter": 20,
    "verse": 27,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 7,
    "text": "Почему сын Иессея не"
  },
  {
    "chapter": 20,
    "verse": 27,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 8,
    "text": "вышел к столу ни вчера,"
  },
  {
    "chapter": 20,
    "verse": 27,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 9,
    "text": "ни сегодня?"
  },
  {
    "chapter": 20,
    "verse": 28,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 1,
    "text": "Ионафан ответил:"
  },
  {
    "chapter": 20,
    "verse": 28,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 2,
    "text": "Давид очень просил меня"
  },
  {
    "chapter": 20,
    "verse": 28,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 3,
    "text": "позволить ему пойти в Вифлеем."
  },
  {
    "chapter": 20,
    "verse": 29,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 1,
    "text": "Он сказал:"
  },
  {
    "chapter": 20,
    "verse": 29,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 2,
    "text": "Отпусти меня,"
  },
  {
    "chapter": 20,
    "verse": 29,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 3,
    "text": "потому что наша семья"
  },
  {
    "chapter": 20,
    "verse": 29,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 4,
    "text": "совершает в городе"
  },
  {
    "chapter": 20,
    "verse": 29,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 5,
    "text": "жертвоприношение,"
  },
  {
    "chapter": 20,
    "verse": 29,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 6,
    "text": "и мой брат"
  },
  {
    "chapter": 20,
    "verse": 29,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 7,
    "text": "велел мне быть там."
  },
  {
    "chapter": 20,
    "verse": 29,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 8,
    "text": "Если ты расположен ко мне,"
  },
  {
    "chapter": 20,
    "verse": 29,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 9,
    "text": "отпусти меня"
  },
  {
    "chapter": 20,
    "verse": 29,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 10,
    "text": "повидаться с братьями."
  },
  {
    "chapter": 20,
    "verse": 29,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 11,
    "text": "Вот почему он не"
  },
  {
    "chapter": 20,
    "verse": 29,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 12,
    "text": "явился к царскому столу."
  },
  {
    "chapter": 20,
    "verse": 30,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 1,
    "text": "Саул разгневался на"
  },
  {
    "chapter": 20,
    "verse": 30,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 2,
    "text": "Ионафана и сказал ему:"
  },
  {
    "chapter": 20,
    "verse": 30,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 3,
    "text": "Ты сын упрямой и мятежной"
  },
  {
    "chapter": 20,
    "verse": 30,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 4,
    "text": "женщины! Разве я не знаю,"
  },
  {
    "chapter": 20,
    "verse": 30,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 5,
    "text": "что ты сговорился с сыном"
  },
  {
    "chapter": 20,
    "verse": 30,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 6,
    "text": "Иессея себе же на стыд"
  },
  {
    "chapter": 20,
    "verse": 30,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 7,
    "text": "и на стыд матери,"
  },
  {
    "chapter": 20,
    "verse": 30,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 8,
    "text": "которая тебя родила?"
  },
  {
    "chapter": 20,
    "verse": 31,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 1,
    "text": "Пока сын Иессея"
  },
  {
    "chapter": 20,
    "verse": 31,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 2,
    "text": "живет на этой земле,"
  },
  {
    "chapter": 20,
    "verse": 31,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 3,
    "text": "ни ты,"
  },
  {
    "chapter": 20,
    "verse": 31,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 4,
    "text": "ни твое царство не устоят."
  },
  {
    "chapter": 20,
    "verse": 31,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 5,
    "text": "Пошли и приведи его ко мне,"
  },
  {
    "chapter": 20,
    "verse": 31,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 6,
    "text": "потому что он"
  },
  {
    "chapter": 20,
    "verse": 31,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 7,
    "text": "должен умереть!"
  },
  {
    "chapter": 20,
    "verse": 32,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 1,
    "text": "- За что предавать его"
  },
  {
    "chapter": 20,
    "verse": 32,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 2,
    "text": "смерти? Что он сделал?"
  },
  {
    "chapter": 20,
    "verse": 32,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 3,
    "text": "спросил отца Ионафан."
  },
  {
    "chapter": 20,
    "verse": 33,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 1,
    "text": "Но Саул метнул"
  },
  {
    "chapter": 20,
    "verse": 33,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
    "part": 2,
    "text": "в него копье,"
  },
  {
    "chapter": 20,
    "verse": 33,
    "book_name": "1 Samuel",
    "book_id": "1Sam",
