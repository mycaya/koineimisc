[
  {
    "chapter": 106,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Аллилуйя! Блажен человек,"
  },
  {
    "chapter": 106,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "боящийся Господа,"
  },
  {
    "chapter": 106,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "получающий большое"
  },
  {
    "chapter": 106,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "наслаждение от Его повелений."
  },
  {
    "chapter": 106,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Могущественным будет"
  },
  {
    "chapter": 106,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "на земле его потомство:"
  },
  {
    "chapter": 106,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "поколение праведных"
  },
  {
    "chapter": 106,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "будет благословлено."
  },
  {
    "chapter": 106,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Изобилие и"
  },
  {
    "chapter": 106,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "богатство будут в его доме,"
  },
  {
    "chapter": 106,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и его праведность"
  },
  {
    "chapter": 106,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "будет длиться вечно."
  },
  {
    "chapter": 106,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Для честных"
  },
  {
    "chapter": 106,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "восходит свет во тьме,"
  },
  {
    "chapter": 106,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "он милостив,"
  },
  {
    "chapter": 106,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "милосерден и праведен."
  },
  {
    "chapter": 106,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Благо человеку,"
  },
  {
    "chapter": 106,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "который великодушен"
  },
  {
    "chapter": 106,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и щедро дает взаймы,"
  },
  {
    "chapter": 106,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "тому,"
  },
  {
    "chapter": 106,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "кто ведет свои"
  },
  {
    "chapter": 106,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "дела справедливо."
  },
  {
    "chapter": 106,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он никогда не поколеблется;"
  },
  {
    "chapter": 106,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "праведник будет"
  },
  {
    "chapter": 106,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "в вечной памяти."
  },
  {
    "chapter": 106,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Не побоится"
  },
  {
    "chapter": 106,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "плохих известий;"
  },
  {
    "chapter": 106,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "сердце его твердо,"
  },
  {
    "chapter": 106,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "уповая на Господа."
  },
  {
    "chapter": 106,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Сердце его крепко"
  },
  {
    "chapter": 106,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "он не испугается;"
  },
  {
    "chapter": 106,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и он увидит"
  },
  {
    "chapter": 106,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "падение своих врагов."
  },
  {
    "chapter": 106,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он щедро раздал"
  },
  {
    "chapter": 106,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "свое имущество бедным,"
  },
  {
    "chapter": 106,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и его"
  },
  {
    "chapter": 106,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "праведность длится вовек,"
  },
  {
    "chapter": 106,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "рог его будет"
  },
  {
    "chapter": 106,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "восхвален в славе."
  },
  {
    "chapter": 106,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Нечестивый"
  },
  {
    "chapter": 106,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "увидит и разозлится,"
  },
  {
    "chapter": 106,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "заскрипит"
  },
  {
    "chapter": 106,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "зубами и исчезнет."
  },
  {
    "chapter": 106,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Желание"
  },
  {
    "chapter": 106,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "нечестивых не сбудется."
  }
]