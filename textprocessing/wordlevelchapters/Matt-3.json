[
  {
    "chapter": 3,
    "verse": 1,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 1,
    "text": "В те дни Иоанн Креститель"
  },
  {
    "chapter": 3,
    "verse": 1,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 2,
    "text": "начал проповедовать в"
  },
  {
    "chapter": 3,
    "verse": 1,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 3,
    "text": "Иудейской пустыне."
  },
  {
    "chapter": 3,
    "verse": 2,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 1,
    "text": "- Покайтесь! говорил он."
  },
  {
    "chapter": 3,
    "verse": 2,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 2,
    "text": "Потому что Царство"
  },
  {
    "chapter": 3,
    "verse": 2,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 3,
    "text": "Божье уже близко!"
  },
  {
    "chapter": 3,
    "verse": 3,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 1,
    "text": "Об Иоанне"
  },
  {
    "chapter": 3,
    "verse": 3,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 2,
    "text": "говорил пророк Исаия:"
  },
  {
    "chapter": 3,
    "verse": 3,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 3,
    "text": "Голос раздается в пустыне:"
  },
  {
    "chapter": 3,
    "verse": 3,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 4,
    "text": "Приготовьте путь Господу!"
  },
  {
    "chapter": 3,
    "verse": 3,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 5,
    "text": "Сделайте прямыми дороги Его!"
  },
  {
    "chapter": 3,
    "verse": 4,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 1,
    "text": "Иоанн носил одежду"
  },
  {
    "chapter": 3,
    "verse": 4,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 2,
    "text": "из верблюжьей шерсти"
  },
  {
    "chapter": 3,
    "verse": 4,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 3,
    "text": "и подпоясывался кожаным"
  },
  {
    "chapter": 3,
    "verse": 4,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 4,
    "text": "поясом."
  },
  {
    "chapter": 3,
    "verse": 4,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 5,
    "text": "Пищей ему служили"
  },
  {
    "chapter": 3,
    "verse": 4,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 6,
    "text": "саранча и дикий мед."
  },
  {
    "chapter": 3,
    "verse": 5,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 1,
    "text": "К нему приходил"
  },
  {
    "chapter": 3,
    "verse": 5,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 2,
    "text": "народ из Иерусалима,"
  },
  {
    "chapter": 3,
    "verse": 5,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 3,
    "text": "со всей Иудеи и из"
  },
  {
    "chapter": 3,
    "verse": 5,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 4,
    "text": "окрестностей Иордана."
  },
  {
    "chapter": 3,
    "verse": 6,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 1,
    "text": "Исповедующих свои грехи"
  },
  {
    "chapter": 3,
    "verse": 6,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 2,
    "text": "он крестил в реке Иордан."
  },
  {
    "chapter": 3,
    "verse": 7,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 1,
    "text": "Фарисеям и саддукеям,"
  },
  {
    "chapter": 3,
    "verse": 7,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 2,
    "text": "которые приходили к нему,"
  },
  {
    "chapter": 3,
    "verse": 7,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 3,
    "text": "Иоанн сказал:"
  },
  {
    "chapter": 3,
    "verse": 7,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 4,
    "text": "Вы,"
  },
  {
    "chapter": 3,
    "verse": 7,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 5,
    "text": "змеиное отродье!"
  },
  {
    "chapter": 3,
    "verse": 7,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 6,
    "text": "Кто предупредил вас,"
  },
  {
    "chapter": 3,
    "verse": 7,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 7,
    "text": "чтобы вы бежали от"
  },
  {
    "chapter": 3,
    "verse": 7,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 8,
    "text": "грядущего возмездия?"
  },
  {
    "chapter": 3,
    "verse": 8,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 1,
    "text": "Покажите плоды"
  },
  {
    "chapter": 3,
    "verse": 8,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 2,
    "text": "вашего покаяния."
  },
  {
    "chapter": 3,
    "verse": 9,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 1,
    "text": "Не думайте говорить в себе:"
  },
  {
    "chapter": 3,
    "verse": 9,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 2,
    "text": "Мы дети"
  },
  {
    "chapter": 3,
    "verse": 9,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 3,
    "text": "Авраама! Говорю вам,"
  },
  {
    "chapter": 3,
    "verse": 9,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 4,
    "text": "что Бог может из этих"
  },
  {
    "chapter": 3,
    "verse": 9,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 5,
    "text": "камней сотворить детей Аврааму."
  },
  {
    "chapter": 3,
    "verse": 10,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 1,
    "text": "Уже и топор лежит"
  },
  {
    "chapter": 3,
    "verse": 10,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 2,
    "text": "у корня деревьев,"
  },
  {
    "chapter": 3,
    "verse": 10,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 3,
    "text": "и каждое дерево,"
  },
  {
    "chapter": 3,
    "verse": 10,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 4,
    "text": "не приносящее"
  },
  {
    "chapter": 3,
    "verse": 10,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 5,
    "text": "хорошего плода,"
  },
  {
    "chapter": 3,
    "verse": 10,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 6,
    "text": "будет срублено и"
  },
  {
    "chapter": 3,
    "verse": 10,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 7,
    "text": "брошено в огонь."
  },
  {
    "chapter": 3,
    "verse": 11,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 1,
    "text": "Я крещу вас водой,"
  },
  {
    "chapter": 3,
    "verse": 11,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 2,
    "text": "но после меня придет Тот,"
  },
  {
    "chapter": 3,
    "verse": 11,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 3,
    "text": "Кто могущественнее меня."
  },
  {
    "chapter": 3,
    "verse": 11,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 4,
    "text": "Я даже не достоин"
  },
  {
    "chapter": 3,
    "verse": 11,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 5,
    "text": "нести Его сандалии."
  },
  {
    "chapter": 3,
    "verse": 11,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 6,
    "text": "Он будет крестить вас"
  },
  {
    "chapter": 3,
    "verse": 11,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 7,
    "text": "Святым Духом и огнем."
  },
  {
    "chapter": 3,
    "verse": 12,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 1,
    "text": "У Него в руках"
  },
  {
    "chapter": 3,
    "verse": 12,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 2,
    "text": "лопата для провеивания,"
  },
  {
    "chapter": 3,
    "verse": 12,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 3,
    "text": "и Он очистит"
  },
  {
    "chapter": 3,
    "verse": 12,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 4,
    "text": "зерно от соломы."
  },
  {
    "chapter": 3,
    "verse": 12,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 5,
    "text": "Пшеницу Он"
  },
  {
    "chapter": 3,
    "verse": 12,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 6,
    "text": "соберет в хранилище,"
  },
  {
    "chapter": 3,
    "verse": 12,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 7,
    "text": "а солому сожжет в"
  },
  {
    "chapter": 3,
    "verse": 12,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 8,
    "text": "неугасимом огне."
  },
  {
    "chapter": 3,
    "verse": 13,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 1,
    "text": "Затем из Галилеи к"
  },
  {
    "chapter": 3,
    "verse": 13,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 2,
    "text": "Иордану пришел Иисус,"
  },
  {
    "chapter": 3,
    "verse": 13,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 3,
    "text": "чтобы принять"
  },
  {
    "chapter": 3,
    "verse": 13,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 4,
    "text": "крещение от Иоанна."
  },
  {
    "chapter": 3,
    "verse": 14,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 1,
    "text": "Иоанн хотел было"
  },
  {
    "chapter": 3,
    "verse": 14,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 2,
    "text": "отговорить Его:"
  },
  {
    "chapter": 3,
    "verse": 14,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 3,
    "text": "Это мне нужно"
  },
  {
    "chapter": 3,
    "verse": 14,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 4,
    "text": "креститься у Тебя,"
  },
  {
    "chapter": 3,
    "verse": 14,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 5,
    "text": "зачем же Ты пришел ко мне?"
  },
  {
    "chapter": 3,
    "verse": 15,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 1,
    "text": "Но Иисус ответил:"
  },
  {
    "chapter": 3,
    "verse": 15,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 2,
    "text": "Пусть сейчас будет так."
  },
  {
    "chapter": 3,
    "verse": 15,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 3,
    "text": "Нам надо"
  },
  {
    "chapter": 3,
    "verse": 15,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 4,
    "text": "исполнить всю истину."
  },
  {
    "chapter": 3,
    "verse": 15,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 5,
    "text": "Тогда Иоанн согласился."
  },
  {
    "chapter": 3,
    "verse": 16,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 1,
    "text": "Как только Иисус"
  },
  {
    "chapter": 3,
    "verse": 16,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 2,
    "text": "крестился и вышел из воды,"
  },
  {
    "chapter": 3,
    "verse": 16,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 3,
    "text": "раскрылись небеса,"
  },
  {
    "chapter": 3,
    "verse": 16,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 4,
    "text": "и Он увидел Духа Божьего,"
  },
  {
    "chapter": 3,
    "verse": 16,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 5,
    "text": "спускающегося в образе"
  },
  {
    "chapter": 3,
    "verse": 16,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 6,
    "text": "голубя и севшего на Него."
  },
  {
    "chapter": 3,
    "verse": 17,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 1,
    "text": "И вот,"
  },
  {
    "chapter": 3,
    "verse": 17,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 2,
    "text": "с небес прозвучал голос:"
  },
  {
    "chapter": 3,
    "verse": 17,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 3,
    "text": "Это Мой любимый Сын,"
  },
  {
    "chapter": 3,
    "verse": 17,
    "book_name": "Matthew",
    "book_id": "Matt",
    "part": 4,
    "text": "в Нем Моя радость!"
  }
]