    "chapter": 6,
    "verse": 1,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 8,
    "text": "к которым"
  },
  {
    "chapter": 6,
    "verse": 1,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 9,
    "text": "приходят израильтяне!"
  },
  {
    "chapter": 6,
    "verse": 2,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 1,
    "text": "Пойдите в Калне и"
  },
  {
    "chapter": 6,
    "verse": 2,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 2,
    "text": "посмотрите на него;"
  },
  {
    "chapter": 6,
    "verse": 2,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 3,
    "text": "оттуда пойдите"
  },
  {
    "chapter": 6,
    "verse": 2,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 4,
    "text": "в великий Емаф,"
  },
  {
    "chapter": 6,
    "verse": 2,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 5,
