    "chapter": 6,
    "verse": 14,
    "book_name": "Amos",
    "book_id": "Amos",
    "part": 7,
    "text": "от Лево-Емафа до"
  },
  {
    "chapter": 6,
    "verse": 14,
    "book_name": "Amos",
