  {
    "chapter": 4,
    "verse": 5,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 2,
    "text": "спотыкаетесь днем и ночью,"
  },
  {
    "chapter": 4,
    "verse": 5,
    "book_name": "Hosea",
    "book_id": "Hos",
    "part": 3,
    "text": "и пророки"
