  {
    "chapter": 8,
    "verse": 50,
    "book_name": "Luke",
    "book_id": "Luke",
    "part": 1,
    "text": "Услышав это,"
  },
  {
    "chapter": 8,
    "verse": 50,
    "book_name": "Luke",
    "book_id": "Luke",
