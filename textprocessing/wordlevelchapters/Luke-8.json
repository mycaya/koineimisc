    "chapter": 8,
    "verse": 49,
    "book_name": "Luke",
    "book_id": "Luke",
    "part": 2,
    "text": "как пришел человек из"
  },
  {
    "chapter": 8,
    "verse": 49,
    "book_name": "Luke",
