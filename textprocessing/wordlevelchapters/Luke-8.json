    "chapter": 8,
    "verse": 49,
    "book_name": "Luke",
    "book_id": "Luke",
    "part": 4,
    "text": "Твоя дочь умерла,"
  },
  {
    "chapter": 8,
    "verse": 49,
    "book_name": "Luke",
    "book_id": "Luke",
    "part": 5,
    "text": "сказал он,"
  },
