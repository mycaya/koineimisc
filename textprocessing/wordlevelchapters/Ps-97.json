[
  {
    "chapter": 97,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Молитва страдальца,"
  },
  {
    "chapter": 97,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "когда он удручен и"
  },
  {
    "chapter": 97,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "изливает свою жалобу перед"
  },
  {
    "chapter": 97,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Господом."
  },
  {
    "chapter": 97,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Услышь мою молитву,"
  },
  {
    "chapter": 97,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 97,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "пусть дойдет до"
  },
  {
    "chapter": 97,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Тебя мой плач."
  },
  {
    "chapter": 97,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Не сокрой от меня Своего"
  },
  {
    "chapter": 97,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "лица в день моей скорби,"
  },
  {
    "chapter": 97,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "приклони ухо Твое ко мне,"
  },
  {
    "chapter": 97,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "когда взываю к Тебе,"
  },
  {
    "chapter": 97,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "ответь скорее."
  },
  {
    "chapter": 97,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Дни мои исчезли,"
  },
  {
    "chapter": 97,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "как дым,"
  },
  {
    "chapter": 97,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и кости мои обожжены,"
  },
  {
    "chapter": 97,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "как в огне."
  },
  {
    "chapter": 97,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Побито,"
  },
  {
    "chapter": 97,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "как трава,"
  },
  {
    "chapter": 97,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "иссохло сердце мое,"
  },
  {
    "chapter": 97,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "так что я даже"
  },
  {
    "chapter": 97,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "забываю есть свой хлеб."
  },
  {
    "chapter": 97,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "От громкого стенания моего"
  },
  {
    "chapter": 97,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "остались от меня лишь кожа"
  },
  {
    "chapter": 97,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "да кости."
  },
  {
    "chapter": 97,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я как пустынная сова,"
  },
  {
    "chapter": 97,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "как домовой сыч"
  },
  {
    "chapter": 97,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "на развалинах."
  },
  {
    "chapter": 97,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Не сплю я и сижу,"
  },
  {
    "chapter": 97,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "как одинокая"
  },
  {
    "chapter": 97,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "птица на кровле."
  },
  {
    "chapter": 97,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Мои враги"
  },
  {
    "chapter": 97,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "поносят меня весь день,"
  },
  {
    "chapter": 97,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "насмехающиеся надо"
  },
  {
    "chapter": 97,
    "verse": 9,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "мной проклинают меня."
  },
  {
    "chapter": 97,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я ем пепел вместо хлеба"
  },
  {
    "chapter": 97,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и напиток свой разбавляю"
  },
  {
    "chapter": 97,
    "verse": 10,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "слезами"
  },
  {
    "chapter": 97,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "из-за великого"
  },
  {
    "chapter": 97,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "гнева Твоего,"
  },
  {
    "chapter": 97,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "потому что Ты вознес"
  },
  {
    "chapter": 97,
    "verse": 11,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "меня и бросил прочь."
  },
  {
    "chapter": 97,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Мои дни,"
  },
  {
    "chapter": 97,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "как уходящая тень,"
  },
  {
    "chapter": 97,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и я увядаю,"
  },
  {
    "chapter": 97,
    "verse": 12,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "как трава."
  },
  {
    "chapter": 97,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но Ты,"
  },
  {
    "chapter": 97,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Господи,"
  },
  {
    "chapter": 97,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "пребываешь вовек,"
  },
  {
    "chapter": 97,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и память о Тебе переходит"
  },
  {
    "chapter": 97,
    "verse": 13,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "из поколения в поколение."
  },
  {
    "chapter": 97,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Ты поднимешься и"
  },
  {
    "chapter": 97,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "сжалишься над Сионом,"
  },
  {
    "chapter": 97,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "потому что пора"
  },
  {
    "chapter": 97,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "помиловать его;"
  },
  {
    "chapter": 97,
    "verse": 14,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "пришло время."
  },
  {
    "chapter": 97,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Даже его камни"
  },
  {
    "chapter": 97,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "дороги Твоим слугам,"
  },
  {
    "chapter": 97,
    "verse": 15,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "они сожалеют о прахе его."
  },
  {
    "chapter": 97,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Народы убоятся"
  },
  {
    "chapter": 97,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Имени Господня,"
  },
  {
    "chapter": 97,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и все земные"
  },
  {
    "chapter": 97,
    "verse": 16,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "цари Твоей славы,"
  },
  {
    "chapter": 97,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "потому что Господь"
  },
  {
    "chapter": 97,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "восстановит Сион"
  },
  {
    "chapter": 97,
    "verse": 17,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и явится в Своей славе."
  },
  {
    "chapter": 97,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он услышит молитву"
  },
  {
    "chapter": 97,
    "verse": 18,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "неимущих и не презрит их молитвы."
  },
  {
    "chapter": 97,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Это будет написано"
  },
  {
    "chapter": 97,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "для будущих поколений,"
  },
  {
    "chapter": 97,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "чтобы грядущее"
  },
  {
    "chapter": 97,
    "verse": 19,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "поколение восславило Господа,"
  },
  {
    "chapter": 97,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "так как Он взглянул с"
  },
  {
    "chapter": 97,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "высот Своего святилища;"
  },
  {
    "chapter": 97,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "с небес Господь"
  },
  {
    "chapter": 97,
    "verse": 20,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "посмотрел на землю,"
  },
  {
    "chapter": 97,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "чтобы услышать стенания"
  },
  {
    "chapter": 97,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "узника"
  },
  {
    "chapter": 97,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и освободить приговоренных"
  },
  {
    "chapter": 97,
    "verse": 21,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "к смерти,"
  },
  {
    "chapter": 97,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Итак,"
  },
  {
    "chapter": 97,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "провозгласится Имя Господне"
  },
  {
    "chapter": 97,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "в Сионе"
  },
  {
    "chapter": 97,
    "verse": 22,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и Его слава в Иерусалиме,"
  },
  {
    "chapter": 97,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "когда соберутся вместе"
  },
  {
    "chapter": 97,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "народы"
  },
  {
    "chapter": 97,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и царства для служения"
  },
  {
    "chapter": 97,
    "verse": 23,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Господу."
  },
  {
    "chapter": 97,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Он истощил на"
  },
  {
    "chapter": 97,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "пути мои силы,"
  },
  {
    "chapter": 97,
    "verse": 24,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "сократил мои дни."
  },
  {
    "chapter": 97,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Я сказал:"
  },
  {
    "chapter": 97,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "О Господи,"
  },
  {
    "chapter": 97,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "не забирай меня в"
  },
  {
    "chapter": 97,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "середине моих дней Ты,"
  },
  {
    "chapter": 97,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "Чьи годы из"
  },
  {
    "chapter": 97,
    "verse": 25,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "поколения в поколение."
  },
  {
    "chapter": 97,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "В начале Ты"
  },
  {
    "chapter": 97,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "положил основание земли,"
  },
  {
    "chapter": 97,
    "verse": 26,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и небеса дело Твоих рук."
  },
  {
    "chapter": 97,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Они погибнут,"
  },
  {
    "chapter": 97,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "но Ты останешься навсегда;"
  },
  {
    "chapter": 97,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "они износятся как одежда,"
  },
  {
    "chapter": 97,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "и,"
  },
  {
    "chapter": 97,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "словно одеяние,"
  },
  {
    "chapter": 97,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "сменишь их на новые,"
  },
  {
    "chapter": 97,
    "verse": 27,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "и они изменятся."
  },
  {
    "chapter": 97,
    "verse": 28,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Но Ты Тот же,"
  },
  {
    "chapter": 97,
    "verse": 28,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "и годам Твоим нет конца."
  },
  {
    "chapter": 97,
    "verse": 29,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Сыновья Твоих"
  },
  {
    "chapter": 97,
    "verse": 29,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "слуг останутся,"
  },
  {
    "chapter": 97,
    "verse": 29,
    "book_name": "Psalms",
