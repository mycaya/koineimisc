[
  {
    "chapter": 132,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Псалом Давида."
  },
  {
    "chapter": 132,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "Буду славить Тебя"
  },
  {
    "chapter": 132,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "всем своим сердцем,"
  },
  {
    "chapter": 132,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "перед богами"
  },
  {
    "chapter": 132,
    "verse": 1,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "воспою хвалу Тебе."
  },
  {
    "chapter": 132,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Поклонюсь перед святым"
  },
  {
    "chapter": 132,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "храмом Твоим"
  },
  {
    "chapter": 132,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и буду славить Имя Твое за"
  },
  {
    "chapter": 132,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Твою милость и верность,"
  },
  {
    "chapter": 132,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "потому что Ты вознес"
  },
  {
    "chapter": 132,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "превыше всего Твое Имя"
  },
  {
    "chapter": 132,
    "verse": 2,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "и Твое слово."
  },
  {
    "chapter": 132,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "В день моего"
  },
  {
    "chapter": 132,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "моления Ты ответил мне,"
  },
  {
    "chapter": 132,
    "verse": 3,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "ободрил и укрепил мою душу."
  },
  {
    "chapter": 132,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господи,"
  },
  {
    "chapter": 132,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "все земные цари"
  },
  {
    "chapter": 132,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "будут славить Тебя,"
  },
  {
    "chapter": 132,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "когда услышат"
  },
  {
    "chapter": 132,
    "verse": 4,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "слова из Твоих уст."
  },
  {
    "chapter": 132,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Они воспоют пути Господни,"
  },
  {
    "chapter": 132,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "потому что"
  },
  {
    "chapter": 132,
    "verse": 5,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "велика слава Господня."
  },
  {
    "chapter": 132,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Высок Господь,"
  },
  {
    "chapter": 132,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "но о смиренном заботится"
  },
  {
    "chapter": 132,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "и высокомерного"
  },
  {
    "chapter": 132,
    "verse": 6,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "издали узнает."
  },
  {
    "chapter": 132,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Даже если пойду я"
  },
  {
    "chapter": 132,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "среди несчастья,"
  },
  {
    "chapter": 132,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Ты оживишь меня,"
  },
  {
    "chapter": 132,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "протянешь Твою руку"
  },
  {
    "chapter": 132,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "против гнева моих врагов,"
  },
  {
    "chapter": 132,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "и спасет меня"
  },
  {
    "chapter": 132,
    "verse": 7,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "Твоя правая рука."
  },
  {
    "chapter": 132,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 1,
    "text": "Господь завершит то,"
  },
  {
    "chapter": 132,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 2,
    "text": "что задумал для меня."
  },
  {
    "chapter": 132,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 3,
    "text": "Господи,"
  },
  {
    "chapter": 132,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 4,
    "text": "Твоя милость"
  },
  {
    "chapter": 132,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 5,
    "text": "на все времена:"
  },
  {
    "chapter": 132,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 6,
    "text": "не оставляй"
  },
  {
    "chapter": 132,
    "verse": 8,
    "book_name": "Psalms",
    "book_id": "Ps",
    "part": 7,
    "text": "творение Твоих рук!"
  }
]