[
    {
        "chapter": 6,
        "verse": 0,
        "text": "Потомки Левия: главные священники",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 1,
        "text": "Сыновья Левия: Гершон, Кааф и Мерари.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 2,
        "text": "Сыновья Каафа: Амрам, Ицгар, Хеврон и Уззиил.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 3,
        "text": "Дети Амрама: Аарон, Моисей и Мирьям. Сыновья Аарона: Надав, Авиуд, Элеазар и Итамар.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 4,
        "text": "Элеазар был отцом Пинехаса, Пинехас – отцом Авишуя,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 5,
        "text": "Авишуй – отцом Буккия, Буккий – отцом Уззия,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 6,
        "text": "Уззий – отцом Зерахии, Зерахия – отцом Мераиофа,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 7,
        "text": "Мераиоф – отцом Амарии, Амария – отцом Ахитува,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 8,
        "text": "Ахитув – отцом Цадока, Цадок – отцом Ахимааца,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 9,
        "text": "Ахимаац – отцом Азарии, Азария – отцом Иоханана,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 10,
        "text": "Иоханан – отцом Азарии (это он был священником в храме, построенном Соломоном в Иерусалиме),",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 11,
        "text": "Азария был отцом Амарии, Амария – отцом Ахитува,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 12,
        "text": "Ахитув – отцом Цадока, Цадок – отцом Шаллума,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 13,
        "text": "Шаллум – отцом Хелкии, Хелкия – отцом Азарии,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 14,
        "text": "Азария – отцом Сераи, и Серая – отцом Иоседека,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 15,
        "text": "Иоседек попал в плен, когда Господь отправил в плен Иудею и Иерусалим рукою Навуходоносора.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 16,
        "text": "Сыновья Левия: Гершон, Кааф и Мерари.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 17,
        "text": "Вот имена сыновей Гершона: Ливни и Шимей.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 18,
        "text": "Сыновья Каафа: Амрам, Ицгар, Хеврон и Уззиил.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 19,
        "text": "Сыновья Мерари: Махли и Муши. Вот кланы левитов, исчисленные по их отцам:",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 20,
        "text": "у Гершона: его сын Ливни, его сын Иахат, его сын Зимма,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 21,
        "text": "его сын Иоах, его сын Иддо, его сын Зерах, его сын Иеатрай.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 22,
        "text": "Потомки Каафа: его сын Аминадав, его сын Корах, его сын Асир,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 23,
        "text": "его сын Элкана, его сын Евиасаф, его сын Асир,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 24,
        "text": "его сын Тахаф, его сын Уриил, его сын Уззия, его сын Саул.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 25,
        "text": "Потомки Элканы: Амасай и Ахимоф,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 26,
        "text": "его сын Элкана, его сын Цофай, его сын Нахат,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 27,
        "text": "его сын Элиав, его сын Иерохам, его сын Элкана, его сын Самуил.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 28,
        "text": "Сыновья Самуила: Иоиль, его первенец, и Авия – второй сын.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 29,
        "text": "Потомки Мерари: Махли, его сын Ливни, его сын Шимей, его сын Узза,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 30,
        "text": "его сын Шима, его сын Хаггия, его сын Асая. Обязанности левитов",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 31,
        "text": "Вот те, кому Давид вверил песенную службу в доме Господнем, после того, как туда был перенесен ковчег.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 32,
        "text": "Они несли песенную службу перед скинией собрания, пока Соломон не выстроил Господу храм в Иерусалиме. Они исполняли свои обязанности по положенным для них установлениям.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 33,
        "text": "Вот имена тех, кто нес эту службу вместе со своими сыновьями: из каафитов – Еман певец, сын Иоиля, сына Самуила,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 34,
        "text": "сына Элканы, сына Иерохама, сына Элиэла, сына Тоаха,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 35,
        "text": "сына Цуфа, сына Элканы, сына Махата, сына Амасая,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 36,
        "text": "сына Элканы, сына Иоиля, сына Азарии, сына Софонии,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 37,
        "text": "сына Тахафа, сына Асира, сына Евиасафа, сына Кораха,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 38,
        "text": "сына Ицгара, сына Каафа, сына Левия, сына Израиля.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 39,
        "text": "И брат Емана Асаф, который совершал службу справа от него: Асаф, сын Берехии, сына Шимы,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 40,
        "text": "сына Михаила, сына Баасеи, сына Малхии,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 41,
        "text": "сына Ефния, сына Зераха, сына Адаи,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 42,
        "text": "сына Етана, сына Зиммы, сына Шимея,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 43,
        "text": "сына Иахата, сына Гершона, сына Левия.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 44,
        "text": "А из их братьев, сыновей Мерари, – с левой стороны: Етан, сын Кишия, сына Авдия, сына Маллуха,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 45,
        "text": "сына Хашавии, сына Амасии, сына Хелкии,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 46,
        "text": "сына Амция, сына Бани, сына Шемера,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 47,
        "text": "сына Махли, сына Муши, сына Мерари, сына Левия.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 48,
        "text": "Их братьям-левитам были поручены все прочие обязанности при доме Божьем.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 49,
        "text": "А Аарон и его потомки совершали приношения на жертвеннике для всесожжений и на жертвеннике для возжигания благовоний, неся службу в Святом Святых, чтобы очистить Израиль, по всем повелениям Божьего слуги Моисея.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 50,
        "text": "Вот потомки Аарона: его сын Элеазар, его сын Пинехас, его сын Авишуй,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 51,
        "text": "его сын Буккий, его сын Уззий, его сын Зерахия,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 52,
        "text": "его сын Мераиоф, его сын Амария, его сын Ахитув,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 53,
        "text": "его сын Цадок, его сын Ахимаац.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 54,
        "text": "Вот места их поселений, выделенные им в надел (они достались потомкам Аарона из клана Каафа, потому что им выпал первый жребий).",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 55,
        "text": "Им отдали Хеврон, что в земле Иуды, с его окрестными пастбищами,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 56,
        "text": "но поля и села вокруг города были отданы Халеву, сыну Иефоннии.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 57,
        "text": "Итак, потомкам Аарона отдали города-убежища Хеврон, Ливну, Иаттир, Эштемоа,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 58,
        "text": "Хилен, Давир,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 59,
        "text": "Ашан и Бет-Шемеш с их пастбищами.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 60,
        "text": "От рода Вениамина они получили Геву, Алемет и Анатот с их пастбищами. Городов, розданных каафитским кланам, было всего тринадцать.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 61,
        "text": "Остальные потомки Каафа по жребию получили десять городов от половины рода Манассии.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 62,
        "text": "Потомки Гершона – по своим кланам – получили тринадцать городов от родов Иссахара, Асира, Неффалима и той части рода Манассии, что в Башане.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 63,
        "text": "Потомки Мерари – по своим кланам – по жребию получили двенадцать городов от родов Рувима, Гада и Завулона.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 64,
        "text": "Так израильтяне отдали эти города с их пастбищами левитам.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 65,
        "text": "От родов Иуды, Симеона и Вениамина они получили по жребию те города, которые уже упомянуты.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 66,
        "text": "Некоторые из каафитских кланов получили в надел города от рода Ефрема.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 67,
        "text": "В нагорьях Ефрема они получили город-убежище Шехем, Гезер,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 68,
        "text": "Иокмеам, Бет-Хорон,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 69,
        "text": "Айялон и Гат-Риммон с их пастбищами.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 70,
        "text": "А из надела половины рода Манассии израильтяне дали остальным каафитским кланам Анер и Билеам с их пастбищами.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 71,
        "text": "Гершониты получили: от половины рода Манассии Голан, что в Башане, и Аштарот с их пастбищами.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 72,
        "text": "От рода Иссахара они получили Кедеш, Давраф,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 73,
        "text": "Рамот и Анем с их пастбищами.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 74,
        "text": "От рода Асира они получили Машал, Авдон,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 75,
        "text": "Хукок и Рехов с их пастбищами.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 6,
        "verse": 76,
        "text": "От рода Неффалима они получили Кедеш, что в Галилее, Хаммон и Кирьятаим с их пастбищами.",
        "translation_id": "NRT",
