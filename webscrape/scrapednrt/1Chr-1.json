[
    {
        "chapter": 1,
        "verse": 0,
        "text": "От Адама до Ноя",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 1,
        "text": ":1-32)",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 2,
        "text": "Адам, Сиф, Енос,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 3,
        "text": "Каинан, Малелеил, Иаред,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 4,
        "text": "Енох, Мафусал, Ламех.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 5,
        "text": "Ной и его сыновья Сим, Хам, Иафет. Потомки Иафета",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 6,
        "text": ":2-4)",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 7,
        "text": "Сыновья Иафета: Гомер, Магог, Мадай, Иаван, Тувал, Мешех и Тирас.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 8,
        "text": "Сыновья Гомера: Ашкеназ, Рифат и Тогарма.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 9,
        "text": "Сыновья Иавана: Элиша, Таршиш, Киттим и Роданим. Потомки Хама",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 10,
        "text": ":6-20)",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 11,
        "text": "Сыновья Хама: Куш, Мицраим, Пут и Ханаан.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 12,
        "text": "Сыновья Куша: Сева, Хавила, Савта, Раама и Савтеха. Сыновья Раамы: Шева и Дедан.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 13,
        "text": "Куш был отцом и Нимрода, который стал первым на земле могучим воином.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 14,
        "text": "Мицраим был отцом лудеев, анамеев, легавеев, нафтухеев,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 15,
        "text": "патрусеев, каслухеев (от которых произошли филистимляне) и кафтореев.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 16,
        "text": "Ханаан был отцом Сидона, первенца его, Хетта,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 17,
        "text": "а также иевусеев, аморреев, гергесеев,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 18,
        "text": "хиввеев, аркеев, синеев,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 19,
        "text": "арвадеев, цемареев и хаматеев. Потомки Сима",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 20,
        "text": ":21-29; 11:10-26)",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 21,
        "text": "Сыновья Сима: Елам, Ашшур, Арпахшад, Луд и Арам. Сыновья Арама: Уц, Хул, Гетер и Мешех.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 22,
        "text": "У Арпахшада родился Шелах, а у Шелаха родился Евер.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 23,
        "text": "У Евера родилось двое сыновей: одного звали Пелег (потому что в его дни земля была разделена), а его брата – Иоктан.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 24,
        "text": "Иоктан был отцом Алмодада, Шалефа, Хацармавета, Иераха,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 25,
        "text": "Гадорама, Узала, Диклы,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 26,
        "text": "Евала, Авимаила, Шевы,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 27,
        "text": "Офира, Хавилы и Иовава. Все они были сыновьями Иоктана.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 28,
        "text": "Сим, Арпахшад, Шелах;",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 29,
        "text": "Евер, Пелег, Реу;",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 30,
        "text": "Серуг, Нахор, Терах;",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 31,
        "text": "Аврам (позже он был назван Авраамом). Сыновья Авраама",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 32,
        "text": "Сыновья Авраама: Исаак и Измаил. Потомки Авраама от Агари",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 33,
        "text": ":12-16)",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 34,
        "text": "Вот их родословия: Невайот – первенец Измаила, Кедар, Адбеел, Мивсам,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 35,
        "text": "Мишма, Дума, Масса, Хадад, Тема,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 36,
        "text": "Иетур, Нафиш и Кедма. Это сыновья Измаила. Потомки Авраама от Хеттуры",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 37,
        "text": ":1-4)",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 38,
        "text": "Сыновья, которых родила Хеттура, наложница Авраама: Зимран, Иокшан, Медан, Мадиан, Ишбак и Шуах. Сыновья Иокшана: Шева и Дедан.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 39,
        "text": "Сыновья Мадиана: Ефа, Ефер, Енох, Авида и Элдага – все они потомки Хеттуры. Потомки Авраама от Сарры",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 40,
        "text": "Авраам был отцом Исаака. Сыновья Исаака: Исав и Израиль. Потомки Исава(A)",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 41,
        "text": "Сыновья Исава: Элифаз, Рагуил, Иеуш, Ялам и Корах.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 42,
        "text": "Сыновья Элифаза: Теман, Омар, Цефо, Гатам, Кеназ, от Тимны – Амалик.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 43,
        "text": "Сыновья Рагуила: Нахат, Зерах, Шамма и Миза. Исконные жители Эдома",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 44,
        "text": ":20-28)",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 45,
        "text": "Сыновья Сеира: Лотан, Шовал, Цивеон, Ана, Дишон, Ецер и Дишан.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 46,
        "text": "Сыновья Лотана: Хори и Гомам. Сестрой Лотана была Тимна.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 47,
        "text": "Сыновья Шовала: Алеан, Манахат, Евал, Шефо и Онам. Сыновья Цивеона: Айя и Ана.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 48,
        "text": "Сын Аны: Дишон. Сыновья Дишона: Хемдан, Эшбан, Ифран и Херан.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 49,
        "text": "Сыновья Ецера: Билган, Зааван и Яакан. Сыновья Дишана: Уц и Аран. Правители Эдома",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 50,
        "text": ":31-43)",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 51,
        "text": "Вот цари, которые правили в Эдоме еще до того, как в Израиле появились цари: Бела, сын Беора, чей город назывался Дингава.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 52,
        "text": "После смерти Белы царем вместо него стал Иовав, сын Зераха, из Боцры.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 53,
        "text": "После смерти Иовава царем вместо него стал Хушам из земли теманитян.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 54,
        "text": "После смерти Хушама царем вместо него стал Гадад, сын Бедада, который разбил мадианитян в земле Моава. Его город назывался Авит.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 55,
        "text": "После смерти Гадада царем вместо него стал Самла из Масреки.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 56,
        "text": "После смерти Самлы царем вместо него стал Саул из Реховота, что у реки.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 57,
        "text": "После смерти Саула царем вместо него стал Баал-Ханан, сын Ахбора.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 58,
        "text": "После смерти Баал-Ханана царем вместо него стал Гадад. Его город назывался Пау, а имя его жены было Мегетавель; она была дочерью Матреды, внучкой Мезагава.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 59,
        "text": "Потом умер и Гадад. Вождями Эдома были: Тимна, Алва, Иетет,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 60,
        "text": "Оголивама, Эла, Пинон,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 61,
        "text": "Кеназ, Теман, Мивцар,",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    },
    {
        "chapter": 1,
        "verse": 62,
        "text": "Магдиил и Ирам. Это вожди Эдома.",
        "translation_id": "NRT",
        "book_id": "1Chr",
        "book_name": "1 Chronicles"
    }
]